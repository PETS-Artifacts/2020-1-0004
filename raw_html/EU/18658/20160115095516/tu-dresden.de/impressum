    <script type="text/javascript">
        /* <![CDATA[ */
        // this is used to have validation and interpreted js at the same time
        if(document.cookie.search(/piwik_ignore=/) < 0)
        {
            document.write(unescape('%3Cscript src="https://piwik.wcms.tu-dresden.de/piwik.js" type="text/javascript"%3E%3C/script%3E'));
        }
        /* ]]> */
    </script>

    <script type="text/javascript">
        /* <![CDATA[ */
        // this is used to have validation and interpreted js at the same time
        jQuery(document).ready(function() {
            try
            {
                var piwikTracker = Piwik.getTracker("https://piwik.wcms.tu-dresden.de/piwik.php", ID_Piwik_Instanz);
                piwikTracker.setVisitorCookieTimeout(2592000); // 30 days
                piwikTracker.setCustomVariable (1, 'Language', page_lang, 'page');
                piwikTracker.trackPageView();
                piwikTracker.enableLinkTracking();
            } catch( err ) {}
        });
        /* ]]> */
    </script>


