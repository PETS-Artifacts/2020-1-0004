						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading">IMSLP:Privacy policy</h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
