                <script>
                    
                    $(document).ready(function() {
                        //real data initialize
                        if (false) loadChunkiesCall();
                        else loadChunkiesCallback();
                        
                        if(false || ( localStorage.getItem("isAutoSelectFirst") != null && localStorage.getItem("isAutoSelectFirst") == "true") ) {
                            scrollToElement("#KijijiNavigation", 0, true);
                        }else {
                            scrollToElement(".article-viewer", -90, true);
                        }
                    });
                    
                    function checkCurrentPlatForm(url) {
                        //console.log('url', url);
                        var platformVal = url.split('#')[1];
                        platformVal = platformVal == '' ? 'web' : platformVal.toLowerCase() == 'ios' ? 'iOS' : platformVal.toLowerCase() == 'android' ? 'android' : 'web';
                        
                        //set default to web
                        if( (platformVal == 'iOS' && ''.trim() == '') || ( platformVal == 'android' && ''.trim() == '') ){
                            platformVal = 'web';
                            window.location.href = '#web';
                        }
                        
                        //console.log(platformVal);
                        setActivePlatform($("[id$=" + platformVal + "View]"));
                    }
                    
                    function setActivePlatform(element) {
                        var activeClass = 'active';
                        element.parent().siblings().find('.' + activeClass).removeClass(activeClass);
                        element.addClass(activeClass);
                        
                        $('.platform-elements').hide();
                        $('.platform-elements.' + element.attr('title')).show();
                        
                        $('.titlePane').hide();
                        $("[id$=title" + element.attr('title') + "]").show();
                        
                        /*
                        if(true) {
                            scrollToElement(".article-viewer", -90, !false);
                        }
                        */
                        
                        if(true && localStorage.getItem("isAutoSelectFirst") != null && localStorage.getItem("isAutoSelectFirst") == "false" ) {
                            scrollToElement(".article-viewer", -90, !false);
                        }
                    }
                    
                    function loadChunkiesCallback() {
                        if(false) {
                            var url = document.URL;
                            if(url.indexOf('#') != -1){
                                checkCurrentPlatForm(url);
                            }else{
                                var isIOS = $("[id$=isIOS]").val();
                                var isAndroid = $("[id$=isAndroid]").val();
                                var platformVal = isIOS == 'true' ? 'iOS' : (isAndroid == 'true' ? 'android' : 'web');  
                                //console.log("platformVal ",platformVal);  
                                
                                //set default to web
                                if( (isIOS && ''.trim() == '') || ( isAndroid && ''.trim() == '') ){
                                    platformVal = 'web';
                                }
                                
                                window.location.href = '#' + platformVal;
                                setActivePlatform($("[id$=" + platformVal + "View]"));
                            }
                        }
                    }
                    
                    $(".clickablePlatformControll").click(function() {
                        setActivePlatform($(this));
                    });
                    
                    function scrollToElement(elementAnchor, alignOffset, isScroll) {
                        if(isScroll){
                            var offset = $(elementAnchor).offset().top + alignOffset;
                            $('html, body').animate({
                                scrollTop: offset
                            }, 500);
                        }
                    }
                    
                    function changePosition(vdoId){
                        scrollToElement(vdoId, -90, true);
                    }
                </script></span>
