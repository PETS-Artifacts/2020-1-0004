    <script>
    
        $(document).ready(function() {
            loadChunkyFAQsCall();
        });
        
        function loadChunkyFAQsCallback() {
            var isFAQOnTop = false;
            if(isFAQOnTop) {
                var scrolFaq = getRelevantArticles();
                $('.hidden-start').show();
                if(scrolFaq){
                    $('html, body').animate({
                        scrollTop: $("#containerFAQTop").offset().top - 82
                    }, 300);
                }
                if (isMobileSystem()) {
                    document.getElementById('meta_viewport').setAttribute('content', "width=device-width, initial-scale=1.0, maximum-scale=1");
                }
            }  
        }
        
