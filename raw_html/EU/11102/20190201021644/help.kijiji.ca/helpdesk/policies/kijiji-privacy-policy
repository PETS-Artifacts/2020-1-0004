	<script type="text/javascript">
  $(document).ready(function(){
      $("#dropdown_cat").change(function(){
       localStorage.setItem("isAutoSelectFirst", "true");
          $(this).find("option:selected").each(function(){
              var optionValue = $(this).attr("value");
              if(optionValue){
               var isShowRelated = true;
                  var a = $("#" + optionValue + " > a");
                  if(a){
                   isShowRelated = false;
                   window.location = a.attr('href');
                  }
                  var defaultArticle = $('.' + optionValue + ' > ul > li').first();
                  if(defaultArticle) {
                   var a = defaultArticle.children().first();
                   if(a){
                    isShowRelated = false;
                    window.location = a.attr('href');
                   }
                  }
                  $(".listCate").hide();
                  if(isShowRelated){
                   $(".listCate").not("." + optionValue).hide();
                   $("." + optionValue).show();
                  }
              } else{
                  $(".listCate").hide();
              }
          });
      });
      $(".list-justify.link-ul > a").click(function(){
       localStorage.setItem("isAutoSelectFirst", "false");
      });
  });
 </script>
