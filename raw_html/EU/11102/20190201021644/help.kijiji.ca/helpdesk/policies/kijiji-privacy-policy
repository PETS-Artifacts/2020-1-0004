    </style>
    <script type="text/javascript">
        $(document).ready(function(){
         var navbar = $("#nav-container");
         var stickyOffset = navbar.parent().parent().offset().top;
         
         $(window).scroll(function() {
          stickNavbar();
   });
         
         function stickNavbar(){
             if(window.pageYOffset >= stickyOffset){
              navbar.parent().addClass("sticky");
              navbar.parent().parent().addClass("sticky-parent");
              
             } else {
              navbar.parent().removeClass("sticky");
              navbar.parent().parent().removeClass("sticky-parent");
             }
         }
         
         $('.tabs.tabbable-line,.nav-tabs').animate({
             scrollLeft: $(".sub-cat > .nav-item.active").offset().left - 25
            }, 0);
         
         $('.tabs.tabbable-line,.nav-tabs').scroll(function(){
          //console.log($(this).width(), $(this).get(0).scrollWidth, $(this).scrollLeft());
          if($(this).width() + $(this).scrollLeft() >= $(this).get(0).scrollWidth){
           $(".gradient-rigth-side").hide();
          }else{
           $(".gradient-rigth-side").show();
          }
         });
        });
    </script>
