    <script type="text/javascript">
        function toggleCategoryLeft(elem, id) {
            if($(elem).closest("li").find('ul.tree > a').length > 0)$(elem).closest("li").find('ul.tree').toggle(300);
            if($(elem).closest("li").hasClass('active-cat')) $(elem).closest("li").removeClass('active-cat');
            else $(elem).closest("li").addClass('active-cat');
 
        }
        $(document).ready(function(){
         $('ul.nav-list.tree').find('a.selected').closest('ul.nav-list.tree').show();
            $('ul.nav-list.tree').find('a.selected').closest("li").addClass('active-cat');
        });
    </script>
    
