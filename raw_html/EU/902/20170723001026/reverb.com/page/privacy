<script>
  function loadZopim() {
    window.$zopim||(function(d,s){var z=$zopim=function(c){z._.push(c)},$=z.s=
    d.createElement(s),e=d.getElementsByTagName(s)[0];z.set=function(o){z.set.
    _.push(o)};z._=[];z.set._=[];$.async=!0;$.setAttribute('charset','utf-8');
    $.src='//v2.zopim.com/?2X5FhtTK3kNc7pohESyEim7wNmHGaI5s';z.t=+new Date;$.
    type='text/javascript';e.parentNode.insertBefore($,e)})(document,'script');
  
    var cachedStatus;
  
    window.$zopim(function configZopim() {
      var meta = JSON.parse(atob(document.querySelector('meta[name="current-user"]').content));
  
      window.$zopim.livechat.window.onShow(function() {
        window.Reverb.Metrics.track('zendesk_chat_opened');
      });
  
      if (meta.user.name !== '') {
        window.$zopim.livechat.setName(meta.user.name);
        window.$zopim.livechat.setEmail(meta.user.userContext.email);
      }
      window.$zopim.livechat.window.setSize('small');
      window.$zopim.livechat.departments.filter(''); // removes department selection dropdown
      window.$zopim.livechat.departments.setVisitorDepartment('US Support');
      window.$zopim.livechat.addTags('us_support');
  
      window.$zopim.livechat.setOnConnected(function() {
        window.$zopim.livechat.setOnStatus(function(status) { // status is one of 'online', 'away', or 'offline'
          var department = window.$zopim.livechat.departments.getDepartment('US Support');
  
          // this will call setOnStatus again, so we need to prevent
          // an infinite loop
          var newStatus = department.status;
          if (cachedStatus === newStatus) { return; }
          cachedStatus = newStatus;
          window.$zopim.livechat.setStatus(newStatus);
  
          var isOnline = (newStatus === 'online');
          Reverb.ZopimActions.onlineStatusUpdated(isOnline);
        });
      });
    });
  
    if (window.Reverb.MobileDetection.isMobileBrowser()) {
      // Hide Zopim on mobile when offline
      window.$zopim(function configZopim() {
        window.$zopim.livechat.button.setHideWhenOffline(true);
      });
    }
  }
  
  window.addEventListener('load', function() {
    window.setTimeout(loadZopim, 1000);
  });
</script>



