<script language="javascript">
	function chkEmail(str)
		{

			var at="@";
			var dot=".";
			var lat=str.indexOf(at);
			var lstr=str.length;
			var ldot=str.indexOf(dot);

			if (str.indexOf(at)==-1 || str.indexOf(at)==0 || str.indexOf(at)==lstr){
			   return false;
			}

			if (str.indexOf(dot)==-1 || str.indexOf(dot)==0 || str.indexOf(dot)==lstr){
			   return false;
			}

			 if (str.indexOf(at,(lat+1))!=-1){
			    return false;
			 }

			 if (str.substring(lat-1,lat)==dot || str.substring(lat+1,lat+2)==dot){
			    return false;
		 	}
			 if (str.indexOf(dot,(lat+2))==-1){
			    return false;
		 	}

			 if (str.indexOf(" ")!=-1)
			    return false;

 		 return true;

			 //  return (str.indexOf(".") > 2) && (str.indexOf("@") > 0) && (str.lastIndexOf(".")>str.indexOf("@"));
		}


function showHide(elementID){
			var divElement = $(elementID);

			if(divElement.style.display == "none" || divElement.style.display == "" ){
				divElement.style.display="block";
			}else{
				divElement.style.display="none";
			}
}
String.prototype.trim = function() {
	return this.replace(/^\s+|\s+$/g,"");
}

function loginFeedback() {


	var str_Text = $('textAreaInput').value;
	var feedback_email = $('feedback_email').value;
	var feedback_name = $('feedback_name').value;
	str_Text = str_Text.replace(/\?/g, "qMark");
	str_Text = str_Text.replace(/##/g, "\##");
	str_Text = str_Text.replace(/\%/g, "percent");
	str_Text = str_Text.replace(/\</g, " lt ");
	str_Text = str_Text.replace(/\>/g, " gt ");
	str_Text = str_Text.replace(/\'/g, " ");
	if(str_Text.trim() == ''){
		$('nameEmailError').innerHTML = 'Comment cannot be empty.';
		return;
	}
	if(feedback_name.trim() == '' ){
		$('nameEmailError').innerHTML = 'Name is a required field.';
		return;
	}
	if(feedback_email.trim() == ''){
		$('nameEmailError').innerHTML = 'Email is a required field.';
		return;
	}


	if(!chkEmail(feedback_email))
	{
		$('nameEmailError').innerHTML = 'Please enter a valid email.';
		return;
	}
	//str_Text = feedback_name+'$$'+feedback_email+'$$'+str_Text;
	if(str_Text.length > 2000){
		str_Text = str_Text.substring(0, 1999);
	}

	if(feedback_email.length > 50){
		feedback_email = feedback_email.substring(0, 49);
	}

	var url = '/secured/functions/Feedback/Feedback.cfc';
	var pars = 'uorgnameshort=nextjump&method=Insert_LoginFeedback&Comments='+str_Text+'&Topic=26&logid=588395907&email='+feedback_email+'&fulln='+feedback_name;

	var myAjax = new Ajax.Request( url, {method: 'get', parameters: pars, onLoading: login_showLoad(), onComplete: login_showResponse,'timeout':2000,'onTimeout':function(req){ alert('Timed Out!'); }} );


	return true;
}

function login_showLoad () {

	$('loginFeedbacktext').innerHTML = '<P><STRONG>Sending Feedback...</STRONG></P>';
}

function login_showResponse (originalRequest) {
	var newData = originalRequest.responseText;

	$('loginFeedbacktext').innerHTML = "<br><br>" + newData;
	setTimeout("$('loginFeedback').style.display='none';", 2000);
}

</script>


<script LANGUAGE="JavaScript" type="text/javascript">
function Set_Cookie( name, value, expires, path, domain, secure )
{
	// set time, it's in milliseconds
	var today = new Date();
	today.setTime( today.getTime() );
	/* if the expires variable is set, make the correct
	expires time, the current script below will set
	it for x number of days, to make it for hours,
	delete * 24, for minutes, delete * 60 * 24
	*/
	if ( expires )
	{
		expires = expires * 1000 * 60 * 60 * 24;
	}
	var expires_date = new Date( today.getTime() + (expires) );

	document.cookie = name + "=" +escape( value ) +
	( ( expires ) ? ";expires=" + expires_date.toGMTString() : "" ) +
	( ( path ) ? ";path=" + path : "" ) +
	( ( domain ) ? ";domain=" + domain : "" ) +
	( ( secure ) ? ";secure" : "" );
}

Set_Cookie('ClientResolution',screen.width + 'x' + screen.height,false,'/','.corporateperks.com',false);

</script>

