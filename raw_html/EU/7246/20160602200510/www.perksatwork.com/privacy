<script type="text/javascript">


/* FtumessagingController */


/* edp_LightboxController */
function showWelcome(cookieName, expires) {
	if(cookieName == null){
		cookieName = "welcomeMessage";
	}
	if(expires == null){
		expires = 365*20;
	}
	myOverlay = new Overlay();
	myOverlay.start(400,false);
	Set_Cookie(cookieName,true, expires, "/", "", false);
}

function cenergystart(){
	$('divbox').innerHTML = $('ltwrap2').innerHTML;
	cpoint = new Overlay();
 	cpoint.start(450,true);
	Set_Cookie("welcomeCenterPoint",true, 7300, "/", "", false);
}

