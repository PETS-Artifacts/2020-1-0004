      <script type='text/javascript'>
        if(!nxj) {
            var nxj = {};
        }
        if(!nxj.security) {
            nxj.security = {};
        }

        nxj.security.csrf = {};
        nxj.security.csrf.name = 'nxjcsrft';
        nxj.security.csrf.value = 'nxjtBELCK7Jd8JMbrE3OOn2-DsmE_jRSi7iZbTWaB1Pj0CfiwDJ_SgwjnGKabar9w-UgvOsp4U57_tEa3P-3ZwxW1pFLMrjhc01nhO99v7N8TG81uYQTcdpDRGAQaYFOVgJ9';

        if(window.Prototype) {
          Ajax.Request.addMethods({
            initialize: function($super, url, options) {
              if(!options) { options = {}; }
              if((!options.method) || options.method.toUpperCase() === 'POST') {
                if(!options.parameters && options.postBody) {
                    url+='?nxjcsrft=nxjtBELCK7Jd8JMbrE3OOn2-DsmE_jRSi7iZbTWaB1Pj0CfiwDJ_SgwjnGKabar9w-UgvOsp4U57_tEa3P-3ZwxW1pFLMrjhc01nhO99v7N8TG81uYQTcdpDRGAQaYFOVgJ9';
                    } else if(!options.parameters) {  
                      options.parameters = {'nxjcsrft' : 'nxjtBELCK7Jd8JMbrE3OOn2-DsmE_jRSi7iZbTWaB1Pj0CfiwDJ_SgwjnGKabar9w-UgvOsp4U57_tEa3P-3ZwxW1pFLMrjhc01nhO99v7N8TG81uYQTcdpDRGAQaYFOVgJ9'};
	                } else {
	                  if (typeof options.parameters === 'string') {
	                    options.parameters = options.parameters.toQueryParams();
	                  }
	                  options.parameters.nxjcsrft = 'nxjtBELCK7Jd8JMbrE3OOn2-DsmE_jRSi7iZbTWaB1Pj0CfiwDJ_SgwjnGKabar9w-UgvOsp4U57_tEa3P-3ZwxW1pFLMrjhc01nhO99v7N8TG81uYQTcdpDRGAQaYFOVgJ9';
	                }
	              }
	              $super(options);
	              this.transport = Ajax.getTransport();
	              this.request(url);
            }
          });

          Ajax.Response.addMethods({
            initialize: function(request){
              this.request = request;
              var transport  = this.transport  = request.transport,
                  readyState = this.readyState = transport.readyState;

              if ((readyState > 2 && !Prototype.Browser.IE) || readyState == 4) {
                this.status       = this.getStatus();
                this.statusText   = this.getStatusText();

                var resp = String.interpret(transport.responseText);

                var pattern = /<.*form.*method\s*=\s*["|']\s*post\s*["|'].*>/gi;
                var matches = resp.match(pattern);
                if (matches) {
                  for (var i=0; i<matches.length; i++) {
                    resp = resp.replace(matches[i], matches[i] + '<input type="hidden" name="nxjcsrft" value="nxjtBELCK7Jd8JMbrE3OOn2-DsmE_jRSi7iZbTWaB1Pj0CfiwDJ_SgwjnGKabar9w-UgvOsp4U57_tEa3P-3ZwxW1pFLMrjhc01nhO99v7N8TG81uYQTcdpDRGAQaYFOVgJ9" />')
                  }
                }

                this.responseText = resp;
                this.headerJSON   = this._getHeaderJSON();
              }

              if (readyState == 4) {
                var xml = transport.responseXML;
                this.responseXML  = Object.isUndefined(xml) ? null : xml;
                this.responseJSON = this._getResponseJSON();
              }
            }
          });

        } else {
          $(document).on('ajaxBeforeSend', function(e, xhr, options){
          if(!options) { options = {}; }
            if(options.type.toUpperCase() === 'POST') {
              if(!options.data) {
                options.data = 'nxjcsrft=nxjtBELCK7Jd8JMbrE3OOn2-DsmE_jRSi7iZbTWaB1Pj0CfiwDJ_SgwjnGKabar9w-UgvOsp4U57_tEa3P-3ZwxW1pFLMrjhc01nhO99v7N8TG81uYQTcdpDRGAQaYFOVgJ9';
              } else {
                options.data += '&nxjcsrft=nxjtBELCK7Jd8JMbrE3OOn2-DsmE_jRSi7iZbTWaB1Pj0CfiwDJ_SgwjnGKabar9w-UgvOsp4U57_tEa3P-3ZwxW1pFLMrjhc01nhO99v7N8TG81uYQTcdpDRGAQaYFOVgJ9';
              }
            }
          });

          $(document).on('ajaxSuccess', function(e, xhr, options){
            var resp = xhr.responseText;
            var pattern = /<.*form.*method\s*=\s*["|']\s*post\s*["|'].*>/gi;
            var matches = resp.match(pattern);

            if (matches) {
              for (var i=0; i<matches.length; i++) {
                resp = resp.replace(matches[i], matches[i] + '<input type="hidden" name="nxjcsrft" value="nxjtBELCK7Jd8JMbrE3OOn2-DsmE_jRSi7iZbTWaB1Pj0CfiwDJ_SgwjnGKabar9w-UgvOsp4U57_tEa3P-3ZwxW1pFLMrjhc01nhO99v7N8TG81uYQTcdpDRGAQaYFOVgJ9" />')
              }
            }

            xhr.responseText = resp;
          });

        }
