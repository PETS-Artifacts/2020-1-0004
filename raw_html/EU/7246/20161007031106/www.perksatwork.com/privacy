<script language="javascript">
function voteLike3(div1,div2){
    if(vote == 0){
        $(div1).removeClassName("off").addClassName("on");
        vote = 1;
        $(div2).removeClassName("on").addClassName("off");
    }
}

function voteDislike3(div1,div2){
    if(vote == 1){
        $(div1).removeClassName("on").addClassName("off");
        vote = 0;
        $(div2).removeClassName("off").addClassName("on");
    }
}
</script>
