//Load Pending Redeems
    function checkPendingRedemptions(){
    new Ajax.Request('/healthypointswidget/getpendingredemptions', {
        method:'get'
        ,parameters:{}
        ,onSuccess:function(t){
        	var pending = parseInt(t.responseText);

        	if(pending>0){
                if($('ptsTickerLinkRedeem')) {
                    $('ptsTickerLinkRedeem').setStyle({textDecoration:'none'});
                    $('ptsTickerLinkRedeem').update('<span style="text-decoration:underline;">Burn</span> <span style="color:red; font-weight:bold; font-size:14px; line-height:14px;">'+pending+'</span>');
                }

        		if($('usTab1') && $('usTab1').innerHTML == 'Redemptions' && 1 != 925){
        			$('usTab1').update('Redemptions <div style="position:relative; font-family:arial; top:-2px;color:white; background:red; border-radius:50%;padding:1px 5px;display:inline">'+pending+'</div>')
        		}
        	}else{
                if($('ptsTickerLinkRedeem')) {
                    $('ptsTickerLinkRedeem').update('<span style="text-decoration:underline;">Burn</span>');
                }

        		if($('usTab1') && $('usTab1').innerHTML.indexOf('Redemptions') !== false){
        			$('usTab1').update('Redemptions');
        		}
        	}
        }});
       }

function checkUserPendingTransactions(){
    new Ajax.Request('/healthypointswidget/getuserpendingtransactions', {
        method:'get'
        ,parameters:{}
        ,onSuccess:function(t){
            var pending = parseInt(t.responseText);

            if(pending>0){
                if($('ptsTickerLinkPending')) {
                    $('ptsTickerLinkPending').setStyle({textDecoration:'none'});
                    $('ptsTickerLinkPending').update('<span style="text-decoration:underline;">Pending</span> <span style="color:red; font-weight:bold; font-size:14px; line-height:14px;">'+pending+'</span>');
                    $('ptsTickerLinkPendingBar').show();    
                }
            }
        }
    });
}

function starAdvLightStars(){
    if (!starAdvDoLightStars || $('starAdvReadoutStar1') == null)
        return; // assume if first star absent, then all stars absent

    for (var i = 1; i <= 0; i++)
        setTimeout("starAdvLightStar("+i+");", i*400);

    if ($('celebrate')){
                    setTimeout("new Effect.Appear('celebrate')", i*400);
            }
}


function positionNew(){
    windowWidth = document.body.clientWidth;
    windowHeight = document.documentElement.clientHeight;
    var left = Math.round((windowWidth - 763)/2);
    if ($('ST_HP_Welcome')){
        $('ST_HP_Welcome').style.left = left + 'px';
    } else if ($('ST_Cat_Welcome')) {
        $('ST_Cat_Welcome').style.left = left + 'px';
    } else if ($('ST_ODP_Welcome')) {
        $('ST_ODP_Welcome').style.left = left + 'px';
    } else if ($('ST_OO_Welcome')) {
        $('ST_OO_Welcome').style.left = left + 'px';
    } else if ($('ST_Login_Welcome')) {
        $('ST_Login_Welcome').style.left = left + 'px';
    }

    if($('pts-ftu')){
        $('pts-ftu').style.left = Math.round((windowWidth - 900)/2) + 'px';
    }

    if(typeof setFooterPosition == 'function'){
        if($('ST_footer')){
            setFooterPosition();
        }else{
            window.onload = setFooterPosition;
        }
    }
}
