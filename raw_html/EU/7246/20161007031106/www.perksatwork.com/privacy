/* paw_HeaderController */
var userPoints = 0;
var degradeMode = 0;

Event.observe(window, 'load', function() {
    	 	 		 	     // Blackstone has requested that all their orgs do not show autocomplete until it can be fixed to hide disney
	 	     
	 	        positionNew();
	 	        // Flicker greeting name to fix chrome font issues
