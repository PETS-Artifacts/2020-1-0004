    // pts badge
    if ($('ptsBadge')) {
        $('ptsBadge').observe('click', function(event) {
            pointsActivity.toggleInfo();
            event.stop();
        });
    }

    if ($('ptsBadgeOffers')) {
        $('ptsBadgeOffers').observe('click', function(event) {
            pointsActivity.onclick();
        });
    }

var pointsActivity = {
	    badgeClicked: false,
	    browserIsIE: false,
	    infoClicked: false,

	    load: function() {
	        new Ajax.Request('/header/pointsactivity', {
	                        method: 'get',
	                        onSuccess: function(transport) {
	                            $('ptsBadgeOffers').update(transport.responseText);
	                            $('ptsBadge').update($('ptsBadgeNumActivities').innerHTML);
	                            new Ajax.Request('/header/showpointsactivitybadge', {
	                                            onSuccess: function(transport) {
	                                                if (transport.responseText != '1') {
	                                                    pointsActivity.badgeClicked = true;
	                                                }
	                                            }});
	                        }});
	    },

	    toggleInfo: function() {
	        if ($('ptsTickerBadgeBG').getStyle('visibility') == 'hidden') {
	            this.hideBadge();
	            this.showInfo();

	            if (!this.badgeClicked) {
	                this.badgeClicked = true;

	                // update session to record that badge clicked
	                new Ajax.Request('/header/badgeclicked', {
	                                parameters: {logRequired: 1}
	                });
	            }
	        } else {
	            this.hideInfo();
	        }
	    },

	    showInfo: function() {
	        $('ptsTickerNormalBG').setStyle({visibility: 'hidden'});
	        $('ptsTickerBadgeBG').setStyle({visibility: 'visible'});
	        $('starAdvArrow').src = 'http://imga.nxjimg.com/emp_image/UserRank/header/header_arrow_up.gif';
	    },

	    hideInfo: function() {
	        if (this.infoClicked) {
	            this.infoClicked = false;
	            return;
	        }

	        $('ptsTickerBadgeBG').setStyle({visibility: 'hidden'});
	        $('ptsTickerNormalBG').setStyle({visibility: 'visible'});
	        $('starAdvArrow').src = 'http://imga.nxjimg.com/emp_image/UserRank/header/header_arrow_down.gif';
	    },

	    showBadge: function() {
	        if ($('ptsBadgeContainer').getStyle('display') != 'none') {
	            return;
	        }

	        if (this.browserIsIE) {
	            $('ptsBadgeContainer').show();
	        } else {
	            new Effect.Appear('ptsBadgeContainer');
	        }
	    },

	    hideBadge: function() {
	        if ($('ptsBadgeContainer').getStyle('display') == 'none') {
	            return;
	        }

	        if (this.browserIsIE) {
	            $('ptsBadgeContainer').hide();
	        } else {
	            new Effect.Fade('ptsBadgeContainer');
	        }
	    },

	    onclick: function() {
	        this.infoClicked = true;
	    }
	};



