				(function ($) {
					$(function () {
						var $itemContent = $("#itemContent1");
						$("#expandButton1").click(function (e) {


							e.preventDefault();
							if ($itemContent.hasClass('opened')) {
								$itemContent.slideUp('slow').toggleClass('closed opened');

								$(this).css('z-index', 3000);
							} else {
								$itemContent.slideDown('slow').toggleClass('opened closed');
							}
							document.srchform.search.focus();
						});

						var $itemContent1 = $("#itemContent2");
						$("#expandButton2").click(function (e) {
							e.preventDefault();

							if ($itemContent1.hasClass('opened')) {
								$itemContent1.slideUp('slow').toggleClass('closed opened');

								$(this).css('z-index', 3000);
							} else {
								$itemContent1.slideDown('slow').toggleClass('opened closed');
							}
							document.srchform.search.focus();
						});

