	// Trackings
	function htrack_header(currenturl,linkurl){
		// Header Track <current url> <link to url>
		_gaq.push(['_trackEvent', 'Header', currenturl, linkurl]);
		//_em.trackAjaxPageview('Header_Accordion');
	}

	function ftrack_facebook()
	{
		// Footer Track
		_gaq.push(['_trackEvent', 'Footer', 'Facebook', 'click']); // Google Analytic
		//_em.trackAjaxPageview('Footer_Facebook'); // Effective Measures
	}
	function ftrack_twitter()
	{
		_gaq.push(['_trackEvent', 'Footer', 'Twitter', 'click']);
		//_em.trackAjaxPageview('Footer_Twitter');
	}
	function ftrack_rss()
	{
		_gaq.push(['_trackEvent', 'Footer', 'RSS', 'click']);
		//_em.trackAjaxPageview('Footer_RSS');
	}
</script>

