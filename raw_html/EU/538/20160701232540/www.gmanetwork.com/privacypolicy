<script type="text/javascript">

	//No drag2share for tablet
	if(!navigator.userAgent.match(/(iPhone|iPod|iPad|Android|BlackBerry)/)) {

		$(document).ready(function(){
			// make all .box draggable on page load/ready
			$('.drag').drag2share({
				icons: 'http://www.gmanetwork.com/themes/default/assets/img/social_icons/normal'
			});

			// make all .box draggable after being served by masonry's autoload
			$('.drag').live('mouseenter',function(){
				$('.drag').drag2share({
					icons: 'http://www.gmanetwork.com/themes/default/assets/img/social_icons/normal'
				});
			});
		});
	}
</script>


