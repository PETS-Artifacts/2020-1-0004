$( document ).ready(function() {


	$('.nav').not('.social-feeds .nav').tinyNav({
		active: 'selected',
		header: 'Home'
	});

	$("img.lazy").lazyload({
		effect: "fadeIn",
		effectspeed: 1000,
		load: function() {

			$(this).removeClass("lazy");

		}

	});

	$('.show_more_events').unbind().on('click',function(e){
		e.preventDefault();

		$('#more-events').toggle(function(){

			$('.show_more_events').remove();

			view_text = !view_text;

			if(view_text){
				$('.show_more_events').text('View Less');
			}else{
				$('.show_more_events').text('View More');
			}
		});
	});
});



function reapplyMasonry() {
	// create a central timer to re-initialize masonry
	if (MasonryObj !== undefined) {
		var cntr = 1;
		Timer = window.setInterval(function(){
			MasonryObj.masonry();
			if (cntr == 10) {
				clearInterval(Timer);
			}
			cntr++;
		}, 300);
	}
};

function initMasonry() {
	// initialize masonry and assign to a global variable
	var $container = $('#container').not('.promo-page #container, .promo-single-page #container, .no-masonry #container');
	var width = $(window).width();
	if (BANDILA == '1'){
		if (width < 900) {
			MasonryObj = $container.masonry({
				gutter: 0,
				columnWidth: 330,
				itemSelector: '.box, .box2',
				stamp: '.corner-stamp',
				transitionDuration: 0
			});
		}
		else{
			MasonryObj = $container.masonry({
				gutter: 0,
				columnWidth: 330,
				itemSelector: '.box',
				stamp: '.corner-stamp',
				transitionDuration: 0
			});
		}
	}
	else{
		MasonryObj = $container.masonry({
			gutter: 0,
			columnWidth: 330,
			itemSelector: '.box',
			stamp: '.corner-stamp',
			transitionDuration: 0
		});
	}



	/* Infinite Scroll Properties */
	$container.infinitescroll({
		navSelector   : '#page-nav',    // selector for the paged navigation
		nextSelector  : '#page-nav a',  // selector for the NEXT link (to page 2)
		itemSelector  : '.box:not(.corner-stamps)',     // selector for all items you'll retrieve
		animate       : true,
		debug      	: false,
		bufferPx      : 1200,
		prefill      	: true,
		extraScrollPx   : 50,

		loading: {
			finishedMsg: 'No more pages to load.',
			img: 'http://i.imgur.com/6RMhx.gif',
			speed: 'fast'
		}
	},


	function( newElements ) {

		var opts = $container.data('infinitescroll').options;
		var current_page = opts.state.currPage;

		
		// hide new items while they are loading
		var $newElems = $( newElements ).css({ opacity: 0 });

		$newElems.imagesLoaded(function(){
			// show elems now they're ready
			$newElems.animate({ opacity: 1 });
			$container.masonry( 'appended', $newElems, true );

			$("img.lazy").lazyload({
				effect: "fadeIn",
				effectspeed: 1000,
				load: function() {

					$(this).removeClass("lazy");

				}

			});

			var a = document.getElementById('myurl');
			$('.mrect').ready(function() {
				$(".mrect").height($(".mrect").height()   );

			});
		});
	});

	reapplyMasonry();
};

function insert_analytics() {
	$.getScript("http://www.gmanetwork.com/themes/default/assets/js/addga.js", function(){
	});
}

function del_cookie(name, path, domain)
{
	path = (path ? ";path=" + path : "");
	domain = (domain ? ";domain=" + domain : "");
	var expiration = "Thu, 01-Jan-1970 00:00:01 GMT";

	document.cookie = name + "=" + path + domain + ";expires=" + expiration;
}

$(window).resize(function(){
	setTimeout(function(){
		$('#container').masonry('destroy');
		initMasonry();
	}, 2000);

});


function changeclassname(type)
{
	if (type == 1)
	{
		var gilids = document.getElementsByClassName('corner-stamp');

		for (i=0;i<gilids.length;i++)
		{

			var tmpclassname = gilids[i].className;
			$('.' + tmpclassname).attr('class',tmpclassname + '1');
			////alert("< : " + tmpclassname);
		}

		var tmpattr = $('#gilid').attr('class');
		$('#gilid').attr('class','box2 ' + tmpattr);
	}
	else
	{
		var gilids = document.getElementsByClassName('corner-stamp1');
		for (i=0;i<gilids.length;i++)
		{
			var tmpclassname = gilids[i].className;

			tmpclassname = tmpclassname.substring(0,tmpclassname.length-1);
			$('.' + tmpclassname).attr('class',tmpclassname);
		}
	}
}


function ifTabletView(){

	_w = $(window).width();
	_h = $(window).height();

	tabletView = (_w >= 593 && _w <= 847)

	if(tabletView){
		return true;
	}else{
		return false;
	}

}

$(document).ready(function(){


	$('.slimscroll').slimscroll({
		height:'100%',
		wheelStep: 5
	});

	$('#logged_name').slimscroll({
		height:'45px',
		wheelStep: 5
	});

	//Featured rotate
	$("#featured").tabs({fx:{opacity: "toggle"}}).tabs("rotate", 5000, true);

	$('#featured').on({
		mouseenter : function(){
			$(this).tabs('rotate', 0, false);
		},
		mouseleave : function(){
			$(this).tabs({ fx: { opacity: 'toggle' } }).tabs('rotate', 5000, true);
		}
	});


	//scrolltotop
	$(window).scroll(function(){
		if ($(this).scrollTop() > 100) {
			$('.scrollup').fadeIn();
		} else {
			$('.scrollup').fadeOut();
		}
	});

	$('.scrollup').click(function(){
		$("html, body").animate({ scrollTop: 0 }, 600);
		return false;
	});

	/* HEADLINE SLIDER */
	var media = $('#media');
	var site = 'privacypolicy';

	if(site != 'kapusofoundation'){
		var defaults = {
			easing: 'easeOutElastic',
			slideClass: 'media',
			buildComplete: function () {
				media.css('visibility', 'visible');
			},
			timeout: 5500
		};
	}
	else{
		var defaults = {
			easing: 'easeOutElastic',
			slideClass: 'media',
			buildComplete: function () {
				media.css('visibility', 'visible');
			},
			timeout: 3500
		};
	}


	function build(x) {
		var opts, current;
		if (!$.isEmptyObject(media.data())) { /* If an zAccordion is found, rebuild it with new settings. */
			media.css('visibility', 'hidden');
			current = media.data('current');
			opts = $.extend({
				startingSlide: current
			}, defaults, x);
			media.zAccordion('destroy', {
				removeStyleAttr: true,
				removeClasses: true,
				destroyComplete: {
					afterDestroy: function() {
						try {
							console.log('zAccordion destroyed! Attempting to rebuild...');
						} catch (e) {}
					},
					rebuild: opts
				}
			});
		} else { /* If no zAccordion is found, build one from scratch. */
			media.css('visibility', 'hidden');
			opts = $.extend(defaults, x);
			media.zAccordion(opts);
		}
	}

	/**/
	enquire.register('screen and (min-width:900px)', { /* Standard screen sizes and a default build for browsers that are unsupported. */
		match : function () {
			build({
				slideWidth: 900,
				width: 990,
				height: 252
				//height: 230
			});
		} /* The *true* value below means this media query will fire by default. */
	}, true).register('(min-device-width : 600px) and (max-device-width: 1280px) and (orientation: portrait)', {
			match : function () {
				build({
					slideWidth: 590,
					width: 660,
					height: 168
					//height: 170
				});
			}
		}, true).register('(max-device-width: 1280px) and (orientation: landscape)', {
			match : function () {
				build({
					slideWidth: 900,
					width: 990,
					height: 252
					//height: 170
				});
			}
		}, true).register('screen and (min-width: 600px) and (max-width: 900px)', {
			match : function () {
				build({
					slideWidth: 590,
					width: 660,
					height: 168
					//height: 170
				});
			}
		}).register('screen and (max-width: 599px)', {
			match : function () {
				if (!$.isEmptyObject(media.data())) {
					media.zAccordion('destroy', {
						removeStyleAttr: true,
						removeClasses: true,
						destroyComplete: {
							afterDestroy: function() {
								try {
									console.log('zAccordion destroyed!');
								} catch (e) {}
							}
						}
					});
				}
			}
		}).listen(5);

	$(".artistdropdown").scrollectBox({
		preset: "dropdown",
		numVisibleOptions: 13,
		scrollOn: "hover",
		scrollInterval: 130
	});
	$(".artistdropdown").show();
	$(".sticky_content").show();
	$(".artistdropdown").css("min-width","200px");

		

	$('.article-inside').find('div.img-beforeAfter').find('img').each(function(){
		//height = $(this).height();
		//width = $(this).width();
		width = 630;
		height = 336;

		$(this).css({'height': height +'px' , 'width': width + 'px'} );

	});

	$('.article-inside').find('div.img-beforeAfter').each(function(){
		$(this).beforeAfter();

	});

	//$('.img-beforeAfter').beforeAfter();

	setTimeout(function(){
		initMasonry();
	}, 2000);


});

</script>

