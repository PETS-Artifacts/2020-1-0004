<script id="scroll-window">
    if (window.matchMedia == undefined || (window.matchMedia != undefined && window.matchMedia("(min-width: 641px)").matches)){
        var scroll_value = 0;

        if (document.body.className.indexOf("is-detail") > -1){
            scroll_value = parseInt("446");
        }

        if (document.body.className.indexOf("is-profile") > -1){
            scroll_value = parseInt("51");
        }

        window.scroll(0, scroll_value);
    }
</script>
