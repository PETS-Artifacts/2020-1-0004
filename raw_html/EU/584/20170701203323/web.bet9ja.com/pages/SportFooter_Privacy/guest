        function LoadPrenotazioneMoneyBack(lnkAvantiClientID, evt) {
            var keyCode = null;

            if (evt.which) {
                keyCode = evt.which;
            } else if (evt.keyCode) {
                keyCode = evt.keyCode;
            }

                   
            if (keyCode == 13) {
                evt.returnValue = false;
                evt.cancel = true;

                var btn = $get(lnkAvantiClientID); 
                btn.click();
            }
        }
        

         //Script per visualizzare la tooltuip con il cambio
         function showCambio(){
            $(".spanCambio").attr("title", "1 ₦ = " + arrotonda(getCambio(), 4).toString().replace(".", sepDec) + getSimboloValSel());
         }

        function mainRefreshCoupon(){
            if ( $('#g_w_PC_cCoupon_lnkMainRefresh').length>0){$('#g_w_PC_cCoupon_lnkMainRefresh')[0].click();}
        }

        
        function setFastAmount(linkClicked){
            var amount = Number($(linkClicked).attr("value"));
            var couponTypeID = $("#" + sHidTipoCoupon).val();

      
	        computeFastAmount($('#'+sTxtImporto), amount);
            computeFastAmount($('#'+sTxtImportoSis), amount);
            computeFastAmount($('#'+sTxtImportoDI), amount);
            
            switch(couponTypeID){
                case '1':
                    $("#" + sTxtImporto).focus();                                        
                    break;
                case '4':
                    $("#" + sTxtImporto).focus();                    
                    break;
                case '2':                    
                    $("#" + sTxtImportoDI).focus();                    
                    break;
                case '3':                    
                    $("#" + sTxtImportoSis).focus();                    
                    break;
            }
        }
		
        function computeFastAmount($objTxt, amount){
             
                if(amount > 0){
                    $objTxt.val( Number($objTxt.val()) + amount);
                }else{
                    $objTxt.val("");
                }
            
           
            $objTxt.triggerHandler('keyup');
        }

               


