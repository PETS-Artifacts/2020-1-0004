                        <script type="text/javascript">

                            function SearchName(e, txtObj) {
                                evt = e || window.event;
                                keyPress(evt);
                            }

                            function keyPress(evt) {
                                if ((evt.keyCode == 13) || (evt.which == 13) || (evt.charCode == 13)) {
                                    location = document.getElementById('g_w_cLogin_ctrlLogin_lnkBtnLogin').href;
                                    if (window.event) { window.event.returnValue = false; }
                                }
                                return true;
                            }

                            function ApriSSL() {
                                var URL = '';
                                if (URL != "") window.open(URL, 'SSL', 'scrollbars=no,resizable=yes,width=100,height=85,status=no,location=no,toolbar=no');
                            }

                            
                            $(document).ready(function () {
                                var objPwdSelector = "#g_w_cLogin_ctrlLogin_Password";
                                var objUsernameSelector = "#g_w_cLogin_ctrlLogin_Username";
                                var createLabel = function (objSelector, defaultvalue) {
                                    var newlabel = jQuery("<span/>", { class: "fieldLabel" }).html(defaultvalue);
                                    var currentValue = $(objSelector).val();
                                    $(objSelector).after(newlabel);
                                    $(newlabel).on("click", function (evt) {
                                        $(newlabel).hide();
                                        $(objSelector).show();
                                        $(objSelector).focus();
                                    });
                                    $(objSelector).on("focusout", function (evt) {
                                        var objThis = $(evt.target);
                                        var currentValue = objThis.attr("value");
                                        if (currentValue === "") {
                                            $(newlabel).show();
                                        };
                                    });
                                    $(objSelector).on("focus", function (evt) {
                                        $(newlabel).hide();
                                    });
                                    if (currentValue !== "") newlabel.hide();       

                                    $(objSelector).bind("input", function () {
                                        var field = $(this).next("span");
                                        field.hide();
                                    });
                                }
                                createLabel(objPwdSelector, "Password");
                                createLabel(objUsernameSelector, "Username");
                            });

                            function checkRequiredField_g_w_cLogin_phAnonimo(objThis, data) {
                                var objPwdSelector = "#g_w_cLogin_ctrlLogin_Password";
                                var objUsernameSelector = "#g_w_cLogin_ctrlLogin_Username";
                                var msgError = "Enter username and password to login";
                                $(objUsernameSelector)[($(objUsernameSelector).val() === "") ? "addClass" : "removeClass"]("req");
                                $(objPwdSelector)[($(objPwdSelector).val() === "") ? "addClass" : "removeClass"]("req");
                                if (($(objUsernameSelector).val() === "") || ($(objPwdSelector).val() === "")) alert(msgError);
                            }                   

                            
                                    
                        </script>

