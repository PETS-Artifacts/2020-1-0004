        $(document).ready(function () {
            $("#btnGMTList_link").click(function () {
                if ($(".GMT_content_wrapper").hasClass("big")) {
                    closeGMTList();
                } else {
                    $(".GMT_content_wrapper").addClass("big");
                    $(".GMT_content_wrapper div.GMTFlags div:not(.Sel)").show("fast");
                }
            });
        });
    
    function closeGMTList() {
        $(".GMT_content_wrapper div.GMTFlags div:not(.Sel)").hide("fast");
        $(".GMT_content_wrapper").removeClass("big");
    }
</script> 




