<script language="javascript" type="text/javascript">

    function showPopupCouponCheck(IDCoupon, path, IDBookmaker, IDUtente) {
        initializePopup("popupCCClose", "popupCC", "backgroundPopupCC");
        centerPopup("popupCC", "backgroundPopupCC");
        loadPopup("popupCC", "backgroundPopupCC");
         document.getElementById("iframeCC").src = path + IDCoupon + '&IDBookmaker=' + IDBookmaker + '&IDUtente='+IDUtente;
    }

    function couponCheckKey(e) {
        var keycode = (e.keyCode ? e.keyCode : e.which);
        if (keycode == '13') {

            var link = $("#g_w_PC_ctl09_lnkCheckCoupon");
            link[0].click();
            e.preventDefault();
            e.stopPropagation();
        }
    }

    function checkCouponDetail(code) {
        var inputBox = $("#g_w_PC_ctl09_txtCodiceCoupon");
        var link = $("#g_w_PC_ctl09_lnkCheckCoupon");        
        inputBox.val(code);
        link[0].click();
    }
</script>

