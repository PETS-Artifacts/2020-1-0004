<script type="text/javascript">
    var numScoRiserva=0;
    var jsRiservaLogged='True';
    var jsRiservaPageVisible = true;

    function checkScoRiserva(){
        //Chiamata al webservice che popola i dettagli del saldo
        //solo se la pagina risulta visibile
        if (jsRiservaPageVisible) {
            ISBets.WebPages.Controls.ScoRiservaWS.GetScoRiserva(0,0,
                function (results) {
                    OnScoRiservaRequestComplete(results);
                    setTimeout(function () { checkScoRiserva(); }, '15000');
                });
        } else {
            setTimeout(function () { checkScoRiserva(); }, '15000');
        }
    }
    
    //Viene chiamata al termine della chiamata asincrona al webservice che restituisce i dati
    function OnScoRiservaRequestComplete(results)
    {
         if (results != null)
         {
            if (numScoRiserva!=results){
                numScoRiserva=results;
                document.getElementById('g_w_PC_ScoRis_btnRefresh').click();
            }else{
                numScoRiserva=results;
            }
        }      
    }


    if (jsRiservaLogged !== 'True') {
        setTimeout(function () { checkScoRiserva(); }, '15000');
    }

    //CONTROLLING EVENTS IN jQuery
    $(document).ready(function() {
        //inizializzo il popup per il dettaglio
        initializePopup("popupContactDettCloseS", "popupContactSR", "backgroundPopupSR");

        //gestione visualizzazione pagina
        document.addEventListener( 'visibilitychange' , function() {
            if (document.hidden) {
                jsRiservaPageVisible = false;
            } else {
                jsRiservaPageVisible = true;
            }
        }, false );
    });   
    
    function showPopupSR(IDCoupon, IDCouponAsincrono,IDRisultatoSalvataggioCoupon, bookmakerId, utenteId) {
        centerPopup("popupContactSR", "backgroundPopupSR");
        loadPopup("popupContactSR", "backgroundPopupSR");

        if(IDCouponAsincrono != -1 && IDRisultatoSalvataggioCoupon != 0){
            document.getElementById("iframeSR").src='/Sport/BetDetailAsyncPopup.aspx?IDCoupon=' + IDCouponAsincrono + '&IDBookmaker='+ bookmakerId + '&IDUtente=' + utenteId;
        }else{
            document.getElementById("iframeSR").src='/Sport/BetDetailPopUp.aspx?IDCoupon=' + IDCoupon + '&IDBookmaker='+ bookmakerId + '&IDUtente=' + utenteId;
        }
    }
</script>





