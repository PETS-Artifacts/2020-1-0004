        function openPrenotatoreSco() {
            //alert("prenotatoresco");

            $("#iframePrenotatoreSco").attr("src", "../../Sport/PrenotatoreScommesse.aspx");
            $.event.trigger('CouponPrenotatoreSco'); 

            
