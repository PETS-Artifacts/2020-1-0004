    <script type="text/javascript">
    (function(w) {           
        var addBetStats = function (event, platform, user_id, sportsBetStatus, betAmount, numOfSelections) {
            if ((typeof dataLayer !== "undefined") &&
                (typeof dataLayer.push === "function")) {
                dataLayer.push({ event: event, platform: platform, user_id: user_id, sportsBetStatus: sportsBetStatus, betAmount: betAmount, numOfSelections: numOfSelections });
            }
        };
        $("body").bind("CouponConfirmed", function (evt, data) {                
            addBetStats("sportsBetPlaced", "desktop", data.idUtente.toString(), ((data.idEsito < 0) ? "fail" : "success"), data.ImportoTotale, data.CountSottoEventi);
        });
        $("body").bind("CouponCanceled", function (evt, data) {                
            addBetStats("sportsBetPlaced", "desktop", data.idUtente.toString(), "cancel", data.ImportoTotale, data.CountSottoEventi)
        });
    })(window)
</script>
    
