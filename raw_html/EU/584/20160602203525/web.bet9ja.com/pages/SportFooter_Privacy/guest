<div id="g_w_PC_cSport_pnlSelectors" class="SportSelector">
	
    <script  type="text/javascript">
        var sTimeValuesSteps = ["2","10","30","240","720","1"]   
        var sTimeHdrSteps = ["Today","1<br />hr","3<br />hrs","24<br />hrs","72<br />hrs","ALL"]        

        function initSportTimeSlider() {
            //init header slider tempo
            $.each(sTimeHdrSteps, function (index, value) {
                $('#sportTimeSliderHdr').append('<div class="s' + index + '">' + value + '</div>');
            });

            var index = sTimeValuesSteps.indexOf('1');
            $('#sportTimeSliderHdr DIV').removeClass("sel");
            $('#sportTimeSliderHdr .s' + index).addClass("sel");
            
            //init slider tempo
            $("#sportTimeSlider").slider({
                max: sTimeValuesSteps.length - 1,
                min: 0,
                value: $.inArray('1', sTimeValuesSteps),
                range: "min",
                animate: true,
                orientation: "horizontal",
                step: 1,
                stop: function (event, ui) {
                    $("#sportTimeSlider").mouseup();
                    $.event.trigger("oddsTimeSliderChanged", sTimeValuesSteps[ui.value]);

                    var index = sTimeValuesSteps.indexOf(sTimeValuesSteps[ui.value]);

                    $('#sportTimeSliderHdr DIV').removeClass("sel");
                    $('#sportTimeSliderHdr .s' + index).addClass("sel");
                }
            });
        }  

        $(document).ready(function () {
            initSportTimeSlider();
            
            $('#divMenuSportGSX').bind("oddsTimeSliderChanged", function (e, timeValue) {  
                $("#g_w_PC_cSport_hidSportTime").val(timeValue);
                $("#g_w_PC_cSport_btnChangeTime").click(); 
            }); 
        });

    </script>
    <div class="Title">Upcoming in next:</div>
