        function LoadPrenotazione(lnkAvantiClientID, evt) {
            var keyCode = null;

            if (evt.which) {
                keyCode = evt.which;
            } else if (evt.keyCode) {
                keyCode = evt.keyCode;
            }

                   
            if (keyCode == 13) {
                evt.returnValue = false;
                evt.cancel = true;

                var btn = $get(lnkAvantiClientID); 
                btn.click();
            }
        }
        

