<script language="javascript" type="text/javascript">
    var numScoRiserva=0;
    var jsRiservaLogged='True'

    function checkScoRiserva(){
        //Chiamata al webservice che popola i dettagli del saldo
        ISBets.WebPages.Controls.ScoRiservaWS.GetScoRiserva(0, OnScoRiservaRequestComplete);
    }
    
    //Viene chiamata al termine della chiamata asincrona al webservice che restituisce i dati
    function OnScoRiservaRequestComplete(results)
    {
         if (results != null)
         {
            if (numScoRiserva!=results){
                numScoRiserva=results;
                document.getElementById('g_w_PC_ScoRis_btnRefresh').click();
            }else{
                numScoRiserva=results;
            }
         }
    }
    
    if(jsRiservaLogged != 'True'){ setInterval("checkScoRiserva()", '15000');}

    
 
