      <script type="text/javascript">
    var NaN_api = NaN_api || [];
    (function() {
      var s = document.createElement('script');
      s.async = true;
      s.src = '//cdn.nanigans.com/NaN_tracker.js';
      var h = document.getElementsByTagName('head')[0];
      h.appendChild(s);
    })();

    var trackNanigansEvent = function(event, userId){
      var userConfig = [320259];

      if (typeof userId !== 'undefined') {
        userConfig.push(userId);
      }

      NaN_api.push(userConfig);
      NaN_api.push(event);
    };
  </script>

    

