                                name="deliveryTime"
                                ng-model="delInfo.delTime"
                                ng-change="delTimes.timeChange()"
                                ng-options="time.name for time in delTimes.getDeliveryTimes(delInfo.delDay) track by time.value" required>
                        </select>
                    </div>
                </div>
