                                ng-model="delInfo.delDay"
                                ng-change="onDayChange()" required>
                            <option value="today">Today</option>
                            <option value="tomorrow">Tomorrow</option>
                        </select>
                    </div>
                </div>
                <div class="col-6 home-form-time">
