        //});

    </script>


    
    <script>
        function onAcceptPrivacyCookies() {
            $.ajax({
                type: "GET",
                url: '/home/acceptprivacypolicycookies',
                success: function (result) {
                    if (result.success == true) {
                        $("#divAcceptPrivacyPolicy").css("display", "none");
                    }
                    else {
                        $("#divAcceptPrivacyPolicy").css("display", "block");
                    }
                },
                error: function (result) {
                    alert('error');
                }
            });
        }
    </script>
