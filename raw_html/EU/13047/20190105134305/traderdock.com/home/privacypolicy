    <script type="text/javascript">

        // Banner Slider
        $(document).ready(function () {

            var selectedSubmenu = 'submenu_' + '';
            $('.navbar-nav li.active').removeClass('active');
            $("#" + selectedSubmenu).addClass('active');

            $(".banner-slider").owlCarousel({
                navigation: true,
                pagination: false,
                singleItem: true,
                navigationText: ["<i class='fa fa-long-arrow-left'></i>", "<i class='fa fa-long-arrow-right'></i>"],
                lazyLoad: true
            });

            var FootersessionData = '';
            if (FootersessionData == 'True') {
                //Show popup
            }
            else {
                GetFooterCMSData();
            }

            //Set the carousel options
            $('#carousel-example-generic').carousel({
                pause: true,
                interval: 3000,
            });

            //Home page Top Slider
            $('#myCarousel').carousel({
                interval: 4000
            });

            var clickEvent = false;
            $('#myCarousel').on('click', '.nav-pills a', function () {
                clickEvent = true;
                $('.nav-pills li').removeClass('active');
                $(this).parent().addClass('active');
            }).on('slid.bs.carousel', function (e) {
                if (!clickEvent) {
                    var count = $('.nav-pills').children().length - 1;
                    var current = $('.nav-pills li.active');
                    current.removeClass('active').next().addClass('active');
                    var id = parseInt(current.data('slide-to'));
                    if (count == id) {
                        $('.nav-pills li').first().addClass('active');
                    }
                }
                clickEvent = false;
            });
            /*.on('translate.owl.carousel', function (e) {
            alert(8);
            }).on('changed.owl.carousel', function (e) {
            alert(8);
            }).on('translated.owl.carousel', function (e) {
            alert(8);
            });*/
        });
        function GetFooterCMSData() {
            $.ajax({
                type: "GET",
                url: '/home/footer',
                success: function (result) {
                    if (result.success == true) {
                        $("#FooterDataID").html(result.Footer_data);
                    }
                    else {
                        //$("#Noti_data").html(result.Notification_data);
                    }
                },
                error: function (result) {
                    alert('error');
                }
            });
        }
        // Banner Slider
        //$(document).ready(function () {
        //    $(".banner-slider").owlCarousel({
