<script>
    function GetUserSignUpPopup() {
        var MemberID = '';
        var id = '';
        var sBalance = '';
        var sMonthLimit = '';
        if (MemberID != '' || MemberID != "") {
            if (MemberID > 0) {
                $('#ReviewPlanSubscriptionLimit').text(sBalance + " Challenge  ");
                $("#Review_subscription_id").val(id);
                $("#currentSubscriptionId").val(id);
                //$('select>option:eq(' + id + ')').attr('selected', true);
                $('#ReviewPlanSubscriptionRate').text("$" + sMonthLimit + "/ month");
                //$("#reviewSubscriptionModal").modal();
                callRevSubscriptionModel(id);
                $("#sBalance").val(sBalance);
                //$('select>option:eq(' + id + ')').attr('selected', true);
                $("#sMonthLimit").val(sMonthLimit);
                $.ajax({
                    type: "Get",
                    url: '/account/getprofileaddress',
                    data: {},
                    success: function (result) {
                        if (result.success == true) {
                            result.Data.address;
                            result.Data.state_name;
                            result.Data.city_name;
                            result.Data.country_id;
                            result.Data.postal_code;
                            $("#Address1").val(result.Data.address);
                            $("select option:contains(" + result.Data.country_name + ")").attr('selected', 'true');
                            $("#administrative_area_level_11").val(result.Data.state_name);
                            $("#locality1").val(result.Data.city_name);
                            $("#postal_code1").val(result.Data.postal_code);
                        }
                        else {
                            alert("Something went wrong!");
                        }
                    },
                    error: function (result) {
                        alert('error');
                    }
                });
                $("#DateYear").datepicker({
                    format: "mm/yyyy",
                    viewMode: "months",
                    minViewMode: "months",
                    autoclose: true,
                });
            }
        }
        else {
            $('#PlanSubscriptionLimit').text(sBalance + " Challenge  ");
            $("#subscription_id").val(id);
            $('#PlanSubscriptionRate').text("$" + sMonthLimit + "/month");
            callProfileModel(parseInt(id));
            $("#sBalance").val(sBalance);
            $("#sMonthLimit").val(sMonthLimit);
            $("#DateYear").datepicker({
                format: "mm/yyyy",
                viewMode: "months",
                minViewMode: "months",
                autoclose: true,
            });
        }
    }
</script>



<script>
    function callProfileModel(id) {
        $(".full-loarder").css("display", "none");
        $('#createProfileModal').load('/home/callprofile/' + id, function () {
            $(this).one('show.bs.modal', function () {
                $("form").removeData("validator");
                $("form").removeData("unobtrusiveValidation");
                $.validator.unobtrusive.parse("form");
            }).modal({
                backdrop: 'static',
                keyboard: false
            }).load(
              jQuery.getScript('https://maps.googleapis.com/maps/api/js?key=' + $('#GooglePlaceAPIKey').val() + '&libraries=places&callback=initAutocomplete'),
              callPaypal()
            );
        });
    }
</script>
<script>
    function callPaypal() {
        var script = document.createElement('script');
        script.onload = function () {
            //do stuff with the script
        };
        script.src = "//www.paypalobjects.com/api/checkout.js";
        document.head.appendChild(script); //or something of the likes
        //alert(200);
    }
    window.paypalCheckoutReady = function () {
        paypal.checkout.setup("support@traderdock.com", {
            environment: 'live',
            click: function (event) {
                event.preventDefault();
                //in case new subscription when already logged in - by shoaib (21mar2018)
                var MemberID = '';
                if (MemberID != '' || MemberID != "") {
                    if (MemberID > 0) {
                        if ($('#mdlProfileForm').valid()) {
                            paypal.checkout.initXO();
                            //$("#addPaymentModal").hide();
                            $.support.cors = true;
                            $(".full-loarder").css("display", "block");

                            var bFirstNameValidated = false;
                            var bCheckBoxValidated = false;
                            var vaddress = $("#Address1").val();
                            var vcountryid = $('#country1 option:selected').val();
                            var vstatename = $("#administrative_area_level_11").val();
                            var vcityname = $("#locality1").val();
                            var vpostcode = $("#postal_code1").val();
                            var vcheckbox = $("#ReviewCheckBox").val();
                            var vsubscriptionid = $("#Review_subscription_id").val();
                            var vpromo_code_id = $("#promo_code_id").val();
                            var model = {
                                subscription_id: vsubscriptionid,
                                AcceptsTerms: bCheckBoxValidated,
                                country_id: vcountryid,
                                state_name: vstatename,
                                city_name: vcityname,
                                address: vaddress,
                                postal_code: vpostcode,
                                promo_code_id: vpromo_code_id
                            };

                            $.ajax({
                                type: "POST",
                                contentType: "application/json; charset=utf-8",
                                dataType: "json",
                                url: '/account/editprofileaddress',
                                data: JSON.stringify(model),
                                success: function (result) {
                                    if (result) {
                                        $("#reviewSubscriptionModal").modal('hide');
                                        $(".full-loarder").css("display", "none");
                                        $('#hdnRegisterMemberID').val(result.id);
                                        $("#DateYear").val("");
                                        $("#CVV").val("");
                                        //CallPaymentModel(result.id, vsubscriptionid,vpromo_code_id);

                                        $.ajax({
                                            url: '/pay/paywithpaypal',
                                            type: "GET",
                                            async: true,
                                            crossDomain: true,
                                            data: {
                                                PaymentType: "ppal",
                                                user_id: $('#hdnRegisterMemberID').val(),
                                                subscription_id: vsubscriptionid,
                                                promo_code_id: ($("#promo_code_id").val()),
                                                platform_id: $('input[name=rbPlatform]:checked').val()
                                            },
                                            //Load the minibrowser with the redirection url in the success handler
                                            success: function (token) {
                                                var url = paypal.checkout.urlPrefix + token;
                                                //Loading Mini browser with redirect url, true for async AJAX calls
                                                paypal.checkout.startFlow(url);
                                                $(".full-loarder").css("display", "none");
                                            },
                                            error: function (responseData, textStatus, errorThrown) {
                                                alert("Due to a period of inactivity, we are unable to process this transaction");
                                                //Gracefully Close the minibrowser in case of AJAX errors
                                                paypal.checkout.closeFlow();
                                                $(".full-loarder").css("display", "none");
                                            }
                                        });
                                    }
                                    else {
                                        alert("Something went wrong!");
                                    }
                                },
                                error: function (result) {
                                    alert('error');
                                }
                            });
                            return false;
                        }
                    }
                }
                else {
                    //in case new subscription when already logged in - by shoaib (21mar2018)

                    //for signup - by shoaib 19mar2018
                    if ($('#mdlProfileForm').valid()) {
                        paypal.checkout.initXO();
                        //$("#addPaymentModal").hide();
                        $.support.cors = true;
                        $(".full-loarder").css("display", "block");
                        var vfn = $("#popup_first_name").val();
                        var vln = $("#popup_last_name").val();
                        var vemail = $("#popup_user_email").val();
                        var vpassword = $("#popup_password").val();
                        var vsubscriptionid = $("#subscription_id").val();
                        var vcountryid = $("#popup_country").val();
                        var vstateid = $("#popup_state_name").val();
                        var vcity = $("#popup_city_name").val();
                        var vaddress = $("#popup_address").val();
                        var vpostcode = $("#popup_postal_code").val();
                        var vcontact = $("#popup_contact_number").val();
                        var vPromoCode = $("#promo_code_id").val();
                        var vPlatform = $('input[name=rbPlatform]:checked').val();
                        var bCheckBoxValidated = false;
                        if ($('#AcceptsTerms').is(":checked") != true) {
                            bCheckBoxValidated = false;
                        }
                        else {
                            bCheckBoxValidated = true;
                        }

                        var model = {
                            first_name: vfn,
                            last_name: vln,
                            user_email: vemail,
                            password: vpassword,
                            confirm_password: vpassword,
                            subscription_id: vsubscriptionid,
                            country_id: vcountryid,
                            city_name: vcity,
                            state_name: vstateid,
                            AcceptsTerms: bCheckBoxValidated,
                            address: vaddress,
                            contact_number: vcontact,
                            postal_code: vpostcode,
                            rbPlatform: vPlatform
                        };
                        //$("#createProfileModal").hide();
                        $(".full-loarder").css("display", "block");
                        $.ajax({
                            type: "POST",
                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            url: 'https://www.traderdock.com/Account/Register',
                            data: JSON.stringify(model),
                            success: function (result) {
                                if (result.success) {
                                    $("#createProfileModal").modal('hide');
                                    $(".full-loarder").css("display", "none");
                                    $('#hdnRegisterMemberID').val(result.id);
                                    $("#DateYear").val("");
                                    $("#CVV").val("");
                                    //alert(300);
                                    $.ajax({
                                        url: '/pay/paywithpaypal',
                                        type: "GET",
                                        async: true,
                                        crossDomain: true,
                                        data: {
                                            PaymentType: "ppal",
                                            user_id: $('#hdnRegisterMemberID').val(),
                                            subscription_id: $("#subscription_id").val(),
                                            promo_code_id: ($("#promo_code_id").val()),
                                            platform_id: $('input[name=rbPlatform]:checked').val()
                                        },
                                        //Load the minibrowser with the redirection url in the success handler
                                        success: function (token) {
                                            var url = paypal.checkout.urlPrefix + token;
                                            //Loading Mini browser with redirect url, true for async AJAX calls
                                            paypal.checkout.startFlow(url);
                                            $(".full-loarder").css("display", "none");
                                        },
                                        error: function (responseData, textStatus, errorThrown) {
                                            alert("Due to a period of inactivity, we are unable to process this transaction");
                                            //Gracefully Close the minibrowser in case of AJAX errors
                                            paypal.checkout.closeFlow();
                                            $(".full-loarder").css("display", "none");
                                        }
                                    });
                                    //callPaypal();//added by shoaib - for paypal payment directly as we have now only one option Paypal Checkout - 19mar2018
                                    //CallPaymentModel(result.id, vsubscriptionid,vPromoCode); //commented by shoaib - 19mar2018
                                    //$("#addPaymentModal").modal();
                                }
                                else {
                                    alert("Something went wrong!");
                                }
                            },
                            error: function (result) {
                                alert(result);
                            }
                        });
                    }
                    //for signup - by shoaib 19mar2018
                }
                //old code here - now moved up
            },
            button: ['btPaypal']
        })
    };
</script>

<script type="text/javascript">
    function ChangeUserEmail() {
        $(".full-loarder").css("display", "block");
        $.ajax({
            type: "POST",
            url: 'https://www.traderdock.com/Account/CheckUserEmailExists',
            data: { email: $("#user_email").val() },
            success: function (result) {
                $(".full-loarder").css("display", "none");
                if (result.success == true) {
                    $('#spanEmail').text("Email id already exist.");
                    $("#btnProfileSubmit").attr("disabled", true);
                    $("#btPaypal").attr("disabled", true);
                    $("#btAW").attr("disabled", true);
                    $("#emailerrormsg").text("Email id already exist.");
                    $("#emailerrormsg").css("display", "block");
                    $("#btnSave").attr("disabled", true);
                }
                else {
                    $('#spanEmail').text('');
                    $("#btnProfileSubmit").attr("disabled", false);
                    $("#btPaypal").attr("disabled", false);
                    $("#btAW").attr("disabled", false);
                    $("#emailerrormsg").css("display", "none");
                    $("#btnSave").attr("disabled", false);
                }
            },
            error: function (xhr, status, p3, p4) {
                //alert("error");
                $(".full-loarder").css("display", "none");
            }
        });
    }
    function ChangeUserEmailPopup() {
        $(".full-loarder").css("display", "block");
        $.ajax({
            type: "POST",
            url: 'https://www.traderdock.com/Account/CheckUserEmailExists',
            data: { email: $("#popup_user_email").val() },
            success: function (result) {
                $(".full-loarder").css("display", "none");
                if (result.success == true) {
                    $('#spanEmail').text("Email id already exist.");
                    $("#btnProfileSubmit").attr("disabled", true);
                    $("#btPaypal").attr("disabled", true);
                    $("#btAW").attr("disabled", true);
                    $("#btnSave").attr("disabled", true);
                }
                else {
                    $('#spanEmail').text('');
                    $("#btnProfileSubmit").attr("disabled", false);
                    $("#btPaypal").attr("disabled", false);
                    $("#btAW").attr("disabled", false);
                    $("#btnSave").attr("disabled", false);
                }
            },
            error: function (xhr, status, p3, p4) {
                //alert("error");
                $(".full-loarder").css("display", "none");
            }
        });
    }

    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode != 40 && charCode != 41 && charCode != 43 && charCode != 45 && charCode > 31 && (charCode < 48 || charCode > 57))
            return false;
        return true;
    }
</script>