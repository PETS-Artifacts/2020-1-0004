            <script>
    $(function () {
        $(".acceptCookie").on("click", function () {
            var d = new Date();
            d.setTime(d.getTime() + (600 * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = "cookie_widget_darkgrey=1; " + expires + "; path=/";
            $("#notice-cookies").hide();
        });
    });
</script>

