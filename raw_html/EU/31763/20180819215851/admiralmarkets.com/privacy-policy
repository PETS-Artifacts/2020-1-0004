    function CPA_SEGMENT_EVENT(param) {
        var _PARAM = param;
        var _EVENT = $(_PARAM.selector);

        if (typeof analytics !== 'undefined') {
            analytics.trackLink(_EVENT, _PARAM.title, {
                action:     'click',
                category:   'cta',
                CTA:        _PARAM.cta,
                label:      _PARAM.label,
                pageTitle:  document.title,
                pageUrl:    location.href
            });
        }
    }

