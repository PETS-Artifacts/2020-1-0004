<script type="text/javascript">
document.querySelector('.box-dropdown-wrapper .nav-login--link').addEventListener('click', function(e){
var active = !this.classList.contains('is-active');
if(active){
var link = document.querySelector('.lk--customer-login');
if(!link.classList.contains('is-active')){
link.click();
/* hide steps */
var stepBoxes = document.querySelectorAll('div.login-box-step--form');
for (i = 0; i < stepBoxes.length; i++) {
stepBoxes[i].style.display='none';
}
}
}
});
document.querySelector('.wrapper-nav .nav-login--link').addEventListener('click', function(e){
var link = document.querySelector('.lk--customer-login');
link.click();
});
document.querySelector('.wrapper-nav .nav-login--eschenker-back').addEventListener('click', function(e){
var link = document.querySelector('.nav-login--back');
link.click();
});
</script>
