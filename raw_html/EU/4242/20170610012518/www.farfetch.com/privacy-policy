!function(){var e=document.getElementsByClassName("ff-nav-a ff-drag-level condensed letter-spacing uppercase "),n=0,l=document.createElement("ul");l.className="ff-nav-ul",l.innerHTML='<li class="ff-nav-title"><span class="condensed letter-spacing uppercase">THE STYLE GUIDE</span></li><li><a class="ff-nav-a no-color" href="/style-guide/?ffref=hd_snav" title="All stories">All stories</a></li>';for(var a=0,s=e.length;s>a;a++){if(-1!==e[a].innerHTML.toUpperCase().indexOf("EDITORIAL")){n++;for(var t=e[a].nextElementSibling.children,i=0,r=t.length;r>i;i++)if(0===t[i].children.length){t[i].appendChild(l.cloneNode(!0));break}}if(2===n)break}}();
