	<!-- Declare General functions -->
	<script>
		$.fn.bindFirst=function bindFirst(eventType,fn) {
		    this.on(eventType, fn);
		    this.each(function() {
		    	var handlers = $._data(this, 'events')[eventType.split('.')[0]];
		        var handler = handlers.pop();
		        handlers.splice(0, 0, handler);
		    });
		}
		
	   	function noErrorsOnForm(siteForm) {
			//add information_message negative
			var errors = $(siteForm).find('.form_field_error');
			return errors.length === 0;
		}
	   		
	   	function getScriptParentNode(scriptId){
	   		var scriptTag = document.getElementById(scriptId);
	   		if (!scriptTag) return null;
	   		
	   		return scriptTag.parentNode;
	   	}
	   	
	   	function isParentValid(parent){
	   		if (!parent) return false;
	   		if ("BODY" === parent.tagName) return false;
	   		
	   		return true;
	   	}
	   	
		function setProductData(productEnclosingTag, productName, productLocation, productPosition){
	   		$(productEnclosingTag).data('gtmProductLocation', productLocation);
	   		$(productEnclosingTag).data('gtmProductPosition', productPosition);
	   		$(productEnclosingTag).data('gtmProductName', productName);	
		}
	
	   	function productLocationNotSet(productEnclosingTag){    		
	   		return !$(productEnclosingTag).data('gtmProductLocation');
	   	}
	   	
	   	function getProductLocation(currentElement, defaultLocation){
	   		var productLocation = $(currentElement).closest(":data(gtmProductLocation)").data("gtmProductLocation");
	   		if (!productLocation) productLocation = defaultLocation;
	   		
	   		return productLocation;
	   	}
	   	
	   	function getProductPosition(currentElement){
	   		var productPosition = $(currentElement).closest(':data(gtmProductPosition)').data('gtmProductPosition');

	   		return productPosition;
	   	}
	   	
	   	function getProductName(currentElement){
	   		var productName = $(currentElement).closest(':data(gtmProductName)').data('gtmProductName');
	   		
	   		return productName;
	   	}
	   	
	   	function addProductImpression(productObj){
	   		window.impressions = window.impressions || [];
	   		window.impressions.push(productObj);
	   	}
  	   	
	   	function pushProductImpressionsOnReady() {
	   		$(document).ready(function() {
	   			pushGtmV2ProductImpressions(window.impressions);
	   		});
	   	}
	   	   	
	   	function pushGtmV2ProductImpressions(products) {
   			if(products){
				dataLayer.push({
					'event': 'eCommerceEvent',
					'eCommerceEventLabel': 'productImpression',
					'ecommerce': {
						 'impressions': products
				 	}
				}); 
   			}
	   	}
	   	
		function productClickPush(productSKU, productName, productLocation, productPosition) {
			dataLayer.push({
				'event': 'eCommerceEvent',
				'eCommerceEventLabel': 'productClick',
			    'ecommerce': {
			    	'click': {
			        	'actionField': {
			        		'list': productLocation
			        	},      
			       		'products': [{
			          		'id': productSKU,
			          		'name': productName, 
			        		'position': productPosition
			         	}]
			       	}
			     }
			  });
		};
	</script>
	

	
	



	


	<!-- Declare Google Tag Manager DataLayer -->	
	<script>
		window.dataLayer = [{
												
			currency : 'GBP',
			
			
				'loggedIn' : 'no',
			
			
			
			page_url : location.href,
			
			
			
			
				
					
				
				
					
				
					
										
					
						
				
				
									
						
							
										
							
										
							
						
							
							
							
									
							page_type : 'Generic Page',
									
					
				
			
			
			page_name : "Privacy Policy | Travis Perkins",
			
			
				
				
					




	
	basket_id: "",
    product_code : "",
    product_name : "",
  	product_price: "",
  	product_quantity: "", 
  	basket_page_revenue : "",
  	total_items : "",
  	
  	quote_id: "",
  	
    quote_product_code : "",
    quote_product_name : "",
    
    
    
    quote_product_quantity: "", 
  	quote_total_items : "0"
		  	
				
			
		}];
	</script>
	<!-- End Declare Google Tag Manager DataLayer -->


	
	


	<!-- Declare Google Tag Manager container -->
	<noscript>
		<iframe
			src="//www.googletagmanager.com/ns.html?id=GTM-KGT66B"
			height="0" width="0" style="display: none; visibility: hidden"></iframe>
	</noscript>
	<script>
		var id = 'GTM-KGT66B';
		var startGTM = function(w, d, s, l, i) {
			w[l] = w[l] || [];
			w[l].push({
				'gtm.start' : new Date().getTime(),
				event : 'gtm.js'
			});
	
			var f = d.getElementsByTagName(s)[0], j = d.createElement(s), dl = l != 'dataLayer' ? '&l='
					+ l
					: '';
	
			j.async = true;
			j.src = '//www.googletagmanager.com/gtm.js?id=' + i + dl;
			f.parentNode.insertBefore(j, f);
		}
	
		startGTM(window, document, 'script', 'dataLayer', id);
	</script>
	<!-- End Declare Google Tag Manager container -->
	

	
	<script>var gtmV2JsInitialized = true</script>
	
	

	


	<!-- Common eCommerce activity tracking -->
	<script>
		pushProductImpressionsOnReady();	
	</script>
 	


	<script>
	if(window.gtmV2JsInitialized) {
		$(document).ready(function() {
			/* click event does not work in several browsers, when clicking on wheel.*/
			$('a[href*="/p/"]').bindFirst('mousedown', function() {
	 			var regexp=/\d{6,}\/?$/;
	 			var link=$(this).attr("href");
	 			if(regexp.test(link)){
					var productSKU = regexp.exec(link);
					if(productSKU.indexOf("/")!==-1)
						productSKU = productSKU.slice(0, productSKU.lastIndexOf("/"));
	
					var productLocation = getProductLocation(this, "On Page: Location not specified");
					var productName = getProductName(this);
					var productPosition = getProductPosition(this);
					
					productClickPush(productSKU, productName, productLocation, productPosition);
	 			}
			}); 
		});
	}
	</script>

	



	<!-- End Common eCommerce activity tracking -->	


