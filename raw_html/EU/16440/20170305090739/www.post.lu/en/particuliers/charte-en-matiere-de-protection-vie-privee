<script type='text/javascript'>
google.load('search', '1', {language: 'en', style: google.loader.themes.V2_DEFAULT});

var autoCompletionOptions = {
  'maxCompletions' : 5,
  'matchType' : 'any',
  'validLanguages' : 'fr,en,de',
  'styleOptions' : {
    'xOffset' : 10
}};

google.setOnLoadCallback(function() {
  var customSearchOptions = {};
  var orderByOptions = {};
  orderByOptions['keys'] = [{label: 'Relevance', key: ''} , {label: 'Date', key: 'date'}];
  customSearchOptions['enableOrderBy'] = true;
  customSearchOptions['orderByOptions'] = orderByOptions;
    var customSearchControl =   new google.search.CustomSearchControl('006746485285158440373:axvgsapi2pu', customSearchOptions);
    customSearchControl.setResultSetSize(google.search.Search.FILTERED_CSE_RESULTSET);
  var options = new google.search.DrawOptions();
  options.setSearchFormRoot('search-block');
    google.search.CustomSearchControl.attachAutoCompletionWithOptions("006746485285158440373:axvgsapi2pu", document.getElementById('q'), 'search-block', autoCompletionOptions);
  }, true);
</script>
	
