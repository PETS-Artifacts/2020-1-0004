</style><div class="newsletter"><section class="newsletter__container"><header><h2 class="newsletter__headline">Love cannabis?</h2><h3 class="newsletter__subhead">Keep up with new strains, products, trends, and deals with Leafly’s curated cannabis newsletter.</h3></header><form class="newsletter__form"><input class="newsletter__input" type="email" name="email" placeholder="Email Address" required="required" aria-label="Email Address"/><button class="newsletter__button btc-button btc-button--primary">Sign Up</button></form><span class="error-message" id="newsletter-signup-error" style="display: none;">Please enter a valid email address and try again.</span><div class="newsletter__disclaimer"><p>By submitting this form, you will be subscribed to news and promotional emails from Leafly and you agree to Leafly’s Terms of Service and Privacy Policy. You can unsubscribe from Leafly email messages anytime.</p></div><script>(function() {
    const newsletterModal = document.querySelector('.newsletter');


    const setCookie =  (name, value, maxAgeInDays) => {
      const maxAge = `max-age=${maxAgeInDays * (60 * 60 * 24)}`;
      return document.cookie = `${name}=${value}; ${maxAge}; domain=${window.sharedCookieDomain}`;
    }

    newsletterModal.addEventListener("click", (event) => {
        if (event.target.contains(newsletterModal)) {
            setCookie('displayNewsLetterModal', 'true', 14 );
            return newsletterModal.classList.remove('show');
        }
    });

    const newsletterSignup = document.querySelector(".newsletter__form");
    if (!window.fetch) {
        newsletterSignup.style.display = "none";
    };

    newsletterSignup.addEventListener("submit", (event) => {
        event.preventDefault();
        const button = document.querySelector(".newsletter__button");
        button.setAttribute("disabled", true);

        const email = document.querySelector(".newsletter__input");
        const data = JSON.stringify({email: email.value})

        return fetch("/cephalopod/header/newsletter-signup", {
            method: "POST",
            headers: {
                'Accept': 'application/json, text/plain, */*',
                'Content-Type': 'application/json'
            },
            body: data
        }).then(function (response) {
            if (response.status && response.status === 200) {
                setCookie('displayNewsLetterModal', 'true', 365.25);
                newsletterSignup.innerHTML = "<p>Thanks for signing up!</p>";
            } else {
                button.removeAttribute("disabled");
                document.getElementById("newsletter-signup-error").style.display = "block";
            }
            return false;
        });
    });
})();
