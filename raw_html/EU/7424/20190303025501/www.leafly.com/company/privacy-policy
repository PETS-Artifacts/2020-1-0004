<script type="text/ng-template" id="m-age-gate.html">
    <div class="copy-centered sans-serif terms-modal">
        <h2 class="copy-lg copy-bold spacer-bottom-xl">Are You Eligible to Visit Leafly.com?</h2>
        <div ng-show="message" class="l-grid padding-rowItem--lg">
            <div class="padding-rowItem--lg" ng-bind-html="message" ng-hide="denied"></div>
            <div class="padding-rowItem--lg" ng-show="denied">We're sorry, we can't let you in.</div>
            <div class="grid-1">
                <a class="m-button m-button--green m-button--xl" ng-click="answer(true)">Yes, I Am</a>
            </div>
            <div class="grid-1">
                <a class="m-button m-button--link m-button--xl copy--bold" ng-click="answer(false)">No, I Am Not</a>
            </div>
            <div class="copy--sm copy-sm--md color--light">If you keep seeing this age prompt whenever you visit Leafly.com, please enable cookies in your web browser. Thank you!</div>
        </div>
        <form name="TermsForm" class="spacer-horizontal-md" ng-if="!message">
            <div row class="spacer-bottom-sm">
                <div col="1" class="relative">
                    <fieldset>
                        <legend class="screen-reader-text">I am at least 21 years old or a valid medical marijuana patient and agree to the <a href="/company/tos">Terms of Use</a></legend>
                        <input type="checkbox" id="acceptedTermsOfUse" ng-model="acceptedTermsOfUse">
                        <label for="acceptedTermsOfUse" class="terms-checkbox" ng-class="{active:acceptedTermsOfUse}"></label>
                    </fieldset>
                </div>
                <div col="11" class="copy-left">
                    I am at least 21 years old or a valid medical marijuana patient and agree to the <a href="/company/tos">Terms of Use</a>
                </div>
            </div>
            <div class="spacer-vertical-lg">
                <div class="error copy-xs gutter-bottom-xxs" ng-cloak 
                      ng-show="TermsForm.$dirty && !acceptedTermsOfUse" ng-messages="TermsForm.$error">To continue, please check the box above</div>
                <button class="terms-button spacer-vertical-md copy-bold" ng-disabled="!acceptedTermsOfUse" ng-click="accept()">Continue</button>
            </div>
            <div class="terms-disclaimer">
                Leafly operates in compliance with state laws regarding access to cannabis. If you keep seeing this age prompt whenever you visit Leafly.com, please enable cookies in your web browser.
            </div>
        </form>
    </div>
</script>
