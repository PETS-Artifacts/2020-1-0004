row-justify-md="center" row-justify-xs="start"><div col-xs="12" col-sm="11" col-md="11"><div row="row" row-justify-lg="center" id="footer-newsletter-signup"><div class="form gutter-bottom-xxs" col="col"><div class="heading-xxs copy-bolder">Newsletter Signup</div><form id="newsletter-signup-form__footer"><div class="conjoined"><input class="conjoined-input" type="email" name="email" placeholder="Email Address" aria-label="Email Address" required="required"/><span class="conjoined-button"><button class="button button-green">Sign Up</button></span></div><span class="error-message" id="newsletter-signup-error" hide="hide">Please enter a valid email address and try again.</span></form></div></div><script>(function() {
    const newsletterSignup = document.getElementById("newsletter-signup-form__footer");
    if (!window.fetch) {
        newsletterSignup.style.display = "none";
    }

    newsletterSignup.addEventListener("submit", (event) => {
        event.preventDefault();
        const button = document.querySelector("#newsletter-signup-form__footer button");
        button.setAttribute("disabled", true);

        const email = document.querySelector("#newsletter-signup-form__footer input[name=email]");
        const data = JSON.stringify({email: email.value})

        return fetch("https://www.leafly.com/cephalopod/footer/newsletter-signup", {
            method: "POST",
            headers: {
                'Accept': 'application/json, text/plain, */*',
                'Content-Type': 'application/json'
            },
            body: data
        }).then(function (response) {
            if (response.status && response.status === 200) {
                newsletterSignup.innerHTML = "<p>Thanks for signing up!</p>";
            } else {
                button.removeAttribute("disabled");
                document.getElementById("newsletter-signup-error").style.display = "block";
            }
            return false;
        });
