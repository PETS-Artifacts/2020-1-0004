<script type="text/ng-template" id="m-review-modal.html">
    <div class="modal-header copy--centered">
        <div class="clearfix">
            <button class="m-button m-button--icon modal-close-button" ng-click="$close($event);">
                <i class="icon-close-x"></i><br>
            </button>
        </div>
        <h2 class="l-centered gutter-bottom-lg heading--md copy--bold copy--capitalize" ng-if="!submitted">Rate This Strain</h2>
    </div>
    <div class="modal-body--hasheader">

        <div class="review" ng-if="!submitted">

            <form ng-submit="submit(reviewForm)" class=" [ m-form m-form--stacked ] review__form clearfix">

                <div class="review-rating">
                    <label for="rating" class="rating-label">Overall Review</label>
                    <rating class="rating-input colored rating-stars pull-right" name="rating" data-ng-model="reviewForm.Rating" required min="1" data-max="5" data-state-off="'icon-star-empty'" data-state-on="'icon-star-filled'"></rating>
                </div>

                <div class="error copy--xs" ng-show="error">You must rate this strain one to five stars</div>

                <div class="review-notes-group clearfix">
                    <label for="reviewText" class="padding-rowItem">What do you think about this strain?</label>
                    <textarea class="notes-group__notes grid-1" name="reviewText" id="" data-ng-model="reviewForm.Notes" rows="10" cols="30"></textarea>
                </div>

                <div>
                    <input id="private" type="checkbox" class="m-checkbox" data-ng-model="reviewForm.Private">
                    <label for="private">Make review private</label>
                </div>

                <button class="ga_Strain_Review_Quick m-button m-button--xl m-button--green button-save-review" type="submit" ng-disabled="disabled || !reviewForm.Rating || !reviewForm.Notes">Save</button>
            </form>
        </div>
        <div class="review--success" ng-if="submitted">
            <h2 class="l-centered gutter-bottom-lg">Thank You for Your Rating!</h2>
            <aside class="copy--md l-centered">Tell us more about your experience.</aside>
            <button class="ga_Strain_Review_Quick_Thanks m-button m-button--xl m-button--xl-wide m-button--green gutter-vertical-md" ng-click="continue();$close()">Ok, add details</button>
            <button class="m-button m-button--link review__not-now-btn" ng-click="$close()">Not now</button>
        </div>
    </div>
</script>


