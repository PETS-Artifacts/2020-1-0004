<script type="text/javascript">

    var quickRegisterLoginUrl = 'Quick/Index';

    var DeepLinkApi = function () { };

    DeepLinkApi.prototype = {
        LoadQuickRegister: function (redirectionUrl) {
            $.ajax({
                url: quickRegisterLoginUrl,
                data: {
                    url: redirectionUrl
                },
                success: function (res) {
                    var apiPopup = new frontApi();
                    apiPopup.manageAjaxResult(res);
                }
            });
        }
    }

    $(document).ready(function () {
        if (typeof displayQuick !== 'undefined' && displayQuick !== '') {

            var deeplinkApi = new DeepLinkApi();
            deeplinkApi.LoadQuickRegister(displayQuick);
        }
    });

</script>


