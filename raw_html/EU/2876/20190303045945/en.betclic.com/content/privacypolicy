    <!-- Live chat button script-->
<!--ADD THE FOLLOWING CODE SOMEWHERE BELOW YOUR BUTTON CODE-->

    <script>

        //The code bellow unbind the event before bind it again, so we only have one event attached to it and we can guarantee only one popup opened per click.
        $(".liveEngageChat").off("click");

        $(".liveEngageChat").click(function () {
            
            var popupName = 'popupLivechat';

            // if the popup with a given name already exists, 'window.open' brings it to the front ( focus ).
            // if it doesn't exist, a blank popup is opened.
            var popupChat = window.open('', popupName, 'toolbar=no,status=no,menubar=no,scrollbars=no,resizable=no,left=-0, top=-0, width=482, height=450, visible=none', true);

            var isBlankPage = false;

            try {
                // The check if it is a blank page is inside a try/catch block because when the livechat is already opened, we get a cross-origin exception when accesing the 'popupChat.location.href'
                isBlankPage = popupChat.location.href == 'about:blank'
            }
            catch (err) {
                //A SecurityError is expected when the chat is already opened, so we show only not expected errors.
                if (err.name != 'SecurityError') {
                    console.log(err);
                }
            }
            finally {
                //if it is a blank page, it means that the livechat is not started yet, so we start it.
                if (isBlankPage) {
                    liveagent.startChatWithWindow(sfButtonCode, popupName);
                }
                else {
                    popupChat.focus();
                }
            }
        });
    </script>

</div>
