<script>
    $(function () {

        // Monitor the content change in order to refresh the bonuses after a deposit
        var contentContainer = document.querySelector("div#quickDepositPopup div.content");

        var contentObserver = new MutationObserver(function (mutations) {
            mutations.forEach(function () {
                if (document.querySelector("div#depositConfirmMessageContainer") !== null && $("div#depositConfirmMessageContainer").is(":visible")) {
                    $("div#qdOptinAction").empty();
                    $("div#depositConfirmMessageContainer button.quickDepo").click(function () {
                        $("div#qdOptinAction").load('/account/bonus/quickdepositoptinbonus');
                    });
                }
            });
        });

        contentObserver.observe(contentContainer, {
            characterData: true,
            childList: true
        });
    });
</script>

