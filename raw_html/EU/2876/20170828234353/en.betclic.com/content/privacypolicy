    <script type="text/javascript">

        (function () {
            function getScript(url, success) {
                var script = document.createElement('script');
                script.src = url;
                var head = document.getElementsByTagName('head')[0],
                    done = false;
                script.onload = script.onreadystatechange = function () {
                    if (!done && (!this.readyState || this.readyState == 'loaded' || this.readyState == 'complete')) {
                        done = true;
                        success();
                        script.onload = script.onreadystatechange = null;
                        head.removeChild(script);
                    }
                };
                head.appendChild(script);
            }
            getScript('https://c.la1-c1-frf.salesforceliveagent.com/content/g/js/40.0/deployment.js', function () {
                var element = document.createElement("script");
                element.type = "text/javascript";
                element.text = "var sfButtonCode = '57358000000L48Y'; liveagent.init('https://d.la1-c1-frf.salesforceliveagent.com/chat', '57258000000L3bP', '00D58000000JG9K'); if (!window._laq) { window._laq = [];} window._laq.push(function() { liveagent.showWhenOnline(sfButtonCode, document.getElementById('liveagent_button_online')); liveagent.showWhenOffline(sfButtonCode, document.getElementById('liveagent_button_offline'));});"
                    + "liveagent.addCustomDetail('Application Id', 'BETCLICCOM'); liveagent.addCustomDetail('Site Code', 'gben'); ";
                document.body.appendChild(element);
            });
        })();
    </script>
