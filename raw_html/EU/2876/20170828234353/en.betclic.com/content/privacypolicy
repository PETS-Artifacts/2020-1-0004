            $subscribeSubLnks.each(function() {
                
                if ($(this).attr('href').indexOf('Expired') != -1) {
                    $btnReg.toggleClass('buttonRegister').toggleClass('buttonRegisterDisable');
                    $(this).attr('href', 'javascript:void(0);');
                } else {
                    var ChallengeId = $(this).attr('href').substring(25);
                    $(this).attr({
                        'href': '/ui/challenge/subscribe' +'/'+ChallengeId,
                    'data-ajax': 'true',
                    'data-ajax-success': 'PopupOnSuccess'
                    });
                }
            });
        });
    </script>

