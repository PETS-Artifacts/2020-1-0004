    <script type="text/javascript">
        $(document).ready(function () {
            $('a.cr-submit').click(function () {
                var form = $(this).closest('#cr-form');

                var valid = validateForm(form);
                var mailParent = $('#cr-email-parent', form);
                showValidStatus(mailParent, true);
                if ($('#chb_Promo', form).attr('checked') == 'checked') {
                    if (!isEmail($('#cr-email', form).attr('value'))) {
                        valid = false;
                        showValidStatus(mailParent, false);
                    }
                }


                if (!valid) {
                    $('#cr-required-fields', form).css('color', '#FF3900');
                    $('#cr-required-fields', form).addClass('invalid');
                    return false;
                }
                $('#cr-required-fields', form).css('color', '');
                $('#cr-required-fields', form).removeClass('invalid');
                var data = getFormData(form);

                data += "||SpecialOffers~~";
                data += ($('#chb_Promo', form).attr('checked') == 'checked') ? '&#x2611;' : '&#x2610;';
              
                submitForm('C53_CALLBACK', data, $('#cr-email', form).attr('value'), function (result) {
                    $('#cr-entry', form).hide();
                    $('#cr-success', form).show();

                    if (result == true) {
                        var date = $('#cr_select_date', form).attr('value');
                        var time = $('#cr_select_time', form).attr('value');
                        $('#success-text', form).html($('#success-text', form).html().replace('{date}', date).replace('{time}', time));
                        $('#success-text', form).show();
                        $('#error-text', form).hide();
                        $(".contactus-back2").click(function () {
                            $(this).closest(".contact-popup").find(".contact-slide-1").show();
                            $(this).closest(".contact-popup").find(".contact-slide-2").hide();
                            return false;
                        });
                    }
                    else {
                        $('#success-text', form).hide();
                        $('#error-text', form).show();
                    }
                });
                return false;
            });
        });
</script>

