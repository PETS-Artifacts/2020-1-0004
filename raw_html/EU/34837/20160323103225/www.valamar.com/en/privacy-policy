<script type="text/javascript">
    $(document).ready(function () {
        $('#gift-voucher-submit').click(function () {
            var data = $('#email').attr('value');
            if (!isEmail(data) && !isPhone(data)) {
                $('#gift-voucher-error').show();
                return false;
            }

            $('#gift-voucher-error').hide();

            submitForm('GIFT_VOUCHER', getFormData('#gift-voucher-entry'), '', function (result) {
                if (result == true) {
                    $('#gift-voucher-entry').hide();
                    $('#gift-voucher-success').show();
                }
                else {
                    alert('greska');
                }
            });

            $('#email').attr('value', '');
        });
    });
</script>

