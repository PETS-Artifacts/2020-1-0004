<script src="https://cdn8.kizi.com/assets/jquery.blueimp-gallery.min-a84835cbf733079889c037f3615448d9.js" type="text/javascript"></script>

<script>
    document.getElementsByClassName('game_thumbs').onclick = function (event) {
        event = event || window.event;
        var target = event.target || event.srcElement,
            link = target.src ? target.parentNode : target,
            options = {index: link, event: event},
            links = this.getElementsByTagName('a');
        blueimp.Gallery(links, options);
    };

    $('#collections_wrapper a').hover(function () {
        $(this).children('div').show('slow');
    }, function () {
        $(this).children('div').hide().stop(true, true);
    });
</script>
