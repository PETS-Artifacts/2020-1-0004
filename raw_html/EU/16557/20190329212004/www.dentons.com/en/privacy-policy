        jQuery(document).ready(function ($) {
            $('a[rel^="mailto"]').click(function () {
                var strtitle = $('title').text().replace(/Dentons - /i, '');               
                    dataLayer.push({
                        'event': 'click',
                        'category': "Email Me",
                        'action': this.rel.replace(/^mailto:/i, ''),
                        'label': window.location.pathname
                    });               
            });
        });
