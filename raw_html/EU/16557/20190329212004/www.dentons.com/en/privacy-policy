  <script type="text/javascript">
      function OpenRedirectionDisclaimer() {
          window.onload = function () {

              $(".RedirectionDisclaimer-bar").css("display", "block").find(".RedirectionDisclaimer-bar").slideDown("slow");
              var totalCookieHeight = 0;
              if ($(window).width() <= 1024) {
                  var cookieHeight = $('.search-results.RedirectionDisclaimer-bar').children().each(function () {
                      totalCookieHeight += parseInt($(this).outerHeight(true));
                  });
              } else {
                  totalCookieHeight = parseInt($('.search-results.RedirectionDisclaimer-bar .cookies-text').outerHeight(true));
              }
              officeDisclamer(totalCookieHeight + 20);
          }
      }
      
</script>

