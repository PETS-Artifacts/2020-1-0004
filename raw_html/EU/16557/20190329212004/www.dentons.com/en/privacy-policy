<script type="text/javascript">
    function SubmitForm() {
        var keyID = (window.event) ? event.keyCode : keyEvent.keyCode;
        if (keyID === 13) {
            document.getElementById("buttonSearchHeader").click();
        }
    }

    function onscrollToAttribute(name) {
        if ($(".home_wrapper").size() == 0) {
            $(name).find(' a,input[type=text],select,textarea').filter(':visible:first').focus();
        } else {
            if (name == '#idCarousel') {
                name = ".home_wrapper";
                $(name).find(' a,input[type=text],select,textarea').filter(':visible:first').focus();
            } else {
                $(name).find(' a,input[type=text],select,textarea').filter(':visible:first').focus();
            }
        }
        $(window).scrollTop($(name).offset().top);

    }  


</script>
