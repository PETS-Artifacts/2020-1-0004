    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $('a[rel^="mailto"]').click(function () {
                var strtitle = $('title').text().replace(/Dentons - /i, '');
                _gaq.push(['_trackEvent', 'email-me', strtitle, this.rel.replace(/^mailto:/i, ''), , true]); //will only record the email of the mail-to                    
            });
        });
    </script>
