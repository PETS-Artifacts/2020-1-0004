      <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $('a[rel^="mailto"]').click(function () {
                var strtitle = $('title').text().replace(/Dentons - /i, '');
                //_gaq.push(['_trackEvent', 'email-me', strtitle, this.rel.replace(/^mailto:/i, ''), , true]); //will only record the email of the mail-to                    
                if (typeof _gaq == 'undefined' || typeof _gaq !== 'object') {
                    dataLayer.push({
                        'event': 'event',
                        'category': "Email",
                        'action': "email-me",
                        'label': strtitle
                    });
                }
                else {
                    _gaq.push(['_trackEvent', 'email-me', strtitle, this.rel.replace(/^mailto:/i, ''), , true]);
                }
            });
        });
    </script>
