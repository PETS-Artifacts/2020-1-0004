<script type="text/javascript">

    function SubmitForm() {
        var keyID = (window.event) ? event.keyCode : keyEvent.keyCode;
        if (keyID === 13) {
            document.getElementById("phheader_0_buttonSearch").click();
        }
    }



    /* ByPass Block:Passing the (id or class name to the name) argument and class/id to the type argument*/
    function onscrollToAttributenew(name) {

        if ($("#outer-header-home").size() == 0) {
            //$(name + " a:visible:first").focus();
            $(name).find(' a,input[type=text],select,textarea').filter(':visible:first').focus();
        } else {
            $("#gallery1 a:visible:first").focus();
        }
        $(window).scrollTop($(name).offset().top);

    }

    /* ByPass Block:Passing the (id or class name to the name) argument and class/id to the type argument*/
    function onscrollToAttribute(name, type) {
        $(name + " a:first").focus();
        $(window).scrollTop($(name).offset().top);

    }


</script>
