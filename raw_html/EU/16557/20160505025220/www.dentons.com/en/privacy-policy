        <script type="text/javascript">
            jQuery(document).ready(function ($) {
                $('a[rel^="mailto"]').click(function () {
                    var strtitle = $('title').text().replace(/Dentons - /i, '');
                    _gaq.push(['_trackEvent', 'email-me', strtitle, this.rel.replace(/^mailto:/i, ''), , true]); //will only record the email of the mail-to
                    //_gaq.push(['_trackEvent', 'email-me', 'shared', location.href]);// will record the page mailto was shared from or clicked
                    //_gaq.push(['_trackEvent', 'email-me', 'send', this.href.replace(/^mailto:/i,'') + ' /Page = ' + location.href,,true]);//email link &amp; page the user was on
                });
            });
    </script>

   

