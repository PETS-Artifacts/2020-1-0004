<script>
    (function () {
        document.addEventListener("DOMContentLoaded", function () {
            $('[data-tag="toggle-show-producttypes"]').click(function (e) {
                var classMinus = "toggle-categories-minus";
                $(this).find(".toggle-categories").toggleClass(classMinus);
                $(this).siblings(".product-types").toggleClass("hidden-xs");

                e.stopPropagation();
            });
        });
    })();
</script>

