<script>
    var CookieKey_PersistentCookie_Suffix = "Persistent";
    var CookieKey_COOKIESENABLED = "GAK_CookiesEnabled";
    var EuroPrice_CookieKey = "GAK_EuroPrice";
    var EuroPrice_ConversionRate = 1;
    var EuroPrice_CurrencySymbol = "£";
    var ShoppingBasketParms = {
        CookieKey: "GAK_ShoppinBasket",
        AddProductToOrderURL: '/ShoppingBasket/AddProductToOrder',
        BuyNowWithPayPalURL: '/ShoppingBasket/BuyNowWithPayPal',
        GoToShoppingBasketURL: '/ShoppingBasket',
        DestroyOrderItemURL: '/ShoppingBasket/DestroyOrderItem',
        UpdateCartURL: '/ShoppingBasket/UpdateCart',
        ApplyVoucherURL: '/ShoppingBasket/ApplyVoucherOnOrder',
        FreeGiftList: [],
        AddFreeGiftListToOrderURL: '/ShoppingBasket/AddFreeGiftListToOrder',
        BasketVouchersURL: '/ShoppingBasket/BasketVouchers',
        UpdateDeliveryOptionsShallowURL: '/ShoppingBasket/UpdateDeliveryOptionsShallow',
    };
    var DeliveryOptionsParms = {
        IndexDeliveryOptionsURL: '/DeliveryOptions',
        UpdateDeliveryOptionsURL: '/DeliveryOptions/Update',
    };
    var EmailRegistration_ProceedToRegisterURL = '/Home/_EmailRegistration';
    var globalParms = {
        LoginPartialURL: '/Home/_LoginPartial'
    };
    var TargetIsMobile = false;

    // Deferred CSS function.
    function GAK_displayHiddenItems() {
        // Display header-social-container.
        $(".header-social-container").removeClass("hidden");
    }

    $(document).ready(function () {

        // Cookies enabled?
        if (CookieConsentService.didConsentToCookies()) {
            $.cookie(CookieKey_COOKIESENABLED, cookiesEnabled());
        }

        setTimeout(function () { GAK_displayHiddenItems(); }, 1000);

        // Disable click on Menu LIs
        $("div.dropdown-menu div.nav-grid ul li.title").click(function (e) {
            e.stopPropagation();
        });

        // Menu options. Brands tab
        var displayBrandLinks = $(".displayBrandLink");
        displayBrandLinks.click(function (e) {
            e.stopPropagation();
            $('[class*="displayBrandBlock"]').addClass("hidden");
            $(".displayBrandBlock" + $(this).data("id")).removeClass("hidden");
        });
        // Display "A" Brands.
        if (displayBrandLinks.length > 0) {
            displayBrandLinks[0].click();
        }

        // spinner
        var $loading = $('.spinnerDiv').hide();
        $(document)
            .ajaxStart(function () {
                $loading.show();
            })
            .ajaxStop(function () {
                $loading.hide();
            });

        Campaigns_BindEvents('4');
    });

    //Top search bar binding
    app.controller("top_banner_search_controller", ['$rootScope', '$scope', 'sharedProperties' , function ($rootScope, $scope, sharedProperties) {
        $scope.topBannerSearchText = "";

        $scope.onKeyPressed = function (keyEvent) {
        };

        //Broadcast-related methods
        $scope.$on('onUpdateSearchQuery', function (event, text) {
            $scope.topBannerSearchText = text;
        });

            
        $scope.$on('onSearchQueryChanged', function (event) {
            //Taking whatever is currently in fragment and passing to Search
            var fragment = location.href.split("#")[1];
            var searchUrl = "/Search/#" + fragment;

            //Override original state with this one
            var newState = history.state || {};
            //Overriding existing OriginalState if any
            newState.OriginalState = angular.copy(newState.OriginalState != null ? newState.OriginalState :
                (window.OriginalSearchPanelModel != null ? window.OriginalSearchPanelModel : newState));

            //Replacing existing state with a reload for when user returns
            var restartState = angular.copy(history.state || {});
            restartState.reloadPage = true;
            history.replaceState(restartState, "Reloading page", location.href);

            //Moving to new Search page state
            history.pushState(newState, "Search page", searchUrl);

            location.reload();
        });
        
    }]);

</script>


