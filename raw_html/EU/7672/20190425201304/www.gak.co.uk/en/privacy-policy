<script type="text/javascript">
    (function () {
        var CookieConsentKey = "GAK_CookieConsent";
        var EnableImpliedCookieConsent = true;        

        //Setting cookie consent key from server side
        //Key needs to be server side since bundling makes use of it
        CookieConsentService.CookieConsentKey = CookieConsentKey;
        CookieConsentService.EnableImpliedCookieConsent = EnableImpliedCookieConsent;
        CookieConsentService.clearAllCookiesIfConsentNotGiven();

        var toggleHidden = function () {
            $(".cookie-banner").toggleClass("hidden");            
        };

        $("[data-id=revokeCookieConsent]").click(function () {
            CookieConsentService.onRevokeConsent();
        });

        if (CookieConsentService.shouldShowCookieBanner()) {
            toggleHidden();

            
            $("[id=buttonCookieConsent]").click(function () {
                CookieConsentService.onManuallyConsentToCookies();
                toggleHidden();
            });

            
            $("[id=buttonCookieImpliedConsent]").click(function () {
                CookieConsentService.onHideImpliedConsentBanner();
                toggleHidden();
            });
        }
    })();
</script>

