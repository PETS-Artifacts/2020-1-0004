    window.onerror = function (message, url, lineNo) {
      window.onerror.count = window.onerror.count || 0;

      if(window.onerror.count < 5) {
        try {
          if (message && url && lineNo) {
            var xhr = new XMLHttpRequest();
            xhr.open('POST', '/log', true);
            xhr.setRequestHeader("Content-Type", "application/json");
            xhr.send(JSON.stringify({ message: message, url: url, lineNo: lineNo, browser: navigator.userAgent }));
          }
        } catch(e) {
          console.error(e);
        }
      }

      window.onerror.count++;
      return false;
    }
