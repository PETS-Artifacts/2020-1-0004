    <script type="text/javascript">
        var isoppened = false;


        function oppen() {
            $(".fastoptions-panel").removeClass('show');

            $zopim.livechat.window.show();
            $zopim.livechat.window.setOffsetVertical(42);
        }

        function closechat() {

            $(".fastoptions-panel").removeClass('show');
            $zopim.livechat.window.hide();
        }

        function oppenchat() {
            isoppened = (isoppened) ? false : true;

            if (isoppened) {
                /*
                 if( $("#fastpanel-write").hasClass( "show" ) ){
                 $("#fastpanel-write").removeClass('show');
                 }
                 */
                oppen();

            } else {
                closechat();
            }



        }


        $('.chat_pat').click(function () {
            oppenchat();
            return false;
        });

    </script>








