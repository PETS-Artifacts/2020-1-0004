<script>
  $(document).ready(function() {
    var $navItem = $('.site_nav__item--as-drop-down > a');
    var $html = $('html');

    if ($html.hasClass('touchevents')) {
      $navItem.on('click', function(e) {
        e.preventDefault();
        if ( !$(this).parent().hasClass('is-active')) {
          $navItem.not(this).parent().removeClass('is-active');
          $(this).parent().addClass('is-active');
        } else {
          $navItem.parent().removeClass('is-active');
        }
      });
    }
  });
</script>
