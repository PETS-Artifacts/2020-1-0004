  <!-- Initialise Cart.js once the page has loaded -->
  <script type="text/javascript">
    jQuery(function() {
      CartJS.init({"token":"8cdd6321027b4dcd3315117780b571a5","note":null,"attributes":{},"original_total_price":0,"total_price":0,"total_discount":0,"total_weight":0.0,"item_count":0,"items":[],"requires_shipping":false,"currency":"USD","items_subtotal_price":0,"cart_level_discount_applications":[]}, {
