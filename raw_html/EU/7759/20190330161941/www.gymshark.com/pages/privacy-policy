<script>
$(document).on("click", ".search-header__submit:not(.blog-search__submit)", function(e) {
  var term = $(".search-header__input.search__input").val();
  if (term.trim() != '') {
    return true;
  }else{
    $(".search-header__input.search__input").focus(); 
  }
  return false;
});
</script>

