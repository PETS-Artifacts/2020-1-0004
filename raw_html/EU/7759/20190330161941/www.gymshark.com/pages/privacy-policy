<script>
  var countryList = "{&quot;change_country_text&quot;=&gt;&quot;Select Country&quot;, &quot;europe&quot;=&gt;{&quot;united_kingdom&quot;=&gt;&quot;United Kingdom&quot;, &quot;roi&quot;=&gt;&quot;Republic of Ireland&quot;, &quot;germany&quot;=&gt;&quot;Germany &amp; Austria&quot;, &quot;france&quot;=&gt;&quot;France&quot;, &quot;sweden&quot;=&gt;&quot;Sweden&quot;, &quot;finland&quot;=&gt;&quot;Finland&quot;, &quot;denmark&quot;=&gt;&quot;Denmark&quot;, &quot;norway&quot;=&gt;&quot;Norway&quot;, &quot;rest_europe&quot;=&gt;&quot;Rest Of Europe&quot;}, &quot;north_america&quot;=&gt;{&quot;united_states&quot;=&gt;&quot;United States&quot;, &quot;canada&quot;=&gt;&quot;Canada&quot;}, &quot;asia_pacific&quot;=&gt;{&quot;australia&quot;=&gt;&quot;Australia &amp; New Zealand&quot;}, &quot;other&quot;=&gt;{&quot;rest_of_world&quot;=&gt;&quot;Rest of World&quot;}}";
  if(countryList != ""){
    countryList = countryList.replace(/&quot;/g,'"');
    countryList = countryList.replace(/&gt;/g,'');
    countryList = countryList.replace(/=/g,':');
    countryList = JSON.parse(countryList);
  }

  var defaultRegion = "Select Region";

  var countryUrl = {
    "united_kingdom":"https://uk.gymshark.com",
    "united_states":"https://www.gymshark.com",
    "roi":"https://ie.gymshark.com",
    "germany":"https://de.gymshark.com",
    "france":"https://fr.gymshark.com",
    "australia":"https://au.gymshark.com",
    "sweden":"https://se.gymshark.com",
    "norway":"https://no.gymshark.com",
    "finland":"https://fi.gymshark.com",
    "denmark": "https://dk.gymshark.com",
    "rest_europe":"https://eu.gymshark.com",
    "rest_of_world": "https://row.gymshark.com",
    "canada":"https://ca.gymshark.com"
  }

  var flagIcon = {
    "united_kingdom":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-uk.svg?532",
    "united_states":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-usa.svg?532",
    "roi":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-roi.svg?532",
    "germany":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-germany.svg?532 ",
    "france":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-france.svg?532",
    "australia":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-australia.svg?532",
    "sweden":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-sweden.svg?532",
    "norway":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-norway.svg?532",
    "finland":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-finland.svg?532",
    "denmark": "//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-denmark.svg?532",
    "rest_europe":"//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-eu.svg?532",
    "rest_of_world": "//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-row.svg?532",
    "canada": "//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/flag-canada-sm.svg?532"
  }


  $(document).ready(function () {

    resetDropDowns = function(){
      $(".country-selector-custom").find(".gs-content").empty();
      $(".country-selector-custom").find(".txt").text(countryList["change_country_text"]);
      $(".country-selector-custom").find(".txt").attr("id", "");

      $(".region-selector-custom").find(".txt").text(defaultRegion);
      $(".locale-btn").text("Select");
    }


    

    $(document).on("touchstart click",".region-selector-custom a", function(e){
      e.preventDefault();
      $(this).parent().parent().find(".txt").text($(this).text());
      $(this).parent().parent().removeClass("open");
      var options = "";
      var n = $(this).attr('id');
      for(var obj in countryList[n]){
        var opt = countryList[n][obj];
        options += '<a id="'+obj+'"><img src="'+flagIcon[obj]+'" /><span>'+opt+'</span></a>';
      }
      $(".country-selector-custom").find(".gs-content").empty().append(options);
      $(".country-selector-custom").find(".txt").text(countryList["change_country_text"]);
      $(".locale-btn").text("Select");
    });


    $(document).on("touchstart click",".country-selector-custom a", function(e){
      e.preventDefault();
      $(this).parent().parent().find(".txt").text($(this).text());
      $(this).parent().parent().removeClass("open");
      $(this).parent().parent().find(".txt").attr("id", $(this).attr('id'));
      $(".locale-btn").text("Confirm");
    }); 


    $(document).on("touchstart click",".gs-selector", function(e){
      e.preventDefault();
      if($(this).parent().hasClass("open")){
        $(this).parent().removeClass("open");
      }else{
        if($.trim($(this).parent().find(".gs-content").text()) != ""){
          $(this).parent().addClass("open");
        }
      }
      
    });

    $(document).on("change", ".region-selector-dd", function(e){
      if($(".region-selector-dd").val() != ""){

        var options = "";
        for(var obj in countryList[$(".region-selector-dd").val()]){
          var opt = countryList[$(".region-selector-dd").val()][obj];
          options += '<option value="'+obj+'">'+opt+'</option>';
        }
        options = '<option value="">'+countryList["change_country_text"]+'</option>'+options;
        $(".country-selector-dd").empty().append(options);
        $(".locale-btn").text("Select");
      }
    });

    $(document).on("change", ".country-selector-dd", function(e){
      if($(".country-selector-dd").val() != ""){
        $(".locale-btn").text("Confirm");
      }
    });


    $(document).on("touchstart click",".locale-btn", function(e){
      e.preventDefault();
      if($(".country-selector-dd").css("display") != "none" && $(".country-selector-dd").val() != ""){
        $.cookie("countryRedirectGS", countryUrl[$(".country-selector-dd").val()], { domain: '.gymshark.com', expires: 90, path:'/' });
        window.location = countryUrl[$(".country-selector-dd").val()] + window.location.pathname + window.location.search;
        //console.log("====="+countryUrl[$(".country-selector-dd").val()]);
      }

      if($(".country-selector-custom").css("display") != "none" && $(".country-selector-custom").find(".txt").attr("id") != ""){
        $.cookie("countryRedirectGS", countryUrl[$(".country-selector-custom").find(".txt").attr("id")], { domain: '.gymshark.com', expires: 90, path:'/' });
        window.location = countryUrl[$(".country-selector-custom").find(".txt").attr("id")] + window.location.pathname + window.location.search;
        //console.log("====="+countryUrl[$(".country-selector-custom").find(".txt").attr("id")]);
      }
    });
    // On country selector link click, create 90 day redirect cookie and send user to chosen store
    $('.country-select-column a').click(function(e) {
        e.preventDefault();
        $.cookie("countryRedirectGS", $(this).attr("href"), { domain: '.gymshark.com', expires: 90, path:'/' });
        window.location = $(this).attr("href")+ window.location.pathname + window.location.search;
    });


    $(".country-select-ui").mouseup(function(e){
      if($(".region-selector-custom").hasClass("open") && e.target.parentNode.parentNode.className.indexOf("region-selector-custom") == -1){
        $(".region-selector-custom").removeClass("open")
      }
      if($(".country-selector-custom").hasClass("open")&& e.target.parentNode.parentNode.className.indexOf("country-selector-custom") == -1){
        $(".country-selector-custom").removeClass("open")
      }
    });

    // Check if the country selector has been shown already. If the cookie doesnt exist, show the popup and create a 90 day cookie
    

  });
</script>





  

<script>
  GeoIPService = (function() {

    var currentStore = 'US';
    var countryName = '';
    var countryStore = 'ROW';
    var cookieName = 'countryRedirectGS';

    /* Store Configuration */
    var stores = {
      'UK' : 'https://uk.gymshark.com',
      'US' : 'https://www.gymshark.com',
      'EU' : 'https://eu.gymshark.com',
      'ROW' : 'https://row.gymshark.com',
      'DE' : 'https://de.gymshark.com',
      'FR' : 'https://fr.gymshark.com',
      'AU' : 'https://au.gymshark.com',
      'NO' : 'https://no.gymshark.com',
      'SE' : 'https://se.gymshark.com',
      'DK' : 'https://dk.gymshark.com',
      'FI' : 'https://fi.gymshark.com',
      'ROI' : 'https://ie.gymshark.com'
    };

    /* Country to Store Configuration */
    var countries = {
      'US' : { 'Name':'USA', 'Store':'US' },
      'GB' : { 'Name':'United Kingdom', 'Store':'UK' },
      'FI' : { 'Name':'Finland', 'Store':'FI' },
      'DK' : { 'Name':'Denmark', 'Store':'DK' },
      'SE' : { 'Name':'Sweden', 'Store':'SE' },
      'NO' : { 'Name':'Norway', 'Store':'NO' },
      'AL' : { 'Name':'Albania', 'Store':'EU' },
      'AD' : { 'Name':'Andorra', 'Store':'EU' },
      'AT' : { 'Name':'Austria', 'Store':'DE' },
      'BY' : { 'Name':'Belarus', 'Store':'EU' },
      'BE' : { 'Name':'Belgium', 'Store':'EU' },
      'BA' : { 'Name':'Bosnia and Herzegovina', 'Store':'EU' },
      'BG' : { 'Name':'Bulgaria', 'Store':'EU' },
      'HR' : { 'Name':'Croatia', 'Store':'EU' },
      'CY' : { 'Name':'Cyprus', 'Store':'EU' },
      'CZ' : { 'Name':'Czech Republic', 'Store':'EU' },
      'EE' : { 'Name':'Estonia', 'Store':'EU' },
      'FO' : { 'Name':'Faroe Islands', 'Store':'EU' },
      'FR' : { 'Name':'France', 'Store':'FR' },
      'DE' : { 'Name':'Germany', 'Store':'DE' },
      'GI' : { 'Name':'Gibraltar', 'Store':'EU' },
      'GR' : { 'Name':'Greece', 'Store':'EU' },
      'HU' : { 'Name':'Hungary', 'Store':'EU' },
      'IS' : { 'Name':'Iceland', 'Store':'EU' },
      'IE' : { 'Name':'Ireland', 'Store':'ROI' },
      'IT' : { 'Name':'Italy', 'Store':'EU' },
      'LV' : { 'Name':'Latvia', 'Store':'EU' },
      'LI' : { 'Name':'Liechtenstein', 'Store':'EU' },
      'LT' : { 'Name':'Lithuania', 'Store':'EU' },
      'LU' : { 'Name':'Luxembourg', 'Store':'EU' },
      'MK' : { 'Name':'Macedonia', 'Store':'EU' },
      'MT' : { 'Name':'Malta', 'Store':'EU' },
      'MD' : { 'Name':'Moldova', 'Store':'EU' },
      'MC' : { 'Name':'Monaco', 'Store':'EU' },
      'NL' : { 'Name':'Netherlands', 'Store':'EU' },
      'PL' : { 'Name':'Poland', 'Store':'EU' },
      'PT' : { 'Name':'Portugal', 'Store':'EU' },
      'RO' : { 'Name':'Romania', 'Store':'EU' },
      'RU' : { 'Name':'Russia', 'Store':'EU' },
      'SM' : { 'Name':'San Marino', 'Store':'EU' },
      'RS' : { 'Name':'Serbia', 'Store':'EU' },
      'SK' : { 'Name':'Slovakia', 'Store':'EU' },
      'SI' : { 'Name':'Slovenia', 'Store':'EU' },
      'ES' : { 'Name':'Spain', 'Store':'EU' },
      'CH' : { 'Name':'Switzerland', 'Store':'EU' },
      'UA' : { 'Name':'Ukraine', 'Store':'EU' },
      'VA' : { 'Name':'Vatican city', 'Store':'EU' },
      'IM' : { 'Name':'Isle of Man', 'Store':'UK' },
      'ME' : { 'Name':'Montenegro', 'Store':'EU' },
      'CA' : { 'Name':'Canada', 'Store':'CA' },
      'GG' : { 'Name':'Guernsey', 'Store':'UK' },
      'JE' : { 'Name':'Jersey', 'Store':'UK' },
      'AU' : { 'Name':'Australia', 'Store':'AU' },
      'NZ' : { 'Name':'New Zealand', 'Store':'AU' }
    };

    /* GeoIPService Constructor */
    function GeoIPService() { }

    /* GeoIPService Functions */
    GeoIPService.prototype = $.extend({}, GeoIPService.prototype, {

      // Perform GeoIP Lookup
      getLocation: function() {
        if (this.getQueryStringParameter('gclid') || this.getQueryStringParameter('gclid') == undefined) {
          var cookied = this.getRedirectCookie();
          if (cookied) {
            if (cookied != 'correct') {
              window.location = cookied;
            }
          } else {
            geoip2.country(this.processResponse, this.processError);
          }
        }
      },

      // Process GeoIP Lookup Results and show appropriate popup
      processResponse: function(data) {

        var location = 'ROW';

        if (typeof(data.country.iso_code) !== 'undefined') {
          if (typeof(countries[data.country.iso_code]) !== 'undefined') {
            if (countries[data.country.iso_code]['Store'] !== currentStore) {
              location = countries[data.country.iso_code]['Store'];
            } else {
              location = 'correct';
              console.log("GeoIP Status: On the correct store!");
            }
          }
        }

        // Show the correct popup based on location
        if (location == "ROW") {
          showPopUpOne(location);
        } else {
          if (location != 'correct') {
            showPopUpTwo(location);
          }
        }

      },

      // Process GeoIP Lookup Error
      processError: function(data) {
        console.log("GeoIP Status: Fail - " + JSON.stringify(data, undefined, 4));
      },

      /* COUNTRY ONLY FUNCTION */

      // Perform IP lookup and return data
      countryLookup: function() {
        geoip2.country(this.setCountry, this.processError);
      },

      // Store the Country Name and correct country store
      setCountry: function(data) {
        countryName = data.country.names.en;

        if (typeof(countries[data.country.iso_code]) !== 'undefined') {
          countryStore = countries[data.country.iso_code]['Store'];
        }
      },

      // Return the stores country name
      getCountryName: function(data) {
        return countryName;
      },

      // Return the stores country name
      getCountryStore: function(data) {
        return countryStore;
      },
      
      
	  IPInsights: function() {
        geoip2.insights(function(data) {
          clientIP = data.traits.ip_address;
        }, function() {});
      },

	  // Return the stores country name
      getClientIP: function(data) {
        return clientIP;
      },

      /* HELPER FUNCTIONS */

      // Check for specified query string parameter
      getQueryStringParameter: function(parameter) {
        var sPageURL = window.location.search.substring(1);
        var sURLVariables = sPageURL.split('&');
        for (var i=0; i<sURLVariables.length; i++) {
          var sParameterName = sURLVariables[i].split('=');
          if (sParameterName[0] == parameter) {
            return sParameterName[1];
          }
        }
      },

      // Check if the redirect cookie already exists
      getRedirectCookie: function() {
        if ($.cookie(cookieName)) {
          var thisLocation =  window.location.host;
          var cookiedLocation = $.cookie(cookieName);
          var cookieNoProtocol = cookiedLocation.replace("https://","").replace("http://", "").replace("\/", "");

          if (cookieNoProtocol == thisLocation) {
            console.log('GeoIP Status: No action required!');
            return "correct";
          } else {
            return "https://" + cookieNoProtocol + window.location.pathname + window.location.search;
          }
        }
        return false;
      }

    });

    return GeoIPService;

  })();
</script>

  <script>
    var geoip = new GeoIPService();
    geoip.getLocation();
  </script>

  <script>

    function getRedirectUrl(base) {
      return base + window.location.pathname + window.location.search;
    }

    // On country selector link click, create 90 day redirect cookie and send user to chosen store
    // Works for both popups
    $('.country-select-ui-geoip .country-select-column a').click(function(e) {
      e.preventDefault();
      $.cookie("countryRedirectGS", $(this).attr("href"), { domain: '.gymshark.com', expires: 90, path:'/' });
      var baseDomain = $(this).attr("href");
      var fullDomain = getRedirectUrl(baseDomain);
      window.location = fullDomain;
    });

    // Show the popup to a client that is on the wrong store.
    function showPopUpOne(suggested_region) {
      $('.country-select-ui').show();
    }

    // Show the popup to a client that has no suggested store..
    function showPopUpTwo(go_to_store) {
      this_store = "US";
      $(".country-select-ui-geoip .country-select-column li").hide();
      $(".stay-store li a[data-region-code='" + this_store + "']").parent().show();
      $(".go-to-store li a[data-region-code='" + go_to_store + "']").parent().show();
      $('.country-select-ui-geoip').show();
    }
  </script>



    










