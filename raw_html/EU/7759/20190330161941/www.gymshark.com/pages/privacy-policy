  <header role="banner" class="site-header main-header__sticky">
    <div class="message-top-bar">
	<div class="message-content">Gymshark uses cookies to provide you with the best browsing experience. By continuing we assume that you are consenting to all cookies on Gymshark Website. <a href="pages/cookie-policy">Read More</a></div>
		<img src='//cdn.shopify.com/s/files/1/0156/6146/t/125/assets/cancel.svg?532' onclick="closeMessageBar()">

</div>

<script>
	$(window).load(function() {
		
		var cookie = $.cookie("gymsharkcookieAccept");

		if(cookie != "true"){
			$(".message-top-bar").css("display","block");
			var textHeight = $(".message-content").outerHeight(true);
			$(".message-top-bar").animate({height: textHeight});
			$(".header_message_slider").animate({paddingTop:57 + textHeight});
		}
	});

	$(window).resize(function(){
		if($(".message-top-bar").css("display") == "block"){
	      $(".message-top-bar").animate({height: textHeight});
	      var textHeight = $(".message-content").outerHeight(true);
		   $(".message-top-bar").css("height", textHeight+"px");
		   $(".header_message_slider").css("padding-top",(57 + textHeight)+"px");
		}
    });

    $(window).unload(function() {
    	if($(".message-top-bar").css("display") == "block"){
			closeMessageBar();
    	}
	 });

	function closeMessageBar(){
		$.cookie("gymsharkcookieAccept", "true", {expires : 30})
		$(".message-top-bar").css("display","none");
		$(".header_message_slider").css("padding-top", "57px");
	}

</script>
