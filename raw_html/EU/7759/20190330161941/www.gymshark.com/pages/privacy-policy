<script>

  $(document).ready(function() {

    function setThresholdMessage(cart_price) {
      
      // console.log("It's Blank");
      
    }

    setThresholdMessage(0);

    var cartName = "Your Cart";

    var $miniCart = $("#minicart").mmenu({
      "navbars": [
        {
          "position": "top"
        }
      ],
      navbar: {
        title: cartName
      },
      "extensions": [
        "pagedim-black"
      ],
      "offCanvas": {
        "position": "right"
      }
    });
    $("#minicart .mm-navbars-top .mm-title").attr('href', '/cart');

    var $header = $('header.site-header');
    var $stickyDetail = $('.template-product .sticky-wrapper.sticky');
    var $stickyFilter = $('.collection-filters.sticky');
    var $icon = $('#open-cart');
    var API = $miniCart.data( 'mmenu' );



    $icon.on( 'click', function(e) {
      if ($('.cart-header__counter').text() !== '0') {
        e.preventDefault();
        API.open();
      }
    });
    API.bind( 'open:start', function() {
      $header.css({'top': $(document).scrollTop(), 'position': 'absolute'});
      $stickyDetail.css({'top': $(document).scrollTop() + 100});
      $stickyFilter.css({'top': $(document).scrollTop() + 100});
    });
    $("#minicart-close").click(function() {
      API.close();
    });
    API.bind( 'close:finish', function() {
      $header.css({'top': 0, 'position': 'fixed'});
      $stickyDetail.css({'top':'100px'});
      $stickyFilter.css({'top':'100px'});
    });

    $(document).on("click", "#minicart .qtyplus", function(e) {
      e.preventDefault();
      var $quantityField = $(this).parent().find('.quantity');
      if ($($quantityField).attr("readonly") == undefined || $($quantityField).attr("readonly") == false) {
        var productIntentory = parseInt($quantityField.map(function () {
          return this.max;
        }).get());
        var currentVal = parseInt($quantityField.val());
        if (!isNaN(currentVal) && currentVal < productIntentory) {
          $quantityField.val(currentVal + 1).trigger('change');
        } else {
          $quantityField.val(productIntentory);
        }
      }
    });

    // This button will decrement the value till 1
    $(document).on("click", "#minicart .qtyminus", function(e) {
      e.preventDefault();
      var $quantityField = $(this).parent().find('.quantity');
      if ($($quantityField).attr("readonly") == undefined || $($quantityField).attr("readonly") == false) {
        var currentVal = parseInt($quantityField.val());
        if (!isNaN(currentVal) && currentVal > 1) {
          $quantityField.val(currentVal - 1).trigger('change');
        } else {
          $quantityField.val(1);
        }
      }
    });

    $(document).on("change", ".mini-cart__product-quantity .quantity", function() {
      var $this = $(this);
      CartJS.updateItem($this.attr('data-update-quantity'), $this.val());
    });
  });

</script>


