    var $icon = $('header .navbar-toggle');
    var $closeinside = $('.mm-listview .navbar-toggle');
    var API = $menu.data( 'mmenu' );
    var $header = $('header.site-header');

    $icon.on( 'click', function() {
      API.open();
    });
    $closeinside.on('click', function () {
      API.close();
    });
    API.bind( 'open:finish', function() {
      $icon.removeClass( 'collapsed' );
    });
    API.bind( 'open:start', function() {
      $header.css({'top': $(document).scrollTop(), 'position': 'absolute'});
    });
    API.bind( 'close:finish', function() {
      $icon.addClass( 'collapsed' );
      $header.css({'top': 0, 'position': 'fixed'});
    });

	$("#mobileMenu .mm-search").append( "<div id='mobile-search-submit'><i class='icon-search'></i></div>" );
    $("#mobileMenu .mm-search input").keyup(function(e){
      if(e.keyCode == 13){
        
        window.location.href = '/search?type=product&q=' + $(this).val();
        
      }
    });
    $(document).on("touchstart click", ".change-country-btn", function(e) {
      e.stopPropagation(); e.preventDefault();
      API.close();
      $('.country-select-ui').show();
    });
  });
  $(document).on("touchstart click", "#mobileMenu .mm-search #mobile-search-submit", function(e) {
    e.stopPropagation(); e.preventDefault();
    
    window.location.href = '/search?type=product&q=' + $("#mobilemenu .mm-search input").val();
    
  });
</script>

