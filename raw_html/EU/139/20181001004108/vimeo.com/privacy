    <script>
        // DO NOT REMOVE. This is a callback for the login & join buttons in TopNavBuilder.
        function completeUserLogin(response) {
            if (response && !response.signup) {
                window.location.reload();
            }
        }
    </script>

<script>
    if (
        window.ABLincoln &&
        window.ABLincoln.getTest('Nav2018').get('nav', 'control') === 'variant'
    ) {
        // Reveal variant nav elements.
        // Only the label of the "create" menu changes in the variant. So we're
        // doing that here rather than duplicate the whole dang menu item in TopNavBuilder.php
        if (document.querySelector('[data-menu-id="create"] a')) {
            document.querySelector('[data-menu-id="create"] a').innerText = 'Manage ';
        }

                    if (document.querySelector('[data-menu-id="create"]')) {
                document.querySelector('[data-menu-id="create"]').style.display = 'inherit';
            }
                    if (document.querySelector('[data-menu-id="pricing"]')) {
                document.querySelector('[data-menu-id="pricing"]').style.display = 'inherit';
            }
                    if (document.querySelector('[data-menu-id="features"]')) {
                document.querySelector('[data-menu-id="features"]').style.display = 'inherit';
            }
                    if (document.querySelector('[data-menu-id="inspiration"]')) {
                document.querySelector('[data-menu-id="inspiration"]').style.display = 'inherit';
            }
                    if (document.querySelector('[data-menu-id="watch_variant"]')) {
                document.querySelector('[data-menu-id="watch_variant"]').style.display = 'inherit';
            }
                    if (document.querySelector('[data-menu-id="stock"]')) {
                document.querySelector('[data-menu-id="stock"]').style.display = 'inherit';
            }
            }
    else {
        // Reveal control nav elements.
                    if (document.querySelector('[data-menu-id="host"]')) {
                document.querySelector('[data-menu-id="host"]').style.display = 'inherit';
            }
                    if (document.querySelector('[data-menu-id="sell"]')) {
                document.querySelector('[data-menu-id="sell"]').style.display = 'inherit';
