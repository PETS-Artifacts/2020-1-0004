    <script type="text/javascript">

        $("#wishlistTab").click(function () {
            $("#wishlistContainer").css("display", "block");
            $("#recentContainer").css("display", "none");
            $("#wishlistTab").addClass("dashboardactive");
            $("#recentTab").removeClass("dashboardactive");
        });

        $("#recentTab").click(function () {
            $("#recentContainer").css("display", "block");
            $("#wishlistContainer").css("display", "none");
            $("#recentTab").addClass("dashboardactive");
            $("#wishlistTab").removeClass("dashboardactive");
        });

        $(".popUpDashboard").click(function () {
            if ($("#dashboard").hasClass("collapsedash")) {
                $("#dashboard").removeClass("collapsedash");
                $("#dashboard").addClass("expanddash");
                if ($("#wishlistTab").hasClass("dashboardactive")) {
                    $("#wishlistContainer").css("display", "block");
                }
                else
                    $("#recentContainer").css("display", "block");
            }
            else {
                $("#dashboard").removeClass("expanddash");
                $("#dashboard").addClass("collapsedash");
            }
        });

        $("#recentTab").click(function () {
            $("#dashboard").removeClass("collapsedash");
            $("#dashboard").addClass("expanddash");
        });
        $("#wishlistTab").click(function () {
            $("#dashboard").removeClass("collapsedash");
            $("#dashboard").addClass("expanddash");
        });

    </script>
</body>
</html>