        <script type="text/javascript">

            $(document).ready(function () {
                OnDocumentReady("LDWishListEmail", "LDWishListProds");
            });

            function SplitData(data) {

                for (i = 0; i < data.length; i++) {
                    var displayPriceImageURL = "/ShowPrice?s=15&amp;c=p&amp;col=rgb000000&amp;f=arial&amp;sd=0&amp;o=opx".replace("opx", data[i].DisplayPriceWithCurrency);
                    var imageFullFileName = "/Images/imagename?v=".replace("imagename", encodeURIComponent(data[i].ImageFileName)) + data[i].ImageFileVer + "&amp;width=125&amp;height=125&amp;switchoff=enabled";
                    BuildWishlist(data[i], imageFullFileName, displayPriceImageURL, "LDWishListEmail", "LDWishListProds")
                }

                $('.jcarouselwishlist').jcarousel({ animation: 750 });

                //scroll to first item
                $('.jcarouselwishlist').jcarousel('scroll', 0);
            }

        </script>

