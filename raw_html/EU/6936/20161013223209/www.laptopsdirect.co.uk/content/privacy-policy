        <script type="text/javascript">

            $(document).ready(function () {
                OnDocumentReady("LDWishListEmail", "LDWishListProds");
            });

            function SplitData(data) {

                for (i = 0; i < data.length; i++) {

                    var imageFullFileName = "/Images/imagename?v=".replace("imagename", encodeURIComponent(data[i].ImageFileName)) + data[i].ImageFileVer + "&amp;width=125&amp;height=125&amp;switchoff=enabled";
                    BuildWishlist(data[i], imageFullFileName, "LDWishListEmail", "LDWishListProds")
                }

                $('.jcarouselwishlist').jcarousel({ animation: 750 });

                //scroll to first item
                $('.jcarouselwishlist').jcarousel('scroll', 0);
            }

        </script>



        <script type="text/javascript">
            $(document).ready(function () {

                $("#recentTab").on('click', function (event) {
                    OnDocumentReadyRV("LDRecentlyViewedProds");
                });

             });
            function RVSplitData(data) {
                for (i = 0; i < data.length; i++) {
                    var imageFullFileName = "/Images/imagename?v=".replace("imagename", encodeURIComponent(data[i].ImageFileName)) + data[i].ImageFileVer + "&amp;width=125&amp;height=125&amp;switchoff=enabled";
                BuildRecentlyViewed(data[i], imageFullFileName, "LDRecentlyViewedProds")
            }

            $('.jcarouselRV').jcarousel({ animation: 750 });

                 //scroll to first item
            $('.jcarouselRV').jcarousel('scroll', 0);
        }
        </script>

