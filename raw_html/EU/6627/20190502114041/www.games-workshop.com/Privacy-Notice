								var	commerceItemCount = tab.element.attr("data-commerce-item-count"),
									price = tab.element.attr("data-cart-total");

								gw.miniCart.button.html('Il tuo carrello <span class="qty test-mini-cart-qty">('+commerceItemCount+')</span> <span class="price">' + price + '</span>');
								gw.core.global.updateHeaderCartInfo('Carrello:','Checkout',price,commerceItemCount);

							} catch (e) {
								gw.miniCart.logError(e);
							}
						}
					}
				],
				gw
			);

		} catch (e) {
			gw.core.global.logError(e);
		}

		

		gw.navMenu = new gw.classes.navMenu({});
	</script>
