<script>
  (function() {
    var t = performance.timing;
  
    window.dataLayer = window.dataLayer || [];
    window.dataLayer.push({
      requestDurationMs: t.responseStart - t.requestStart,
      responseDurationMs: t.responseEnd - t.responseStart,
      responseToDomLoadingMs: t.domLoading - t.responseStart,
    });
  
    function drop(rate) {
      return Math.floor(Math.random() * rate) !== 0;
    }
  
    function track() {
      if (drop(10)) return;
  
      window.dataLayer.push({
        event: 'trackable event',
        eventName: 'Page Performance Timing',
  
        eventParams: {
          responseToDomInteractiveMs: t.domInteractive - t.responseStart,
          responseToDomCompleteMs: t.domComplete - t.responseStart,
  
          responseToDomContentLoadedStartMs: t.domContentLoadedEventStart - t.responseStart,
          responseToDomContentLoadedEndMs: t.domContentLoadedEventEnd - t.responseStart,
          domContentLoadedEventDurationMs: t.domContentLoadedEventEnd - t.domContentLoadedEventStart,
  
          responseToDomLoadStartMs: t.loadEventStart - t.responseStart,
          responseToDomLoadEndMs: t.loadEventEnd - t.responseStart,
          domLoadEventDurationMs: t.loadEventEnd - t.loadEventStart,
        },
      });
    }
  
    if (typeof PerformanceObserver !== 'undefined') {
      var observer = new PerformanceObserver(track);
      observer.observe({ entryTypes: ['navigation'] });
    } else {
      window.addEventListener('load', function() { setTimeout(track) });
    }
  })();
</script>


