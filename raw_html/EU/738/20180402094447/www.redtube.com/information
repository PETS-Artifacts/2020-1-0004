    <script type="text/javascript">
        var ClassNoPopunder = function(){
            "use strict";
            var Self = this;

            Self.init = function(options){
                Self.params = options;
                Self.selectHtmlElements();
            };

            Self.selectHtmlElements = function(){
                var myElements = Self.params.htmlElements;
                var i = 0;
                var j = myElements.length;

                for (; i<j; i++) {
                    Self.addClassToElement(myElements[i]);
                }
            };

            // Check if element has the class name in it and if not then add the class name
            Self.checkElement = function(element) {
                if (!MG_Utils.hasClass(element, Self.params.existingNoPopUnderClass)) {
                    MG_Utils.addClass(element, Self.params.classNameToAdd);
                }
            };

            Self.addClassToElement = function(element){
                var elements = document.getElementsByTagName(element);
                var i=0;
                var j=elements.length;

                for (; i<j; i++) {
                    Self.checkElement(elements[i]);
                }
            };
        };

        //SET UP TO FILE_NAME_HERE
        page_params.classNoPopUnder = {
            classNameToAdd : 'js-pop',
            existingNoPopUnderClass : 'removeAdLink',
            htmlElements : ['a', 'button']
        };

        try{
            var classNoPopUnder = new(ClassNoPopunder);
            classNoPopUnder.init(page_params.classNoPopUnder);
        }catch(e){
            console.log(e);
        }
    </script>

