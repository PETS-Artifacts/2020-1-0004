    <script>
        function initCaptcha() {
            var captchas = document.querySelectorAll('.g-recaptcha');
            if (captchas.length > 0) {
                var captchaId,
                    recaptchas = {},
                    parameters = {
                    'sitekey': "6LfqJkIUAAAAAIjp22P-TZxJ6PqKlpSMlHnrRczJ",
                    'theme': 'dark',
                    'callback': verifiedCallback
                };
                for (var i = 0; i < captchas.length; i++) {
                    captchaId = captchas[i].getAttribute('id');
                    captchaId = (captchaId)? captchaId : i;
                    recaptchas[captchaId] = grecaptcha.render(captchas[i], parameters);
                }
                page_params.recaptchas = recaptchas;
            }
        }
        var recaptchaComplete;
        function verifiedCallback() {
            recaptchaComplete = true;
        }
    </script>
    <script type="text/javascript" src="https://www.google.com/recaptcha/api.js?onload=initCaptcha&render=explicit" async defer></script>
