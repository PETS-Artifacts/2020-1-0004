<script>
var ua = window.navigator.userAgent,
isLegacy = ua.match( /MSIE 8|MSIE 7/ );

// show adblock message on all pages when cookie "adBlockClosed" is not set
var re = new RegExp("adBlockClosed=([^;]+)");
var adBlockClosedCookie = re.exec(document.cookie);
if (adBlockClosedCookie == null) {  //if cookie not set, show adBlock message
	var IS_ADBLOCK = function() {
		"use strict";
		var Self = this;

		/**
		 * Init method to set up more vars or initialite methods inside the object
		 */
		Self.init = function( options ) {
			Self.params = options;
			page_params.holiday_promo ? "" : Self.adBlockAlert();
		};

		Self.adBlockAlert = function() {
			var adBlockAlertWrapper = document.getElementById(Self.params.wrapper),
			msg = document.getElementById(Self.params.alert),
            adBlockAlertClose = document.getElementById("close_link");

			adBlockAlertWrapper.className = "adBlockAlertWrapper";
            msg.style.display = 'block';

			adBlockAlertClose.addEventListener("click", function() {
				//set cookie "adBlockClosed" for 30 days on clicking close button
				// TODO - Need a utils file for functions that are used may time like creating cookie, dleeting cookie, etc...
				var d = new Date(),
				expires;
				d.setTime( d.getTime() + (30*24*60*60*1000) );
				expires = "expires=" + d.toUTCString();
				document.cookie = "adBlockClosed=1; "+expires;

				msg.className = "";
				msg.style.display = 'none';
			});
		}
	};

	try {
		myAdDetector === undefined; //SHOULD GIVE US AN ERROR IF ADBLOCKER IS ON AND adblock_detector.js IS BLOCKED
	} catch(e) {
		window.myAdDetector = true; //TRUE MEANS ADBLOCKER IS ACTIVATED
	}

	try{
		if( window.myAdDetector ){
			var adblockInstance = new IS_ADBLOCK();
			adblockInstance.init({
				'wrapper' : "ab_banner_container",
				'alert'   : "ab_banner",
				'isAdblock' : window.myAdDetector
			});
		}
	} catch(e) {
		console.log( 'is_adblock_controller: ' + e.message );
	}
}
</script>
