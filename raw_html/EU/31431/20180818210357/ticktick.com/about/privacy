            var svgIconInline = function(id, className) {
                var doms = [];
                doms.push('<svg class="svg-icon ' + id + ' ')
                if (className) {
                    doms.push(className)
                }
                doms.push('"><use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="#')
                doms.push(id)
                doms.push('"></use></svg>')
                return doms.join("")
            }
          </script>
          
