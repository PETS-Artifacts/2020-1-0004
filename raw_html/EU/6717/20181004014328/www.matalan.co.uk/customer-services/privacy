    <script>
  if(get_from_query_string_by_key('utm_medium') && (get_from_query_string_by_key('utm_medium').toLowerCase() == 'email' || get_from_query_string_by_key('utm_medium').toLowerCase() == 'app') && get_from_query_string_by_key('utm_term')) {
   set_cookie('_user_hash', get_from_query_string_by_key('utm_term'));
  }
  var userHash = read_cookie('_user_hash')
  if (userHash) {
    dataLayer.push({
      "_user_hash": userHash
    })
  }
  var dataObjectsToPush = []

  // Confirmation page regex has to follow: /orders/{integer_order_id}/confirmation
  var validConfirmationPageRegex = /^\/orders\/\d*\/confirmation$/

  if (read_cookie("_user_data_layer_attributes")) {
    // Parse cookie into a usable format
    var user_data_layer_attributes = JSON.parse(decodeURIComponent(read_cookie("_user_data_layer_attributes")).replace(/\+/g, " "))
    // Add Cookie to dataLayer
    dataLayer.push(user_data_layer_attributes)

    // If guest user and on confirmation page - Remove cookie
    if (user_data_layer_attributes.customerType === "guest" && location.pathname.match(validConfirmationPageRegex)) {
      delete_cookie("_user_data_layer_attributes")
    }
  }

  window.cart_ndd = true
</script>


    

