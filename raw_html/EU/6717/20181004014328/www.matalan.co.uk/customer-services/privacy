    <div class="row header--search-bar hidden@lg-up">
      <div class="col-12@xs">
        <!-- <div class=""> -->
        <div class="o-header__search-bar">
          <form name="searchProducts" class="o-form--inline c-search-form" action="/products/search" method="get" role="search"
  data-behavior="mini_search">
  <div class="o-form__row">
    <div class="o-form__input-contain o-form--input-pack o-form--input-pack--styled o-form__input--icon
      o-form--input-pack--typeahead">

      <input id="search_bar_id" type="text" title="Search - Enter Keyword or Product Code"
        data-minlength="3" data-toggle="popover" data-trigger='manual' data-placement="bottom"
        data-content="Must be a minimum of 3 characters" name="q" class="js-search-typeahead o-input o-input--textual
        input-addon--expand input-group--addon__inset-left js-exclude-unsaved js-search-input"
        placeholder="Enter Keyword or Product Code" data-attribute="query"
        data-suggestive-delay="1000"
        analytics_re="Header-_-Search-_-Input"
        >

      <div class="input-group__btn c-search__button">
        <button type="submit" class="c-btn c-btn--secondary u-button--nostyle" data-action="submit_search"
          data-validate="typeaheadlength" analytics_re="Header-_-Search-_-Button">Search</button>
      </div>

      <i class="icon icon-search">&nbsp;</i>
    </div>
  </div>
</form>
  <script type="text/javascript">
    (function() {
      var search_bar_input = document.getElementById('search_bar_id');
      var search_bar_input_placeholder = search_bar_input.placeholder;
      var short_placeholder = "Enter Keyword...";

      var changePlaceholderText = function(placeholderText) {
        search_bar_input.placeholder = placeholderText
      };

      var checkViewportSize = function() {
        if (window.innerWidth < 375) {
          changePlaceholderText(short_placeholder);
        } else {
          changePlaceholderText(search_bar_input_placeholder);
        }
      };

      checkViewportSize();

      window.addEventListener('resize', function() {
        checkViewportSize();
      });

    })();
  </script>

        </div>
        <!-- </div> -->
      </div>
    </div>
    <!-- end search bar mobile -->
  </div>
</div>

