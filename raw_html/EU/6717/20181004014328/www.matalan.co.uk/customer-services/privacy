<script>
  (function() {
    var cookieBannerLabel = 'cookieBannerClosed',
        cookieBannerClass = 'has-cookie-banner'

    /*
      read_cookie() should be a global function, it'll return null if the
      cookie doesn't exist
    */
    if(! read_cookie(cookieBannerLabel)) {
      /*
        Only show the cookie on the first page load. After that, hide it (for
        30 days)
        2592000000 seconds is 30 days
      */
      var date = new Date()
      date.setTime(date.getTime() + 2592000000)
      document.cookie = cookieBannerLabel + '=1;expires=' + date.toUTCString() + ';path=/'

      /*
        Get the <body> element, we'll apply a class to this to show/hide the
        cookie banner. Also useful, as on mobile, we have to nudge down the
        fixed mobile nav if the cookie banner is visible
      */
      var bodyElement = document.querySelector('body')
      bodyElement.classList.add(cookieBannerClass)

      /*
        Get the buttons which are used to close the banner. Clicking one of
        these will hide the banner, but also write a cookie to say that the
        banner has been closed. The cookie should last 30 days
      */
      var cookieBannerCloseCtas = document.querySelectorAll('.js-cookie-banner-close')
      for(var i = 0, len = cookieBannerCloseCtas.length; i < len; i++) {
        cookieBannerCloseCtas[i].addEventListener('click', function() {
          bodyElement.classList.remove(cookieBannerClass)
        })
      }
    }
  })()
</script>


