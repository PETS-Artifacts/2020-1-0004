    <script>
var render_line_item_count = function() {
  var val = read_cookie('_matalan_line_item_count');
  if (val === undefined || val === null) {
    set_cookie('_matalan_line_item_count', 0);
    val = 0;
  }
  var lineItemCountEl = document.querySelectorAll('[data-ui="bag-linecount__numtotal"]');
  for (var i = 0, max = lineItemCountEl.length; i < max; i++) {
    lineItemCountEl[i].innerText = val;
  }
};

var re_render_line_item_count = function(value) {
  if (isNaN(value)) value = 0;
  var lineItemCountEl = document.querySelectorAll('[data-ui="bag-linecount__numtotal"]');
  set_cookie('_matalan_line_item_count', value);
  for (var i = 0, max = lineItemCountEl.length; i < max; i++) {
    lineItemCountEl[i].innerText = value;
  }
};

var display_logout_link = function() {
  var logoutEls = document.querySelectorAll("[data-behavior='logout_link']");
  for (var i = 0, max = logoutEls.length; i < max; i++) {
    if (logoutEls[i].classList.contains('is-hidden')) {
      logoutEls[i].classList.remove('is-hidden');
    }
  }
};

var handleCanCheckoutOnload = function() {
  var val = read_cookie('_matalan_line_item_count');
  var checkBtn = document.querySelectorAll('.js-minibag--checkout')[0];
  if (val > 0) {
    checkBtn.removeAttribute("disabled");
  } else {
    checkBtn.setAttribute("disabled", true);
  }
};

var handleCanCheckout = function(canCheckout) {
  var checkBtn = document.querySelectorAll('.js-minibag--checkout')[0];
  if (!canCheckout) {
    checkBtn.setAttribute("disabled", true);
  } else {
    checkBtn.removeAttribute("disabled");
  }
};

function read_cookie(key) {
  var result;
  return (result = new RegExp('(?:^|; )' + encodeURIComponent(key) + '=([^;]*)').exec(document.cookie)) ? (result[1]) : null;
}

function delete_cookie(name) {
  document.cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT;';
}

function set_cookie(key, value) {
  document.cookie = key+'='+value+';path=/';
}
</script>

