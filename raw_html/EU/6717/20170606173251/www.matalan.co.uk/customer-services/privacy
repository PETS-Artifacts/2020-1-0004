    <script async type="text/javascript">
      (function(){
        var docHeader = document.documentElement.childNodes[0];
        var meta = document.createElement('meta');
        meta.name = 'viewport';
        var vpw = 'device-width';
        var landscape = 1024;
        var portrait = 800;

        function buildMeta(metaWidth) {
          return ("initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,width="+metaWidth+",user-scalable=0");
        }

        if (navigator.userAgent.indexOf("Android 4.1.1") !== -1) {
          if (window.matchMedia("(orientation: portrait)").matches) {
            meta.content = buildMeta(portrait);
          } else {
            meta.content = buildMeta(landscape);
          }
        }
        else {
          meta.content = buildMeta('device-width');
        }
        docHeader.appendChild(meta);
      })();
    </script>
