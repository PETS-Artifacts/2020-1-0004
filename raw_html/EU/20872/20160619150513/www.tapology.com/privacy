<section class='searchNews clearfix'>
<script>
  $(document).ready(function() {
    var site_search_url = '/search/nav';
    var site_search_model = 'fighters';
    var site_search_current_request = null;
    var site_search_current_term = "";
    var site_search_timeout = null;
    $("#siteSearch").keyup(function(){
      if(site_search_timeout != null)
        clearTimeout(site_search_timeout);
      var this_term = $('#siteSearch').val();
      if (this_term == "") {
        $("#site_search_spinner").hide();
        $('#searchSuggest').hide();
        return;
      }
      if (this_term == site_search_current_term)
        return;
      site_search_timeout = setTimeout(function() { doSiteSearch(this_term) },10);
    });
    $(".mainSearchCat input").change(function(e) {
      site_search_model = $(e.target).val();
      var term = $('#siteSearch').val();
      if (term != null && term != "" && term.length > 1)
        doSiteSearch(term);
    });
    $(".mainSearch #search").click(function(e) {
      $(".mainSearch").submit();
    });
    function hide_suggest(e,be){
      if($(e.target).parents("#searchSuggest").length == 0) {
        $("#searchSuggest").hide();
        $(document).unbind(be);
      }
    }
    function doSiteSearch(this_term) {
      if (site_search_current_request != null)
        site_search_current_request.abort();
      site_search_current_term = this_term;
      current_request = $.ajax({
        url: site_search_url,
        data: {
          'ajax': true,
          'model': site_search_model,
          'term': site_search_current_term
        },
        'beforeSend': function() {
          $("#searchSuggest").hide();
          $("#site_search_spinner").show();
        },
        'success': function(r) {
          if (site_search_current_term == this_term) {
            $("#site_search_spinner").hide();
            $("#searchSuggest").html(r);
            $("#searchSuggest").show();
            var be = $(document).bind('click',function(e){
              hide_suggest(e,be);
            });
          }
        }
      });
  
    }
  });
</script>
<form action='/search' class='mainSearch' style='position: relative'>
<fieldset style='position: relative'>
<input autocomplete='off' id='siteSearch' name='term' type='text'>
