<script>
  $(function() {
    if ($('#newsContent').length) slideTicker($('#newsContent ul'), 6000, 600, false, false);
  });
  
  var playTicker = true;
  
  slideTicker = function (c, time, trans, imm, to) {
    
    stime = (imm) ? 10 : time;
      
    if (to) clearTimeout(to);
    
    var slider = setTimeout( function () {
      
      if (playTicker) {
      
        $(c).queue( [ ] );
        
        var ml = $(c).children(':first').css('margin-left');
        
        $(c).children(':first').stop().animate({
        
          marginLeft: '-' + (parseInt($(c).children(':first').width()) + parseInt(ml)) + 'px'
        
        }, trans, 'easeInQuint', function () {
          
          $(this).css('margin-left', ml).parent().append($(this));
          
          slideTicker(c, time, trans, false, to);
          
          $(c).find('li').css('margin-left', '0px');
          
        }); 
      
      }
    
    }, stime);
    
    if ($(c).parent().attr('id').match(/news/)) {
      
      $('#pausePlay').unbind('click').bind('click', function () { 
      
        $('.tickerBtn').removeClass('tickerBtn').addClass('tickerPause');
        stopSlideTicker(slider); 
        
      });
    
    }
    else {
    }
  
  }
  
  stopSlideTicker = function (to) {
    
    playTicker = false;
    
    $('#pausePlay').unbind('click').bind('click', function () { 
      $('.tickerPause').removeClass('tickerPause').addClass('tickerBtn');
      slideTicker($('#newsContent ul'), 6000, 500, true);
      playTicker = true; 
    });	
  }
</script>

