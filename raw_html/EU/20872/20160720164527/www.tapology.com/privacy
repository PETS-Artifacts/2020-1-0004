<script>
    
  showLogin = function () {
    $('#mainLogin').animate({ top: '0', opacity: '1'}, 500, 'easeInQuint'); 
    $('*').bind('click', function (e) {
      if (!$(e.target).parents('#mainLogin').length && $(e.target).attr('id') != 'mainLogin') hideLogin();
    });
    return false;
  }
  
  hideLogin = function () {
    $('#mainLogin').css({ top: '-51px', opacity: '0' })
      //.find('#uid').val('Username').end()
      //.find('#password').val('Password').end()
      .find('.errorAlert').hide().end()
      .find('.error').removeClass('error');
  }
  
  $(function(){
    hideLogin();
    $('#tapin').bind('click', showLogin);
  });
</script>

