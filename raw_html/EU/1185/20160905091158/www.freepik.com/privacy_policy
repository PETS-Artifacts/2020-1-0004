<script type="text/javascript">
	function cdn_test(cdn_ok, cdn_name)
	{
		cookie_name = false;
		switch (cdn_name)
		{
			case 'highwind': cookie_name = 'use_hwcdn'; break;
			case 'edgecast': cookie_name = 'use_edgecdn'; break;
			case 'maxcdn': cookie_name = 'use_maxcdn'; break;
		}

		if (cookie_name && (jQuery.cookie(cookie_name) == null))
		{
			expiration_date = new Date();
			expiration_date.setSeconds(3600);

			if (cdn_ok)
				jQuery.cookie(cookie_name, '1', { expires: expiration_date, path: '/', domain: '.freepik.com' });
			else
			{
				jQuery.cookie(cookie_name, '0', { expires: expiration_date, path: '/', domain: '.freepik.com' });
				ga('send', 'event', 'cdn_fails', cdn_name);
				$('#' + cdn_name + '_test_gif').attr('src', 'http://cdntest.freepik.com/track' + cdn_name + '.gif');
			}
		}
	}
</script>
<img id="highwind_test_gif" height="1" width="1" style="display:none" src="http://testhw.cdnpk.net/test.gif" onload="cdn_test(1, 'highwind');" onerror="cdn_test(0, 'highwind');" />
<img id="edgecast_test_gif" height="1" width="1" style="display:none" src="http://cdns2.freepik.com/test.gif" onload="cdn_test(1, 'edgecast');" onerror="cdn_test(0, 'edgecast');" />



