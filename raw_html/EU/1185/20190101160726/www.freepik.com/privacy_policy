<script type="text/javascript">
    var blowjoll_alert = document.getElementById('cc_expiry');

    if (typeof gr !== 'undefined' && typeof gr.request !== 'undefined' && gr.auth.me.cc_will_expire) {
        blowjoll_alert.className += ' show';
    }

    blowjoll_alert.querySelector('.close-alert').addEventListener('click', function() {
        blowjoll_alert.className = blowjoll_alert.className.replace(' show', '');
    });
</script>

