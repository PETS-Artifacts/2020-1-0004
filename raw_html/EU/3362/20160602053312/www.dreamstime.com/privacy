$(window).load(function(){ $('iframe[name=google_conversion_frame]').hide(); $('img[src*=googlead]').hide(); });
function InitSlideKeys()
{
var event = 'keydown';
var handler = SlideHandleKeyPress;
if (document.addEventListener) document.addEventListener(event, handler, false);
else
{
document.attachEvent('on' + event, handler);
if (window.attachEvent) window.attachEvent("onunload", function() { document.detachEvent('on' + event, handler); });
}
}
function SlideHandleKeyPress(e)
{
var target;
if (!e) var e = window.event;
if (e.target) target = e.target;
else if (e.srcElement) target = e.srcElement;
if (target.nodeType == 3) target = target.parentNode;
if(!(target.tagName == 'INPUT' || target.tagName == 'TEXTAREA'))
{
var key = e.which || e.keyCode;
if (SlideHandleKeyPress._keynaviprev && key == 37)
window.location = SlideHandleKeyPress._keynaviprev;
if (SlideHandleKeyPress._keynavinext && key == 39)
window.location = SlideHandleKeyPress._keynavinext;
}
}
SlideHandleKeyPress._keynaviprev = null;
SlideHandleKeyPress._keynavinext = null;
InitSlideKeys();
</script>
