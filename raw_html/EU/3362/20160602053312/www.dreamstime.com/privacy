<script type="text/javascript">
function togglesearch(x)
{
    $('.dt-tab-default').slideToggle(x?0:'fast', function() { setCookie2('showsearch',$('.dt-tab-default').css('display')=='block'?1:0); if(x) $('#ast'+getCookie('advsearchtab')).click(); });
$('#dt-ss').toggleClass('dt-search-setting-on');
$('.dt-adv-search-menu').toggleClass('dt-adv-search-menu-on');
}
function mrchecker2(x)
{
if(x == 1 && $('#s_mr').attr('checked'))
$('#s_wp').attr('checked',false);
if(x == 2 && $('#s_wp').attr('checked'))
$('#s_mr').attr('checked',false);
if($('#s_mr').attr('checked'))
$('#dt-checkbox-1').addClass('dt-checkbox-selected');
else
$('#dt-checkbox-1').removeClass('dt-checkbox-selected');
if($('#s_wp').attr('checked'))
$('#dt-checkbox-2').addClass('dt-checkbox-selected');
else
$('#dt-checkbox-2').removeClass('dt-checkbox-selected');
if($('#s_mr').attr('checked'))
{
$('input[id^=s_mrc]').removeAttr('disabled');
$('input[id^=s_mra]').removeAttr('disabled');
$('input[id^=s_mre]').removeAttr('disabled');
$('#s_mrg').removeAttr('disabled');
}
else
{
$('input[id^=s_mrc]').attr('disabled',true);
$('input[id^=s_mra]').attr('disabled',true);
$('input[id^=s_mre]').attr('disabled',true);
$('#s_mrg').attr('disabled',true);
}
}
function closeautocomplete(){ $('#srh_field').blur().focus(); }
$.widget("custom.catcomplete", $.ui.autocomplete, {
_create: function() {
this._super();
this.widget().menu( "option", "items", "> :not(.ui-autocomplete-category)" );
},
_renderMenu: function( ul, items ) {
var that = this,
currentCategory = "";
$.each( items, function( index, item ) {
var li;
if ( item.category != currentCategory ) {
ul.append( "<li class='ui-autocomplete-category'>" + item.category + "</li>" );
currentCategory = item.category;
}
li = that._renderItemData( ul, item );
if ( item.category ) {
li.attr( "aria-label", item.category + " : " + item.label );
}
});
}
});

$(function() {
var ls;
ls="wedding";
if(ls != null && ls != '')
{
document.searchfrm.srh_field.value=ls;
document.searchfrm.firstvalue.value=ls;
}
document.searchfrm.lastsearchvalue.value=ls;
if(getCookie('showsearch') == 1)
togglesearch(true);
$('.dt-color-pick-1').miniColors({letterCase: 'uppercase', prefix: ''});
$('.dt-color-pick-2').miniColors({letterCase: 'uppercase', prefix: ''});
mrchecker2(0);
//autocomplete
var termTemplate = "<span class='ui-autocomplete-term'>%s</span>";
$("#srh_field").catcomplete({
source: function( request, response ) {
$.ajax({
url: "/ajax/best_autocomplete.php",
dataType: "jsonp",
data: { value1: request.term },
success: function( data ) { response( data ); }
});
},
minLength: 3,
select: function( event, ui ) {
if (ui.item.value!='')
$("#srh_field").val(ui.item.value);
else
$("#srh_field").val(ui.item.label);
$("form[name=searchfrm]").submit();
},
    open: function() {
styledTerm = termTemplate.replace('%s', $("#srh_field").val().trim());
$('.ui-autocomplete li[id^=ui-id-]').each(
function() {
var me = $(this);
me.html( me.text().replace($("#srh_field").val().trim(), styledTerm) );
if (me.html().indexOf('(')!=-1)
me.css('font-weight','normal');
}
);
$( this ).removeClass( "ui-corner-all" ).addClass( "ui-corner-top" );
$('#ui-id-1').css('top',$('#ui-id-1').offset().top-1).append('<span class="dt-close" onclick="closeautocomplete()">&nbsp</span>');
},
    close: function() {
$( this ).removeClass( "ui-corner-top" ).addClass( "ui-corner-all" );
}
});
$('html').click(function() {
  $('.dt-all-cnt').hide();
$('.dt-search-all').addClass('dt-search-all-open');  
});
$('a.dt-search-all').click(function(event){
    event.stopPropagation();
});
$('.dt-all-cnt').click(function(event){
    event.stopPropagation();
});
});
function gglabel(no)
{
val=''; text='';
$('ul.horiz.dt-all-cnt li').children('input').each(function(){ if ($(this).prop('checked')){val+='y'; if (no!=0) text+=$('label[for='+$(this).attr('id')+']').text()+', ';} else val+='n';});
text=text.substring(0,text.length-2);
if (no==0 && val.substring(no,no+1)=='y')
{
$('#allcontentsrh span em').text('All Content');
$('ul.horiz.dt-all-cnt li').children('input:not(#s_ad)').prop('checked',true);
$('ul.horiz.dt-all-cnt li').children('input#s_ad').prop('checked',false);
}
else if (no==0 && val.substring(no,no+1)=='n')
{
$('#allcontentsrh span em').text('All Content');
$('ul.horiz.dt-all-cnt li').children('input').prop('checked',false);
}
else if (val.substring(no,no+1)=='y' && val.substring(1,5)=='yyyy')
{
// $('input#s_all').prop('checked',true);
text=text.replace('All Content, ', '');
text=text.replace('Photos, Illustrations, only vector/RAW, Videos','All Content');
if (val=='yyyyy')
$('#allcontentsrh span em').text(text);
else
$('#allcontentsrh span em').text(text==''?'All Content':text);
}
else
{
$('input#s_all').prop('checked',false);
text=text.replace('All Content, ', '');
text=text.replace('Photos, Illustrations, only vector/RAW, Videos','All Content');
if (val=='yyyyy')
$('#allcontentsrh span em').text('All Content');
else
$('#allcontentsrh span em').text(text==''?'All Content':text);
}
if ($('#allcontentsrh span em').width()<150)
$('.dt-corner').hide();
else
$('.dt-corner').show();
}
function toggleSearchAll( target ) {
if ( $( target ).hasClass('dt-search-all-open') ) {
$('.dt-all-cnt').show();
$( '.dt-search-all span i' ).addClass( 'dt-up' );
$('.dt-search-all' ).removeClass('dt-search-all-open');
} else {
$('.dt-search-all').addClass('dt-search-all-open');
$('.dt-all-cnt').hide();
$( '.dt-search-all span i' ).removeClass( 'dt-up' );
}
}
</script>
