<script>
var cartAttributes = '';

getIpAddress(cartAttributes);

function getIpAddress(cartAttributes, ipUrl){
  if(cartAttributes ==  "" || cartAttributes == undefined){
    jQuery.ajax({
      type: 'GET',
      url: 'https://pro.ip-api.com/json/?key=MtFBLfUlKCmyheH',
      dataType: 'json',
      success: function(response){
        var country = response.country;
        setCountryCartAttr(country);
      }
    });
  }
}
function setCountryCartAttr(country){
  jQuery.ajax({
    url: '/cart/update',
    type: 'POST',
    dataType: 'json',
    data: {
      attributes: {
        "country": country
      }
    },
    success: function(){
      console.log('set');
      window.location.href = window.location.href;
    }
  });
}
</script>


