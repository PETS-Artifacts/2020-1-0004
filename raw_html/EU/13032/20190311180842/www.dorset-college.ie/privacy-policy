            $(this).removeAttr("style");
            $(this).Selectyze({
                theme : 'grey',
                click : true
            }); 
        });     
    }); 
});
function selectyzeClick(id) {
    document.location = "/backend/norender/coursefinder?type=main&select_category="+$("select[name='select_category'] option:selected").val()+"&select_course="+id;
}
