    <div class="row">
      
                      <h1 class="col-lg-8 col-md-8 col-sm-12 col-xs-12" id="page-title">
                    Privacy policy        </h1>
      
      
      <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
          <div class="region region-tools">
    <div id="block-system-user-menu" class="btn-group block block-system block-menu">
  <a href="/european-union/ecas" title="EU login" class="btn btn-default" data-image="log-in" data-toggle="tooltip" data-placement="bottom" data-container="body"><span class="glyphicon glyphicon-log-in" aria-hidden="true"></span> EU Login</a><a href="/european-union/account_request_en" title="Request new account" class="btn btn-success" data-image="plus" data-display-title="0" data-toggle="tooltip" data-placement="bottom" data-container="body"><span class="glyphicon glyphicon-plus menu-no-title" aria-hidden="true"></span><span class="sr-only">Create an EU Login account</span></a>    
</div>
  </div>
      </div>
