<script type="text/javascript">
    jQuery(document).ready(function() {
        App.init();
        LayerSlider.initLayerSlider();
    });

    var cachetjs = new cachetjs;

    cachetjs.setBaseURL('https://status.truckersmp.com/api/v1/');

    var health = cachetjs.getComponents(function (components) {
        var status = 1;
        for (var i = 0; i < components.length; i++) {
            if (components[i].status > status) {
                status = components[i].status;
            }
        }

        switch (status) {
            case 1:
                style = 'green';
                break;
            case 2:
                style = 'blue';
                break;
            case 3:
                style = 'yellow';
                break;
            default:
                style = 'red';
        }

        $('#status').before('<i class="fa fa-circle" style="color:' + style + '"></i> ');
    });


    
</script>
