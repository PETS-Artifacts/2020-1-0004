        <script type="text/javascript">
        
            (function() {
        
                var siteJS = {"desktop":"site-desktop","tablet":"site-tablet","phone":"site-phone"}
                var sectionJS = {"desktop":"section-desktop","tablet":"section-tablet","phone":"section-phone"};
                var pageJS = {"desktop":"info","tablet":"info","phone":"info"};
                var type = getStartupDeviceTypeString();
        
        
        		/* site and section build=true */
        
        			var siteScripts = [];
                    var scripts = [];
        
                    if (siteJS[type]) {
                        siteScripts.push('//www-league.nhlstatic.com/builds/site-core/f21074fde0f44698c4c190a86560ffbd9eb32f65_1454363000/scripts/'+siteJS[type]+'.js.gz');
                    }
        
                    if (sectionJS[type]) {
                        scripts.push('//www-league.nhlstatic.com/sections/info/builds/e4ca4bfb3d1d0c67f950c472ff3db3f22b1fc423_1454346417/scripts/'+sectionJS[type]+'.js.gz');
                    }
                    if (pageJS[type]) {
                         scripts.push('//www-league.nhlstatic.com/sections/info/builds/e4ca4bfb3d1d0c67f950c472ff3db3f22b1fc423_1454346417/scripts/'+pageJS[type]+'.js.gz');
                    }
                    if (siteScripts.length) {
                        requirejs.config({
                            waitSeconds: 18,
                            paths : {
                                "bam-video-bootstrap": "//www-league.nhlstatic.com/video/videocore/video"
                            }
                        });
        
                        requirejs(siteScripts, function(){
                            if (scripts.length) {
                                requirejs(scripts);
                            }
                        });
                    }
        
        
            })();
        </script>

