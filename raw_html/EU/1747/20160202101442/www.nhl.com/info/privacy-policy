	             document.write('<link rel="stylesheet" type="text/css" href="//www-league.nhlstatic.com/builds/site-core/f21074fde0f44698c4c190a86560ffbd9eb32f65_1454363000/styles/'+siteLess[type]+'.css.gz" />');
