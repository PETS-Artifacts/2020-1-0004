<!--F0134015 old browser check-->
<script type="text/javascript">
    $(document).ready(function () {
        //old browser check
        var hasWarnedAboutOldBrowser = getCookie('FAOldBrowserWarned');
        var isOldBrowser =  IsOldBrowser();
        if (hasWarnedAboutOldBrowser == null || hasWarnedAboutOldBrowser == "")
            hasWarnedAboutOldBrowser = false;

        if (isOldBrowser)
            if (!hasWarnedAboutOldBrowser)
                showOldBrowserMessage();
    });

    function IsOldBrowser() {
        var ret = false;

        Brwsr = {
            init: function () {
                this.browser = this.searchString(this.dataBrowser) || "Other";
                this.version = this.searchVersion(navigator.userAgent) || this.searchVersion(navigator.appVersion) || "Unknown";
            },
            searchString: function (data) {
                for (var i = 0; i < data.length; i++) {
                    var dataString = data[i].string;
                    this.versionSearchString = data[i].subString;

                    if (dataString.indexOf(data[i].subString) !== -1) {
                        return data[i].identity;
                    }
                }
            },
            searchVersion: function (dataString) {
                var index = dataString.indexOf(this.versionSearchString);
                if (index === -1) {
                    return;
                }

                var rv = dataString.indexOf("rv:");
                if (this.versionSearchString === "Trident" && rv !== -1) {
                    return parseFloat(dataString.substring(rv + 3));
                } else {
                    return parseFloat(dataString.substring(index + this.versionSearchString.length + 1));
                }
            },

            dataBrowser: [
                {string: navigator.userAgent, subString: "Chrome", identity: "Chrome"},
                {string: navigator.userAgent, subString: "Edge", identity: "Explorer"},
                {string: navigator.userAgent, subString: "MSIE", identity: "Explorer"},
                {string: navigator.userAgent, subString: "Trident", identity: "Explorer"},
                {string: navigator.userAgent, subString: "Firefox", identity: "Firefox"},
                {string: navigator.userAgent, subString: "Safari", identity: "Safari"},
                {string: navigator.userAgent, subString: "Opera", identity: "Opera"},
                {string: navigator.userAgent, subString: "OPR", identity: "Opera"},
                {string: navigator.userAgent, subString: "Netscape", identity: "Netscape"}
            ]

        };

        Brwsr.init();
        if ((Brwsr.browser == 'Chrome' && Brwsr.version < 41) ||
            (Brwsr.browser == 'Explorer' && Brwsr.version < 9) ||
            (Brwsr.browser == 'Firefox' && Brwsr.version < 5) ||
            (Brwsr.browser == 'Safari' && Brwsr.version < 5.1) ||
            (Brwsr.browser == 'Opera' && Brwsr.version < 12) ||
            (Brwsr.browser == 'Netscape' && Brwsr.version < 12)) {
            ret = true;
        }

        return ret;
    }

    function showOldBrowserMessage() {
        var title = "Browser Version";

        var cText = '' +
            '<div id="browserWarning" style="text-align:center;padding:10px">' +
            '<p>We’ve noticed that you may be using an old browser to view our website.</p>' +
            '<p>Did you know that using an old browser can considerably slow down the time it takes to load a web page?<br>In addition, many websites don’t appear as they should or function properly in old browsers.</p>' +
            '<p>As we want you to have the best possible experience using The FA website,<br>we recommend that you upgrade to a newer browser such as: </p>' +
            '</div>' +
            '<div>'+
            '<ul class="browsers">'+
            '   <li><a target="_blank" href="http://www.google.co.uk/intl/en_uk/chrome/browser/index.html#eula" class="browser-chrome">Google Chrome</a></li>'+
            '   <li><a target="_blank" href="https://www.mozilla.org/en-GB/firefox/new/" class="browser-firefox">Mozilla Firefox</a></li>'+
            '   <li><a target="_blank" href="http://www.opera.com/download/" class="browser-opera">Opera</a></li>'+
            '   <li><a target="_blank" href="http://support.apple.com/en_GB/downloads/#safari" class="browser-safari">Safari</a></li>'+
            '   <li><a target="_blank" href="http://windows.microsoft.com/en-us/internet-explorer/download-ie" class="browser-ie">Microsoft Internet Explorer</a></li>'+
            '</ul>'         +
            '</div>' +

            '';


        $.fancybox(
            {
                'title': title,
                'content': cText,
                'autoDimensions': false,
                'width': 350,
                'height': 'auto',
                'transitionIn': 'none',
                'transitionOut': 'none'
            }
        );
        setCookie('FAOldBrowserWarned', true, 1);
    }
</script>


    <!--Adding cookies via ajax -->
    <script type="text/javascript">
        var analyticsCookie = "FAAnalytics";
        var thirdPartyCookie = "FAThirdParty";
        var functionalCookie = "FAFunctional";
        var cookieLiftimeDays = 3650;
        var complianceSet = "FACompliance";

        if ((ReadCookie(complianceSet) != "1") && (ReadCookie(complianceSet) != "0")) {
            $.ajax({
                url: '/cookiecompliance/cookies-ajax.htm',
                success: function(data) {
                    $('body').append(data);
                }
            });
        }

        function ReadCookie(n) {
            var cookiecontent = new String();
            if (document.cookie.length > 0) {
                var cookiename = n + '=';
                var cookiebegin = document.cookie.indexOf(cookiename);
                var cookieend = 0;
                if (cookiebegin > -1) {
                    cookiebegin += cookiename.length;
                    cookieend = document.cookie.indexOf(";", cookiebegin);
                    if (cookieend < cookiebegin) {
                        cookieend = document.cookie.length;
                    }
                    cookiecontent = document.cookie.substring(cookiebegin, cookieend);
                }
            }
            return unescape(cookiecontent);
        }
    </script>

<script type="text/javascript">
    AdTakeover = {
        init: function () {
            $('.adLeft, .adRight').click(function (e) {
                window.open(pageBackgroundLinkUrl); // replace with URL variable set by OX / sitecore
            });
            $(window).scroll(function () {
                var $window = $(window);
                var windowsize = $window.width();
                if (windowsize > 768) {
                    AdTakeover.deskTop();
                } else {
                    // Mobile do nothing
                }
            });
        },
        deskTop: function () {
            winPosition = $(document).scrollTop();
            if(winPosition < 98) {
                $('.adLeft, .adRight').css({ "top": (98 - winPosition) + "px" });
                $('body').css({ "background-position": "center " + (98 - winPosition) + "px" });
            } else {
                $('.adLeft, .adRight').css({ "top": "0" });
                $('body').css({ "background-position": "center 0" });
            }
        }
    }

    // F0103477 - thefa - Adserver OpenX
    $(document).ready(function () {
        var backgroundImage = pageBackgroundUrl;
        var cursorStyle = hasBackgroundLink  ? "cursor:pointer;" : "";
        if (backgroundImage) {
            $('body').css('background', 'url(' + backgroundImage + ') no-repeat fixed center 0');

            if(hasBackgroundLink)
            {
                $('body').append('<div class="adLeft" style="' + cursorStyle + ' position: fixed; top: 98px; left: 50%; margin-left:-820px; width: 340px; height: 800px; z-index: 0;"></div>');
                $('body').append('<div class="adRight" style="' + cursorStyle + ' position: fixed; top: 98px; left: 50%; margin-left:480px; width: 340px; height: 800px; z-index: 0;"></div>');
            }
            else
            {
                $('body').append('<div class="adLeft" style="' + cursorStyle + ' position: fixed; top: 0px; left: 50%; margin-left:-820px; width: 340px; height: 800px; z-index: 0;"></div>');
                $('body').append('<div class="adRight" style="' + cursorStyle + ' position: fixed; top: 0px; left: 50%; margin-left:480px; width: 340px; height: 800px; z-index: 0;"></div>');
     
            }

            
            if (hasBackgroundLink)
                AdTakeover.init();
        }
    });
</script>
</body>

</html>
