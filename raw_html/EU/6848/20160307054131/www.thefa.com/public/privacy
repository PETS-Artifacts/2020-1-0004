        function signupAjaxFailure() {
            $(".newsletter-email .status-bg").html('<span aria-live="assertive" id="statusBox" class="status error" style="display: block;">An error occured.</span>');
            showSignupStatus();
        }

        function hideSignupStatus() {
            $(".newsletter-email .status-bg").empty();
            $(".newsletter-email .status-bg").hide();
            $(".newsletter-email .status").hide();

