        function showSignupStatus() {
            $(".newsletter-email .status-bg").css('background-position', '0 -38px').show();
            $(".newsletter-email .status-bg .error").parent().css('background-position', '0 0').show();
            $(".newsletter-email .status").show().css('display', 'block');
