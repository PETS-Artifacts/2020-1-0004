function setShoppingCartVisibility(cartActive) {
  function toggleElementHidden(selector, toggleState) {
    var element = document.querySelector(selector);
    if (element) {
      element.classList.toggle('hidden', toggleState)
    }
  }

  if (cartActive) {
    window.OptimizelyFeatureFlags = {CartActive: true};
  }

  function whenReady (fn) {
    if (document.readyState != 'loading') {
      fn();
    } else {
      document.addEventListener('DOMContentLoaded', fn);
    }
  }

  whenReady(function () {
    // Set visibility of cart in the header
    toggleElementHidden('#shopping-cart-mobile-header-item', !cartActive);
    toggleElementHidden('#shopping-cart-desktop-header-item', !cartActive);

    // Set visibility of 'add to cart' button
    toggleElementHidden('#payment_options_container--without-cart', cartActive);
    toggleElementHidden('#payment_options_container--with-cart', !cartActive);
  });
}</script><script>setShoppingCartVisibility(true);</script><script>window['optimizely'] = window['optimizely'] || [];
