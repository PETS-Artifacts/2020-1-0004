<script>
    (function ($) {
        var SignUpModal = (function () {
            var signupModal = function ($elem) {
                this.$elem = $elem;
                this.cookieName = 'newsletter-signup-cookie';
                if (this.shouldShowModal()) {
                    this.showModal();
                }
                var _this = this;
                this.$elem.on('click', '.js-accept', function () {
                    _this.permHide();
                    _this.hideModal();
                });


                this.$elem.on('click', '.js-reject', function (e) {
                    e.preventDefault();
                    _this.permHide();
                    _this.hideModal();
                });
                this.$elem.on('closed.fndtn.reveal', function (e) {
                    _this.tempHide();
                });
            };
            signupModal.prototype.shouldShowModal = function () {
                if ($.cookie(this.cookieName)) {
                    return false;
                }
                return true;
            };
            signupModal.prototype.tempHide = function () {
                if (!$.cookie(this.cookieName)) {
                    $.cookie(this.cookieName, 'temp-hidden', { path: '/' });
                }
            }
            signupModal.prototype.permHide = function () {
                $.cookie(this.cookieName, 'perm-hidden', { expires: 365, path: '/' });
            }
            signupModal.prototype.showModal = function () {
                var _this = this;
                setTimeout(function () {
                    _this.$elem.foundation('reveal', 'open');
                }, 6000);
            };
            signupModal.prototype.hideModal = function () {
                this.$elem.foundation('reveal', 'close');
            }
            return signupModal;
        })();
        var modal = new SignUpModal($('#newsletterSignupModal'));
    })(jQuery);
</script>
