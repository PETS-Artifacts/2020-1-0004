<script>
    var CountryDropDown = document.getElementById("CountriesList");
    var LanguageDropDown = document.getElementById("LanguagesList");

    var OriginalCountryValue = CountryDropDown !== null ? CountryDropDown.value : "";
    if (CountryDropDown !== null){
        $("#CountriesList").msDropdown();
    }
    if(LanguageDropDown !== null){
        $("#LanguagesList").msDropdown();
    }
    var OriginalLanguageValue = LanguageDropDown !== null ? LanguageDropDown.value : "";

    $(document).ready(function (e) {
        if (CountryDropDown !== null) {
            if (CountryDropDown.value != OriginalCountryValue) {
                CountryDropDown.value = OriginalCountryValue;
            }
            if (LanguageDropDown !== null) {
                if (LanguageDropDown.value != OriginalLanguageValue) {
                    LanguageDropDown.value = OriginalLanguageValue;
                }
            }
        }
    });
</script>
