            var wrap = $("#s4-workspace");
            var imgUnScrolledSrc = "/Style Library/Images/CPU_logo.png";
            var imgScrolledSrc = "/Style Library/Images/CPU_logo-scroll.png";
            wrap.on("scroll", function (e) {

                if (this.scrollTop > 40) {
                    wrap.addClass("scrolled");
                    $("#onetidHeadbnnr2").attr("src", imgScrolledSrc);
                } else {
				if($("#searchInputBox").attr("data-visible") != "visible")
				{
				if(wrap.attr("enable-scrolling") != "false"){
                    wrap.removeClass("scrolled");
                    setTimeout(function(){$("#onetidHeadbnnr2").attr("src", imgUnScrolledSrc);}, 70);
					}
					}
                }
            });
        </script>
        
