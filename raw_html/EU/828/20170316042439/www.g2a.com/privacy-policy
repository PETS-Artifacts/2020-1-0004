<script>
    jQuery(document).ready(function() {
        jQuery(document).on('click', 'button.buttonExpand', function () {
            var content = jQuery('.expandPipePopup');
            var productId = jQuery(this).attr('data-product-id');
            var auctionId = jQuery(this).attr('data-auction-id');

            content.find('.imagesContent').empty();
            content.find('.spinner').show();

            content.fadeIn('slow', function() {
                var data = {'auctionId': auctionId, 'productsId': productId};
                jQuery.post('/steam/index/mediagallery', data, function(data) {
                    if (data.html) {
                        content.find('.spinner').hide();
                        content.find('.imagesContent').empty().html(data.html);
                    }
                }, 'json');
            });
        });

        jQuery('.expandPipePopup').on('click', function(e) {
            if (jQuery(e.target).hasClass('expandPipePopup')) {
                hidePopup();
            }
        });

        jQuery(document).on('keydown', function(event){
            if (27 == event.which) {
                hidePopup();
            }
        });

        jQuery(document).on('click', '.closePopupGallery', function(){
            hidePopup();
        });
    });

    function hidePopup()
    {
        jQuery('.expandPipePopup').fadeOut('slow', function() {
            jQuery(this).find('.imagesContent').empty();
            jQuery(this).find('.spinner').show();
        });
    }
</script>        <div class="g2a-shield-popup-overlay mp-popup-wrapper jq-marketplace-shieldpopupinfo" style="display: none;">
