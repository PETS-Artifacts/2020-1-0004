<script type="text/javascript">
jQuery(function($) {

    $('#rwd-menu').hide();
    var interv = setInterval(function() 
    {
        if ($('#rwd-menu').length) 
        {
            clearInterval(interv);
            /*kod*/
            $('#rwd-menu').detach().show().appendTo('#box_menu .innerbox');
            /*kod - koniec*/
        }
    }, 100);

    if($(window).width()<764){
        clearInterval(interv);
        var interv = setInterval(function() 
    {
        if ($('#rwd-menu').length) 
        {
            clearInterval(interv);
            /*kod*/
            $('#rwd-menu').detach().appendTo('.innermenu').addClass("none");
            /*kod - koniec*/
        }
    }, 100);
    }
});
</script>

