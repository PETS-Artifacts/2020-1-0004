    <script>
    $(document).ready(function() {
        var documentElem = $(document);
        var mobileHeader = $('.mobile-header');
        var lastScrollTop = 0;
        documentElem.on('scroll', function() {
            var currentScrollTop = $(this).scrollTop();
            if (currentScrollTop > 100 && currentScrollTop > lastScrollTop + 4) mobileHeader.addClass('mobile-header-hidden');
            else if ((currentScrollTop < 5 || currentScrollTop < lastScrollTop - 4)) mobileHeader.removeClass('mobile-header-hidden');
            lastScrollTop = currentScrollTop;
        });
    });
</script>
