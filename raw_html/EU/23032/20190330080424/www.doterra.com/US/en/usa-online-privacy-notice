<script id="modalCartSummaryTemplate" type="text/x-jquery-tmpl">
	${($data.currencySymbol = '$'),''}
	<div id="cart-totals" class="col-xs-6 col-sm-3">
		<div id="label-your-cart">Your Cart:</div>
		<div id="total-guest">Guest:&nbsp;<span>{{= data.totalPrice.formattedValue}}</span></div>
		<div id="total-member">Member:&nbsp;<span>{{= data.totalWholesalePrice.formattedValue}}</span></div>
	</div>
	<div class="col-xs-6 col-sm-3 col-sm-push-6">
		<div id="savings-circle">You'll<br/>Save:<div>{{= currencySymbol}}{{= data.savings}}</div></div>
	</div>
	{{for(i=0;i<3;i++)}}
		{{if data.entries[i] && data.entries[i].product.thumbnail}}
		<div class="col-xs-12 col-sm-2 col-sm-pull-3">
			<table id="order-items" class="table">
				<tr class="order-item" id="orderItem{{=i}}">
					<td class="item-image">
						<div class="prod-image-container">
							<a class="prod-image" style="background-image: url({{= data.entries[i].product.thumbnail.url}})"></a>
						</div>
					</td>
					<td class="item">
						<span class="item-name">{{= data.entries[i].product.name}}</span><br/>
						{{if data.entries[i].product.unitOfMeasure}}{{= data.entries[i].product.unitOfMeasure}}<br/>{{/if}}
						&#35;{{= data.entries[i].product.code}}
					</td>
				</tr>
			</table>
		</div>
		{{/if}}
	{{/for}}
</script>
