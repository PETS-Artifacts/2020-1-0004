<script type="text/javascript">
    window.components = window.components || {};

    window.topBarSearchData = {
        texts: {
            recentSearchText:'My Recent Searches'
        }
    };

    (function(window, document, $, topBarSearchData) {
        'use strict';

        var isOpen = false;
        var isRenderedResults = false;
        var fXAutoCompleteInstance;

        var $document = $(document);
        var $form;
        var $formInput;
        var $searchSubSuggestions;
        var $suggestionGroupView;
        var $suggestionItemView;
        var $items;
        var itemsLength = 0;
        var selectedItemIndex = null;

        $document.ready(function() {
            $searchSubSuggestions = $('#suggestionsResults');

            // get the Item View and remove it from DOM
            $suggestionItemView = $searchSubSuggestions.find('.js-item').remove();
            $suggestionItemView.click(function() {
                   goToItem(this, true);
            });

            // get the Group View and remove it form DOM
            $suggestionGroupView = $searchSubSuggestions
                .find('.js-group')
                .remove()
                .removeClass('displayNone');

            // put the recent searches first group
            api.appendSearchGroup(topBarSearchData.texts.recentSearchText, getSearchedResults);
        });

        var searchGroups = [];

        var api = window.components.topBarSearch = {
            open: function() {
                isOpen = true;
                if(isRenderedResults) {
                    showSuggestionsBox();
                    return;
                }

                searchGroups
                    .map(getGroupView)
                    .forEach(function(group) {
                        if(group) $searchSubSuggestions.append(group);
                    });
                showSuggestionsBox();

                $items = $searchSubSuggestions.find('.js-item');
                itemsLength = $items.length;
                isRenderedResults = true;

                return api;
            },
            close: function() {
                isOpen = false;
                hideSuggestionsBox();

                return api;
            },
            addSearchGroup: function(title, items,category) {
                searchGroups.push({
                    title: title,
                    // items can be an array, or a function that returns an array.
                    items: (typeof items === 'function') ? items : function() {
                        return items;
                    },
                    category:category || ''

                });

                return api;
            },
            appendSearchGroup: function(title, items,category) {
                searchGroups.unshift({
                    title: title,
                    // items can be an array, or a function that returns an array.
                    items: (typeof items === 'function') ? items : function() {
                        return items;
                    },
                    category:category || ''
                });

                return api;
            },
            bindToForm: function(formEl, $searchInput, fxInstance) {
                $form = formEl.submit(function(e) {
                    e.preventDefault();
                    if(!isOpen) {
                        location.href=$(this).attr('action');
                        return false;
                    }

                    goToItem($items.eq(selectedItemIndex));
                });

                fXAutoCompleteInstance = fxInstance;
                $formInput = $searchInput
                    .click(function () {
                        if (isOpen || (fxInstance && fxInstance.isSearchBoxOpen())) return;

                        api.open();
                        bindToClose();
                    })
                    .keyup(function (e) {
                        if (!isOpen) return;

                        switch (e.keyCode) {
                            case 40:
                                moveToNextItem();
                                break;
                            case 38:
                                moveToPrevItem();
                                break;
                            case 13:
                                e.stopPropagation();
                                e.preventDefault();
                                goToItem($items.eq(selectedItemIndex));
                                break;
                        }
                    });

                fxInstance.Element.onStartSearch = api.close;
                fxInstance.Element.onSubmit = function(item) {
                    saveItemToSearchedResults(item.pairId, item.link, item.symbol, item.name, item.type);
                };

                return api;
            }
        };

        function moveToNextItem() {
            if(selectedItemIndex === null) {
                selectedItemIndex = 0;
            } else if(selectedItemIndex < (itemsLength - 1)) {
                selectedItemIndex++;
            }
            changeInputForItem($items.eq(selectedItemIndex));
        }

        function moveToPrevItem() {
            if(selectedItemIndex === 0) {
                selectedItemIndex = null;
            } else if(selectedItemIndex > 0) {
                selectedItemIndex--;
            }
            changeInputForItem($items.eq(selectedItemIndex));
        }

        function goToItem(item) {
            var itemData = $(item).data('item');
            if(itemData.category !=='popular') {
                saveItemToSearchedResults(itemData.pairId, itemData.link, itemData.symbol, itemData.name, itemData.type);
            }
            // go to item url:
            window.location.href = itemData.link;
        }

        function changeInputForItem($item) {
            $items.removeClass('hoverSearch');
            $item.addClass('hoverSearch');
            var item = $item.data('item');
            $formInput.val(item.symbol);
        }

        function bindToClose() {
            setTimeout(function() {
                $document.bind('click', checkClickOutsideForm);
            }, 1);
        }

        function checkClickOutsideForm(e) {
            var target = e.target;
            if((target === $form[0]) || $form.find(target).length) return;
            if((target === $searchSubSuggestions[0]) || $searchSubSuggestions.find(target).length) return;

            api.close();
            $document.unbind('click', checkClickOutsideForm);
        }

        function getGroupView(group) {
            var items = group.items();
            if(!items || !items.length) return false;
            var $group = $suggestionGroupView.clone(true);

            $group.find('.js-title').text(group.title);
            var $items = $([]);
            items.forEach(function(item) {
                var $item = $suggestionItemView
                    .clone(true)
                    .attr('data-pair-id', item.pairId || '')
                    .attr('data-link', item.link || '')
                    .data('item', item);
                
		item.category=group.category;
                $item.find('.js-item-symbol').text(item.symbol);
                $item.find('.js-item-name').text(item.name);
                $item.find('.js-item-type').text(item.type);

                $items = $items.add($item);
            });

            $group.find('.js-results').html($items);
            return $group;
        }

        function showSuggestionsBox() {
            $searchSubSuggestions.removeClass('displayNone');
        }

        function hideSuggestionsBox() {
            $searchSubSuggestions.addClass('displayNone');
        }

        function getSearchedResults() {
            var recentSearches = getCookie('searchedResults');
            if(recentSearches) recentSearches = JSON.parse(recentSearches);
            return (recentSearches instanceof Array) ? recentSearches : [];
        }

        function saveItemToSearchedResults(pairId, link, symbol, name, type) {
            var results = getSearchedResults();
            $.each(results, function (index, item) {
                if (item.pairId !== pairId) return;
                results.splice(index,1);
                return false;
            });

            results.length >= 5 && results.pop();
            results.unshift({
                pairId: pairId,
                link: link,
                symbol: symbol,
                name: name,
                type: type
            });
            setCookie('searchedResults', JSON.stringify(results), 3);
        }

    })(window, document, jQuery, topBarSearchData);

    $(document).ready(function(){
// // Top Search -------------------------------------------------------
        var searchTop = window.searchTop = new FXautoComplete({
            $no_search_results: 'No results matched your search'
        }, {
            searchTextInputId: 'searchTextTop',
            resultsInnerContainerId: 'searchTopResults',
            resultsOuterContainerId: 'subContainerTop',
            formElementId: 'combineSearchFormTop',
            hasTabs: true,
            tabsContainerId: 'search_tabs_Top',
            tabsType: 'li',
            tabsDivId: 'searchBoxTabsTopTabsTop',
            countriesType: 'td',
            url: '/search/service/search',
            mainResultRowIdPrefix: 'searchRowIdtop_',
            tabsPrefix: 'search_box_top',
            countryRowPrefix: 'tdCountry_',
            countriesWrapperId: 'countries_wrapper_Top',
            countriesTableId: 'countries_table_Top',
            dynamicVar: '',
            hiddenNoResultsDefine: 'noResultsDefinesearchTopResults',
            hiddenDocDirectionId: 'doc_directionsearchTopResults',
            googleSearchDefineId: 'search',
            googleLinkID: 'googleLinkSearchTop',
            recentSearchText: 'My Recent Searches',
            loadOnTabChange: true,
            trackSearches: true,
            shouldSendForm: true
        });

        var $form = $('#'+ searchTop.Element.formElementId);
        var $searchInput = $('#searchTextTop');
        var popularSearches = [{"pairId":"6408","link":"\/equities\/apple-computer-inc","symbol":"AAPL","name":"Apple Inc","type":"Equity - NASDAQ"},{"pairId":"1","link":"\/currencies\/eur-usd","symbol":"EURUSD","name":"EUR\/USD","type":"FX"},{"pairId":"8849","link":"\/commodities\/crude-oil","symbol":"CL","name":"Crude Oil","type":"Commodity - NYMEX"},{"pairId":"166","link":"\/indices\/us-spx-500","symbol":"US500","name":"S&P 500","type":"Index - NYSE"},{"pairId":"13994","link":"\/equities\/tesla-motors","symbol":"TSLA","name":"Tesla Motors Inc","type":"Equity - NASDAQ"}];
        if(popularSearches.length > 5) popularSearches.length = 5;

        searchTop.setTabsText('[{"id":"search_box_topAll","name":"All","select":true},{"id":"search_box_topIndices","name":"Indices","select":false},{"id":"search_box_topStocks","name":"Equities","select":false},{"id":"search_box_topETFs","name":"ETFs","select":false},{"id":"search_box_topCommodities","name":"Commodities","select":false},{"id":"search_box_topForex","name":"Forex","select":false},{"id":"search_box_topBonds","name":"Bonds","select":false},{"id":"search_box_topCountries","name":"All Countries","select":false}]');
        searchTop.inputFieldBind( $searchInput );

        $form.submit(function() {
            if($(this).attr('action') == '/') {
                return false;
            }
        });
        window.components.topBarSearch
            .bindToForm($form, $searchInput, searchTop)
            .addSearchGroup('Popular Searches', popularSearches,'popular');
    });
</script>
