    $(window).on('mouseleave', function(e) {
        var y = e.pageY - $(document).scrollTop();
        if (
            ((y - 10) < 1 || y >= $(window).height())
            ||
            (e.pageX < 1 || e.pageX >= $(window).width())
        ) {
            $(window).off('mouseleave');
            showPromoteSignUpPopUp('dark_graph');
        }

    });

