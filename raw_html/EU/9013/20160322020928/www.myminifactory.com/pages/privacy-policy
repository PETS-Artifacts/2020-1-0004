<script type="text/javascript">
    var objecttitles = null;
    var listurl = null;

    console.log('document test');

    $( document ).ready(function() {
        console.log('test document ready');
        $("#mymini_userinterfacebundle_search_query").autocomplete({
            source: function (request, response) {
                var title = $("#mymini_userinterfacebundle_search_query").val();
                listurl = "/search_v2/searchv2-suggester/XXXXXXXX";
                listurl = listurl.replace('XXXXXXXX', title);
                console.log('test auto suggest');

                $.ajax({
                    url: listurl
                }).done(function (data) {
                    response(data);
                });
            },
            minLength: 3
        });
    });
</script>
                    
    		
                                                                                
                                                                                                                                                                                                                                                            
                                                                                                          

                                    
                                                                                                                                                                                
