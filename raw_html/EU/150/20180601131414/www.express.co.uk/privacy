                            var storeGigyaComment = function () {
                    var sl_date = new Date();
                    sl_date.setTime(sl_date.getTime() + (5 * 60 * 1000));
                    $.cookie('sl_hascookie', true, {expires: sl_date, path: '/'}); // ca 30 minutes
                    $.cookie('sl_backto', window.location.href, {expires: sl_date, path: '/'}); // ca 30 minutes

                    if (window.sessionStorage) {
                        if ($('.gig-composebox-textarea').length && $('.gig-composebox-textarea').text() != '') {
                            window.sessionStorage.setItem('comments_' + window.location.href, $('.gig-composebox-textarea').text());
                        }
                    } else {
                        if ($('.gig-composebox-textarea').length && $('.gig-composebox-textarea').text() != '') {
                            $.cookie('sl_content', $('.gig-composebox-textarea').text(), {expires: sl_date, path: '/'});
                        }
                    }
                };

                $('#nse-widget').on('click', 'a.nse_register-link', function (e) {
                    // prevent expresso from calling its handlers
                    e.stopImmediatePropagation();
                    e.preventDefault();

                    storeGigyaComment();
                    window.location.href = SECURE_LOGIN_URL + 'users/register';
                });
                $('#nse-widget').on('click', 'a.nse_login-link, .gig-composebox-site-login, .gig-composebox-post', function (e) {
                    // prevent expresso from calling its handlers
                    e.stopImmediatePropagation();
                    e.preventDefault();

                    storeGigyaComment();
                    window.location.href = SECURE_LOGIN_URL + 'users/login';
                });
            
