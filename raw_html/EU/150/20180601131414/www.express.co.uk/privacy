    var showAdverts = true;
    function check_for_pii(){
        currentUrl = window.location.href;
        currentUrl = decodeURIComponent(currentUrl);
        if (currentUrl.indexOf('@') == -1){ 
            return false;
        } else {
            var a = currentUrl;
            var newurl = a;
            while (newurl.indexOf('@') > 0) {
                var newurl = a.slice(0, a.indexOf('@')); // on the copy, slice it so that what's left is from the beginning to where the @ is
                var endnum = Math.max(newurl.lastIndexOf('&'), newurl.lastIndexOf('?'), newurl.lastIndexOf('/'));// then find the last &, ? or / before the @ in that order and replace the copied URL with it
                newurl = newurl.slice(0, endnum);
            }
            window.showAdverts = false;
            //window.setTimeout(function(){ redirect_if_pii(newurl); }, 500);
            return true;
        }
    }
    function redirect_if_pii(newurl){
        window.location.replace(newurl);
    }
</script>
