<script>
window.dataLayer = window.dataLayer || [];
window.dataLayerByEvent = {};
(function() {
	function getCookie(cname) {
	    var name = cname + "=";
	    var ca = document.cookie.split(';');
	    for(var i=0; i<ca.length; i++) {
	        var c = ca[i];
	        while (c.charAt(0)==' ') c = c.substring(1);
	        if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
	    }
	    return "";
	}
	function getSubCookie(cookie, subIndex, value ) {		
		if ( cookie && subIndex ) {
			var chunks = decodeURIComponent(cookie).split('&');
		    for( var i = 0; i < chunks.length; i ++ ) {
		        var chunk = chunks[i].split('=');
		        if ( chunk[0] && chunk[0] == subIndex && chunk[1] ) {
		            value = chunk[1]
		            break;
		        }
		    }	
		}
	    return value;
	}	
	var auCookie = getCookie( 'au' );
	var userId = getSubCookie( auCookie, 'u', null );
	var loggedIn = userId ? 'true' : 'false';
	var author = null;
	var description = 'World Surf League - The global home of surfing.';
	var title = 'Privacy Policy';
	var url = 'http://www.worldsurfleague.com/pages/privacy-policy';
	var type = 'page';
	var updatedTimeStamp = '';
	var configObj = {
		'event': 'dataLayer-initialized',
		'page': {
			'author' : undefined,
			'description' : undefined,
			'title' : undefined,
			'url' : undefined,
			'type' : undefined,
			'updated-time' : undefined,
			'embed-parent' : undefined
		},
		'site': {
			'siteVersion' : '408f2d2',			
			'siteLayout' : 'Desktop',
			'deploymentStatus' : 'Production'
		},
		'user': {
			'loggedIn': loggedIn,
			'id' : undefined		
		}
	}
	if ( author ) {
		configObj['page']['author'] = author;
	}
	if ( userId ) {
		configObj['user']['id'] = userId;	
	}
	if ( description ) {
		configObj['page']['description'] = description;	
	}
	if ( title ) {
		configObj['page']['title'] = title;	
	}
	if ( url ) {
		configObj['page']['url'] = url;	
	}
	if ( type ) {
		configObj['page']['type'] = type;	
	}	
	if ( updatedTimeStamp ) {
		configObj['page']['updated-time'] = updatedTimeStamp;	
	}
	if ( type == 'video-embed' && document.referrer ) {
		configObj['page']['embed-parent'] = document.referrer;		
	}

	window.dataLayer.push(configObj);
	window.dataLayerByEvent['global-virtual-pageview'] = configObj;
	})();
</script>
