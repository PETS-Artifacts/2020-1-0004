var redirectFromHash = function() {
    var href = document.location.href;
    var delimiter = href.indexOf('#!') !== -1 ? '#!state=' : '#state=';
    var pieces = href.split(delimiter);

    if ( pieces[1] ) {
        if ( pieces[1] && pieces[1].length > 1) {
            window.location.replace( unescape(pieces[1].replace(/\+/g, " ")));
        }
    }
}();
</script>


