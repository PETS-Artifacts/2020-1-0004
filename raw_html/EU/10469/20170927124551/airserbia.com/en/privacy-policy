 <script language="JavaScript" type="text/javascript" >var reCaptchaInit = {

	captchas: {},

	add: function(id, sitekey, type, size, theme) {
		this.captchas[id] = {
			'sitekey': sitekey,
			'type': type,
			'size': size,
			'theme' : theme
		};
	},

	render: function() {
        for (var index in this.captchas) {
			grecaptcha.render(index, this.captchas[index]);
		};
	}
};
var reCaptcha_onloadCallback = function() {
    reCaptchaInit.render();
};</script>
