 <script language="JavaScript" type="text/javascript" >$(document).ready(function() {
                    var newsletterRequest = false;
                    var newsletter_form_action = $('#newsletter_form').attr('action');

                    $('.newsletter-box').on('submit', '#newsletter_form', function(e) {
                        e.preventDefault();
                        if (newsletterRequest === false) {// Prevent multiple submits 
                            newsletterRequest = true;
                            // Make AJAX call and display response in popup
                            $.ajax({
                                type: 'POST',
                                url: newsletter_form_action,
                                data: $(this).serialize(),
                                timeout: 30000,
                                beforeSend: function() { 
                                    // Preloader

                                }
                                }).done(function(data) {
                                    $('.newsletter-box').hide()
                                                        .html(data)
                                                        .fadeIn('slow');
                                }).error(function() {
                                    alert('fail');
                                }).always(function() { 
                                    newsletterRequest = false;
                                }); 
                        }   
                    });
                });</script>
