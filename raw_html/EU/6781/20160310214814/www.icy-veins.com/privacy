  jQuery(document).ready(function() {
    jQuery("#box_recent_posts .box_switch span").on("click", function () {
      if (!jQuery(this).hasClass("selected")) {
        jQuery("#box_recent_posts .box_switch span.selected").removeClass("selected");
        jQuery(this).addClass("selected");
        jQuery("#box_recent_posts ul.selected").removeClass("selected").addClass("not_selected");
