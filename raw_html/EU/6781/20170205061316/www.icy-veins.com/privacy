        <div id="switch_to_desktop">Force desktop version</div>
        <script type="text/javascript">
  jQuery("#switch_to_desktop").click(function() {
    var myDate = new Date();
    myDate.setMonth(myDate.getMonth() + 12);
    document.cookie = "force_desktop=yes;expires=" + myDate + ";domain=" + window.location.host.replace("www", "") + ";path=/";
    location.reload();
  });
        </script>
        <div id="switch_to_mobile">Force mobile version</div>
        <script type="text/javascript">
  jQuery("#switch_to_mobile").click(function() {
    var myDate = new Date();
    myDate.setMonth(myDate.getMonth() + 12);
    document.cookie = "force_desktop=no;expires=" + myDate + ";domain=" + window.location.host.replace("www", "") + ";path=/";
    location.reload();
  });
        </script>
