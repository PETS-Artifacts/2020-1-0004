  <script type="text/javascript">
// <![CDATA[
  jQuery(document).ready(function() {
    jQuery("#box_latest_news .box_switch span").on("click", function () {
      if (!jQuery(this).hasClass("selected")) {
        jQuery("#box_latest_news .box_switch span.selected").removeClass("selected");
        jQuery(this).addClass("selected");
        jQuery("#box_latest_news ul.selected").removeClass("selected").addClass("not_selected");
        game = jQuery(this).attr("id").replace("_switch_latest_news", "");
        jQuery("#box_latest_news ul#" + game + "_latest_news").removeClass("not_selected").addClass("selected");
      }
    });
  });
// ]]>
  </script>
