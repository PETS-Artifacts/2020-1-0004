      <script type="text/javascript">
      //<![CDATA[
  if (icy_veins_force_desktop) {
    jQuery(".nav_entry.nav_entry_with_content>a").on("touchstart", function(e) {
      if (jQuery(this).parent().hasClass('nav_entry_active')) {
        jQuery(this).parent().removeClass('nav_entry_active');
        return false;
      } 
      else {
        jQuery(this).parent().addClass('nav_entry_active');
        jQuery(".nav_entry").not(jQuery(this).parent()).removeClass('nav_entry_active');
        return false;
      }
    });
  }

  function set_focus_search_field() {
    if (jQuery("input.gsc-input").length == 0) {
      setTimeout(function() { set_focus_search_field(); }, 200);
    } else {
      jQuery("input.gsc-input").first().focus();
    }
  }

if (document.documentElement.clientWidth >= 550 && window.location.pathname.match(/^\/search/) == null) {
    jQuery("#search_link>a").click(function() {
      jQuery("#search_header_container").html('<gcse:searchbox-only resultsUrl="/search"></gcse:searchbox-only>');
      jQuery.getScript((document.location.protocol == 'https:' ? 'https:' : 'http:') + '//cse.google.com/cse.js?cx=000040352823895858750:oequpevo0d8', function() {
        jQuery("#header").addClass("search_bar_active");
        set_focus_search_field();
      });
      return false;
    });
  }


  if (jQuery("#mobile_menu_button").css('display') != 'none') {
    jQuery("#mobile_menu_button").click(function () {
      jQuery("#games").toggle();
      jQuery("#nav").toggle();
    });
    jQuery(".nav_entry_with_content>a").click(function(e) {
      e.preventDefault();
      jQuery(this).siblings(".nav_content").toggle();
    });
  }

  if (jQuery("#mobile_menu_button").css('display') == 'none' && !navigator.userAgent.match(/Android.*Chrome/)) {
    var minified = false;
    jQuery(window).scroll(function (event) {
      if (jQuery("#nav").length > 0) {
        if (jQuery(this).scrollTop() > 86 && minified == false) {
        jQuery("#header").addClass("minified");
        jQuery("#nav").addClass("minified");
        jQuery("#padding_for_minified_menu").css("height", "172px");
        minified = true;
        } else if (jQuery(this).scrollTop() <= 86 && minified == true) {
          jQuery("#header").removeClass("minified");
          jQuery("#nav").removeClass("minified");
          jQuery("#padding_for_minified_menu").css("height", "auto");
          minified = false;
        }
      } else {
        if (jQuery(this).scrollTop() > 80 && minified == false) {
        jQuery("#header").addClass("minified");
        jQuery("#nav").addClass("minified");
        jQuery("#padding_for_minified_menu").css("height", "130px");
        minified = true;
        } else if (jQuery(this).scrollTop() <= 80 && minified == true) {
          jQuery("#header").removeClass("minified");
          jQuery("#nav").removeClass("minified");
          jQuery("#padding_for_minified_menu").css("height", "auto");
          minified = false;
        }
      }
    });
  }
      //]]>
      </script>
