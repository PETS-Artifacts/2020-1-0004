function getCookie (name) {
  var key = name + '='
  var cookie = decodeURIComponent(document.cookie)
  var arr = cookie.split(';')

  for (var i = 0; i < arr.length; i++) {

    var c = arr[i]

    while (c.charAt(0) == ' ') {
      c = c.substring(1)
    }

    if (c.indexOf(key) == 0) {
      return c.substring(key.length, c.length)
    }
  }

  return null
}
var icy_veins_force_desktop = false;
if (document.cookie.match(/force_desktop=yes/)) {
  icy_veins_force_desktop = true;
}
var icy_veins_disable_minified_menu = false;
