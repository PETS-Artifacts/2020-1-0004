        <script>
  jQuery.get('https://use.fontawesome.com/6cbc36349f.js', function() {});

  jQuery.get('/forums/?app=icyveinsps&module=test&controller=topbar&do=getToken', function (response) {
    jQuery('input[name=csrfKey]').val(response)
  });

  if (getCookie('ips4_member_login')) {
    jQuery.get('/forums/?app=icyveinsps&module=test&controller=topbar&do=view', function (response) {
      jQuery('#user_bar').html(response);
      jQuery('#elUserNav').show();
      jQuery("#elUserLink").click(function(e) {
        if (jQuery("#elUserLink_menu").is(":visible")) {
          jQuery("#elUserLink_menu").hide();
        } else {
          jQuery("#elUserLink_menu").show();
        }
        e.preventDefault();
        return false;
      });
      jQuery("body").click(function(e) {
        if (jQuery("#elUserLink_menu").is(":visible") && e.target.getAttribute("id") != "elUserLink") {
          element = e.target;
          while (element.getAttribute("id") != "elUserLink_menu" && element.nodeName.toLowerCase() != "body") {
            element = element.parentElement;
          }
          if (element.getAttribute("id") != "elUserLink_menu") {
            jQuery("#elUserLink_menu").hide();
          }
        }
      });
      return false;
    })
  } else {
    jQuery("#elUserNav").show();
  }

  jQuery("#elUserSignIn").click(function(e) {
    if (jQuery("#elUserSignIn_menu").is(":visible")) {
      jQuery("#elUserSignIn_menu").hide();
    } else {
      jQuery("#elUserSignIn_menu").show();
    }
    return false;
  });
  jQuery("body").click(function(e) {
    if (jQuery("#elUserSignIn_menu").is(":visible") && e.target.getAttribute("id") != "elUserSignIn") {
      element = e.target;
      while (element.getAttribute("id") != "elUserSignIn_menu" && element.nodeName.toLowerCase() != "body") {
        element = element.parentElement;
      }
      if (element.getAttribute("id") != "elUserSignIn_menu") {
        jQuery("#elUserSignIn_menu").hide();
      }
    }
  });
        </script>
