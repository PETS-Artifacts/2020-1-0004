// <![CDATA[
  jQuery(document).ready(function() {
    jQuery("#box_popular_threads .box_switch span").on("click", function () {
      if (!jQuery(this).hasClass("selected")) {
        jQuery("#box_popular_threads .box_switch span.selected").removeClass("selected");
        jQuery(this).addClass("selected");
        jQuery("#box_popular_threads ul.selected").removeClass("selected").addClass("not_selected");
        game = jQuery(this).attr("id").replace("_switch_popular_threads", "");
        jQuery("#box_popular_threads ul#" + game + "_popular_threads").removeClass("not_selected").addClass("selected");
      }
    });
  });
// ]]>
  </script>
