<script>

        $(window).load(function() {

            var fblike_callback = function() {
                var fbid = '';
                fbid = FB.getUserID();
                if (!fbid && FB.getAuthResponse()) {
                    fbid = FB.getAuthResponse().userID || '';
                }

                $.ajax({
                    type: 'GET',
                    url: '//www.gofundme.com/iapi/index.php?route=share3/record_fbshare',
                    data: {
                        cat: 1,
                        page: 4,
                        platform: 0,
                        fbid: fbid
                    }
                });
            };

            // workaround to detect when someone clicks on iframe first time
            var listener = addEventListener('blur', function() {
                try {
                    if (document.activeElement === document.getElementsByClassName('home-fb-like')[0]) {
                        fblike_callback();
                    }
                } catch(err) {}
                removeEventListener(listener);
            });

        });

        $(document).ready(function(){
                $('.pg_msg a').attr('target', '_blank');
                $('.update_content a:not(.shareopts a)').attr('target', '_blank');

                //SIGN UP MOD
                $('body').click(function() {
                        $('.currency-select-div').hide();

                });
                $('.currency-select').click(function() {
                        $('.currency-select-div').show();
                        return false;
                });

                $('.currency-item').click(function() {
                        $('.currency-select-div').hide();
                        $('.currency-select').text($(this).text());
                        $('.currency-mark').val($(this).attr('mark'));
                        $('.currency-select').removeClass('c_usd c_gbp c_cad c_aud c_eur');
                        $('.currency-select').addClass('c_'+$(this).text().toLowerCase());
                        $('#currencyfield').val($(this).text());



                        return false;
                });
                $('.pricefield-2').click(function () {
                        setTimeout(function () {
                                $('.pricefield-1').focus();
                        }, 100);
                        return false;
                });
                $('.pricefield-2').focusin(function () {
                        setTimeout(function () {
                                $('.pricefield-1').focus();
                        }, 100);
                        return false;
                });

                $('.signupmod-link').click(function () {
                        $('form.signup-mod-form').eq(0).submit();
                        return false;
                });

