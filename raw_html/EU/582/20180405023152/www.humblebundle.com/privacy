require([
  'lib/lodash',
  'lib/require-domReady',
  'account/Modal',
  'lib/disableBodyScroll',
], function(
  _l,
  domReady,
  openAccountModal,
  DisableBodyScroll
) {
  domReady(function() {
    var Dropdown = (function() {
      function Dropdown(options) {
        var that = this;

        this.el = {
          $toggle: options.toggleEl,
          $container: options.containerEl,
          $caret: options.caretEl
        };

        this.$navbar = $('.js-navbar');

        this.events = {
          toggleClick: function(event) {
            event.stopPropagation();
            that.toggleState();
            that.render();
            if (that.visible) {
              return that.bindToDocument();
            } else {
              return that.unbindToDocument();
            }
          },
          hide: function() {
            that.visible = false;
            that.render();
            return that.unbindToDocument();
          }
        };

        if (_l.has(options, 'visible')) {
          this.visible = options.visible;
        } else {
          this.visible = true;
        }
        if (options.hiddenClass) {
          this.hiddenClass = options.hiddenClass;
        } else {
          this.hiddenClass = 'is-hidden';
        }
        if (options.selectedClass) {
          this.selectedClass = options.selectedClass;
        } else {
          this.selectedClass = 'is-selected';
        }
        this.validateOptions().bind().render();
      }

      Dropdown.prototype.validateOptions = function() {
        // Require both toggle and container or neither.
        // We assume something was missed if only one element is found.
        var messages;
        messages = {
          toggleNotFound: 'Toggle element not found',
          containerNotFound: 'Container element not found'
        };
        if (!this.el.$toggle.length && this.el.$container.length) {
          throw new Error(messages.toggleNotFound);
        }
        if (!this.el.$container.length && this.el.$toggle.length) {
          throw new Error(messages.containerNotFound);
        }
        return this;
      };

      Dropdown.prototype.remove = function() {
        this.unbind();
        delete this.el;
        delete this.events;
        return this;
      };


      // Ctrl: Event handling
      Dropdown.prototype.bind = function() {
        this.el.$toggle.on('click', this.events.toggleClick);
        return this;
      };

      Dropdown.prototype.unbind = function() {
        this.el.$toggle.off('click', this.events.toggleClick);
        return this;
      };

      Dropdown.prototype.bindToDocument = function() {
        $(document).on('click', this.events.hide);
        this.$navbar.find('.js-button-title').on('click', this.events.hide);
        return this;
      };

      Dropdown.prototype.unbindToDocument = function() {
        $(document).off('click', this.events.hide);
        return this;
      };

      // Ctrl: State handling
      Dropdown.prototype.toggleState = function() {
        this.visible = !this.visible;
        return this;
      };


      // View: DOM manipulation
      Dropdown.prototype.render = function() {
        if (this.visible) {
          this.$show();
        } else {
          this.$hide();
        }
        return this;
      };

      Dropdown.prototype.$hide = function() {
        this.el.$caret.removeClass('caret-active');
        if (!this.el.$container.hasClass(this.hiddenClass)) {
          this.el.$container.addClass(this.hiddenClass);
          this.el.$toggle.removeClass(this.selectedClass);
          this.$navbar.removeClass('secondary-dropdown-active');

          DisableBodyScroll.toggle(false, '.navbar-item-dropdown-items');

          // We want to ensure the navbar is still pinned even if a user has scrolled inside the active menu
          _l.delay(function() {this.$navbar.removeClass('headroom--unpinned').addClass('headroom--pinned')}.bind(this), 10);
        }
        return this;
      };

      Dropdown.prototype.$show = function() {
        this.el.$caret.addClass('caret-active');
        if (this.el.$container.hasClass(this.hiddenClass)) {
          this.el.$container.removeClass(this.hiddenClass);
          this.el.$toggle.addClass(this.selectedClass);
          this.$navbar.addClass('secondary-dropdown-active');

          _l.delay(function() {
            // It's important that we disable body scroll on a short timeout, so that it always occurs
            // _after_ another element has re-enabled scrolling, in the case of clicking directly
            // from one menu to another, for example.
            DisableBodyScroll.toggle(true, '.navbar-item-dropdown-items', true);
          }, 1)
        }
        return this;
      };

      return Dropdown;

    })();

    var allDropdowns = [
      {
        toggle: '.js-about-item-dropdown-toggle',
        container: '.about-item-dropdown-container',
        caret: '.about-item-dropdown-caret',
      },
      {
        toggle: '.js-user-item-dropdown-toggle',
        container: '.user-item-dropdown-container',
        caret: '.user-item-dropdown-caret',
      },
    ];

    allDropdowns.forEach(function(dropdownData) {
      var dropdown = new Dropdown({
        toggleEl: $(dropdownData.toggle),
        containerEl: $(dropdownData.container),
        caretEl: $(dropdownData.caret),
        fadeTime: 200,
        visible: false
      });
      dropdown.render();
    });

    $('.navbar-content .js-account-login').click(function() {
      openAccountModal({attributes: window.modal_attributes, name: 'login'});
    });
    $('.navbar-content .js-create-account').click(function() {
      openAccountModal({attributes: window.modal_attributes, name: 'signup'});
    });
    $('.js-navbar-item-account-button').click(function() {
      openAccountModal({attributes: window.modal_attributes, name: 'account-start'});
    });
  });
});
