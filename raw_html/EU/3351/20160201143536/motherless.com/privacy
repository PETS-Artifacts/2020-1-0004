            $(function() {
                try {
                    initThumbPopup();
                    
                    Shadowbox.init({
                        "animate": false,
                        "animateFade": false,
                        "displayCounter" : false,
                        "overlayOpacity" : 0.9,
                        "initialHeight": 432,
                        "onFinish" : function(el) {
                            var player = $(el.player);
                            var codename = $(el.link).attr('rev');
                            if (codename) {
                                if (el.player == 'img') {
                                    player.css({ cursor: "pointer" });
                                    
                                    player.click(function() {
                                        document.location.href = "/" + codename;
                                    });
                                    $.get("/view/popup", { "codename" : codename });
                                }
                                var html = '<a href="/favorites/add?codename=' + codename + '" id="media-link-favorites" rel="' + codename + '" class="pop plain">Add to Favorites</a>';
                                $('#sb-counter').html(html);
                            }
                        }
                    });
                } catch (e) {}
                
                $('#media-link-favorites').live('click', function() {
                    var target = $(this);
                    $.ajax({
                        url: '/favorites/add?codename=' + target.attr('rel'),
                        type: 'post',
                        success: function(data) {
                            target.html('Added to favorites');
                        }
                    });
                    return false;
                });
            });
