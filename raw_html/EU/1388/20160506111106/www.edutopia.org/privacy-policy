                  <header role="main" id="page-title">
                                <h1 class="page-title">Privacy Policy</h1>
      	      	      </header><!-- /#page-title -->
      <div class="element-invisible"><a id="main-content"></a></div>
            <section id="content" role="main" class="clearfix">
  <div class="gutter">
          <article id="node-447910" class="node node-page clearfix">


<div id="preface">
  <header>
    <h1 class="page-title">Privacy Policy</h1>
      </header>
</div><!--/#preface-->
        <div id="socialtools">
        <div class="inner">
          <div id="share-header"><span id="float-menu"><img src="/sites/all/themes/edutopia/images/share/share-menu.png" alt="Print" /></span><a href="/" class="logo-float"><img src="/sites/all/themes/edutopia/images/logo-float-small.png" alt="Print" /></a></div>
          <!-- AddThis Button BEGIN -->
          <div class="addthis_toolbox addthis_default_style" addthis:url="http://www.edutopia.org/privacy-policy">
            <ul class="sharebar"><li class="fb-share"><a class="addthis_button_facebook"><img src="/sites/all/themes/edutopia/images/share/share-fb.png" alt="Facebook" /></a></li><li class="tw-share"><a class="addthis_button_twitter"><img src="/sites/all/themes/edutopia/images/share/share-tw.png" alt="Twitter" /></a></li><li class="pn-share"><a class="addthis_button_pinterest_share" pi:pinit:media="http://www.edutopia.org/sites/all/themes/edutopia/images/share/pinterest.jpg"><img src="/sites/all/themes/edutopia/images/share/share-pn.png" alt="Pinterest" /></a></li><li class="addthis-share"><a class="addthis_button_more"><img src="/sites/all/themes/edutopia/images/share/share-addthis.png" alt="Share" /></a></li></ul>
          </div>
        </div>
      </div>
      <script type="text/javascript">
