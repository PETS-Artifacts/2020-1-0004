<script type="text/javascript">
<!--//--><![CDATA[//><!--
(function(i,s,o,g,r,a,m){i["GoogleAnalyticsObject"]=r;i[r]=i[r]||function(){(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)})(window,document,"script","//www.google-analytics.com/analytics.js","ga");ga("create", "UA-539491-1", "auto");ga("require", "linkid", "linkid.js");ga("require", "displayfeatures");ga("set", "dimension1", "node");ga("set", "dimension2", "page");ga("set", "dimension4", "NONE");ga("set", "dimension5", "NONE");ga("set", "dimension6", "NONE");ga("set", "dimension7", "NONE");ga("set", "dimension8", "NONE");ga("set", "dimension9", "anonymous user");ga("send", "pageview");(function($){
  // Debug flag
  var debugMode = false;

  // Default time delay before checking location
  var callBackTime = 100;

  // # px before tracking a reader
  var readerLocation = 150;

  // Set some flags for tracking & execution
  var timer = 0;
  var scroller = false;
  var endContent = false;
  var didComplete = false;

  // Set some time variables to calculate reading time
  var startTime = new Date();
  var beginning = startTime.getTime();
  var totalTime = 0;

  // Track the article load
  if (!debugMode) {
    //_gaq.push(['_trackEvent', 'Reading', 'ArticleLoaded', '', , true]);
    ga('send', 'event', 'Reading', 'ArticleLoaded', {'nonInteraction': 1});
  }

  // Check the location and track user
  function trackLocation() {
    bottom = $(window).height() + $(window).scrollTop();
    height = $(document).height();

    // If user starts to scroll send an event
    if (bottom > readerLocation && !scroller) {
      currentTime = new Date();
      scrollStart = currentTime.getTime();
      timeToScroll = Math.round((scrollStart - beginning) / 1000);
      if (!debugMode) {
        //_gaq.push(['_trackEvent', 'Reading', 'StartReading', '', timeToScroll]);
        ga('send', 'event', 'Reading', 'StartReading', '', timeToScroll);
      } else {
        alert('started reading ' + timeToScroll);
      }
      scroller = true;
    }

    // If user has hit the bottom of the content send an event
    if (bottom >= $('body.page-node div.node').scrollTop() + $('body.page-node section#content').innerHeight() && !endContent) {
      currentTime = new Date();
      contentScrollEnd = currentTime.getTime();
      timeToContentEnd = Math.round((contentScrollEnd - scrollStart) / 1000);
      if (!debugMode) {
        //_gaq.push(['_trackEvent', 'Reading', 'ContentBottom', '', timeToContentEnd]);
        ga('send', 'event', 'Reading', 'ContentBottom', '', timeToContentEnd);
      } else {
        alert('end content section '+timeToContentEnd);
      }
      endContent = true;
    }

    // If user has hit the bottom of page send an event
    if (bottom >= height && !didComplete) {
      currentTime = new Date();
      end = currentTime.getTime();
      totalTime = Math.round((end - scrollStart) / 1000);
      if (!debugMode) {
        // Pass reading time.
        ga('set', 'metric1', totalTime);

        // Pass the type of reader this is.
        if (totalTime < 60) {
          //_gaq.push(['_setCustomVar', 3, 'ReaderType', 'Scanner', 2]);
          ga('set', 'dimension10', 'Scanner');
        } else {
          //_gaq.push(['_setCustomVar', 3, 'ReaderType', 'Reader', 2]);
          ga('set', 'dimension10', 'Reader');
        }
        //_gaq.push(['_trackEvent', 'Reading', 'PageBottom', '', totalTime]);
        ga('send', 'event', 'Reading', 'PageBottom', '', totalTime);
      } else {
        alert('bottom of page '+totalTime);
      }
      didComplete = true;
    }
  }

  // Track the scrolling and track location
  $(window).scroll(function() {
    if (timer) {
      clearTimeout(timer);
    }

    // Use a buffer so we don't call trackLocation too often.
    timer = setTimeout(trackLocation, callBackTime);
  });
})(jQuery);
//--><!]]>
</script>
