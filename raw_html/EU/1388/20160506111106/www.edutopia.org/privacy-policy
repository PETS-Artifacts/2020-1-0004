var googletag = googletag || {};
googletag.cmd = googletag.cmd || [];/* SocialFlow campaign info */
googletag.cmd.push(function() {
  function sfcGetURLParameter(name) {
    var search_regexp = new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)');
    var search = search_regexp.exec(location.search) || [null, ""];
    var value = search[1];
    value = value.replace(/\+/g, '%20');
    return decodeURIComponent(value) || null;
  }
  googletag.pubads().setTargeting("SFCampaign", sfcGetURLParameter('sfc'));
});

googletag.slots = googletag.slots || {};
