<!-- For cart pop async loading of cart pop -->
<script type="text/javascript">

  if(!document.getElementById("cart_pop_replace")) {
    $(".cart_btn").one('mouseenter click focus', function(e) {
      $.ajax({
        url: "/cart/cart_pop"
      }).done(function() {
        Navigation.showCartAfterAjax();
        $('.cart_pop .cart_pop_show').css('display', 'block');
        $('.cart_btn').addClass('cart_pop_inserted');
        Navigation.cartPopInserted();
      });
    });
    
    $(".btn-checkout").one('mouseenter click focus', function(e) {
      $.ajax({
        url: "/cart/cart_pop"
      });
    });
  }
  
</script>

