});</script><script>// @ts-check
"use strict";

function _classCallCheck(instance, Constructor) {
  if (!(instance instanceof Constructor)) {
    throw new TypeError("Cannot call a class as a function");
  }
}

// This file is inlined in the page HTML
// Avoid importing any dependencies to keep it small

var TRACK_LINK_TIMEOUT = 300;
var USER_ID_COOKIE_KEY = "I";

var ArrayProto = Array.prototype;

/**
 * @property {Array<string>} services List of data sinks to send analytics to
 * @property {Object} gtmLayer The Google Tag Manager data layer
 *      @see {@link https://developers.google.com/tag-manager/devguide}
 * @property {Tracking.Analytics} analytics Analytics data passed from server to client
 * @property {Object|undefined} performance The Web Performance API, if available
 *      @see {@link https://developer.mozilla.org/en-US/docs/Web/API/Window/performance}
 * @property {Object} experiments A map of experiment names to active variation
 * @property {Object} section_properties A map of properties for different "sections" of the flow
 *      (ex: "search", "results", "checkout", etc.) usually sent with all events
 * @property {Object} state Hold arbitrary internal tracking state
 *      (ex: used in Checkout to send only certain events once)
 *
 * @implements {Tracking.Tracker}
 */

var Tracker = (function() {
  /**
   * @param {Object} gtmLayer
   * @param {Tracking.Analytics} analytics
   * @param {Experiments.Variations} experiments
   * @param {Object|undefined} [performance]
   * @param {() => Object|undefined} [getAmplitude]
   * @param {Object|undefined} [connection]
   * @param {Screen|undefined} [device_screen]
   */
  function Tracker(
    gtmLayer,
    analytics,
    experiments,
    performance,
    getAmplitude,
    connection,
    device_screen
  ) {
    _classCallCheck(this, Tracker);

    this.services = ["ga", "mixpanel", "customerio"];

    // Flag set on class instance to allow testing
    this._has_native_object_assign = typeof Object.assign === "function";

    this.gtmLayer = gtmLayer;
    this.analytics = analytics;
    this.performance = performance;
    this.getAmplitude = getAmplitude || function noop() {};
    this.connection = connection;
    this.device_screen = device_screen;

    // Format experiments data to shape used by GTM tags
    this.experiments = Object.keys(experiments).reduce(function(acc, xp_name) {
      var variation = experiments[xp_name];
      acc[xp_name] = { variation: variation };
      return acc;
    }, {});

    // Ensure defaults for analytics data
    this.analytics.config = this.analytics.config || {};
    this.analytics.page = {
      name: this.analytics.page.name,
      path: this.analytics.page.path || window.location.pathname,
      site_section: this.analytics.page.site_section || "none",
      previous_site_section:
        this.analytics.page.previous_site_section || "none",
      details: this.analytics.page.details || {},
      content: this.analytics.page.content || {}
    };
    this.analytics.connection = this.analytics.connection || {};
    this.analytics.device_screen = this.analytics.device_screen || {};

    this.section_properties = this.analytics.section_properties;

    this.state = {};
  }

  Tracker.prototype.init = function init() {
    // Push initial values to GTM data layer
    var gtm_base = this._assign(
      {},
      this.analytics.config,
      this.analytics.page,
      { content: null }
    );

    // Used by certain GTM tags, see:
    // https://tagmanager.google.com/#/container/accounts/3438046/containers/1204309/workspaces/99/variables
    var gtm_custom = {
      /** GTM Variable "operator name" */
      operator: this.analytics.page.details.operator,
      /** GTM Variable "partner name" */
      source: this.analytics.page.details.partner_source,
      /** GTM Variable "origin country" */
      origin_country: this.section_properties.search_origin_country,
      /** GTM Variable "route name" */
      route: this.section_properties.search_route,
      /** GTM Variable "trip type" */
      trip_type: this.section_properties.search_trip_type,
      /** GTM Variable "trip direction" */
      direction: this.analytics.page.details.results_direction,
      /** GTM Variable "discount code" */
      discount_code: this.section_properties.search_discount_code
    };

    var gtm_advanced = {
      ecommerce: this.analytics.ecommerce || {},
      user: this.analytics.user || {},
      experiments: this.experiments || {},
      section_properties: this.section_properties
    };

    this.gtmLayer.push(this._assign({}, gtm_base, gtm_custom, gtm_advanced));

    if (this.connection && this.connection.addEventListener) {
      this.connection.addEventListener(
        "change",
        this._saveNetworkInfo.bind(this)
      );
      this._saveNetworkInfo();
    }

    this._saveDeviceScreenInfo();

    if (this.analytics.config.amplitude_enabled) {
      this.amplitudeInit();
    }
  };

  /**
   * Track user interaction or other application event
   *
   * @param {Tracking.Event} ev
   * @param {Array<string>} [srv] Specify which data sinks to send to
   * @param {Function} [callback]
   */

  Tracker.prototype.track = function track(ev, srv, callback) {
    ev.services = srv || this.services;
    var bb_event = this._assign({}, ev, { ecommerce: null });
    var ecommerce = ev.ecommerce || this.analytics.ecommerce || {};
    var user = ev.user || this.analytics.user || {};
    var experiments = ev.experiments || this.experiments || {};

    this.gtmLayer.push({
      event: ev.trigger || "Manual event",
      bb_event: bb_event,
      ecommerce: ecommerce,
      user: user,
      experiments: experiments
    });
    this.gtmLayer.push({ bb_event: null });

    if (this.analytics.config.amplitude_enabled) {
      this.amplitudeEvent(this.getAmplitude(), {
        event_properties: bb_event,
        user_properties: user,
        purchased_products: ecommerce.purchase
          ? ecommerce.purchase.products
          : undefined,
        experiments: experiments,
        site_section: this.analytics.page.site_section,
        site_language: this.analytics.config.language,
        site_currency: this.analytics.config.currency,
        site_app: this.analytics.config.app
      });
    }

    if (callback) callback();
  };

  /**
   * Track a link click, deferring browser redirect so event has time to send
   *
   * @param {Tracking.Event} ev
   * @param {Event} dom_event
   * @param {Element} elem
   */

  Tracker.prototype.trackLink = function trackLink(ev, dom_event, elem) {
    var href = elem.getAttribute("href");

    this.track(ev);

    if (href && elem.getAttribute("target") !== "_blank") {
      dom_event.preventDefault();

      setTimeout(function() {
        if (href) window.location.href = href;
      }, TRACK_LINK_TIMEOUT);
    }
  };

  /**
   * Manually track a page view.
   * Only use this for "virtual page views" (ex: client-side routing),
   * as page views are tracked automatically when GTM loads.
   *
   * @param {Tracking.Page} ev
   * @param {Array<string>} [srv] Specify which data sinks to send to
   * @param {Function} [callback]
   */

  Tracker.prototype.page = function page(ev, srv, callback) {
    ev.services = srv || this.services;

    this.analytics.page.name = ev.name;
    this.analytics.page.path = ev.path;

    var bb_event = this._assign({}, ev, { ecommerce: null });
    var user = ev.user || this.analytics.user || {};
    var experiments = ev.experiments || this.experiments || {};

    this.gtmLayer.push({
      event: ev.trigger || "Virtual pageview",
      path: ev.path || this.analytics.page.path,
      bb_event: bb_event,
      ecommerce: ev.ecommerce || this.analytics.ecommerce || {},
      user: user,
      experiments: experiments
    });
    this.gtmLayer.push({ bb_event: null });

    if (this.analytics.config.amplitude_enabled) {
      this.amplitudePageview(this.getAmplitude(), {
        page_properties: this.analytics.page.details,
        event_properties: bb_event,
        user_properties: user,
        experiments: experiments,
        site_section: this.analytics.page.site_section,
        site_language: this.analytics.config.language,
        site_currency: this.analytics.config.currency,
        site_app: this.analytics.config.app
      });
    }

    if (callback) callback();
  };

  /**
   * Associate tracked events to this user
   *
   * @param {Tracking.User} user
   * @param {Function} [callback]
   */

  Tracker.prototype.identify = function identify(user, callback) {
    this.analytics.user.user_id = user.user_id;
    this.analytics.user.locale = user.locale;

    this._setCookie(USER_ID_COOKIE_KEY, user.user_id, 730, "/"); // 730 days = 2 years

    this.gtmLayer.push({
      event: "Identification",
      user: {
        user_id: user.user_id
      }
    });

    if (this.analytics.config.amplitude_enabled) {
      this.amplitudeIdentify(this.getAmplitude(), {
        user_id: user.user_id,
        user_properties: user
      });
    }

    if (callback) callback();
  };

  /**
   * Push arbitrary data to GTM's {@link DataLayer}
   *
   * @param {Object} payload
   * @param {Object} payload.section_properties Pushing new section properties
   *    will merge them with existing ones and make them available to be sent
   *    with future events
   */

  Tracker.prototype.push = function push(payload) {
    this.gtmLayer.push(payload);

    // Also update internal section properties so they can be sent with future events
    var section_properties = payload.section_properties;
    if (section_properties) {
      this.section_properties = this._assign(
        this.section_properties,
        section_properties
      );
    }
  };

  Tracker.prototype.amplitudeEvent = function amplitudeEvent(
    amplitude,
    payload
  ) {
    if (!amplitude) {
      return;
    }

    var event_properties = payload.event_properties || {};
    var purchased_products = payload.purchased_products;

    var amplitude_properties = event_properties.details || {};

    if (purchased_products) {
      var routes = [];
      var sources = [];
      var operators = [];
      purchased_products.forEach(function(p) {
        routes.push(p.name);
        sources.push(p.brand);
        operators.push(p.variant);
      });
      amplitude_properties.purchased_sources = sources;
      amplitude_properties.purchased_routes = routes;
      amplitude_properties.purchased_operators = operators;
      if (purchased_products.length > 0) {
        amplitude_properties.purchased_tickets =
          purchased_products.length * purchased_products[0].quantity;
      }
    }

    var amplitude_name = event_properties.action;
    var core_props = {
      site_section: this.analytics.page.site_section,
      previous_site_section: this.analytics.page.previous_site_section,
      locale: this.analytics.config.locale,
      language: this.analytics.config.locale,
      currency: this.analytics.config.currency,
      app: this.analytics.config.app
    };

    amplitude_properties = this._assign(
      core_props,
      this.section_properties,
      this._getExperimentProperties(),
      this._getWhitelabelProperty(),
      amplitude_properties,
      this.analytics.connection,
      {
        device_screen: this.analytics.device_screen,
        preferred_language: this.analytics.user.preferred_language
      }
    );

    amplitude.getInstance().logEvent(amplitude_name, amplitude_properties);
  };

  Tracker.prototype.amplitudeIdentify = function amplitudeIdentify(
    amplitude,
    payload
  ) {
    if (!amplitude) {
      return;
    }

    var user_id = payload.user_id;
    var user_properties = this._assign(
      {},
      this._omit(payload.user_properties || {}, [
        "client_id",
        "user_agent_quality",
        "currency",
        "logged_in",
        "phone"
      ])
    );

    if (user_properties.logged_in) delete user_properties.logged_in;

    var instance =
      amplitude && amplitude.getInstance && amplitude.getInstance();

    if (instance) {
      instance.setUserId(user_id);
      instance.setUserProperties(user_properties);
    }
  };

  Tracker.prototype.amplitudeIdentifyOnLanding = function amplitudeIdentifyOnLanding(
    amplitude,
    payload
  ) {
    if (!amplitude) {
      return;
    }

    var site_section = payload.site_section;
    if (this.analytics && this.analytics.page.name) {
      var identify = new amplitude.Identify().set(
        "landing_site_section",
        site_section
      );
      amplitude.getInstance().identify(identify);
    }
  };

  Tracker.prototype.amplitudePageview = function amplitudePageview(
    amplitude,
    payload
  ) {
    if (!amplitude) {
      return;
    }

    if (!this.analytics.page.name) {
      return;
    }

    var page_properties = payload.page_properties || {};
    var event_properties = payload.event_properties || {};
    var amplitude_properties = this._assign(
      {},
      page_properties,
      event_properties.details
    );

    var amplitude_name = this.analytics.page.name + "_pageview";
    amplitude_properties = this._assign(
      {
        site_section: this.analytics.page.site_section,
        previous_site_section: this.analytics.page.previous_site_section,
        locale: this.analytics.config.locale,
        language: this.analytics.config.locale,
        currency: this.analytics.config.currency,
        app: this.analytics.config.app,
        user_agent_quality: this.analytics.user.user_agent_quality
      },
      amplitude_properties,
      this.section_properties,
      this._getExperimentProperties(),
      this._getWhitelabelProperty()
    );

    amplitude.getInstance().logEvent(amplitude_name, amplitude_properties);
  };

  Tracker.prototype.amplitudeInit = function amplitudeInit() {
    // Initial page view
    this.amplitudePageview(this.getAmplitude(), {
      page_properties: this.analytics.page.details,
      event_properties: {},
      user_properties: this.analytics.user,
      experiments: this.experiments,
      site_section: this.analytics.page.site_section,
      site_language: this.analytics.config.language,
      site_currency: this.analytics.config.currency,
      site_app: this.analytics.config.app
    });

    // Set user properties on landing pages
    this.amplitudeIdentifyOnLanding(this.getAmplitude(), {
      site_section: this.analytics.page.site_section
    });
  };

  /**
   * Not used anymore. Remove once Customer.io GTM tags are completely removed.
   *
   * @deprecated
   * @param {any} _cio
   * @param {any} _payload
   */
  // eslint-disable-next-line no-unused-vars
  Tracker.prototype.cioEvent = function cioEvent(_cio, _payload) {};
  // noop

  /**
   * Not used anymore. Remove once Customer.io GTM tags are completely removed.
   *
   * @deprecated
   * @param {any} _cio
   * @param {any} _payload
   */
  // eslint-disable-next-line no-unused-vars
  Tracker.prototype.cioIdentify = function cioIdentify(_cio, _payload) {};
  // noop

  /**
   * @returns {Object}
   */

  Tracker.prototype._getExperimentProperties = function _getExperimentProperties() {
    var _this = this;

    return Object.keys(this.experiments).reduce(function(acc, xp_name) {
      acc["xp_" + xp_name] = _this.experiments[xp_name].variation;
      return acc;
    }, {});
  };

  /**
   * @returns {Object}
   */

  Tracker.prototype._getWhitelabelProperty = function _getWhitelabelProperty() {
    return {
      whitelabel: this.analytics.config.whitelabel_name
    };
  };

  /**
   * Populates this.analytics.network with some data from Browser Network Information API
   */

  Tracker.prototype._saveNetworkInfo = function _saveNetworkInfo() {
    this.analytics.connection.connection_type = this.connection.type;
    this.analytics.connection.connection_downlink = this.connection.downlink;
    this.analytics.connection.connection_rtt = this.connection.rtt;
    this.analytics.connection.connection_downlink_max = this.connection.downlinkMax;
    this.analytics.connection.connection_effective_type = this.connection.effectiveType;
    this.analytics.connection.connection_save_data = this.connection.saveData;
  };

  Tracker.prototype._saveDeviceScreenInfo = function _saveDeviceScreenInfo() {
    if (this.device_screen) {
      this.analytics.device_screen.availHeight = this.device_screen.availHeight;
      this.analytics.device_screen.availWidth = this.device_screen.availWidth;
      this.analytics.device_screen.height = this.device_screen.height;
      this.analytics.device_screen.width = this.device_screen.width;
    }
  };

  Tracker.prototype._assign = function _assign(target) {
    if (this._has_native_object_assign) {
      // @ts-ignore
      return Object.assign.apply(Object, arguments);
    }

    if (target === undefined || target === null) {
      throw new TypeError("Cannot convert undefined or null to object");
    }

    var output = Object(target);

    for (var index = 1; index < arguments.length; index++) {
      var source = arguments[index];

      if (source !== undefined && source !== null) {
        for (var next_key in source) {
          if (source.hasOwnProperty(next_key)) {
            output[next_key] = source[next_key];
          }
        }
      }
    }

    return output;
  };

  Tracker.prototype._omit = function _omit(obj) {
    var copy = {};
    var omit_keys = ArrayProto.concat.apply(
      ArrayProto,
      ArrayProto.slice.call(arguments, 1)
    );
    for (var key in obj) {
      if (omit_keys.indexOf(key) === -1) {
        copy[key] = obj[key];
      }
    }
    return copy;
  };

  Tracker.prototype._setCookie = function _setCookie(
    name,
    value,
    days,
    path,
    domain,
    secure
  ) {
    var expires = void 0;
    if (days) {
      var date = new Date();
      date.setTime(date.getTime() + days * 24 * 60 * 60 * 1000);
      // @ts-ignore
      expires = date.toGMTString();
    }
    if (!path) path = "/";
    if (!domain) {
      if (window.location.host.indexOf("busbud.com") !== -1) {
        domain = ".busbud.com";
      }
    }
    // @ts-ignore
    var cookie = name + "=" + escape(value);
    cookie += expires ? ";expires=" + expires : "";
    cookie += path ? ";path=" + path : "";
    cookie += domain ? ";domain=" + domain : "";
    cookie += secure ? ";secure" : "";
    document.cookie = cookie;
  };

  return Tracker;
})();

typeof module !== "undefined" && (module.exports = Tracker);

// Must wrap in a function instead of passing object directly
// because after Amplitude SDK loads it will overwrite
// the `window.amplitude` reference
function getAmplitude() {
  // @ts-ignore
  return window.amplitude;
}

if (typeof window !== "undefined") {
  var tracker = new Tracker(
    // @ts-ignore
    window.gtmLayer,
    // @ts-ignore
    window.BB.analytics,
    // @ts-ignore
    window.BB.experiments,
    window.performance,
    getAmplitude,
    // @ts-ignore
    window.navigator && window.navigator.connection,
    window.screen
  );

  // @ts-ignore Modules access `tracker` through the global `window.tracker` object
  window.tracker = tracker;
  // @ts-ignore GTM tags access `tracker` through the global `window.GTMtracker` object
  window.GTMtracker = tracker;

  tracker.init();
}
