<script>
	var oldQuery = '';
	function submitNewRelicTracker(query){
		// Prepare some information to send to NewRelic
		var viewportWidth = $(window).width();
		var viewportHeight = $(window).height();	
		var newRelicInfo = {
			'search_token' : '7e215cef81d521b38eed38a2f0a5f24e',
			'url' : '/privacy?main_page=privacy',
			'query' : query,
			'autocompleted' : '',
			'product_selected' : false,
			'viewportWidth' : viewportWidth,
			'viewportHeight' : viewportHeight
		};
		newRelicInfo['query'] = query;
		$.ajax({
			type: "POST",
			async: false,
			url: '/api/nr_search.php',
			data: newRelicInfo
		});
	}
	function autocompleteNewRelicTracker(autocompleted){
		// Prepare some information to send to NewRelic
		var viewportWidth = $(window).width();
		var viewportHeight = $(window).height();	
		var newRelicInfo = {
			'search_token' : '7e215cef81d521b38eed38a2f0a5f24e',
			'url' : '/privacy?main_page=privacy',
			'query' : oldQuery,
			'autocompleted' : autocompleted,
			'product_selected' : true,
			'viewportWidth' : viewportWidth,
			'viewportHeight' : viewportHeight
		};
		$.ajax({
			type: "POST",
			async: false,
			url: '/api/nr_search.php',
			data: newRelicInfo
		});
	}
	$( document ).ready(function() {  
		$("#search-form").submit(function(index) {
			// We need to send everything to NewRelic.
			submitNewRelicTracker($('#search').val());
		});
		$("#mobile-search-form").submit(function(index) {
			// We need to send everything to NewRelic.
			submitNewRelicTracker($('#mobile-search').val());
		});
	});
</script>
