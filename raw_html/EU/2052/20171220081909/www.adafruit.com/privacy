                                <input type="hidden" name="pid" value="{{objectID}}" />
                                <input type="hidden" name="qty" value="1" />
                                <input type="hidden" name="source_page" value="instant-search" />
                                <input type="hidden" name="source_id" value="instant-search" />
                                <button type="submit"  class="blue-button-large" data-pid="{{objectID}}" data-qty="1">{{button_text}}</button>
                            </form>                 
                        {{/add_to_cart}}
                        {{^add_to_cart}}
                            <a href="product/{{objectID}}"
                            class="ec_click_product"
                            data-pid="{{objectID}}"
                            data-name="{{name}}"
                            data-listname="Product Listing (more info) instant-search">
                                <button class="blue-button-large" >{{button_text}}</button>
                            </a>
                        {{/add_to_cart}}
                    </div>
                    <div class="price-stock col-lg-6 col-md-6 col-sm-6 col-xs-12">
                        {{^carrot_only}}
                        {{#on_sale}}
                            <div class="price "><span class="strikethrough">{{#price}}${{price}}{{/price}}</span><span class="red-sale-price"> ${{sale_price}}</span></div>
                            <div class="stock">
                                <span class='out-of-stock'>{{list_tag}}</span>
                            </div>                   
                        {{/on_sale}}
                        {{^on_sale}}
                            <div class="price">{{#price}}${{price}}{{/price}}</div>
                            <div class="stock">
                                <span class='out-of-stock'>{{list_tag}}</span>
                            </div>
                        {{/on_sale}}
                        {{/carrot_only}}
                    </div>
                </div>
            </div>
        </div>

        <!-- mobile view -->
        <div class="row top-twenty visible-xs">
            <div class="col-xs-12">
                {{#add_to_cart}}
                    <form action="added" method="post">
                        <input type="hidden" name="securityToken" value="72c5e04a2ba5670a8eadfd35f005d02c" />                    
                        <input type="hidden" name="action" value="add_product" />
                        <input type="hidden" name="pid" value="{{objectID}}" />
                        <input type="hidden" name="qty" value="1" />
                        <div class="mobile-button-row">
                        <button type="submit"  class="blue-button-large add_to_cart_ec" 
                        data-pid="{{objectID}}" 
                        data-qty="1"
                        data-name="{{name}}"
                        >ADD TO CART</button>
                        </div>
                    </form>
                {{/add_to_cart}}
                {{^add_to_cart}}                
                    <a href="product/{{objectID}}"
                    class="ec_click_product"
                    data-pid="{{objectID}}"
                    data-name="{{name}}"
                    data-listname="Product Listing (more info) instant-search">
                        <div class="mobile-button-row">
                            <button class="blue-button-large">MORE INFO</button>
                        </div>
                    </a>
                {{/add_to_cart}}
            </div>
        </div>
        <hr />
    {{/hits}}
</script>

