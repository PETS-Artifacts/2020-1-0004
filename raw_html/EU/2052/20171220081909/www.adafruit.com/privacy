function hideMessage(msgid) {
        var cont = document.getElementById('messaging-container-' + msgid);
        cont.style.display = 'none';
		cont.dispatchEvent(new CustomEvent('hidden'));

        document.cookie = "seen_msg_"+msgid+"=1; path=/;domain=.adafruit.com;";
    }
</script>
