	// see GTM facebook events
	function executeFbPixelEventQueue(timeoutCounter){
		timeoutCounter--;
		if (timeoutCounter >= 0){
			if (typeof fbq != "undefined" && fbq){
				fbPixelEventQueue.forEach(function(pushee){
					pushee();
				});
			}
			else{
				setTimeout(function(){executeFbPixelEventQueue(timeoutCounter)}, 600);
			}
		}
	}
