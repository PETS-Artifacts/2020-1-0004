	EC.tagCommander.sendColourSwatchesEvent($(this));
});

$('.tc-fredhopper-detailed-product-grid .tc-fredhopper-stock-availability').on('click', function () {
	EC.tagCommander.sendStockAvailabilityEvent($(this));
});

$('.tc-fredhopper-detailed-product-grid a').on('click', function () {
