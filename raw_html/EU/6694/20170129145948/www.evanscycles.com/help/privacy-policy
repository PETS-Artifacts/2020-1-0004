    EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': 'productcarousel-arrow', 'position': 'right', 'html_id': htmlID, 'campaign_id': campaignId});
});

$('.section-header').on('click', function () {
	var tcAccordionIsOpen;

	if($(this).parents('.tc-accordion-trigger').length > 0){
		// triggered from javascript
		return;
	} else if($(this).attr('aria-selected') != undefined) {
		tcAccordionIsOpen = $(this).attr('aria-selected') == 'false';
	} else {
		return;
	}
	var tcAction = tcAccordionIsOpen ? 'close' : 'open';
	var tcAccordionHtmlID = EC.tagCommander.tcGetHtmlId($(this));
	EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': 'accordion-component', 'html_id': tcAccordionHtmlID, 'action': tcAction});
});

$('.tc-show-toggle-button').on('click', function(e) {
	var toggleClass = '.' + $(this).attr('data-toggle');
	var campaignId = $(this).attr('data-campaignid');
	var tcIsShowedMore = $(toggleClass).hasClass('active');
	var tcAction = tcIsShowedMore ? 'paragraph-less' : 'paragraph-more';
	EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': tcAction, 'campaign_id': campaignId});
});

$('.tc-fredhopper-image').on('click', function () {
    var imageLinkTagHref = $(this).find('a,.link-block').attr('href');
    var imageDivHtmlID = $(this).attr('data-htmlid');
    var imageDivCampaignID = $(this).attr('data-campaignid');
    EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': 'image-component', 'html_id': imageDivHtmlID, 'link_url': imageLinkTagHref, 'campaign_id': imageDivCampaignID});
});

$('.tc-fredhopper-banner-area').on('click', function (event) {
  event.stopPropagation();
  imageDivHtmlID = $('.tc-fredhopper-banner-component').attr('data-htmlid');
  imageDivCampaignID = $('.tc-fredhopper-banner-component').attr('data-campaignid');
  areaHref = this.href;
  EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': 'banner-component', 'html_id': imageDivHtmlID, 'link_url': areaHref, 'campaign_id': imageDivCampaignID});
});

