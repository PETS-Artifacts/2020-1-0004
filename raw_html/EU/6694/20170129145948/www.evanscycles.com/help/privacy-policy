$('.main-nav-wrapper a').on('click', function () {
	var category1 = ACCMSG.messages.webCategoryRoot;
	var category2 = '';
	var category3 = '';
	if($(this).parent('.dim').length) {
		category2 = $(this).attr('title');
	} else {
		category2 = $(this).parents('.dim').find('a:first').attr('title');
		category3 = $(this).attr('title');
	}
	EC.tagCommander.tcEventTrigger($(this), 'dropdown', {'category1':category1, 'category2':category2, 'category3': category3});
});

$('#checkout__payment-place-order-btn').on('click',function() {
    var self = $(this);
	if($('#checkout__delivery-method-finance').is(':checked')) {
		EC.tagCommander.tcEventTrigger(self, 'button', {'click_name':'applied_finance'});
	}
});

