            $('.tc-login').click(function () {
                EC.tagCommander.tcEventTrigger($(this),'button',{click_name:'login', user_email: $('#j_username').val() })
            });


// ALL CALL TO ACTION
$(document).on('click','a, button, *[ng-click]', function() {
	var linkUrl = typeof $(this).attr('href') !== 'undefined' ? $(this).attr('href') : '';
	var htmlId = typeof $(this).attr('id') !== 'undefined' ? $(this).attr('id') : '';
	EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': 'call_to_action', 'html_id': htmlId, 'link_url': encodeURI(EC.tagCommander.tcAbsoluteUrl(linkUrl))});
});

// PAGINATION
            $(document).on('click','.tc-pagination-wrapper .bx-next',function() {
                var position = parseInt($(this).closest('.tc-pagination-wrapper').attr('data-tc-pagination-page')) + 2;
                EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': 'pagination', 'position': position});
            });

            $(document).on('click','.tc-pagination-wrapper .bx-prev:not(.disabled)',function() {
                var position = parseInt($(this).closest('.tc-pagination-wrapper').attr('data-tc-pagination-page'));
                EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': 'pagination', 'position': position});
            });

            $(document).on('click','.tc-pagination',function() {
                EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': 'pagination', 'position': $(this).attr('data-tc-pagination-page')});
            });


