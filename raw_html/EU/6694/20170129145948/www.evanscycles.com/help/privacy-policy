	var htmlID = $(this).attr('data-html-id');
	var linkUrl = $(this).attr('data-link-url');
    EC.tagCommander.tcEventTrigger($(this), 'button', {'click_name': 'category-link-component', 'html_id': htmlID, 'link_url': linkUrl});
});

