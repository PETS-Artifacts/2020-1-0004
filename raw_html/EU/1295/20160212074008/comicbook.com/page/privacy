			<!-- Start Sonobi -->
			<script>
				//Step 1
			    window.sbi_trinity = {};

			    //This object should include all of the placements that will be enabled with our implementation
			    var obj = {
			        'Middle1': '3402191bde3c7f6a91c3', // 300x250
			        'Middle2': 'ff21c24efc81bab057c1', // 300x250
			        'Middle3': 'dda32cfe2c83ff23a6db', // 300x250
			        'Position1': 'c08da0618f73ecb63ea0', // 300x250
			        'x91': '3c187990683044e3aa3a', // 300x250
			        'x96': '60229b688cd00c21f254', // 320x50 Mobile
			        'x93': '5c5c64c7f95680246fea', // 320x50 Mobile
			        'top': 'eb1578ca1d84c13bfd8c', // 728x90
			        'Top1': 'b8cde72def0c7599d78a' // 728x90
			    };

			    window.load_trinity = function (callback) {
			        //Timeout in case of unexpected slow load
			        var st = new Date().getTime();
			        var td = 1500;
			        var ran = false;
			        var f = (function () {
			            try {
			                if (new Date().getTime() >= st + td) {
			                    if (!ran) callback();
			                    ran = true;
			                    return;
			                }
			            } catch (e) {}
			            setTimeout(f, 2);
			        })
			        f();
			        //Loading the trinity object
			        var sbi_script = document.createElement('script');
			        sbi_script.async = true;
			        sbi_script.type = 'text/javascript';
			        sbi_script.src = 'http://apex.go.sonobi.com/trinity.js?key_maker=' + JSON.stringify(obj) + '&s=' +
			            Math.floor(Math.random() * 1000);
			        if (callback == null) callback = function () {};
			        if (sbi_script.readyState) {
			            sbi_script.onreadystatechange = function () {
			                if (sbi_script.readyState == "loaded" || sbi_script.readyState == "complete") {
			                    sbi_script.onreadystatechange = null;
			                    if (!ran) callback();
			                    ran = true;
			                }
			            };
			        } else {
			            sbi_script.onload = function () {
			                if (!ran) callback();
			                ran = true;
			            };
			        }
			        var sonobiNode = document.getElementsByTagName('script')[0];
			        sonobiNode.parentNode.insertBefore(sbi_script, sonobiNode);
			    };

		        //Define OAS Site page
		        (function () {
		            oas_tag.version = '1';
		            oas_tag.url = 'oasc-training7.247realmedia.com'; //Define OAS URL
		            oas_tag.site_page = 'sonobi.com';
		            var oas = document.createElement('script'),
		                protocol = 'https:' == document.location.protocol ? 'https://' : 'http://',
		                node = document.getElementsByTagName('script')[0];
		            oas.type = 'text/javascript';
		            oas.async = true;
		            oas.src = protocol + oas_tag.url + '/om/' + oas_tag.version + '.js';
		            sonobiNode.parentNode.insertBefore(oas, sonobiNode);
		        });

			    window.oas_tag = {};
			    oas_tag.allowSizeOverride = true;
			    //Wrap the call to OAS with the method to retrieve key/values specified above
			    load_trinity(function () {
			        if (oas_tag.query == null) oas_tag.query = '';
			        if (Object.keys(sbi_trinity).length > 0) {
			            for (var placement in sbi_trinity) {
			                if (Object.keys(sbi_trinity[placement]).length > 0) {
			                    var priority = sbi_trinity[placement].sbi_apoc;
			                    var size = sbi_trinity[placement].sbi_size;
			                    if (oas_tag.query != '') oas_tag.query += '&';
			                    oas_tag.query += 'sbi_' + size + '_' + priority + '_' + placement + '=true';
			                }
			            }
			        }
			        oas_tag.sizes = function () {
			            for(var placement in obj) {
			                if(placement in sbi_trinity && sbi_trinity[placement].sbi_size){
			                    oas_tag.definePos(placement, sbi_trinity[placement].sbi_size.split('x'));
			                } else {
			                    oas_tag.definePos(placement, obj[placement]);
			                }
			            };
			        }
			    });
			</script>
			<!-- End Sonobi -->

