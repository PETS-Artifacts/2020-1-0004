<script type="text/javascript">
        $(document).ready(function() {
            $("#header .sfsearchTxt").keypress(function(e) {
                if (e.which == 13) {
                    $(this).closest(".sfsearchBox").find(".sfsearchSubmit").addClass("load");
                    window.location = '/search?indexCatalogue=full-index&searchQuery=' + $(".sfsearchTxt").val() + '&wordsMode=0'
                }
            });
            $("#header #search input[type=submit]").click(function() {
              window.location = '/search?indexCatalogue=full-index&searchQuery=' + $(".sfsearchTxt").val() + '&wordsMode=0'
                $(this).addClass("load");
              	return false;
            });
        });
