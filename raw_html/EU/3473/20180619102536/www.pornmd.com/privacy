function loadCSS() {
	//if (typeof loadCSS !== 'undefined') {
		head.load(loadCSS);	
	//}
}

function loadJS() {
	// Prepare array of JS files to be loaded by HeadJS
	pmdApp.loadHeadJS = [];
	for( var i=0, j=pmdApp.loadJS.length; i<j; i++ ){
		var obj = {};
		obj[pmdApp.loadJS[i].label] = pmdApp.loadJS[i].url;
		pmdApp.loadHeadJS.push(obj);
	}
	// Place jQuery first in the list
		pmdApp.loadHeadJS.unshift({ url     :   "//cdn1d-static-shared.phncdn.com/jquery-2.0.3.js" });
	
	pmdApp.loadHeadJS.push( {"pmdReady" : "//e-static.pornmd.com/js/pmd.ready.js?cache=7b6b234b19db23ba811c81c4b71d8f8622d1ee0f1533235403" } );
	//load JS
	head.load(pmdApp.loadHeadJS);
}

if ((typeof window.performance === 'undefined') || (pmdApp.utmMedium)) {
	loadJS();
} else {
	var mgPerformanceCallbacks = [ function(){
			//if (typeof loadCSS !== 'undefined') loadCSS();
			loadJS();	
		}
	];
}

