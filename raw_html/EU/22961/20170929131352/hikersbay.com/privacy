<script>
$(window).load(function() {
var max_box_size = 0;
_.each($('.hotel-box-left'), function(el) {
  var size = $(el).height();
    if (size > max_box_size) max_box_size = size;
  })
  if (max_box_size > 0) {
    $('.hotel-box-left').height(max_box_size)
  }
})
</script>
