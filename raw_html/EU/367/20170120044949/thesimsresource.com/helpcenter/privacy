 	<script type='text/javascript'>
    
    //Default to not displaying ads
    window.googletag = window.googletag || {};
	googletag.cmd = googletag.cmd || [];
    window.displayAd = function(elmId) {
      window.googletag.cmd.push(function() {
        console.log("Display ad "+elmId);
        window.googletag.display(elmId);
      });
    };
    window.m2hb = window.m2hb || {};
    m2hb.que = m2hb.que || [];
    m2hb.ssl = 'https:' == document.location.protocol;
    window.M2_TIMEOUT = 1000;
    m2hb.maxRetries = 20;
    m2hb.retries = 0;

    m2hb.loadJS = function(src, async) {
        m2hb.loadedJS = m2hb.loadedJS || [];
        m2hb.loadedJS.push(src);
        (function() {
         var script = document.createElement("script");
         script.async = async || true;
         script.type = "text/javascript";
         script.src = (m2hb.ssl ? 'https:' : 'http:') + src;
         var target = document.getElementsByTagName("head")[0];
         target.insertBefore(script, target.firstChild);
         })();
    };
    if (typeof m2hb.initAdserver == 'undefined') {
        m2hb.initAdserver = function() {
            if (m2hb.initAdserverSet) return;
            if (typeof m2hb.timeRemaining == 'undefined') {
                m2hb.timeRemaining = M2_TIMEOUT;
            }
            if (m2hb.timeRemaining > 0) {
                m2hb.timeRemaining -= 100;
                if (!m2hb.m2hbBidsReady) {
                    setTimeout(function() {m2hb.initAdserver();}, 100);
                    return;
                }
            }
            if (!googletag.pubadsReady && (m2hb.retries <= m2hb.maxRetries)) {
                m2hb.retries++;
                setTimeout(function() {m2hb.initAdserver();}, 50);
                return;
            }
            m2hb.initAdserverSet = true;
            googletag.cmd.push(function () {
                m2hb.que.push(function () {
                    m2hb.setTargetingForGPTAsync();
                });
                googletag.pubads().refresh();
            });
        };
    }
    
        
      //Begin ad codes
      if( forceLogout || !isVIP()) {
        var M2EL_SRC = "//m2d.m2.ai/m2hb.TSR.desktop.frontpage-ROS.js";
        var adConfig = { newhbs: true };
        if(!adConfig) adConfig = {};

        function initAdserver() {
            if(adConfig.newhbs) return m2hb.initAdserver();
            if (m2hb.initAdserverSet) {
                console.log('NOT loading gpt');
                return;
            }
            (function() {
                var gads = document.createElement('script');
                gads.async = true;
                gads.type = 'text/javascript';
                var useSSL = 'https:' == document.location.protocol;
                gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
                var node = document.getElementsByTagName('script')[0];
                node.parentNode.insertBefore(gads, node);
            })();
            m2hb.initAdserverSet = true;
        };
        function initM2H() {
          var m2El = document.createElement("script"); m2El.type = "text/javascript";
          m2El.async = true; var isHttps = 'https:' === document.location.protocol;
          m2El.src = (isHttps ? "https://" : "http://") + M2EL_SRC;
          var m2TargetEl = document.getElementsByTagName("head")[0];
          m2TargetEl.insertBefore(m2El, m2TargetEl.firstChild);
        };
        if(!adConfig.newhbs) {
            initM2H();
            setTimeout(initAdserver, M2_TIMEOUT);
        } else {
            m2hb.loadJS(M2EL_SRC);
            googletag.cmd.push(function () {
              googletag.pubads().disableInitialLoad();
              m2hb.disabledGptInitialLoad = true;
            });
        }

        googletag.cmd.push(function() {
          googletag.pubads().set("adsense_background_color", "EDEDED");
          googletag.pubads().set("page_url", "http://www.thesimsresource.com/helpcenter/privacy");
          var adsLoadedEventTimer = 0;
          var adsDisplayed = false;
          var timeNow = new Date().getTime();
          googletag.pubads().addEventListener('impressionViewable', function(event) {
              var impressionTime = (new Date().getTime() - timeNow);
              console.log("Impression viewable "+impressionTime+" ms");
              $('body').removeClass('has-adblock');
              window.isAdBlocked = false;
              if(window.sessionStorage) {
                window.sessionStorage.loadedAds = 3;
              }
              window.iHaveLoadedAds = true;
              if(adsDisplayed) return;
              if(adsLoadedEventTimer) {
                  clearTimeout(adsLoadedEventTimer);
              }
              adsLoadedEventTimer = setTimeout(function() {
                      adsDisplayed = true;
                      if(window.onAdsLoaded) window.onAdsLoaded();
              }, 300); //impressionTime > 5000 ? 0 : 2000);
          });
        });

        if(adConfig.newhbs) {
            m2hb.loadJS('//www.googletagservices.com/tag/js/gpt.js');
            m2hb.initAdserver();
        }

        googletag.cmd.push(function() {
                                                                                          googletag.defineSlot('/88938325/RestOfSite-LEADER-ATF-TOP', [[728, 90],[930, 180],[970, 66],[970, 90],[970, 250],[980, 120],[980, 240]], 'div-gpt-ad-1425592159215-18').addService(googletag.pubads());
                                        googletag.defineSlot('/88938325/RestOfSite-RECTANGLE-ATF-TOP', [[300, 250],[300, 600]], 'div-gpt-ad-1425592159215-21').addService(googletag.pubads());
                                        googletag.defineSlot('/88938325/RestOfSite-RECTANGLE-BTF-MID1', [[300, 250]], 'div-gpt-ad-1425592159215-22').addService(googletag.pubads());
                                        googletag.defineSlot('/88938325/RestOfSite-RECTANGLE-BTF-MID2', [[300, 250]], 'div-gpt-ad-1425592159215-23').addService(googletag.pubads());
                                        googletag.defineSlot('/88938325/RestOfSite-RECTANGLE-BTF-MID3', [[300, 250]], 'div-gpt-ad-1425592159215-24').addService(googletag.pubads());
                                        googletag.defineSlot('/88938325/RestOfSite-LEADER-BTF-MID', [[728, 90],[930, 180],[970, 66],[970, 90],[970, 250],[980, 120],[980, 240]], 'div-gpt-ad-1425592159215-20').addService(googletag.pubads());
                                        googletag.defineSlot('/88938325/RestOfSite-LEADER-BTF-BOT', [[728, 90],[930, 180],[970, 66],[970, 90],[970, 250],[980, 120],[980, 240]], 'div-gpt-ad-1425592159215-19').addService(googletag.pubads());
                                        googletag.defineSlot('/88938325/Sitewide-SITESKIN', [[1, 1]], 'div-gpt-ad-1425592159215-25').addService(googletag.pubads());
                    
          if(!adConfig.newhbs) {
              m2hb.que.push(function() {
                m2hb.setTargetingForGPTAsync();
              });
          } else {
              googletag.pubads().enableSingleRequest();
          }
          googletag.enableServices();
        });
      } //End ad codes
    
