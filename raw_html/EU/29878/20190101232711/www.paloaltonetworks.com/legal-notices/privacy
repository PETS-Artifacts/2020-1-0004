	<script type="text/javascript">
	(function($) {
		var footerLinks = document.querySelectorAll('.pan-mega-footer li > a');
		console.log('footerlinks: ', footerLinks);
		var linksArr = Array.prototype.slice.call(footerLinks);
		linksArr.forEach( function(link){
			var linkSection = $(link).closest('.col-xs-12').find('h3').text()
			if(linkSection){
				var linkPrefix = 'footer:' + linkSection + ':'
				var linkText = $(link).text()
				var linkTrackVal = (linkPrefix + linkText).toLowerCase()
				$(link).attr('footer-track', true)
				$(link).attr('nav-track-breadcrumb', linkTrackVal)
			}
		});
	})(jQuery);
	</script>

