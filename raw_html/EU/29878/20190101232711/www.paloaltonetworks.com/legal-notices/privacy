<script type="text/javascript">
	var navConfigPath = '/content/pan/en_US/newTopNav';
	var dataUrl = '/apps/pan/public/mainNavigation.json'+navConfigPath+'.json'
	window.isNavInitialized = false;
	var currentPagePathSplit = navConfigPath.split("_")[1];
	if(currentPagePathSplit.length > 0){
		var currentLang = currentPagePathSplit.split("/newTopNav")[0];
	}
	if(currentLang == "US"){
		currentLang = "EN";
	}
	$('.currentLang').html(currentLang);
	if(!(true)){
        $('.nav-search-button').css("border-right","0px");
		$('.nav-search-button').css("margin-right","0px");        
    }

	function initPanNav() {
        $.get( dataUrl, function(data) {
			var target = document.getElementById('pan-nav');
			var panNav = new PanNav(target,data,window.userHeaderModel);
					window.isNavInitialized = true;
					panNav.registerEvents();
		})
	}

	$(document).ready(function() {
			if (typeof isJanrainProfilePage == 'undefined') {
				initPanNav();
			}
	})

</script>

		
		
	

	
	
		
		
			
		
		
		
		
		
