    for (var i = 0; i < preloadImages.length; ++i) {
        var img = new Image();
        img.src = preloadImages[i];
    }
    $(document).ready(function() {
        var documentHeight = $(document).height();
        var footerHeight = $(".footer").height() + $("#footerBoxNav").height();
        setTimeout(function(){
            $(".privacy").css("height", $(".privacy .content").height());
        }, 500);
        $(window).resize(function(){
            $(".privacy").css("height", $(".privacy .content").height());
        });
        //var bodyBottom = documentHeight - footerHeight - $(".privacy .illustration").height() - 100;
        var bodyBottom = documentHeight - footerHeight - 45;
        var breadcrumbHeight = 53;
        var imageFolderRoot = "/content/dam/pan/en_US/images/privacy/"

        var length = $(".privacy .content").height() - $(".privacy .illustration").height() + $(".privacy .content").offset().top;

        /*
         * Fixed header area (init)
         */
        if ($(window).scrollTop() >= breadcrumbHeight) {
            $(".privacy .heading").addClass("fixed");
        }
        else {
            $(".privacy .heading").removeClass("fixed");
        }

        var illuToPrivacyBottom = $(".privacy").height() + $("navbar").height() + breadcrumbHeight - ($(".privacy .illustration").offset().top + $(".privacy .illustration").height() ) +185;
        $(window).scroll(function() {
            /*
             * Stop illustration from scrolling all the way into the footer
             */
            //if ($(window).scrollTop() + $(window).height() >= $(document).height() - $(".footer").height() - $("#footerBoxNav").height() - 45 -67) {
            var illuToPrivacyBottom = $(".privacy").height() + $("navbar").height() + breadcrumbHeight - ($(".privacy .illustration").offset().top + $(".privacy .illustration").height() );
            if ( $(window).scrollTop() + $(window).height()  >= $(".privacy").height() + $("navbar").height() + breadcrumbHeight &&
                illuToPrivacyBottom <= 0) {
                var absoluteTop = $(".privacy").height() + $("navbar").height() + breadcrumbHeight - $(".privacy .illustration").height() ;
                absoluteTop = absoluteTop + "px";
                $(".privacy .illustration").css({"position": "absolute", "bottom": "47px"}); //47 +66
            }
            else {
                $(".privacy .illustration").css({"position": "fixed", "bottom": ""});
            }

            /*
             * Fixed header area
             */
            if ($(window).scrollTop() >= 53) {
                $(".privacy .heading").addClass("fixed");
            }
            else {
                $(".privacy .heading").removeClass("fixed");
            }

            /*
             * Heading change with scroll
             */
            var sectionTopBuffer = 250;
            var headingTopBuffer = 100;
            $(".privacy section").each(function(){
                var currentId = $(this).attr("id");
                var currentIcon = "";
                if ($(this).attr("id") !== undefined) {
                    currentIcon = ( ($(this).attr("data-icon") !== undefined) ? $(this).attr("data-icon") : $(this).attr("id") );
                    currentIcon = "shade-" + currentIcon + ".png";

                    if ($(window).scrollTop() >= $(this).offset().top - sectionTopBuffer - headingTopBuffer) {
                        var currentHeading = $("#" + currentId + " h2").html();
                        $(".heading span").html(currentHeading);
                        $(".privacy .illustration .shade").show();
                        $(".privacy .illustration .shade").attr("src", imageFolderRoot + currentIcon);
                    }
                }
                else if (currentId === undefined){
                    $(".heading span").html("Privacy");
                    $(".privacy .illustration .shade").hide();
                }
            });
        });

        /*
         * Select menu to scroll
         */
        $(".heading select").change(function() {
            var selectedId = $(".heading select option:selected").val();
            var selectedVal = $(".heading select option:selected").html();
            var breadcrumbHeight = 96;
            var sectionTopBuffer = 20;
            $("html, body").animate({
                scrollTop: $("#" + selectedId).offset().top - breadcrumbHeight - $(".heading").height() - sectionTopBuffer
            }, 1000);
            setTimeout(function(){
                $(".heading span").html(selectedVal);
                $(".heading select option:eq(0)").prop("selected", true);
            }, 1200);
        });

        /*
         * Adjust main content area width
         */
        var illustrationMinWidth = 480;
        var initWindowWidth = $(window).width();
        if (initWindowWidth <= 1600 && initWindowWidth > 1010){
            $(".privacy .content").width(initWindowWidth - illustrationMinWidth);
            $(".privacy .content .heading").width(initWindowWidth - illustrationMinWidth);
            $(".privacy .illustration").css({"right": "", "left": "-15px"});
        }
        else if (initWindowWidth <= 1010) {
            $(".privacy .content").width("75%");
            $(".privacy .content .heading").width("100%");
            $(".privacy .content .heading.fixed").width("75%");
            $(".privacy .illustration").css({"right": "60%", "left": ""});
        }
        else {
            $(".privacy .content").width("70%");
            $(".privacy .content .heading").not($(".heading.fixed")).width(currentContentWidth);
            $(".privacy .content .heading.fixed").width(currentContentWidth);
            $(".privacy .illustration").css({"right": "70%", "left": ""});
        }
        $(window).resize(function(){
            var currentWindowWidth = $(window).width();
            var currentContentWidth = $(".privacy .content").width();
            if (currentWindowWidth <= 1600 && currentWindowWidth > 1010){
                $(".privacy .content").width(currentWindowWidth - illustrationMinWidth);
                $(".privacy .content .heading").width(currentWindowWidth - illustrationMinWidth);
                $(".privacy .illustration").css({"right": "", "left": "-15px"});
            }
            else if (currentWindowWidth <= 1010) {
                $(".privacy .content").width("75%");
                $(".privacy .content .heading").width("100%");
                $(".privacy .content .heading.fixed").width("75%");
                $(".privacy .illustration").css({"right": "60%", "left": ""});
            }
            else {
                $(".privacy .content").width("70%");
                $(".privacy .content .heading").not($(".heading.fixed")).width(currentContentWidth);
                $(".privacy .content .heading.fixed").width($currentContentWidth);
                $(".privacy .illustration").css({"right": "70%", "left": ""});
            }
        });
    });
</script>

