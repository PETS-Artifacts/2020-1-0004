<script type="text/javascript" >
   function getLocationOrigin() {
        var origin = window.location.origin;
        if (!window.location.origin) {
            origin = "//" + window.location.hostname + (window.location.port ? ":" + window.location.port : "");
        }
        return origin;
    }

    var pageOrigin = getLocationOrigin();

    function frogEvent(e)
    {
        e.preventDefault();
        var button = e.currentTarget;
        var frogType = button.getAttribute("frog-type");
        if (frogType)
        {
            var dest = button.getAttribute("href");
            var name = button.getAttribute("frog-name");
            var target = button.getAttribute("target");
            new Image().src = "//frog.wix.com/hp?src=19&evid=312&type=" + frogType + "&url=" + dest + "&button_name=" + name;
            window.open(dest, target);
        }

    }
    var frogs = document.getElementsByClassName('frog');
    for (var i = 0; i < frogs.length; i++)
    {
        frogs[i].addEventListener('click',frogEvent,true);
    }

</script>

        
    </div>
    
    
    
    <div class="hiddenTillReady hiddenTillReadyBottom">
        
         <!-- Embed Styles for Header and Footer Component -->
 <style>
    body.wix-menu-enabled{
        padding-top: 78px;
    }
    #wix-menu {
        position: absolute;
        top: 0;
        width: 100%;
    }
 </style>
 
 <script type="text/javascript">
 
   var selectedNavMap = {
      '/': 'wm-homepage-link',
      '/about/features': 'wm-explore-link',
      '/ecommerce/website': 'wm-ecommerce-link',
      '/domain/names': 'wm-subscriptions',
      '/about/mailboxes': 'wm-subscriptions',
      '/ecommerce/website': 'wm-ecommerce-link',
      '/about/contact-us': 'wm-contact-us',
      '/upgrade/website': 'wm-subscriptions',
      '/blog': 'wm-mobile-blog',
      '/stories': 'wm-stories'
    }
 
    function isHeaderAndFooterMobile() {
        var isAndroidMobile = /Android/.test(navigator.userAgent) && /Mobile/.test(navigator.userAgent);
        return /webOS|iPhone|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent) || isAndroidMobile;
    }
 
    if (isHeaderAndFooterMobile()) {
        document.getElementById("wix-menu").style.display = "none";
        setTimeout(function() {
            document.getElementById("wix-menu").style.display = "block";
            W.Site.Common.DefaultLayout.init({
                mobileOptimized: true,
                selectedNav: selectedNavMap[window.location.pathname],
                showTopBanner: false
            });
        }, 1000);
    } else {
        W.Site.Common.DefaultLayout.init({
            mobileOptimized: true,
            selectedNav: selectedNavMap[window.location.pathname],
            showTopBanner: false
        });
    }
 </script>
        
    </div>
    
    
    
