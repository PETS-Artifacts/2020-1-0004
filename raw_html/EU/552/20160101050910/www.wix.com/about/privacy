<script type="text/javascript" >
    function frogEvent(e)
    {
        var frogType = e.currentTarget.getAttribute("frog-type");
        if (frogType)
        {
            var dest = e.currentTarget.getAttribute("href");
            new Image().src = "//frog.wix.com/hp?src=19&evid=312&type=" + frogType + "&url=" + dest;
        }
    }
    var frogs = document.getElementsByClassName('frog');
    for (var i = 0; i < frogs.length; i++)
    {
        frogs[i].addEventListener('click',frogEvent,true);
    }
</script>

