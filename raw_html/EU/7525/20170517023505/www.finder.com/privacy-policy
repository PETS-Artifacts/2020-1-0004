<script>
  function ready(fn) {
    if (document.readyState != 'loading'){
      fn();
    } else {
      document.addEventListener('DOMContentLoaded', fn);
    }
  }

  ready(function () {
    // Credit Score or Finder Academy
    var finderAcademy = window.location.pathname.indexOf('course-overview') >= 0;
    var creditScore = window.location.pathname.indexOf('credit-score') >= 0;

    // Login Modal
    var modalTrigger = document.querySelector('.js-user-login-component');
    var loginModal = document.querySelector('.js-sitewide-login-modal');
    var loginCloseBtn = loginModal.querySelector('.js-sitewide-login-modal-close');
    var loginOverlay = loginModal.querySelector('.js-sitewide-login-modal-overlay');

    // Modal triggers
    var courseOverviewBtn = Array.prototype.slice.call(document.querySelectorAll('.js-fa-overview-button'));
    var couserOverviewLessonTitle = Array.prototype.slice.call(document.querySelectorAll('.js-fa-course-overview__lesson-title'));

    var onSuccesUrl = '/fa-home-buying-guide-step-1-preliminary-lifestyle-check';
    var isUserLoggedIn = false;

    var hideLoginModal = function () {
      $.trackEvent('Top Nav sign up/in', 'click', 'Modal close', 0, true);
      loginModal.setAttribute('aria-hidden', true);
    };
    var openLoginModal = function () {
      $.trackEvent('Top Nav sign up/in', 'click', 'Register / Login', 0, true);
      loginModal.setAttribute('aria-hidden', false);
    };
    var scrollToLogin = function () {
      $('html, body').animate({
        scrollTop: $('#user-login-anchor').offset().top
      }, 'slow');
    };

    // Bind event listeners
    if (!creditScore && !isUserLoggedIn) {
      modalTrigger.addEventListener('click', openLoginModal);
      loginCloseBtn.addEventListener('click', hideLoginModal);
      loginOverlay.addEventListener('click', hideLoginModal);
    } else if (creditScore && !isUserLoggedIn) {
      // credit score page needs to scroll to the user login component
      modalTrigger.addEventListener('click', scrollToLogin);
    }

    // finder academy buttons and links on course overview page
    courseOverviewBtn.concat(couserOverviewLessonTitle).forEach(function (btn) {
      btn.addEventListener('click', handleLogin);
    });

    function handleLogin(e) {
      // Get href for callback on successful login
      var href = this.getAttribute('href');
      if (href) {
        onSuccesUrl = href;
      }

      if (!isUserLoggedIn) {
          e.preventDefault();
          openLoginModal();
      }
    }

    function callback() {
      if (finderAcademy) {
        window.location = window.location.protocol + '//' + window.location.hostname + onSuccesUrl;
      } else if (creditScore) {
        ga('send', 'event', 'Credit Score Sign In', 'successful', window.location.href,
        {'nonInteraction': true});
        var signinSuccessful = document.createEvent('Event');
        signinSuccessful.initEvent('signinSuccessful', true, false);
        document.dispatchEvent(signinSuccessful);
      } else {
        location.reload();
      }
    }

    function signupCallback() {
      if (creditScore) {
        // credit score
        ga('send', 'event', 'Credit Score Sign Up', 'successful', window.location.href,
          {'nonInteraction': true});
        var signupSuccessful = document.createEvent('Event');
        signupSuccessful.initEvent('signupSuccessful', true, false);
        document.dispatchEvent(signupSuccessful);
      } else if (finderAcademy) {
        // finder academy
        ga('send', 'event', 'finder_academy_user_signup', 'submit', window.location.href, {'nonInteraction': true});
        ga('send', 'event', 'finder_academy_verify', 'submit', window.location.href, {'nonInteraction': true});

        // to reload the course overview page so users will be signed in
        loginCloseBtn.addEventListener('click', callback);
        loginOverlay.addEventListener('click', callback);
      } else {
        // sitewide signup
        $.trackEvent('Top Nav sign up/in', 'click', 'Sign up success', 0, true);
        location.reload();
      }
    }

    function resendVerifyCallback() {
      // finder academy
      if (finderAcademy) {
        ga('send', 'event', 'finder_academy_verify_resend', 'submit', window.location.href, {'nonInteraction': true});
      }
      return null;
    }

    var setConfig = {};

    if (finderAcademy) {
      setConfig = {
        flow: 'finder_academy',
      };
    } else if (creditScore) {
      setConfig = {
        flow: 'credit_score',
        hideInput: true,
      };
    } else {
      setConfig = {
        facebookLogin: true,
      };
    }

    UserLogin.init(setConfig, callback, signupCallback, resendVerifyCallback);
  });
</script>


