	googletag.cmd.push(function() {
    googletag.pubads().addEventListener('slotRenderEnded', function (event) {
      var host = window.location.host;
      var bottomBanner = '';

      if (host == 'finder.com') {
        bottomBanner = '/30988645/FIN_Top_Banner';
      } else {
        bottomBanner = '/30988645/AUFIN_DEV-TopBanner';
      }
      
      if (event.slot.getAdUnitPath() === bottomBanner && document.querySelector('.banner-ad').style.display !== 'none') {
        $('.banner-ad').hide();
      }

    });
  });
