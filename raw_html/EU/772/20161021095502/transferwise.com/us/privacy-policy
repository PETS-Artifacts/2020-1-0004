	function setMixPanelCookies() {
		if (!mixpanel.get_distinct_id) {
			setTimeout(setMixPanelCookies, 50);
			return;
		}

		var analytics = {
			userId: mixpanel.get_distinct_id(),
			isRegistered: mixpanel.get_property('isRegistered')
		};

		
		window.dataLayer.push({mixpanelId: mixpanel.get_distinct_id()});
		

		jQuery.cookie('analytics', JSON.stringify(analytics), { path: '/'});

		
