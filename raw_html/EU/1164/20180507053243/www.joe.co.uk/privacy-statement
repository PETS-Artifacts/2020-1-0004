<script>
var PREBID_TIMEOUT = isMobile() ? 1000 : 1100;

function getStaticPrebidAdUnits() {
    var adunits = [];
    var googleSlots = googletag.pubads().getSlots();
    googleSlots.forEach(function(slot) {
        var slotId = slot.getSlotElementId();
        if (staticAdUnits[slotId]) {
            adunits.push(staticAdUnits[slotId]);
        }
    });
    return adunits;
}

function sendAdserverRequest() {
    if (pbjs.adserverRequestSent) {
        return;
    }
    pbjs.adserverRequestSent = true;
    googletag.cmd.push(function() {
        pbjs.que.push(function() {
            pbjs.setTargetingForGPTAsync();
            googletag.pubads().refresh();
        });
    });
}

googletag.cmd.push(function() {
    pbjs.que.push(function() {
        pbjs.setConfig({
            bidderSequence: 'random',
            enableSendAllBids: true,
            priceGranularity: customGranularity,
            userSync: {
                enabledBidders: ['rubicon','openx','appnexus'],
                iframeEnabled: true
            }
        });
        var adUnits = getStaticPrebidAdUnits();
        if (adUnits.length > 0) {
            pbjs.addAdUnits(adUnits);
            pbjs.requestBids({
                bidsBackHandler: sendAdserverRequest
            });
        }
    });
    setTimeout(function() {
        sendAdserverRequest();
    }, PREBID_TIMEOUT);
});
</script>

