  mixpanelVars.utm_source = '';
  mixpanelVars.utm_campaign = '';

  mixpanelVars.isLoggedIn = false;
  if (mixpanelVars.isLoggedIn) {
    mixpanelVars.firstSignInWeb = new Date(0 * 1000);
    mixpanelVars.email = '';
    mixpanelVars.memberId = '';
  }
</script>

