  mixpanelVars.apiToken = 'CHANGEME';
  mixpanelVars.originalUrl = 'http://pixlr.com/privacy-policy';
  mixpanelVars.hasEmail = 'FALSE';
  mixpanelVars.tier = '1';
  mixpanelVars.permission = 'FALSE';
  mixpanelVars.untrackedId = 'IjBlYzk0YzRjLTkyZjktNGEzZS1hNDUzLWNjNjA1ZjliYjE4MSI=--6bd64bc4a2aba62251f1a7a9c9eca131e0a88691';
