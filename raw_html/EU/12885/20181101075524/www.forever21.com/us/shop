                    chatInvitationId = '3370349221231783733';
                    chatAppId = '4332861841112675445';
                }
                else {
                    chatWebsiteId = '462947034492824386';
                    chatInvitationId = '3370349221231783733';
                }

                window._bcvma = window._bcvma || [];
                _bcvma.push(["setAccountID", "838196574815666505"]);
                _bcvma.push(["setParameter", "WebsiteID", chatWebsiteId]);
                _bcvma.push(["setParameter", "InvitationID", chatInvitationId]);
                _bcvma.push(["setParameter", "VisitName", ""]);
                _bcvma.push(["setParameter", "VisitPhone", ""]);
                _bcvma.push(["setParameter", "VisitEmail", ""]);
                _bcvma.push(["setParameter", "VisitRef", ""]);
                _bcvma.push(["setParameter", "VisitInfo", ""]);
                _bcvma.push(["setParameter", "CustomUrl", ""]);
                _bcvma.push(["setParameter", "WindowParameters", ""]);
                _bcvma.push(["addFloat", { type: "chat", id: chatAppId }]);
                _bcvma.push(["pageViewed"]);
                var bcLoad = function () {
                    if (window.bcLoaded) return; window.bcLoaded = true;
                    var vms = document.createElement("script"); vms.type = "text/javascript"; vms.async = true;
                    vms.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + "vmss.boldchat.com/aid/838196574815666505/bc.vms4/vms.js";
                    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(vms, s);
                };
                if (window.pageViewer && pageViewer.load) pageViewer.load();
                else if (document.readyState == "complete") bcLoad();
                else if (window.addEventListener) window.addEventListener('load', bcLoad, false);
                else window.attachEvent('onload', bcLoad);
            </script>
