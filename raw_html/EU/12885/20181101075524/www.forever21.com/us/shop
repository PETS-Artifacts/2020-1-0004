    <script>
        $(document).ready(function () {

            //[JPRenewal temporary] old JP App Notice
            checkOldJpApp();

            // Privacy Modal
            $.flexModal.add('#privacy', function (api) {
                $(this).on('close.flexModal', function () {
                    $('html').removeClass('noscroll');
                });
            });
            // Cookies Modal-CK
            $.flexModal.add('#cookies', function (api) {
                $(this).on('close.flexModal', function () {
                    var showCookiesPopup = getCookie('hideUseOfCookies_' + SITE_PREFIX);
                    if (showCookiesPopup !== "1" && (SITE_PREFIX.toLowerCase() == '/uk/shop' && SITE_PREFIX.toLowerCase() == '/eu/shop')) {
                        $("#cookiesPopup").show();
                    }
                });
            });
            // Render cookies pop up
            renderCookies();

            if (fnCheckCountry('us')) {
                dataLayer.push({ 'event': 'optimize.activate' });
            }
        });

        //[JPRenewal temporary] old JP App Notice
        function checkOldJpApp() {
            if ("US" === "JP" && "true" === getCookie("fuzz_app")) {
                $('#old-jpapp-notice').show();
            }
        };

        // Render Cookies Function E
        var setShowHP = function () {
            setCookie('lcpSHP_' + COOKIE_POSTFIX, '1', 1);
        }
    </script>

