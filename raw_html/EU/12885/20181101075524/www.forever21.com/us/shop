                // set autoplay time
                var slideTime = 4000;
                if ($('#m1,#m1_c').attr('data-slide-time') != undefined)
                    slideTime = $('#m1,#m1_c').attr('data-slide-time');

                //for video carousel
                $("#m1,#m1_c").on('initialized.owl.carousel', function () {
                    if ($('.owl-item').find('#video1')[0] != undefined)
                        $('.owl-item').find('#video1')[0].play();
                });

                var loopStatus;

                if ($('#m1') && $('#m1')[0].childElementCount == 1) {
                    loopStatus = false;
                }
                else {
                    loopStatus = true;
                }

                var homepage_a_m1 = $("#m1").owlCarousel({
                    loop: loopStatus,
                    items: 1,
                    video: true,
                    autoplay: true,
                    autoplayTimeout: slideTime,
                    responsive: {
                        0: {
                            nav: false,
                            dots: true
                        },
                        767: {
                            nav: false,
                            dots: true
                        },
                        1000: {
                            nav: true,
                            dots: false
                        }
                    }
                });

                //for video carousel
                $("#m1").on('changed.owl.carousel', function (event) {
                    if ($('.owl-item').find('video')[0] != undefined) {
                        $('.owl-item').find('video').each(function () {
                            this.pause();
                        });

                        var itemIdx = event.item.index - 1;

                        if ($('.owl-item').find('#video' + itemIdx)[0] != undefined) {
                            if (itemIdx == 1) {
                                $('.owl-item').find('#video' + itemIdx)[0].play();
                            }
                            else {
                                $('.owl-item').find('#video' + itemIdx)[1].play();
                            }
                        }
                    }
                });
            </script>
