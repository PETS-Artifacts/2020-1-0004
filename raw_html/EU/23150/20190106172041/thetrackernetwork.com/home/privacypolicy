<script type="text/javascript">
    var notifs = [];
    var added = 0;

    siteNotificationKeys = [];
    function checkSiteNotifications() {
        if (typeof (iziToast) === undefined) {
            return;
        }

        fetch('//notifications.thetrackernetwork.com/api/notifications/?site=TTN&userName=207.241.232.186')
            .then(function (response) {
                return response.json();
            })
            .then(function (json) {
                json.forEach(function (item, index) {
                    if (siteNotificationKeys.length > 5 || siteNotificationKeys.indexOf(item.RowKey) != -1) {
                        return;
                    }

                    iziToast.show({
                        title: item.title,
                        message: item.body,
                        class: 'trn-izitoast',
                        closeOnClick: false,
                        drag: false,
                        theme: 'dark',
                        position: 'topRight',
                        animateInside: false,
                        layout: 2,
                        progressBar: false,
                        timeout: false,
                        onClosed: function () {
                            fetch('//notifications.thetrackernetwork.com/api/notifications', {
                                method: 'DELETE',
                                headers: {
                                    'Accept': 'application/json',
                                    'Content-Type': 'application/json'
                                },
                                body: JSON.stringify({
                                    site: 'TTN',
                                    userName: '207.241.232.186',
                                    id: item.RowKey,
                                    type: item.type
                                })
                            });
                        }
                    });

                    siteNotificationKeys.push(item.RowKey);
                });
            });
    }
    checkSiteNotifications();
    setInterval(checkSiteNotifications, 90000);
</script>
