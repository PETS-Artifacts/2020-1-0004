    });


  var micrositeId = '';
  

  var isMsiteActive = 'false';

  if ( micrositeId ) {
    isMsiteActive = 'true';
  }



  

  var networkCode = 4788;
  var topLevelAdUnit = 'hn.us.hl.x.x.x.x';
  var s1 = 'otherhealthsystems';
