      require(["jquery"], function( $ ){
      $(document).ready(function(){
      var placeholderInput = setInterval(function(){
      if($('#gsc-i-id1').length){
      clearInterval(placeholderInput);
      } else {
      return;
      }

      var $_gsc = $('#gsc-i-id1');

      if($_gsc.length){
