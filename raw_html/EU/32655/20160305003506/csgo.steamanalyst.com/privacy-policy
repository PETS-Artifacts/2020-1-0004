$tradeCalc.find('ul').append('<li><div class="trade-calc-remove"><img src="/images/remove.gif" /></div><span class="itemNameCalc"><input type="hidden" name="LIDS[]" value="'+id+'" />'+name+'</span><span class="priceCalc">' + keys + ' keys / ' + price + '</span></li>');
} else if($side == 'right'){
$tradeCalc.find('ul').append('<li><div class="trade-calc-remove"><img src="/images/remove.gif" /></div><span class="itemNameCalc"><input type="hidden" name="RIDS[]" value="'+id+'" />'+name+'</span><span class="priceCalc">'+price+'</span></li>');
}


if($liTradeCalc.find('div.trade-total div.trade-total-'+$side).is(':empty')){
	totalprice = (parseFloat(price)).toFixed(2);
} else {
	totalprice = (parseFloat($liTradeCalc.find('div.trade-total div.trade-total-'+$side).html()) + parseFloat(price)).toFixed(2);
}
$liTradeCalc.find('div.trade-total div.trade-total-'+$side).html(totalprice);


if($liTradeCalc.find('div.trade-total div.trade-total-'+$side+'-box div.tradeCalc-keys').is(':empty')){

} else {
	keys = parseInt($liTradeCalc.find('div.trade-total div.trade-total-'+$side+'-box div.tradeCalc-keys').html()) + parseInt(keys);
}

$liTradeCalc.find('div.trade-total div.trade-total-'+$side+'-box div.tradeCalc-keys').html(keys);
$liTradeCalc.find('div.trade-total div.trade-total-'+$side+'-box input[type="hidden"][name="ItemKeyC"]').val(keys);


    }
});
$('#tradeSuggBtn').on('click', function() {

    if ($('ul#trade-calculator li.tradeCalc.suggestions div.trade-total div.trade-total-left-box div.tradeCalc-keys').html() == 0) {
        return false;
    } else {

	$(this).hide(0);

if($('#suggloader').length == 0){
    $('<div style="float:right;margin-right:10px;" id="suggloader">Loading...</div>').insertBefore(this);
}

        $('ul#tradeSuggestion').html('');
        var keys = $(this).parents('.tradeCalc').find(
            'div.tradeCalc-keys').html();
        $.ajax({
            type: "GET",
            context: this,
            url: "/fetchTradeCalcSuggestion.php",
            dataType: "json",
            data: 'keys=' + keys,
            success: function(data) {
				
				$(this).show(0);
				
				$('#suggloader').remove();

                $.each(data, function(key, value) {
                    var market_name = value.market_name;
                    var keys = value.keys;
                    $('ul#tradeSuggestion').append('<li>' + market_name + '<span style="float:right;">' + keys + ' Keys</span></li>');
                });
            }
        });
    }
});
$('ul#trade-calculator').on('click', 'div.trade-calc-remove',function(){
	
	var $parent = $(this).parents('.tradeCalc');

	oldprice = $(this).parent().find('span.priceCalc').html();
	side = $(this).parent().parent().parent().attr('class').replace('trade-calc-','');
	totalprice = $parent.find('div.trade-total div.trade-total-'+side).html();
	newTotal = (parseFloat(totalprice) - parseFloat(oldprice)).toFixed(2);

	$parent.find('div.trade-total div.trade-total-'+side).html(newTotal);

	oldkeys = Math.round(oldprice / 2.5);
	totalkeys = Math.round(totalprice / 2.5);
	newkeys = parseInt(totalkeys) - parseInt(oldkeys);

	$parent.find('div.trade-total div.trade-total-'+side+'-box div.tradeCalc-keys').html(newkeys);

	$(this).parent().remove();

});
$('ul#trade-calculator li.tradeCalc h1 input.tradeCalc-save').on('click', function() {
        var $saveButton = $(this);

        var $this = $(this).parent().parent().parent();

        var serializedData = $this.find(
            'input[type="hidden"][name="LIDS[]"],input[type="hidden"][name="RIDS[]"]'
        ).serialize();
        if (serializedData) {

            $saveButton.parent().html(
                '<img src="/images/ajax-loader.gif" />'
            );

            $.ajax({
                type: "POST",
                url: "/TradeCalcSave.php",
                dataType: "json",
                data: serializedData,
                success: function(data) {
                    if (data.status) {

$this.find('.tradeCalcTopRight').html('<p id="saved">Saved</p>').delay(2000).fadeOut(100, function(){
$this.find('.tradeCalcTopRight').html('<input type="button" value="Save" class="tradeCalc-save">').fadeIn(100);
});
                    }
                }
            });
        }
    });
$('ul#trade-calculator li.tradeCalc h1 span.tradeCalcTopRight div.tradeCalcUpdate input.tradeCalc-save').on('click', function() {


        var $saveButton = $(this);

        var $this = $(this).parent().parent().parent().parent();

		var $tradeID = $this.attr('id').replace('tradeCalc-','');

