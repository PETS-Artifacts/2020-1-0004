<script>

$(document).ready(function() {

	var docHeight = $(window).height();
	var footerHeight = $('#footer_wrapper').height();
	var footerTop = $('#footer_wrapper').position().top + footerHeight;
	
	if (footerTop < docHeight) {
		$('#footer_wrapper').css('margin-top', 0 + (docHeight - footerTop) + 'px');
	} 
	
});
</script>
<script type="text/javascript">

	$('input[type="text"]').on({
	    focus: function(){
	        var $this = $(this);
	        if($this.val() === $this[0].defaultValue) $this.val('');
	    },
	    blur: function(){
	        var $this = $(this);
	        if($this.val() === '') $this.val($this[0].defaultValue);
	    }
	});

$('#searchBar input#autocomplete').on('click', function(){
    $('div.autocomplete-suggestions').css('width', 'auto');
});

$('#autocomplete').autocomplete({
    serviceUrl: '/autocomplete.php',
    minChars: 3,
    
    deferRequestBy: 500,
    triggerSelectOnValidInput: false,
    onSearchStart: function (){

    $('div.autocomplete-suggestions').css('width', 'auto');

		$('#autocompleterloader').show(0);

    },
    onSearchComplete: function (){
    
    $('div.autocomplete-suggestions').css('width', 'auto');
    
	    $('#autocompleterloader').hide(0);
	    
    },
    onSelect: function (suggestion) {
    
		window.location.href = '/id/'+suggestion.data;
    }
});

$('.textbox').click(function() {
  $(this).val('');
});

$('#pricecheck div.requestPC form textarea').on({
    focus: function(){
        var $this = $(this);
        if($this.val() === $this[0].defaultValue) $this.val('');
    },
    blur: function(){
        var $this = $(this);
        if($this.val() === '') $this.val($this[0].defaultValue);
    }
});




