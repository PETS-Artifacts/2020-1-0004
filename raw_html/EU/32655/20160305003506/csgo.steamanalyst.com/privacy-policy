$("#header > ul > li").hover(
	function () {
		$(this).find('ul').fadeIn(100);
	},
	function () {
		$(this).find('ul').fadeOut(100);
	}
);

$('#currency, #currencyL').on('click',function(){
	var $this = $(this);
	$this.find('ul').slideDown(150);
	$this.addClass('opened');
});

$('#currency ul li, #currencyL ul li').on('click',function(){
	var currency = $(this).find('input[type="hidden"]').val();
	$.post( "/currency.php", { currency: currency }, function(){

		location.reload();

	});
});

$('#currency ul, #currencyL ul').mouseleave(function(){
	$(this).slideUp(150);
	$('#currency').removeClass('opened');
});

if (!$.cookie('notice')) {
    $(".notice" ).show();
    $("#dismiss").click(function() {
        $(".notice").slideUp();
        $.cookie('notice', true, { expires: 30, path: "/", domain: ".steamanalyst.com" });
    });
}

