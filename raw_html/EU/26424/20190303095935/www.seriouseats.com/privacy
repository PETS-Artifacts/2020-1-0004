<script>
  // Configure the adblock detector
  (function(){
    if (typeof window.adblockDetector === 'undefined') {
      adBlockDetected();
    } else {
      window.adblockDetector.init(
        {
          debug: true,
          found: function(){
            dataLayer.push({
              "event": "gaEvent",
              "category": "adblock", 
              "action": "true",
              "label": ""
            });
          },
          notFound: function(){
            dataLayer.push({
              "event": "gaEvent",
              "category": "adblock", 
              "action": "false",
              "label": ""
            });
          }
        }
      );
    }
  }());
</script>

