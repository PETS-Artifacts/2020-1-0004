  <script>
    $(document).ready(function(){
      var $mobileStickyAd = $('.mobile-sticky-ad');
      $mobileStickyAd.find('button').on('click', function(){
        // Kills the pop-up completely:
        // $mobileStickyAd.remove();
        // Allows for the refresh to bring the unit back into view:
        $mobileStickyAd.addClass('collapsed');
      });
      var googletagCheckInt = 500,
          googletagCheckTO = 10000;
      function initMobileStickyAd() {
        if(typeof googletag !== "undefined" && typeof googletag.pubads === "function") {
          googletag.pubads().addEventListener('slotRenderEnded', function(event) {
            if (typeof _adSlots !== "undefined" && _adSlots.hasOwnProperty("mobile-sticky-ad__ad") && event.slot === _adSlots["mobile-sticky-ad__ad"]) {
              if(!event.isEmpty) {
                $mobileStickyAd.removeClass("collapsed");
              } else {
                $mobileStickyAd.addClass("collapsed");
              }
            }
          });
        } else if(googletagCheckTO > 0) {
          setTimeout(initMobileStickyAd, googletagCheckInt);
          googletagCheckTO -= googletagCheckInt;
        }
      }
      initMobileStickyAd();
    });
  </script>

