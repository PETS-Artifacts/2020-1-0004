            $.each( graph_data, function( key, val ) {

                var data = new google.visualization.DataTable();

                data.addColumn('number', 'X');
                data.addColumn('number', 'Rate');

                data.addRows(
                    val.rows
                );

                var options = {
                    title: '',
                    curveType: 'function',
                    series: {
                        0: {color: '#e67e22'}
                    },
                    hAxis: {
                        textPosition: 'none',
                        gridlines: {
                            color: 'transparent'
                        },
                        baselineColor: 'transparent'
                    },
                    vAxis: {
                        textPosition: 'none',
                        gridlines: {
                            color: 'transparent'
                        },
                        baselineColor: 'transparent'
                    },
                    legend: { position: 'none' },
                    tooltip : {
                        trigger: 'none'
                    },
                    chartArea: {'width': '90%', 'height': '60px'},
                    backgroundColor: { fill: '#f5f5f5'}
                };

                var chart = new google.visualization.LineChart(document.getElementById(val.coin_attr + '_chart'));

                chart.draw(data, options);
            });
        }

        $('.slick-arrow').click(function() {
            $.cookie('slick_index', $('.slick-rates').slick('slickCurrentSlide'), {
                expires     :   365,
                path        :   '/'
            });
        });

        $('.slick-item-footer').click(function( event ) {
            $('#index').val($(this).attr('data-index'));
            event.preventDefault();
        });
    });
</script>
