<script>
		$('.js-dismiss-cookie-notice').on('click', function () {
			setCookie("dismissed_cookie_notice", 1);
			$('.cookie-notice').hide();

            function setCookie(name, value) {
                var d = new Date();
                d.setTime(d.getTime() + (60 * 60 * 24 * 365 * 10 * 1000));
                document.cookie = name + "=" + value + ";path=/;expires=" + d.toUTCString() + ";Secure; SameSite=Lax;";
            }
		});
	</script>
