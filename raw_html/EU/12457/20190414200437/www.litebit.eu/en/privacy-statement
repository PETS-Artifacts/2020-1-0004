<script src="https://js.pusher.com/4.1/pusher.min.js"></script>
<script>
			var pusher = new Pusher('67ccb7d37e99d46bc0f4', {
				cluster: 'eu',
				encrypted: true
			});

			function preciseValue(x) {
				var priceAsIntegerLength = parseInt(x).toString().length;
				var preferredNumbers = 6;
				var minimumDecimals = 2;

				return parseFloat(x).toFixed(Math.max(preferredNumbers - priceAsIntegerLength, minimumDecimals));
			}

			function parseChunk(data) {
				var result = {};
				for (var i = 0; i < data.length; i++) {
					for (var j = 0; j < Object.keys(data[i]).length; j++) {
						result[Object.keys(data[i])[j]] = Object.values(data[i])[j];
					}
				}
				return result;
			}

			var channel = pusher.subscribe('web');
			channel.bind('rate_update', function(data) {
				if (Array.isArray(data)) {
					data = parseChunk(data);
				}
				$.each(data, function( key, value ) {
					var update = '[data-updated-by="' + key + '"]';
					$(update).each(function() {
						switch($(this).attr('data-update-type')) {
							case 'html' :
								$(this).html(value);
								break;

							case 'raw' :
								$(this).val(value).trigger('change');
								break;

							case 'price' :
								var last_value = preciseValue($(this).html());

								var precise_value = preciseValue(value);
								$(this).text(preciseValue(precise_value));

								var parent = $(this).parent().parent().parent();
								parent.removeClass('trend-up trend-down');
								if(precise_value > last_value) {
									parent.addClass('trend-up');
								} else if(precise_value < last_value) {
									parent.addClass('trend-down');
								}

								break;

							case 'trend' :
								$(this).html(parseFloat(value) >= 0 ? '<i class="fa fa-level-up" style="color:green"></i>' : '<i class="fa fa-level-down" style="color:red"></i>');
								break;

							default :
								$(this).text(value);
								break;
						}
					});
				});
			});
		</script>

<script type="text/javascript">
		var _paq = _paq || [];
		/* tracker methods like "setCustomDimension" should be called before "trackPageView" */
		
		_paq.push(['trackPageView']);
		_paq.push(['enableLinkTracking']);
		(function () {
			var u = "//litebitanalytics.eu/";
			_paq.push(['setTrackerUrl', u + 'piwik.php']);
			_paq.push(['setSiteId', '1']);
			var d = document, g = d.createElement('script'), s = d.getElementsByTagName('script')[0];
			g.type = 'text/javascript';
			g.async = true;
			g.defer = true;
			g.src = u + 'piwik.js';
			s.parentNode.insertBefore(g, s);
		})();
	</script>
