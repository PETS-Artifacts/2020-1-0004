            <a href="#" @click="close" class="auto-complete-search--close"><i class="icon-remove"></i></a>
        </div>

        <div v-if="canShowSuggestionTray" class="auto-complete-tabs" @keydown.left.right="changeTab" @keydown.up.down.prevent="changeSelectedItem" tabindex="-1" v-focus="isFocused('tray')">
            <nav v-if="canShowRecommendations || performedSearch">
                <a v-if="canShowTab('trending')" class="trending"><i class="icon-trending"></i> ${'auto.complete.search.tab.trending' | trans}</a>
                <a v-if="canShowTab('recent')" class="recent"><i class="icon-recently-viewed"></i> ${trans("auto.complete.search.tab.recent")}</a>
                <a @click.prevent="currentSearchTab = 'events'" v-if="performedSearch" :class="{active: activeTab === 'events' }">${trans("auto.complete.search.tab.events")}</a>
                <a @click.prevent="currentSearchTab = 'locations'" v-if="performedSearch" :class="{active: activeTab === 'locations' }">${trans("auto.complete.search.tab.locations")}</a>
                <a @click.prevent="currentSearchTab = 'cities'" v-if="performedSearch" :class="{active: activeTab === 'cities' }">${trans("auto.complete.search.tab.cities")}</a>
            </nav>

            <div v-if="canShowTab('trending')" :class="{'loading': fetchingTrending}" class="auto-complete-tabs--item auto-complete-tabs--trending">
                <search-result-item v-for="event in tabs.trending.items" v-focus="isFocused(event)" tab="trending" :url="event.url" :title="event.title" :content="generateEventResult(event)"></search-result-item>

                <div class="auto-complete-tabs--more">
                    <a :href="route('discover')" class="auto-complete-discover-link" data-mobile-app-navigate="discover">${trans("auto.complete.search.tab.discover")}</a>
                </div>
            </div>

            <div v-if="canShowTab('recent')" class="auto-complete-tabs--item auto-complete-tabs--recent">
                <search-result-item v-for="event in tabs.recent.items" v-focus="isFocused(event)" tab="recent" :url="event.url" :title="event.title" :content="generateEventResult(event)"></search-result-item>
            </div>

            <div v-if="canShowTab('events')" class="auto-complete-tabs--item auto-complete-tabs--events">
                <div v-if="tabs.events.noResults" class="auto-complete-search--no-results">
                    <h2>${translated('auto.complete.search.no.results')}</h2>
                </div>

                <h4 v-if="tabs.events.showGlobalHeading">${translated('auto.complete.search.global')}</h4>
                <search-result-item ref="eventItems" v-for="event in tabs.events.items" v-focus="isFocused(event)" :url="event.url" :title="event.title" :content="generateEventResult(event)"></search-result-item>

                <div v-if="tabs.events.total > 5" class="auto-complete-tabs--more">
                    <a :href="tabs.events.url">${translated('auto.complete.search.more.events')}</a>
                </div>
            </div>

            <div v-if="canShowTab('locations')" class="auto-complete-tabs--item auto-complete-tabs--locations">
                <div v-if="tabs.locations.noResults" class="auto-complete-search--no-results">
                    <h2>${translated('auto.complete.search.no.results')}</h2>
                </div>

                <h4 v-if="tabs.locations.showGlobalHeading">${translated('auto.complete.search.global')}</h4>
                <search-result-item v-for="location in tabs.locations.items" v-focus="isFocused(location)" :url="location.url" :title="location.name" :content="location.city.name + ', '  + location.city.translated_country"></search-result-item>
            </div>

            <div v-if="canShowTab('cities')" class="auto-complete-tabs--item auto-complete-tabs--cities">
                <div v-if="tabs.cities.noResults" class="auto-complete-search--no-results">
                    <h2>${translated('auto.complete.search.no.results')}</h2>
                </div>

                <h4 v-if="tabs.cities.showGlobalHeading">${translated('auto.complete.search.global')}</h4>
                <search-result-item v-for="city in tabs.cities.items" v-focus="isFocused(city)" :url="city.url" :title="city.name" :content="city.translated_country"></search-result-item>
            </div>
        </div>
    </form>
