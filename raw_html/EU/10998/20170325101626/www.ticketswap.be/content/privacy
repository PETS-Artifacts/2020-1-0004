            <input v-show="hasFocus" :id="'query' + _uid" name="query" class="act" type="text" autocomplete="off" autocorrect="off" autocapitalize="off" spellcheck="false" ref="input" v-focus="isFocused('input')" :class="{'loading': fetchingResults}" v-model="query" @keyup="search" @keydown.up.down="changeSelectedItem" @focus="focus" :placeholder="inputPlaceholder" />

                
    
    
    
    
