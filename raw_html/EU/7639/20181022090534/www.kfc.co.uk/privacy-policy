<script defer src="https://assets.kfc.co.uk/dist/modernizr.min.js"></script>
<script src="https://assets.kfc.co.uk/jspm_packages/system.js"></script>
<script>SystemJS.config({ baseURL: 'https://assets.kfc.co.uk/' })</script>
<script defer src="https://assets.kfc.co.uk/config.js"></script>

<script>
document.addEventListener('DOMContentLoaded', function() {
    // SystemJS init
    SystemJS.set('server-args', SystemJS.newModule({
    siteKey: '6LdSSg4UAAAAAPEwpDZBYgeukVetPXJF7rFxZZ0y',
    dsn: 'https://eece04bb3a8d4a55b9be7bba169a30d4@sentry.io/175394'

    }));
    SystemJS.import('babel-polyfill')
        .then(function() {
            return SystemJS.import('babel-polyfill');
        }).then(function() {
            return SystemJS.import('aurelia-bootstrapper');
        }).then(function() {
          SystemJS.import('lazysizes-respimg');
            SystemJS.import('lazysizes-unveilhooks');
            SystemJS.import('lazysizes-bgset');
            return SystemJS.import('lazysizes');
        });

    window.lazySizesConfig = window.lazySizesConfig || {};
    window.lazySizesConfig.lazyClass = 'lazy';
    window.lazySizesConfig.expand = 200;
    window.lazySizesConfig.expFactor = 1.5;
    window.lazySizesConfig.loadHidden = false;

    // https://github.com/aurelia/framework/issues/227#issuecomment-246425032
    if (/Edge/.test(navigator.userAgent)) {
      Function.prototype.call = function(t) {
          return this.apply(t, Array.prototype.slice.apply(arguments, [1]));
      }
    }

    // Variables for checking jQuery plugins
    var UA = navigator.userAgent || navigator.vendor || window.opera;
    var existsLity = document.querySelector('[data-lity]');
    var existsRecaptcha = document.querySelector('recaptcha');

    // Load recaptcha when it exists in the DOM
    if (existsRecaptcha !== null) {
        initRecaptcha();
    }

    // Load jQuery when there's a reference to a jQuery plugin
    if ((existsLity !== null)) {
        if (!window.jQuery) {
            SystemJS.import('jquery').then(function() {
                jQueryPlugins();
            });
        } else {
            jQueryPlugins();
        }
    }

    function jQueryPlugins() {
        if (existsLity !== null) {
            SystemJS.import('lity');
        }
    }

    function initRecaptcha() {
        var scripts = document.getElementsByTagName('script');
        var lastScript = scripts[scripts.length-1];
        var newScript = document.createElement("script");
        newScript.src = 'https://www.google.com/recaptcha/api.js?render=explicit';
        lastScript.parentNode.insertBefore(newScript, lastScript.nextSibling);
    }

    var website = window.location.hostname;
    var anchorEls = document.querySelectorAll('a');
    var anchorElsLength = anchorEls.length;
    var internalLinkRegex = new RegExp('^((((http:\\/\\/|https:\\/\\/)(www\\.)?)?'
                                     + website
                                     + ')|(localhost:\\d{4})|(\\/.*))(\\/.*)?$', '');

    for (var i = 0; i < anchorElsLength; i++) {
        var anchorEl = anchorEls[i];
        var href = anchorEl.getAttribute('href');

        if (href == null || href == '') {
            break;
        } else if (!internalLinkRegex.test(href) && href.search('#') == 0 || href.search('.pdf') > 0) {
            anchorEl.setAttribute('target', '_blank');
            anchorEl.setAttribute('rel', 'noopener');
        }
    }
});
</script>

