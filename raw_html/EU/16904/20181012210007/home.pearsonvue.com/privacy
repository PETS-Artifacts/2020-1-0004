<script>
document.querySelector(".relatedLinks ul").setAttribute("class", "relatedLinks-list");
//		  	   
function relatedlinkfunc(){
 if ($(window).width() < 481) {
      //
	   document.getElementsByClassName("relatedLinks-hdg")[0].setAttribute("data-toggle", "collapse");
       document.getElementsByClassName("relatedLinks-hdg")[0].setAttribute("data-target", ".relatedLinks-list");
       document.getElementsByClassName("relatedLinks-list")[0].setAttribute("class", "relatedLinks-list collapse");
 } else {
      //back to normal in desktop	
       document.getElementsByClassName("relatedLinks-hdg")[0].setAttribute("data-toggle", "none");
       document.getElementsByClassName("relatedLinks-hdg")[0].setAttribute("data-target", "none");
       document.getElementsByClassName("relatedLinks-list")[0].setAttribute("class", "relatedLinks-list");
 }
}
$( window ).resize(function() {
     relatedlinkfunc()
  }).resize(); // trigger resize event initially
</script>
<!-- END collapse.js-->


<script type="text/javascript">
    jQuery(document).ready(function($) {
        $('#ppc').panorama({			        
            size   : '300px'
        });
        $('#pvtc').panorama({			        
            size   : '300px'
        });
      
        // setup the focus for the search box
        $('.search-icon').click(function (e) {
            //e.preventDefault();
            setTimeout(function(){ $('.form-item-full-text-search .text-search').focus(); },0);
        });
      
      // localize prev and next text
      $('.carousel .left.carousel-control span.sr-only').text('Previous');
      $('.carousel .right.carousel-control span.sr-only').text('Next');
      
      // move the pause/play control
      var $carouselControl = $('aside').find('[aria-label="carousel pause/play control"]');
      var $leftControl = $('.carousel .left.carousel-control');
      $leftControl.insertAfter($carouselControl);
      
    });
</script>

