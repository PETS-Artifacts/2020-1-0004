<script>
    /* Video post support for Android */
    jQuery(function($) {
        var video = document.getElementById('video_post');
        var ua = navigator.userAgent.toLowerCase();
        var isAndroid = ua.indexOf("android") > -1; //&& ua.indexOf("mobile");
        if(isAndroid) {
            var width = $('#play_btn').parents('.video-wrapper').width();
            var btn_height = $('#play_btn').height();
            var btn_width = $('#play_btn').width();
            video.addEventListener( "loadedmetadata", function (e) {
                var v_height = this.videoHeight;
                var v_width = this.videoWidth;
                var height = v_height*width / v_width;
                $('#play_btn').css('top',(height/2 - btn_height/2)+'px');
                $('#play_btn').css('left',(width/2 - btn_width/2)+'px');
                $('#play_btn').css('display','block');
                
            }, true );
        }

        video.addEventListener('click',function(){
            $('#play_btn').css('display','none');
            video.play();
        },false);
    });
    /* End Video post support for Android */
jQuery('#commentform > p.form-submit > #submit').live('click', '.option', function(event){
        console.log("clicked");
		event.preventDefault ? event.preventDefault() : event.returnValue = false;
		var commentValue = jQuery('textarea#comment').val();
		var kPostID = jQuery('#comment_post_id').attr("class");
		//alert("click footer");
		if(commentValue != ''){
			check_comment_login(commentValue, kPostID);
		}
	}
);
jQuery('#loginwithfacebook').live('click', '.option', function(event){
		event.preventDefault ? event.preventDefault() : event.returnValue = false;
		var commentValue = jQuery('textarea#comment').val();
		var kPostID = jQuery('#comment_post_id').attr('class');
		console.log(kPostID);
		login_to_facebook(commentValue, kPostID);
	}
);
jQuery('#kwpsubmit').live('click', '.option', function(event){
		event.preventDefault ? event.preventDefault() : event.returnValue = false;
		k_wp_login();
	}
);
jQuery('#kwpsignupsubmit').live('click', '.option', function(event){
		event.preventDefault ? event.preventDefault() : event.returnValue = false;
		k_wp_signup();
	}
);
jQuery('#kpasswordsubmit').live('click', '.option', function(event){
		event.preventDefault ? event.preventDefault() : event.returnValue = false;
		k_wp_pass_reset();
	}
);

</script>
