jQuery('body').on('click','#yt2',function(){jQuery.ajax({'success':
						function(response)
						{
							response = eval("(" +response +")");

							if (response.success == "success")
							{
								$("#emailValidatorAddressFiled").html("");
								$("#email-validator .content p").text(response.message);
								$("#email-validator .content .buttons .cancel").attr("value","OK");
								$("#email-validator .content .buttons .ok").remove();
							}
							else if(response.success == "false")
							{
								$("#email-validator .content p").text(response.message).css("color","red");
							}
						}
				,'type':'POST','url':'/site/emailValidator','cache':false,'data':jQuery(this).parents("form").serialize()});return false;});
