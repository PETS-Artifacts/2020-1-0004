    <script type="text/javascript">
        $(document).ready(function () {
            setTimeout(function () {
                $.get('/site/popup', {name: 'preorderPopup'}, function (content) {
                    var preorderStep = 1;

                    $('body').append(content);
                    $('#newMessage').show().animate({bottom: '20px'}, 1000);

                    $('#newMessage').on('click', '.close-popup', function () {
                        $('#newMessage').animate({bottom: '-135px'}, 1000, function () {
                            if (preorderStep === 1) {
                                $.post('/game/preorderPopupClose', {});
                            }
                            $('#newMessage').remove();
                        });
                        $('#preorderPopup').remove();
                    });

                    $('#newMessage').on('click', '.content', function () {
                        $('#preorderPopup').show();
                        $('#newMessage').remove();
                    });

                    $('#preorderPopup').on('click', '.close-popup', function () {
                        if (preorderStep === 1) {

                            $.post('/game/preorderPopupClose', {});
                        }
                        $('#preorderPopup').remove();
                    });

                    $('#preorderPopup').on('click', '.purchase', function () {
                        var optionId = $(this).data('id');
                        $.post('/game/getIFrameUrl', {
                            id: $(this).data('preorder-id'),
                            optionId: optionId,
                        }, function (data) {
                            if (typeof data.success != 'undefined' && data.success == true) {
                                var paymentFrame = $('#paymentIFrame');
                                paymentFrame.attr('src', data.url);
                                $('#segPayPaymentForm .success').hide();
                                $('#segPayPaymentForm .overlay-loader').hide();

                                $('.buyPopupPartTwo').hide();
                                $('#segPayPaymentForm').height(815).center().show();

                                var bounds = $('#segPayPaymentForm')[0].getBoundingClientRect();
                                if (bounds.top < 40)
                                    $('#segPayPaymentForm').css({'top': 40});

                                if (bounds.height > ($(window).height() - 100)) {
                                    $('#segPayPaymentForm').css({'bottom': 40, height: 'auto'});
                                }
                            }
                            if (typeof data.error != 'undefined' && data.error == 'no_user') {
                                window.sessionStorage.setItem('preorderOptionId', optionId);
                                Layout.displayRegistrationForm();
                            }
                        });
                    });
                    $('#preorderPopup').on('click', '.gift1', function () {
                        $('#gift1').fadeIn(100);
                        $('.gallery-big-content', '#gift1').height($('.wrapper', '#gift1').height());
                        $('img', '#gift1').height($('.wrapper', '#gift1').height());
                        $('.gallery-big-content', '#gift1').width($('img', '#gift1').width())
                    });
                    $('#preorderPopup').on('click', '.gift2', function () {
                        $('#gift2').fadeIn(100);
                        $('.gallery-big-content', '#gift2').height($('.wrapper', '#gift2').height());
                        $('img', '#gift2').height($('.wrapper', '#gift2').height());
                        $('.gallery-big-content', '#gift2').width($('img', '#gift2').width())
                    });

                    $('.gallery-big .close-popup').click(function () {
                        var id = $(this).parents('.gallery-big').eq(0).attr('id');
                        $('#' + id).fadeOut(100);
                    });
                });
            }, 1000);
        });
    </script>

