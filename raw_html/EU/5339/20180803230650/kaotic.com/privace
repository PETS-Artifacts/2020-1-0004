												<script>
						BetterJsPop.config({
							'bindTo': ["a", "button"],
							'webkitAnchorBlank': false
						}).add("http://ccxja.com/srv/cc8837/", {
							'under': true,
							'newTab': true,
							'device': "any",
							'shouldFire': function() {
								var clicked_el = BetterJsPop.getClickedElement();
								
								if(clicked_el && clicked_el.tagName && (("a" !== clicked_el.tagName.toLowerCase()) && ("button" !== clicked_el.tagName.toLowerCase()))) {
									return false;
								}
								
								if(clicked_el && clicked_el.className) {
									// check if popup prevention classname is present
									var classes = (" "+ clicked_el.className +" ");
									classes = classes.replace(/[\n\t]/g, " ");
									classes = classes.toLowerCase();
									
									if(classes.indexOf(" prevent_pop ") > -1) {
										return false;
									}
								}
								
								return true;
							}
						});
					</script>
										
