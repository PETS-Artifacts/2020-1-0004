<script type="text/javascript">
  $('#submit_login').on('click',function(){

	var username = $('#username').val();
	var password = $('#password').val();
	var urlLogin = $('#urlLogin').val();
	var getUrl   = $('#getUrl').val();
  var getUrlUpload = $('#getUrlUpload').val();

	jQuery.ajax({
           type: "POST", url: urlLogin, dataType: "json", data:"username="+username+"&password="+password+"&getUrl="+getUrl,
            complete: function(data){
              if(getUrlUpload == 'message'){

                  getUrlUpload = 'https://kaotic.com/user/'+username+'/message';

                }
           if(username == ''){
           	$('#usererror').html('Username field is required');
           }else{
           		$('#usererror').html('');
           }
           if(password == ''){
           	$('#passworderror').html('Password field is required');
           }else{
           		$('#passworderror').html('');
           }
           if( typeof data =='object'&& typeof data.responseJSON == 'object'){
           	$('#message1').html('<div class="alert alert-danger"><a href="#" class="close" data-dismiss="alert">&times;</a>'+data.responseJSON.message+'</div>');
           }else{
        if(getUrlUpload == ''){
              window.location.replace(data.responseText);
           }else{
              window.location.replace(getUrlUpload);
             }
           }
            }
        });
	});
</script>      </div>
     
