        var eu_array = ['be', 'bg', 'cz', 'de', 'dk', 'ee', 'es', 'eu', 'fi', 'fr', 'gr', 'hu', 'it', 'lt', 'lv', 'pl', 'pt', 'ro', 'se', 'si', 'sk', 'uk', 'tr'];
        $.ajax({
            url:'https://www.msi.com/redirect/get_now_country_code',
            data:{msi_official_csrf:csrf_hash}
        }).done(function (data) {
            if(data && $.inArray( data, eu_array ) != '-1'){
                //$.when( CookieControl.load( cookies_config2 ) ).then(function(){ CookieControl.notifyAccept(); });
                CookieControl.load( cookies_config2 );
            }
            else{
                //$.when( CookieControl.load( cookies_config ) ).then(function(){ CookieControl.notifyAccept(); });
                CookieControl.load( cookies_config );
            }
        });

    </script>
