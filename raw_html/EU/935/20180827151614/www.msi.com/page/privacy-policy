<script type="text/javascript">
    jQuery(document).ready(function($) {
        var tab_nav  = $(".tab_nav");
        // alert(tab_nav.length);
        if(tab_nav.length > 0)
        {
            var tab_nav_ul = tab_nav.children('ul'),
                tab_body = tab_nav.next(".tab_body");
            tab_nav_ul.on('click', 'li', function(){
                var li   = $(this),
                    a    = li.children('a')
                href = a.attr('href');
                if(!li.hasClass('on'))
                {
                    li.siblings('li').removeClass('on').end().addClass('on');
                    tab_body.children('div.on').fadeOut(250, function(){
                        $(this).removeClass('on').addClass('hide');
                        tab_body.children(href).fadeIn(250, function(){
                            $(this).removeClass('hide').addClass('on');
                        })
                    });
                }
                return false;
            });
        }
        var hash = window.location.hash,
            tab_nav_ul = tab_nav.children('ul'),
            tab_nav_a = tab_nav_ul.children('li').children('a');

        if(hash){
            //console.log(hash);
            $(tab_nav_a).each(function(){
                //console.log($(this).attr('href'));
                if($(this).attr('href') == hash){
                    $(this).parent('li').click();
                }
            });
        }
        $("#bread").children('span').text(function(){
	if((location.href).match(/about/))
	{
		return $('.box960 #mainbox h2:eq(0)').text();
	}
	else
	{
		return $('.box960 h1:eq(0)').text();
	}
	
});    });
</script><footer>
