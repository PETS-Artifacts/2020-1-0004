            var a = $("#mobile_search_text");
            $("#mobile_search").submit(function() {
                var b = a.val();
                var c = base_url + "search/" + encodeURIComponent(b.replace("/", "")).replace("(", "").replace(")", "") + "/product/1";
                if(b.length > 0){
                    location.href = c;
