<script>
    $(document).ready(function() {
        var toggle = $('.menu-toggle');
        var w = $(window).width();
        $(toggle).click(function() {
            $(toggle).toggleClass('active');
            if($(toggle).hasClass('active')){

                //$('.main_nav').css('z-index','99999');
            }
            else{
                //$('.main_nav').css('z-index','0');
            }

            $('body').toggleClass('active');
        });
        function refreshVar() {
            w = $(window).width();
        }
        $(window).resize(function() {
            refreshVar();
            //console.log(w);
            if (w <= 768) {
                $('.footertop li ul').hide();
                $('.footertop li').click(function() {
                    $(this).addClass('active').children('ul').slideDown('100');
                    $(this).siblings('li').removeClass('active').children('ul').slideUp('100');
                });
            } else if (w >= 769) {
                $('.footertop li').removeClass('active').children('ul').show();
            }

        });


        var base_href = 'javascript:location.href='
            _content = decodeURIComponent($(this).data('content'));
        //nb default
        $('#menu_area_main_info_url0').attr('onclick', base_href+"'/Laptops'");

        $('.product_sub_navigation_href').on("mouseover",function(){
            var _image = $(this).data('image'),
            _area  = $(this).data('area'),
            _href  = $(this).data('href'),
            main_info_image_string = '#menu_area_main_info_image'+_area,
            main_info_content_string = '#menu_area_main_info_content'+_area,
            main_info_href_string = '#menu_area_main_info_url'+_area,
            _content = decodeURIComponent($(this).data('content'));

            if(_image && _image !== false && _image !== 'false')
            {
                $(main_info_image_string).attr('src',_image);
                $(main_info_content_string).html(_content);
                $(main_info_href_string).attr('onclick', base_href+"'"+_href+"'");
            }
        });

        $(".listmenujs").delay(1500).on("mouseover",function(){
            $(".listmenujs").removeClass("active");
            $(this).addClass("active");

            $(".menu-area-js").delay(1500).hide();
            $("#" + $(this).data("area")).delay(1500).show();
        });
        $("html, body").click(function(e) {
            if ($(e.target).hasClass('navbtn') || $(e.target).hasClass('listmenujs')) {

            }
            else{
                //alert(2);
                $(".navbtn").removeClass('active');
                $('.subnav').hide();
            }
            //alert('d');
        });
        $(".navbtn").on('click',function(){
            if ($($(this).data('id')).is(":hidden")) {
                $('.subnav').hide();
                $(".navbtn").removeClass('active');
                $($(this).data('id')).fadeIn();
                $(this).addClass('active');
            }
            else{
                $('.subnav').hide();
                $(".navbtn").removeClass('active');
            }
        });
        // $("body:not('.msi_nav')").click(function () {
        //     $('.subnav').hide();
        // });
        $('.menu-toggle').click(function () {
            if($(this).hasClass('active')){
                //$('.nav_0').fadeIn();
                $(".nav_0").show().animate({"right": "-=300px"}, "slow");
            }
            else{
                $('.nav_0').animate({"right": "300px"}, "slow").fadeOut();
            }
            //$('.mobile_nav').hide().fadeIn().css('')
        });
        $(".menu_0 ul li a").on("click",function(){
            var id = $(this).data("menu");
            //$(".open-overlay").animate({"left": "-=300px"}, "slow").fadeOut();
            $(".open-overlay").each(function(){
                if ($(this).data("menu") == id) {
                    $(this).show().animate({"left": "-=300px"}, "slow");
                }
            });
        });

        $(".itemBack").on("click",function(){
            $(".open-overlay").animate({"left": "300px"}, "slow").fadeOut();
            $(".nav_0").show();
        });

        $(".m-dropdown-list").on("click",function(){
            $('.openbox').removeClass('active');
            $(this).children('.openbox').addClass('active');
            var id = $(this).data("sublist");
            $(".subList").each(function(){
                if ($(this).data("sublist") == id && !$(this).hasClass('active')) {
                    $(this).addClass('active');
                    $(this).toggle("slow");
                }
                else if($(this).data("sublist") == id && $(this).hasClass('active'))
                {
                    $(this).hide();
                    $(this).removeClass('active');
                }
                else{
                    $(this).hide();
                }
            });
        });
    });
</script>
