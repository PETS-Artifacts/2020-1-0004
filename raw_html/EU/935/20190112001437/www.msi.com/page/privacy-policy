<script type="text/javascript" src="https://storage-asset.msi.com/template/js/jquery.cookie.2.1.4.js"></script>
<script>
    var Cookies3 = Cookies.noConflict();
    //console.log(Cookies3.get('msi_cc_analytics'));
    $("body")
        .on('click', "#ccc-overlay:not(#ccc-module)", function(){
            //alert(1);
            $('#ccc-close').trigger('click');
            //$('#ccc-icon').trigger('click');
        })
        .on('click', '.ccc-notify-button', function(){
            $("body").data('cc-open', true);
            $('#header').css('padding-top','');
            $('#ccc-notify').css('top','');
            $('#ccc-notify').hide();
            $('#ccc-notify').prop('visible',false);
        })
        .on('click', '#ccc-notify-dismiss', function(){
            if( !$("body").data('cc-open') )
            {
                localStorage.removeItem("ccClosed");
            }
        })
        .on('click', '.checkbox-toggle-toggle', function(){
            if( $(this).hasClass('ccc-toggle-active') )
            {
                $(this).removeClass('ccc-toggle-active');
            }
            else
            {
                $(this).addClass('ccc-toggle-active');
            }
        })
        .on('click', '.checkbox-toggle-on', function(){
            $(this).siblings('.checkbox-toggle-toggle').removeClass('ccc-toggle-active');
        })
        .on('click', '.checkbox-toggle-off', function(){
            $(this).siblings('.checkbox-toggle-toggle').addClass('ccc-toggle-active');
        })
        .on('click', '#ccc-icon', function(){
            $('#header').css('padding-top','');
            $('#ccc-notify').css('top','');
            $('#ccc-notify').hide();
            $('#ccc-notify').prop('visible',false);
            if( Cookies3.get('msi_cc_analytics') && Cookies3.get('msi_cc_analytics') == 'accepted' ){ $("[class='optional-cookie'][data-index='0']").find('.checkbox-toggle-toggle').addClass('ccc-toggle-active'); }
            if( Cookies3.get('msi_cc_marketing') && Cookies3.get('msi_cc_marketing') == 'accepted' ){ $("[class='optional-cookie'][data-index='1']").find('.checkbox-toggle-toggle').addClass('ccc-toggle-active'); }
            if( Cookies3.get('msi_cc_social') && Cookies3.get('msi_cc_social') == 'accepted' ){ $("[class='optional-cookie'][data-index='2']").find('.checkbox-toggle-toggle').addClass('ccc-toggle-active'); }        });
    $(document).ready(function() {
        var w = $(window).width();
        refreshVar();
        $(window).resize(function() {
            refreshVar();
        });
        function refreshVar() {
            w = $(window).width();
            if (w <= 768) {
                $('.footertop li ul').hide();
                $('body').on('click', '.footertop li h2', function() {
                    $(this).parent('li').siblings('li').children('ul').stop().slideUp();
                    $(this).siblings('ul').stop().slideToggle('100');
                });
            } else if (w >= 769) {
                $('body').on('click', '.footertop li h2', function() {
                    return false;
                });
                $('.footertop li').children('ul').show();
            }
        }

        $(window).resize(function() {
            if($('#ccc-notify').is('visible',true)){
                $('#header').css('padding-top',$('#ccc-notify').outerHeight(true));
            }
            else{
                $('#header').css('padding-top','');
            }
        });
    });

    $(".send-btn").click(function() {
        var email = $(".ui-autocomplete-input").val();

        if(checkEmail(email) === true)
        {
            $.ajax({
                type :"GET",
                url  : "/ajax/sam",
                dataType: "json",
                data : { 
                    email : email,
                    type  : 'blog'                            
                },
                success : function(data) {
                    alert(data['status']);                      
                },
                error: function(jqXHR) {
                    console.log(jqXHR);
                }
            })
        }
    });

    function checkEmail(email) 
	{ 
	    var tReg = new RegExp("[a-zA-Z0-9._%-]+@[a-zA-Z0-9._%-]+\.[a-zA-Z]{2,4}"); 
	 
	    if(!email.match(tReg)) 
	    { 
	        alert('You have entered an invalid email address!'); 
	        return false; 
	    } 
	    else 
	    { 
	        return true; 
	    } 
	} 
</script></body>
</html>