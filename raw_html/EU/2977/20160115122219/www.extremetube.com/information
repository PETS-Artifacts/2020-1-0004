<script type="text/javascript">
	var areTjJSrequestsEnabled = false;
	page_params.isSegmentGay = false;

	function loadJS() {
		try{
			var myHead_JS = new Load_scripts();
			myHead_JS.init({
				'jsFileList'    : page_params.jsFileList, //json object with file list
				'head'          : head //head.js plugin object
			});
		}catch( e ){
			console.log( e.message );
		}
	}

	if ((typeof window.performance === 'undefined') || (page_params.isTablet)) {
		loadJS();
	} else {
		var mgPerformanceCallbacks = [
			function(){
				loadJS();
			}
		];
	}
</script>

