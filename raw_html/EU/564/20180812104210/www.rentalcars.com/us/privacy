		var RenderedPageViewEvent = function() {};
		RenderedPageViewEvent.prototype.fire = function(peripheralData) {
          peripheralData = peripheralData || {};
          peripheralData.pageUrl = '/PrivacyPolicy.do';
          $.post('/tracking/AjaxRenderedPageViewEvent.do', peripheralData);
		}
		window.RC = window.RC || {};
		(function(RC){
			RC.tracking = RC.tracking || {};
			RC.tracking.events = RC.tracking.events || {};
			RC.tracking.events.pageView = RC.tracking.events.pageView || new RenderedPageViewEvent();
			RC.options = RC.options || {
			  'pageViewIsAutomaticallyFired': true
			};
			if (RC.options.pageViewIsAutomaticallyFired) {
				RC.tracking.events.pageView.fire();
			}
		})(window.RC);
