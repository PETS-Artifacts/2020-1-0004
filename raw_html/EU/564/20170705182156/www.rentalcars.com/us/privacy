<script type="text/javascript" nonce="1r875fir8prdoid4hkc7l9r9gp">

    (function () {

        var protocolCookie = 'tj_protocol';

        // If the cookie is different the protocol has either changed or we've never recorded it
        if($.cookies.get(protocolCookie) !== window.location.protocol) {
            trackProtocolChange(window.location.protocol)
        }

        function trackProtocolChange(protocol) {
            var secure = protocol === 'https:';
            GAQPush('_trackEvent', 'landing_secure', secure.toString(), '', 1, true);
            $.cookies.set(protocolCookie, protocol);
        }

    })();

</script>
