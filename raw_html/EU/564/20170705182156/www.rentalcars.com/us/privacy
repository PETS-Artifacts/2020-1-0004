	$(document).ready(function() {
		var currentPage = "naviPrivpolicy";
		//If we're not on an Affiliate-related page, hide the Affiliate sub-menu.
		$(".affiliatePages li").hide();
		$("#termsPages li").hide();
		if(currentPage === "naviAffiliate") {
			$(".affiliatePages li").show();
			$("#linkAffiliate").addClass("partners_title");
		} else if(currentPage === "naviTerms") {
			$("#termsPages").show();
			$("#termsPages li").show();
			$("#linkTerms").addClass("partners_title");
		} else if(currentPage === "naviHelp") {
        } else {
			$("#" + currentPage).addClass("active");
		}
	});
</script>

