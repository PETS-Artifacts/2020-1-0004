			<script type="text/javascript" nonce="1r875fir8prdoid4hkc7l9r9gp">
			var placeholder = "Your Email Address";
			
			$(document).ready(function(){
				
				
				$("#email_footer").keyup(function(e){
					if (($(this).val().length > 0) && (e.which == 13)) { $("#subscribe_footer").trigger("click"); }
				});
				
				$("#subscribe_footer").click(function(){
					var email = $("#email_footer").val();
						if ($("#email_footer").val().search(/^\w+((-\w+)|(\.\w+)|(\.\-\w+)|(\-\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/) != -1) {
							doAjaxFooterSubscribe();
							
						} else {
							$("#subscribeFail_footer").slideDown();
							
						}
					GAQPush("_trackEvent", "FE-1434", "Footer SignUp", "SignUp Clicked", 1, true);
				});
				
				if ($.browser.msie && ($.browser.version <= 9)) {
					$("#email_footer").val(placeholder).css("color","#a9a9a9");
					$("#email_footer").bind("focus", function(){
						if ($(this).val() == placeholder) { $(this).val("").css("color","#000"); }
					}).bind("blur", function(){
						if ($(this).val() == "") { $(this).val(placeholder).css("color","#a9a9a9"); }
					});
				}
			});
			
			function doAjaxFooterSubscribe() {
				var email = $("#email_footer").val();
				$.ajax({
					type: "POST",
					url: "/ResponsiveProcessOfferSubscription.do",
					data: "email=" + email + "&fsFirst=USA+-+Florida",
					success: function(response){
						$("#subscribe_footer, #email_footer, #subscribeFail_footer, .footerbar_right_icon").hide();
						//$(".footerbar_right").css("float", "none");
						$("#subscribeSuccess_footer").show();
						setTimeout(function(){
							$("#footer_signup").slideUp("slow");
						}, 5000);
					},
					error: function(e){
						alert("Error: " + e);
					}
				});
			}
		</script>
