			        if (typeof(jQuery.fn.live) !== "undefined"){
                        
                        $(document).ready(function(){
			            	var trackingUri = "/PrivacyPolicy.do";
			                if(!trackingUri){
			                    trackingUri = "LandingPage";
			                }
			                if(trackingUri.indexOf(';') > -1){
			                    var newTrackingUri = trackingUri.split(';');
			                    trackingUri = newTrackingUri[0];
			                }
			                
			                $('.LPMcontainer').live('click', function(){
			                    GAQPush('_trackEvent', 'contact points', "LCDisplay", trackingUri, 1, true);
			                    RCActionEventPush('contact points','LCDisplay', trackingUri,1, 'sessionId: EED98AC42B339E78CE3C706FE228B025.node230a');
			                });
			                $('.lp_submit_button').live('click', function(){
			                    GAQPush('_trackEvent', 'contact points', "Livechat Engaged", trackingUri, 1, true);
			                    RCActionEventPush('contact points','Livechat Engaged', trackingUri,1, 'sessionId: EED98AC42B339E78CE3C706FE228B025.node230a');
			                });
