			<script>
				$(document).ready(function() {
					var crmCreateAccountErrorAlreadyExists = "Email Already Registered";
					var crmCreateAccountErrorInvalidEmail = "Please enter a valid email address";
					var crmCreateAccountErrorInvalidPassword = "Enter Password";
					var crmCreateAccountErrorUnknown = "Sorry an error has occurred. Please try again.";
					GAQPush("_trackEvent", "FE-5430", "Variant B", "Viewed", 1, true);
					$("#crmAccountSignupFooterAction").click(function() {
						GAQPush("_trackEvent", "FE-5430", "Variant B", "Sign-up attempted", 1, true);
						if($("#crmAccountSignupFooterEmail").val().search(/^\w+((-\w+)|(\.\w+)|(\.\-\w+)|(\-\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/) != -1) {
							if(!$("#crmAccountSignupFooterPassword").val()) {
								$("#subscribeFail_footer").text(crmCreateAccountErrorInvalidPassword);
				    			$("#subscribeFail_footer").slideDown();
							} else {
								$.ajax({
							       type: "POST",
							       data: { email : $("#crmAccountSignupFooterEmail").val() },
							       url: '/CheckEmailRegistered.do;jsessionid=0BABEFD3C3E6385681ABE96DDEB49B80.node414a',
							       cache: false,
							       success: function(data) {
							    		if(data.emailRegistered) {
							    			// Email already exists on account.
							    			GAQPush("_trackEvent", "FE-5430", "Variant B", "Sign-up failed: email already registered", 1, true);
							    			$("#subscribeFail_footer").text(crmCreateAccountErrorAlreadyExists);
							    			$("#subscribeFail_footer").slideDown();
							    		} else {
							    			// Create account.
							    			$("#crmCreateEmail").val($("#crmAccountSignupFooterEmail").val());
							    			$("#crmCreatePsw").val($("#crmAccountSignupFooterPassword").val());
							    			if(createAccount() === false) {
							    				GAQPush("_trackEvent", "FE-5430", "Variant B", "Sign-up failed: error making account", 1, true);
							    				$("#subscribeFail_footer").text(crmCreateAccountErrorUnknown);
							    				$("#subscribeFail_footer").slideDown();
							    			} else {
							    				GAQPush("_trackEvent", "FE-5430", "Variant B", "Sign-up successful", 1, true);
							    			}
							    		}
									},
									error: function() {
										GAQPush("_trackEvent", "FE-5430", "Variant B", "Sign-up failed: validation error", 1, true);
										$("#subscribeFail_footer").text(crmCreateAccountErrorUnknown);
										$("#subscribeFail_footer").slideDown();
									}
								});
							}
						} else {
							GAQPush("_trackEvent", "FE-5430", "Variant B", "Sign-up failed: invalid email format", 1, true);
							$("#subscribeFail_footer").text(crmCreateAccountErrorInvalidEmail);
							$("#subscribeFail_footer").slideDown();
						}
					});
				});
			</script>
		<script type="text/javascript" nonce="ti4j8afipnpniirbkpboavn93t">
			var placeholder = "Your Email Address";
			
			$(document).ready(function(){
				
				
				$("#email_footer").keyup(function(e){
					if (($(this).val().length > 0) && (e.which == 13)) { $("#subscribe_footer").trigger("click"); }
				});
				
				$("#subscribe_footer").click(function(){
					var email = $("#email_footer").val();
						if ($("#email_footer").val().search(/^\w+((-\w+)|(\.\w+)|(\.\-\w+)|(\-\.\w+))*\@[A-Za-z0-9]+((\.|-)[A-Za-z0-9]+)*\.[A-Za-z0-9]+$/) != -1) {
							doAjaxFooterSubscribe();
							
						} else {
							$("#subscribeFail_footer").slideDown();
							
						}
					GAQPush("_trackEvent", "FE-1434", "Footer SignUp", "SignUp Clicked", 1, true);
				});
				
				if ($.browser.msie && ($.browser.version <= 9)) {
					$("#email_footer").val(placeholder).css("color","#a9a9a9");
					$("#email_footer").bind("focus", function(){
						if ($(this).val() == placeholder) { $(this).val("").css("color","#000"); }
					}).bind("blur", function(){
						if ($(this).val() == "") { $(this).val(placeholder).css("color","#a9a9a9"); }
					});
				}
			});
			
			function doAjaxFooterSubscribe() {
				var email = $("#email_footer").val();
				$.ajax({
					type: "POST",
					url: "/ResponsiveProcessOfferSubscription.do",
					data: "email=" + email + "&fsFirst=U.A.E.",
					success: function(response){
						$("#subscribe_footer, #email_footer, #subscribeFail_footer, .footerbar_right_icon").hide();
						//$(".footerbar_right").css("float", "none");
						$("#subscribeSuccess_footer").show();
						setTimeout(function(){
							$("#footer_signup").slideUp("slow");
						}, 5000);
					},
					error: function(e){
						alert("Error: " + e);
					}
				});
			}
		</script>
