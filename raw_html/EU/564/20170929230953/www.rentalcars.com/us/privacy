        $(document).ready(function() {

			// Handle enter keypress on manage booking form inputs
            $('#tmp_email, #tmp_ref').keypress(function(event){
                var ENTER_KEY = 13;
                var keycode = event.keyCode || event.which;

                if (keycode == ENTER_KEY) {
                    $('#mybookinglogin').click()
                    event.preventDefault();
                    return;
                }
			});
        });
