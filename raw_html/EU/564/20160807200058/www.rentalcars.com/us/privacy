<script type="text/javascript" nonce="23nu0hti24kb5lnf7uvciolkdg">
	
	var withLogosTxt = 'We are currently searching the suppliers below to get you the best deal on your rental car...';
	var withoutLogosTxt = 'We are currently searching for the best deal on your rental car...';
	var suppliers = ['alamo', 'avis', 'budget', 'europcar', 'guysalmon', 'hertz', 'sixt', 'thrifty', 'centauro', 'goldcar', 'recordgo', 'orlando', 'hiper', 'dollar', 'national', 'buchbinder', 'locauto', 'maggiore', 'sicilybycar', 'guerin', 'caro', 'caldera_car_hire', 'amc', 'scandiarent', 'foxautorent', 'firstcar', 'ezirent', 'nationwide', 'greenmotion', 'owners', 'advantage', 'first_car_new', 'thai', 'redspot', 'cundari_rent', 'aircar', 'direct_rent', 'touring_rent_auto', 'enterprise', 'os', 'avantcar', 'ace_rent_a_car', 'aurigacrown', 'al_muftah', 'autonom', 'payless', 'usave', 'east_coast_car_rentals'];
	var images = {};	
	
	$(function() {
		$( suppliers ).each(function() {		
			images[ this ] = $( document.createElement('img') )
				.attr('src','/images/suppliers/' + this.replace(' ', '_') + '_logo.gif')
				.attr('width',60)
				.attr('height',30)
				.attr('onerror','this.style.display=none');
		});      
	});
	
	function updateSuppliers() {
		
		var country = '';
		var iso = '';
		var city = '';
		var location = '';
		
		if ($('form[name=FreeTextSearchResultsForm] input[name=countryIso]').length > 0) {
			iso = $('form[name=FreeTextSearchResultsForm] input[name=countryIso]').val();
			if (!iso) {
				iso = '';
			}
		}
		else {
			
							if ($('form[name=SearchResultsForm] select[name=country]').length > 0) {
								country = $('form[name=SearchResultsForm] select[name=country]').val();
							}
							if ($('form[name=SearchResultsForm] select[name=city]').length > 0) {
								city = $('form[name=SearchResultsForm] select[name=city]').val();
							}
							if ($('form[name=SearchResultsForm] select[name=location]').length > 0) {
								location = $('form[name=SearchResultsForm] select[name=location] option:selected').text();
							}
						
			
			
		
		}
  
 	 	if((country != '' && city != '' && location != '') || (iso != '')) {
		
			$.ajax({
				url: '/AjaxSuppliers.do;jsessionid=E41D9808AE80E480E9ECCEC58E223A1E.node038a',
				data: { 
					country: country,
					iso: iso,
					city: city
					//location: location
				},
				timeout: 200,
				async: false,
				dataType: 'json',			
				success: function( result ) {
					$( result ).each(function() {
						$('.loading_logos').append(	images[ this.name ] );
					});
				}
			});	
			
		}
				
	}				
	
	
	function throb() {	
		updateSuppliers();
		if( $('.loading_logos img').length > 0 ) {
			$('#loading_box h3').html( withLogosTxt );
		}			
		$('#loading_box').lightbox_me({	centered: true	});
	}

</script>

