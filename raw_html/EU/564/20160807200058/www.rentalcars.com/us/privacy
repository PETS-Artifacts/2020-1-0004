        	$(function() {
          		$('#same_loc_check').click(function() {
            		$('#same_loc_hide').slideToggle('fast');          
          		})
          
            	if( $('input[name=puSearchAgainInput]').val() == $('input[name=doSearchAgainInput]').val() || $('input[name=doSearchAgainInput]').val() == '' ) {
            		$('#same_loc_hide').hide();
            		$('#same_loc_check').attr('checked',true);
          		} else {
            		$('#same_loc_hide').show();
            		$('#same_loc_check').attr('checked',false);
          		}
            });            
