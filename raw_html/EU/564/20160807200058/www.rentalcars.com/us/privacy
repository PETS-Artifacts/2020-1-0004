	 <script type="text/javascript" nonce="23nu0hti24kb5lnf7uvciolkdg">
	 $(document).ready( function () {
		  $('.opt-toggle').bind('click', function () { 
			   if ($('#options-site').css('display') == 'none'){
				    $('#options-site, .opt-label, .opt-arrow').css('display', 'block'); 
			   }else{
				   $('#options-site, .opt-label, .opt-arrow').css('display', 'none');
			   }
		  });
		  
		    $('html').click(function() {
			    if ($('.btn-dd').hasClass('active')){
			        $('.btn-dd').removeClass('active');
			    }
			});
		    
			 
		 			$('#btn-my-booking').click(function(event){
					    $('#btn-language, #btn-currency, #btn-notification').removeClass('active');
					    $(this).toggleClass('active');
					    event.stopPropagation();
					});			
		 			
		 			
					
					$('#btn-currency').click(function(event){
		                $('#btn-language, #btn-my-booking, #btn-notification').removeClass('active');
		                $(this).toggleClass('active');
		                event.stopPropagation();
		            });
					
					$('#btn-language').click(function(event){
		                $('#btn-currency, #btn-my-booking, #btn-notification').removeClass('active');
		                $(this).toggleClass('active');
		                event.stopPropagation();
		            });
					
					$('#btn-notification').click(function(event){
						$('#btn-language, #btn-currency, #btn-my-booking').removeClass('active');
						$(this).toggleClass('active');
						event.stopPropagation();
					});
			 	
			
			$('.dd-menu').click(function(event){
			    event.stopPropagation();      
			});
	 });
	 </script>
	
