<script type="text/javascript" nonce="23nu0hti24kb5lnf7uvciolkdg">

$(function() {

	$('#btnproceed').click(function() {
		if( !$('#same_loc_hide').is(':visible') ) {              	    	  	
	    	$("input[name=doSearchInput]").val('');
	    	$("input[name=doSearchAgainInput]").val('');
	    	$("input[name=doLocationType]").val('');
	    	$("input[name=dropCoordinates]").val('');  
		} 
		throb();
		document.FreeTextSearchResultsForm.submit();
	});
});

$(function() {
    $("input[name=puSearchInput]").autocomplete(
    { source:function(request,response){
          $.getJSON('/Suggestions.do',{term:$.trim(request.term)},response);
         }
    },
    { delay:0,
      minChars:3,
      minLength:3,
      matchContains:true,
      matchSubset:false,
      max:10,
      scrollHeight:175,
      width:200,
	  search: function() {
		  $("input[name=searchFromAutoSuggest]").val("");
      },
      select: function(event, ui) {
    	  $("input[name=puSearchInput]").val(ui.item.label);
    	  $("input[name=puSearchAgainInput]").val(ui.item.label);
    	  $("input[name=puLocationType]").val(ui.item.type);
    	  $("input[name=coordinates]").val(ui.item.coordinates);
          $("input[name=isoCode]").val(ui.item.iso);
          $("input[name=searchFromAutoSuggest]").val("true");
     }
    });
    $("input[name=puSearchInput]").data( "autocomplete" )._renderItem = function( ul, item ) {        
        if(item.type == 'airport')
        	return $( "<li></li>" )
                .data( "item.autocomplete", item )
               	.append( "<a><img class='li-icon' src='/images/airport-icon.gif' alt=''>" + item.label + "</a>" )
                .appendTo( ul );
        else
            return $( "<li></li>" )
            .data( "item.autocomplete", item )
            .append( "<a>" + item.label + "</a>" )
            .appendTo( ul );
    };
    
});

