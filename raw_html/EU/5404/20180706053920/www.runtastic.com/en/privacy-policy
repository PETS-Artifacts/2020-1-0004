        var node = document.createElement('script');node.type = 'text/javascript';node.async = true;node.src = '//platform.linkedin.com/in.js';var script_tag = document.getElementsByTagName('script')[0];script_tag.parentNode.insertBefore(node, script_tag);
      }
    });
  //]]>
</script>

