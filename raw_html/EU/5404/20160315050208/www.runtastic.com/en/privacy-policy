  var $;
  $ = jQuery;
  return $(".download_selection .dropdown").each(function() {
    var $dropdown, $menu, dropdown_w, image, left, menu_w;
    image = "";
    $dropdown = $(this);
    $menu = $dropdown.find(".dropdown-menu");
    menu_w = parseInt($menu.css("width").slice(0, -2));
    dropdown_w = parseInt($dropdown.css("width").slice(0, -2));
    left = Math.floor((dropdown_w - menu_w) / 2);
    $menu.css({
      "margin-left": left + "px"
    });
    return $dropdown.hover(function() {
      $(this).addClass("open");
      return $(this).find(">:first-child").find(">:first-child").addClass(image);
    }, function() {
      $(this).removeClass("open");
      return $(this).find(">:first-child").find(">:first-child").removeClass(image);
    });
