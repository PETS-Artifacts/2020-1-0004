      $("#button-container").delay(3000).animate({"opacity": "1"}, 700);      
      if( $.cookie('lightbox_viewed') === null ) { 
        $(function(){                      
          $.ajax({ 
              dataType: "json",
              url : "https://api.ninjakiwi.com/utility/xpromo",
              success: (result)=> {
                   var promotions = result.promotions;
                   var currentPromotions = promotions["4"];
                   var currentPromotion = null; 
                   currentPromotions.forEach(function(promo){
                       if(promo.startTime < new Date().getTime())
                       {  currentPromotion = promo }
                   })
                   $.ajax({
                        dataType : "json",
                        url : "https://priority-static-api.nkstatic.com/storage/static/appdocs/" + currentPromotion.appid +"/appdocs/cross_promotion",
                        success : (result) => {
                                var assets = result.assets;
                                var asset = null;
                                assets.forEach(function(as) { 
                                      if(as.name == "website")
                                      { asset = as; }
                                });
                                $('#lightbox_all').css("background-image","url('" + asset.url + "')")
                                console.log(result.links);
                                //$('.full-link').attr("href", result.links["iosweb"]).toggle(result.links["iosweb"] != "");
                                if (result.links["defaultweb"] !== "") {
                                  $('.full-link').attr("href", result.links["defaultweb"]).toggle(result.links["defaultweb"] != "");
                                } else {
                                  $('.full-link').attr("href", result.links["iosweb"]).toggle(result.links["iosweb"] != "");
                                }
                        
                                $('.buy-iphone').attr("href", result.links["iosweb"]).toggle(result.links["iosweb"] != "");
