                }

                return LAST_VISIBLE = new Date().getTime();
            }

            var now = new Date().getTime();
            if (now - LAST_VISIBLE > KILL_SOCKET_AFTER_IDLE) {
                if (IS_CONNECTED) {
                    console.log('GOING OUT!');
                    socket.disconnect();
                }
            }

        }, 1000 * 1);
    }

