    var LAST_VISIBLE = new Date().getTime();

    function saveSocketNetworkBW() {

        if (TE_URL.indexOf("calendar") > -1) return;
        if (TE_URL.indexOf("stream") > -1) return;
        if (TE_URL.indexOf("/currencies") > -1) return;
