        // Leaving website confirmation.
        function LeavingWebsiteDialogue(url, target) {
            var message = $(".leavingWebsiteText").val();
            if (confirm(message)) {
                if (target.toLowerCase() == "blank") {
                    window.open(url);
                } else {
                    eval(target + ".location='" + url + "'");
                }
            } else { LeavingWebsiteDialogue.stop(); }
        }
        
    </script>
    
