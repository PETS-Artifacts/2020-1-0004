            <!-- GIS MAP -->
            <div id="gis_map" class="gMapContainer startingPosition">
                <div id="gis_map_content">
                    


<script type="text/javascript">
    
    var directionsDisplay;
    var googleMarkers = {};
    var loaderCheck = false;
    var markerArray = [];

    $(function () { 

        /* autocomplete locations */
        $("#ctl00_ctl00_Body_header_banner_gMap_txtSource").each(function () {
            new google.maps.places.Autocomplete(this, { types: ['geocode']});
        });
        $("#ctl00_ctl00_Body_header_banner_gMap_txtDestination").each(function () {
            new google.maps.places.Autocomplete(this, { types: ['geocode']});
        });

        travelMode = google.maps.TravelMode.DRIVING;

        // overwrite map is disabled by default
        $("#ctl00_ctl00_Body_header_banner_gMap_ddlOverwriteMap").prop("disabled",true);

        // overwrite map
        $("#ctl00_ctl00_Body_header_banner_gMap_rbOverwriteMap").click(function (){
            if ($(this).prop("checked")){
                // switch radio buttons
                $("#ctl00_ctl00_Body_header_banner_gMap_rbNewMap").prop("checked",false)
                // load the user's maps
                $.ajax({
                    contentType: "application/json; charset=utf-8",
                    url: "https://www.visitmalta.com/WebServices/gMapWebService.asmx/GetEndUserMaps",
                    type: "POST",
                    data: JSON.stringify({
                        "iEndUserID": ""
                    }),
                    datatype: "json",
                    success: function (results) {           
                        var listItems = ""             
                        var mapList = JSON.parse(results.d);
                        for (var key in mapList) {
                            var value = mapList[key];
                            listItems += "<option value='" + key + "'>" + value + "</option>";
                        }

                        $("#ctl00_ctl00_Body_header_banner_gMap_ddlOverwriteMap").html(listItems);
                        $("#ctl00_ctl00_Body_header_banner_gMap_ddlOverwriteMap").prop("disabled", false);
                    },
                    error: function (request, status, errorThrown) {
                        //errorMessage($("#subgurim_gMap"),'Error', 'Error loading maps');

                        $("#divSaveMsgAlert").css("display","");
                        $("#divSaveMsgAlert").addClass("alert-danger");
                        $("#ctl00_ctl00_Body_header_banner_gMap_lblSaveMsg").text("Error, Error loading maps");
                    }
                });
            }
        });

        $("#ctl00_ctl00_Body_header_banner_gMap_rbNewMap").click(function (){
            if ($(this).prop("checked")){
                // switch radio buttons 
                $("#ctl00_ctl00_Body_header_banner_gMap_rbOverwriteMap").prop("checked",false)
                $("#ctl00_ctl00_Body_header_banner_gMap_ddlOverwriteMap").prop("disabled",true)
                $("#ctl00_ctl00_Body_header_banner_gMap_ddlOverwriteMap").html("");
            }
        });

        // on load default to the new map
        $("#ctl00_ctl00_Body_header_banner_gMap_rbNewMap").click();

        // send by e-mail
        $("#ctl00_ctl00_Body_header_banner_gMap_btnSendByEmail").click(function () {
            var staticMap = $(this).parents(".gMap").find(".sendByEmail");
            if ($(staticMap).is(":visible")){
                $(staticMap).hide();
                $("#ctl00_ctl00_Body_header_banner_gMap_btnSend").prop("disabled","disabled");
            }
            else{                
                $(staticMap).show();
                updateImage($("#ctl00_ctl00_Body_header_banner_gMap_staticMapIMG"), $("#ctl00_ctl00_Body_header_banner_gMap_txtStaticMapUrl"), subgurim_gMap, getMarkers($("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers")), function(error){
                    if (error == "route") {
                        //errorMessage($("#subgurim_gMap"),'Error', 'The selected route is too long');                        
                        $("#divEmailMsgAlert").css("display","");
                        $("#divEmailMsgAlert").addClass("alert-danger");
                        $("#ctl00_ctl00_Body_header_banner_gMap_lblEmailMsg").text("Error, The selected route is too long");
                    }
                    else{
                        //errorMessage($("#subgurim_gMap"),'Error', 'There are too many markers, please filter your search');                        
                        $("#divEmailMsgAlert").css("display","");
                        $("#divEmailMsgAlert").addClass("alert-danger");
                        $("#ctl00_ctl00_Body_header_banner_gMap_lblEmailMsg").text("Error, There are too many markers, please filter your search");
                    }
                });
                $("#ctl00_ctl00_Body_header_banner_gMap_btnSend").prop("disabled","");
            };
        });
        
        // sends the map
        $("#ctl00_ctl00_Body_header_banner_gMap_btnSend").click(function() {
            var errorMsg = "";
            if($('#ctl00_ctl00_Body_header_banner_gMap_txtEmailFromName').val() == "" || $('#ctl00_ctl00_Body_header_banner_gMap_txtEmailToName').val() == ""){
                errorMsg += "First name is required";
            }

            
            if($('#ctl00_ctl00_Body_header_banner_gMap_txtEmailFromAddress').val() == "" || $('#ctl00_ctl00_Body_header_banner_gMap_txtEmailToAddress').val() == ""){

                if(errorMsg != ""){
                    errorMsg += ", ";
                }

                errorMsg += "E-mail is required";
            }


            // extract the markers
            var markers = getMarkers($("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers"));
            
            // extract the route
            var route = "";
            if (directions != null) {
                var myDirections = directionsDisplay.directions.routes[0].overview_path;
                route = {
                    "Source": {
                        "ID": $("#ctl00_ctl00_Body_header_banner_gMap_txtSource").val(),
                        "Points": [{
                            "Latitude": myDirections[0].lat(),
                            "Longitude": myDirections[0].lng()
                        }]
                    },
                    "Destination": {
                        "ID": $("#ctl00_ctl00_Body_header_banner_gMap_txtDestination").val(),
                        "Points": [{
                            "Latitude": myDirections[myDirections.length - 1].lat(),
                            "Longitude": myDirections[myDirections.length - 1].lng()
                        }]
                    }
                };
            }

            if(markers.length == 0){
                errorMsg += "Error"
            }                  
            
            if(errorMsg != ""){
                $("#divEmailMsgAlert").css("display","");
                $("#divEmailMsgAlert").addClass("alert-danger");
                $("#ctl00_ctl00_Body_header_banner_gMap_lblEmailMsg").text(errorMsg);
                return false;
            };

            $.ajax({
                contentType: "application/json; charset=utf-8",
                url: "https://www.visitmalta.com/WebServices/gMapWebService.asmx/SendXmlCoordinates",
                type: "POST",
                data: JSON.stringify({
                    "strCoordinates": JSON.stringify(markers),
                    "strRoute": JSON.stringify(route),
                    "strFromName": $("#ctl00_ctl00_Body_header_banner_gMap_txtEmailFromName").val(),
                    "strToName": $("#ctl00_ctl00_Body_header_banner_gMap_txtEmailToName").val(),
                    "strFromAddress": $("#ctl00_ctl00_Body_header_banner_gMap_txtEmailFromAddress").val(),
                    "strToAddress": $("#ctl00_ctl00_Body_header_banner_gMap_txtEmailToAddress").val(),
                    "strMessage": $("#ctl00_ctl00_Body_header_banner_gMap_txtEmailMessage").val(),
                    "strStaticMapUrl": $("#ctl00_ctl00_Body_header_banner_gMap_txtStaticMapUrl").val(),
                    "iLanguageID": "1"
                }),
                datatype: "json",
                success: function (results) {
                    if (results == false) {
                        //errorMessage($("#subgurim_gMap"),'Error', 'Failed to send the map. Please try again later');
                        $("#divEmailMsgAlert").css("display","");
                        $("#divEmailMsgAlert").addClass("alert-danger");
                        $("#ctl00_ctl00_Body_header_banner_gMap_lblEmailMsg").text("Error, Failed to send the map. Please try again later");
                    }
                    else{                                             
                        //successMessage($("#subgurim_gMap"),'Success', 'The map has been sent successfully to the address provided');
                        $("#ctl00_ctl00_Body_header_banner_gMap_txtEmailToName").val("");
                        $("#ctl00_ctl00_Body_header_banner_gMap_txtEmailToAddress").val("");
                        $("#ctl00_ctl00_Body_header_banner_gMap_txtEmailMessage").val('The following map might be of interest to you:');
                        
                        ///$.fancybox.close();  
                        
                        $("#divEmailMsgAlert").css("display","");
                        $("#divEmailMsgAlert").removeClass("alert-danger");
                        $("#divEmailMsgAlert").addClass("alert-success");
                        $("#ctl00_ctl00_Body_header_banner_gMap_lblEmailMsg").text("Success, The map has been sent successfully to the address provided");
                    }
                },
                error: function(){
                    // errorMessage($("#subgurim_gMap"),'Error', 'Failed to send the map. Please try again later');
                    $("#divEmailMsgAlert").css("display","");
                    $("#divEmailMsgAlert").addClass("alert-danger");
                    $("#ctl00_ctl00_Body_header_banner_gMap_lblEmailMsg").text("Error, Failed to send the map. Please try again later");
                }
            });

            return false;
        });
        
        // print the map
        $("#ctl00_ctl00_Body_header_banner_gMap_btnPrint").click(function () {
            var nodes = [];

            // image            
            nodes.push($(this).parents(".gMap").find(".gmap_wrapper")[0]);

            // directions
            if (directions != null) {    
                nodes.push($("#directions_info")[0]);
            }

            printSelection(nodes);
        });

       
        // open the saved maps dialog
        $("#ctl00_ctl00_Body_header_banner_gMap_btnSavedMaps").click(function () {
            //$(this).parents(".gMap").find(".savedMaps").slideToggle();
            if($(this).hasClass("notavailable")){
                $('#myModalMap_save').modal('hide');
            }else{
                $('#myModalMap_save').modal('show');
            };
        });

        // save the map
        $("#ctl00_ctl00_Body_header_banner_gMap_btnSave").click(function () {
            var title = $("#ctl00_ctl00_Body_header_banner_gMap_txtSaveTitle").val();
            var markers = getMarkers($("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers"));
            
            // extract the route
            var route = "";
            if (directions != null) {
                route = {
                    "Source": {
                        "ID": $("#ctl00_ctl00_Body_header_banner_gMap_txtSource").val(),
                        "Points": [{
                            "Latitude": directions.getMarker(0).getPosition().lat(),
                            "Longitude": directions.getMarker(0).getPosition().lng()
                        }]
                    },
                    "Destination": {
                        "ID": $("#ctl00_ctl00_Body_header_banner_gMap_txtDestination").val(),
                        "Points": [{
                            "Latitude": directions.getMarker(directions.getNumGeocodes() - 1).getPosition().lat(),
                            "Longitude": directions.getMarker(directions.getNumGeocodes() - 1).getPosition().lng()
                        }]
                    }
                };
            }

            var mapID = -1;
            var bReady = true;

            // check if the user selected overwrite or new
            if ($("#ctl00_ctl00_Body_header_banner_gMap_rbOverwriteMap").prop("checked")) {
                mapID = $("#ctl00_ctl00_Body_header_banner_gMap_ddlOverwriteMap option:selected").val();
                title = $("#ctl00_ctl00_Body_header_banner_gMap_ddlOverwriteMap option:selected").text();
                if (mapID == -1) {
                    //errorMessage($("#subgurim_gMap"),'Error', 'Please select a map to overwrite');
                    $("#ctl00_ctl00_Body_header_banner_gMap_ddlOverwriteMap").focus();
                    bReady = false;

                    $("#divSaveMsgAlert").css("display","");
                    $("#divSaveMsgAlert").addClass("alert-danger");
                    $("#ctl00_ctl00_Body_header_banner_gMap_lblSaveMsg").text("Error, Please select a map to overwrite");
                }
            }
            else{
                if (title == "") {
                    // errorMessage($("#subgurim_gMap"),'Error', 'Please input a valid map name');
                    $("#ctl00_ctl00_Body_header_banner_gMap_txtSaveTitle").focus();
                    bReady = false;

                    $("#divSaveMsgAlert").css("display","");
                    $("#divSaveMsgAlert").addClass("alert-danger");
                    $("#ctl00_ctl00_Body_header_banner_gMap_lblSaveMsg").text("Error, Please input a valid map name");
                }
            }

            if (bReady){                
                $.ajax({
                    contentType: "application/json; charset=utf-8",
                    url: "https://www.visitmalta.com/WebServices/gMapWebService.asmx/CreateXmlCoordinates",
                    type: "POST",
                    data: JSON.stringify({
                        "iEndUserID": "",
                        "strCoordinates": JSON.stringify(markers),
                        "strRoute": JSON.stringify(route),
                        "strTitle": title,
                        "iMapID": mapID
                    }),
                    datatype: "json",
                    success: function (results) {
                        if ($.parseJSON(results.d) == -1) {
                            //errorMessage($("#subgurim_gMap"),'Error', 'Failed to save the map. Please try again later.');

                            
                            $("#divSaveMsgAlert").css("display","");
                            $("#divSaveMsgAlert").addClass("alert-danger");
                            $("#ctl00_ctl00_Body_header_banner_gMap_lblSaveMsg").text("Error, Failed to save the map. Please try again later.");
                        }else{
                            

                            $("#divSaveMsgAlert").css("display","");
                            $("#divSaveMsgAlert").removeClass("alert-danger");
                            $("#divSaveMsgAlert").addClass("alert-success");
                            $("#ctl00_ctl00_Body_header_banner_gMap_lblSaveMsg").text("Success, The map has been saved successfully");
                        }
                    },
                    error: function (request, status, errorThrown) {
                        

                        $("#divSaveMsgAlert").css("display","");
                        $("#divSaveMsgAlert").addClass("alert-danger");
                        $("#ctl00_ctl00_Body_header_banner_gMap_lblSaveMsg").text("Error, Failed to save the map. Please try again later.");
                    }});
            }

            //// clear the user data
            //$(".savedMaps input").val();
        });    
        
        // hide the info window
        $(".infoWindow").css('top', '-9999px').hide();          
        
        // if in read only mode, load the specific javascript methods
        if ("True" == "True") {
            // updates the route and route steps
            GetDirections = function(source, destination) {
                var newFrom = source;
                var newTo = destination;
                var contentChanged;

                if (source != "") {
                    oldHTML = $('#directions_info').html();
                    newHTML = oldHTML;

                    var directionsContainer = $('#directions_info');
                    if ( directionsContainer.find('.contentwrapper').length > 0 ) {
                        var directionsWrapper = directionsContainer.find('.contentwrapper');
                    } else {
                        var directionsWrapper = directionsContainer;
                    }

                    // create a new instance of GDirections
                    if (directions == null) {
                        
                        fleXenv.fleXcrollMain(directionsContainer[0]);
                        
                        //directions = new GDirections(subgurim_gMap, directionsWrapper[0]);
                        directionsDisplay = new google.maps.DirectionsRenderer();
                        directionsDisplay.setMap(subgurim_gMap);
                        directionsDisplay.setPanel(directionsWrapper[0]);
                        directions =  new google.maps.DirectionsService();

                        // error handler
                        google.maps.event.addListener(directions, 'error', function () {
                            errorMessage($("#subgurim_gMap"),'Error', 'Unable to find a suitable route');
                        });

                        google.maps.event.addListener(directions, 'addoverlay', function() {
                            setTimeout(function() {
                                if (contentChanged) {
                                    directionTimer = setInterval( function() {
                                        if (oldHTML != newHTML) { 
                                            $("#directions_info")[0].fleXcroll.updateScrollBars();
                                            clearInterval(directionTimer); 
                                        } else { newHTML = $("#directions_info").html(); }
                                    }, 200);
                                }
                            }, 200);
                        });
                    }                                        

                    if (destination.toLowerCase().indexOf(", malta") == -1){
                        destination = destination.concat(", Malta");
                    }


                    // direction options
                    var directionsOptions = {
                        origin: source,
                        destination: destination,
                        travelMode: travelMode,
                    }                 

                    oldFrom = newFrom;
                    oldTo = newTo;
                    contentChanged = true;

                    directions.route(directionsOptions, function(response, status) {
                        if (status == google.maps.DirectionsStatus.OK) {
                            directionsDisplay.setDirections(response);
                        }else{
                            $(".directionsErrorMsg").css("display","");
                            $(".directionsErrorMsg").addClass("alert-danger");
                            $(".directionsErrorMsg").text(status)
                        }
                    });

                    // load the directions
                    //directions.load('from: ' + source + ' to: ' + destination, directionsOptions);
                }
                else {
                    errorMessage($("#subgurim_gMap"),'Error', 'Please enter from where you will be travelling');
                    // focus on the field
                    // $("#ctl00_ctl00_Body_header_banner_gMap_txtSource").focus();
                }
            }                       

            // updates the map with the checked layers
            updateLayers = function(input) { 
                // load the markers through AJAX
                if ($(input).is(":checked")) {          
                    // CHECK A LAYER
                    // ajax loader
                    loaderCheck = true;
                    setTimeout('if(loaderCheck) { toggleLoader("#subgurim_gMap"); }', 500);
                    
                    // show markers that match the checked LayerID
                    var layerID = $(input).parent().parent().children(":hidden").val();
                    // retrieve already existent markers
                    var markers = getMarkers($("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers"));
                    // keyword
                    var keyword = $(input).parents(".showonmap").find(".searchBox").val();
                    if (keyword.toLowerCase() == "search") {
                        keyword = ""
                    }
                    // first retrieve all the coordinates through AJAX
                    $.ajax({
                        contentType: "application/json; charset=utf-8",
                        url: "https://www.visitmalta.com/WebServices/gMapWebService.asmx/SearchCoordinates",
                        type: "POST",
                        data: JSON.stringify({
                            "iDefaultClientID": "1",
                            "iPortalClientID": "1",
                            "iLanguageID": "1",
                            "strLayerID": layerID,
                            "strKeywords": ""
                        }),
                        datatype: "json",
                        success: function (results) {
                            loaderCheck = false;
                            if ( $("#subgurim_gMap").parentsUntil('.gMap').find('.ajaxloader').is(':visible') ) {
                                toggleLoader("#subgurim_gMap");
                            }                            
                            var searchCriteria = $(".search").prev(".searchBox").val();
                        
                            if (searchCriteria.toLowerCase() != "search") {
                                toggleSearchIcon($(".search"), true);
                            }                            

                            var coordinateList = $.parseJSON(results.d);
                            
                            for (var j = 0; j < markers.length; j++) {                            
                                if (layerID == markers[j].LayerID) {
                                    markers[j].Selected = true;
                                    markers[j].Visible = true;
                                }
                            }

                            for (var i = 0; i < coordinateList.length; i++) {
                                var coordinate = {
                                    "ID": "Coordinate_" + coordinateList[i].ID,
                                    "Title": coordinateList[i].Title,
                                    "LayerID": coordinateList[i].LayerID,
                                    "ImageUrl": coordinateList[i].ImageUrl,
                                    "Summary": coordinateList[i].Summary,
                                    "Url": coordinateList[i].Url,
                                    "Points": coordinateList[i].Points
                                };

                                var bFound = false;
                                // go through the markers
                                for (var j = 0; j < markers.length; j++) {
                                    // if the coordinate ID matches 
                                    if (coordinate.ID == markers[j].ID) {
                                        // re-show the markers
                                        if (googleMarkers[coordinate.ID] != undefined){
                                            googleMarkers[coordinate.ID].setMap(subgurim_gMap);
                                            markers[j].Visible = true;
                                            bFound = true;
                                        }
                                        
                                    }                                    
                                }

                                // if the marker is new 
                                if (!bFound) {
                                    // insert it in the page variable
                                    insertMarker($("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers"), coordinate);

                                    // show on the map
                                    if (coordinate.Points.length > 1) {
                                        // polygon
                                        var polygonPoints = [];
                                        for (var k = 0; k < coordinate.Points.length; k++) {
                                            polygonPoints.push(new google.maps.LatLng(coordinate.Points[k].Latitude, coordinate.Points[k].Longitude));
                                        }
                                        polygonPoints.push(new google.maps.LatLng(coordinate.Points[0].Latitude, coordinate.Points[0].Longitude));
                                        var polygon = new google.maps.Polygon(polygonPoints, "#AA0000", 2, 1, "#FF0000", "0.5");
                                        polygon.id = coordinate.ID;
                                        polygon.gtype = 'polygon';
                                        polygon.setMap(subgurim_gMap);

                                        // infowindow on hover 
                                        //var x = 0;
                                        //var y = 0;

                                        //for (var k = 0; k < polygon.getVertexCount(); k++) {
                                        //    x = x + polygon.getVertex(k).lat();
                                        //    y = y + polygon.getVertex(k).lng();
                                        //}
                                        //x = x / polygon.getVertexCount();
                                        //y = y / polygon.getVertexCount();
                                         
                                        google.maps.event.addListener(polygon, 'mouseover', function () {
                                            markerHover(this.id, getMarkers($("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers")));  
                                        });
                                            google.maps.event.addListener(polygon, 'mouseout', function () {
                                                markerOut();
                                            });

                                        // click
                                            google.maps.event.addListener(polygon, 'click', function () {
                                                loaderCheck = true;
                                                setTimeout('if(loaderCheck) { toggleLoader("#subgurim_gMap"); }', 500);
                                            });
                                        }
                                        else {
                                        // marker
                                            var icon = {
                                                url: $(input).closest('div').siblings().children(".layerImage").attr("src"),
                                                size: new google.maps.Size(24, 24),
                                                origin: new google.maps.Point(0, 0),
                                                anchor: new google.maps.Point(12, 24)
                                            };

                                            var marker = new google.maps.Marker({ 
                                                position: new google.maps.LatLng(coordinate.Points[0].Latitude, coordinate.Points[0].Longitude), 
                                                icon: icon
                                            });
                                            marker.id = coordinate.ID;
                                            marker.gtype = 'marker';
                                            marker.setMap(subgurim_gMap); 
                                            markerArray.push(marker);                                        
                                            google.maps.event.addListener(marker, 'click', function() { 
                                                $.post('/MarkersHandler.ashx', { GMap_ID: 'subgurim_gMap', who: $(this).attr("id"), index: ($.inArray(this, markerArray)) }, function (response) {
                                                    eval(response);
                                                });
                                                // obsolete with Google Maps v3
                                                //registerServerEvent('MarkerClick', subgurim_gMap, marker); 
                                            });
                                        
                                        // dynamic variable that will contain the markers added to the map from the client-side
                                        googleMarkers[marker.id] = marker;
                                            
                                        // infowindow on hover 
                                        google.maps.event.addListener(marker, 'mouseover', function (LatLng) {
                                            markerHover(this.id, getMarkers($("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers")));  
                                        });
                                        google.maps.event.addListener(marker, 'mouseout', function () {
                                            markerOut();
                                        });

                                        // click
                                        google.maps.event.addListener(marker, 'click', function () {
                                            loaderCheck = true;
                                            setTimeout('if(loaderCheck) { toggleLoader("#subgurim_gMap"); }', 500);
                                        });
                                    }
                                }
                                else{
                                    // update the hidden variable
                                    $("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers").val(JSON.stringify(markers));
                                }
                            }
                            // success                            
                        },
                        error: function (request,status,errorThrown) {
                            errorMessage($("#subgurim_gMap"),'Error', 'Failed to load the selected layers');
                            // error
                            loaderCheck = false;
                            if ( $("#subgurim_gMap").parentsUntil('.gMap').find('.ajaxloader').is(':visible') ) {
                                toggleLoader("#subgurim_gMap");
                            }
                        }
                    });
                }
                else {
                    var markers = getMarkers($("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers"));

                    if ($(input).hasClass("search")) {
                        // SEARCH
                        var searchCriteria = $(input).prev(".searchBox").val();
                        
                        if (searchCriteria.toLowerCase() == "search") {
                            searchCriteria = ""
                        }

                        if (searchCriteria == "") {
                            // remove the search criteria and show all the markers
                            // go through the markers
                            for (var i = 0; i < markers.length; i++) {
                                // if the LayerID matches
                                var coordinateID = markers[i].ID;
                                if (markers[i].Selected != false){
                                    // show the markers
                                    if(googleMarkers[coordinate.ID] != undefined){
                                        googleMarkers[coordinate.ID].setMap(subgurim_gMap);
                                        markers[i].Visible = true;
                                    }
                                }
                            }      

                            // store the updated marker list
                            $("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers").val(JSON.stringify(markers));                      
                        }
                        else{
                            // ajax loader
                            loaderCheck = true;
                            setTimeout('if(loaderCheck) { toggleLoader("#subgurim_gMap"); }', 500);
                            // go through the markers
                            for (var i = 0; i < markers.length; i++) {
                                // if the LayerID matches
                                var coordinateID = markers[i].ID;
                                // hide the markers
                                if(googleMarkers[coordinateID] != undefined){
                                    googleMarkers[coordinateID].setMap(null);
                                    markers[i].Visible = false;
                                }
                            }

                            // insert the new markers
                            $.ajax({
                                contentType: "application/json; charset=utf-8",
                                url: "https://www.visitmalta.com/WebServices/gMapWebService.asmx/SearchCoordinates",
                                type: "POST",
                                data: JSON.stringify({
                                    "iDefaultClientID": "1",
                                    "iPortalClientID": "1",
                                    "iLanguageID": "1",
                                    "strLayerID": "-1",
                                    "strKeywords": searchCriteria
                                }),
                                datatype: "json",
                                success: function (results) {
                                    loaderCheck = false;
                                    if ( $("#subgurim_gMap").parentsUntil('.gMap').find('.ajaxloader').is(':visible') ) {
                                        toggleLoader("#subgurim_gMap");
                                    }
                                    var coordinateList = $.parseJSON(results.d);
                                    for (var i = 0; i < coordinateList.length; i++) {
                                        var coordinate = {
                                            "ID": "Coordinate_" + coordinateList[i].ID,
                                            "Title": coordinateList[i].Title,
                                            "LayerID": coordinateList[i].LayerID,
                                            "ImageUrl": coordinateList[i].ImageUrl,
                                            "Summary": coordinateList[i].Summary,
                                            "Url": coordinateList[i].Url,
                                            "Points": coordinateList[i].Points
                                        };
                                        
                                        var bFound = false;
                                        // go through the markers
                                        for (var j = 0; j < markers.length; j++) {
                                            // if the coordinate ID matches
                                            if (coordinate.ID == markers[j].ID) {
                                                // show the markers
                                                if (markers[j].Selected != false){                                                    
                                                    //getGMapElementById('subgurim_gMap', coordinate.ID).show();
                                                    $.grep(markerArray, function(e){ 
                                                        return e.id === coordinate.ID; })[0].setMap(subgurim_gMap);
                                                    markers[j].Visible = true;                                                    
                                                    }
                                                    bFound = true;
                                                }
                                            }
                                        // if the marker is new 
                                            if (!bFound) {
                                                // don't show it                                            
                                            }
                                        }

                                    // store the updated marker list
                                    $("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers").val(JSON.stringify(markers));
                                },
                                error: function (request,status,errorThrown) {
                                    errorMessage($("#subgurim_gMap"),'Error', 'Failed to search for the selected keywords');
                                    // error
                                    loaderCheck = false;
                                    if ( $("#subgurim_gMap").parentsUntil('.gMap').find('.ajaxloader').is(':visible') ) {
                                        toggleLoader("#subgurim_gMap");
                                    }
                                }
                            });                            
                        }                        
                    }
                    else
                    {
                        // UNCHECK A LAYER
                        var layerID = ($(input).parent().parent().children(":hidden").val());
                        // hide the markers
                        var markers = getMarkers($("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers"));
                        // go through the markers
                        for (var i = 0; i < markers.length; i++) {
                            if (markers[i].LayerID == layerID) {
                                // if the LayerID matches
                                var coordinateID = markers[i].ID;
                                // hide the markers
                                if(googleMarkers[coordinateID] != undefined){
                                    googleMarkers[coordinateID].setMap(null);
                                    // mark the marker as hidden
                                    markers[i].Selected = false;
                                    markers[i].Visible = false;
                                }
                            }
                        }

                        // store the updated marker list
                        $("#ctl00_ctl00_Body_header_banner_gMap_hdnMarkers").val(JSON.stringify(markers));
                    }
                }
            }
        }         
    });

</script>
