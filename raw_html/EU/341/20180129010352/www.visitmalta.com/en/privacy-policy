<script type="text/javascript">
    function EnableMap() {
        // used to increment the mapview counter
        $.ajax({
            contentType: "application/json; charset=utf-8",
            url: "/WebServices/gMapWebService.asmx/IncrementMapViewCounter",
            data: JSON.stringify({
                "strIpAddress": "54.165.59.7",
                "iLanguageID": "1",
                "strUrl": "http://www.visitmalta.com/en/privacy-policy"
            }),
            type: "POST",
            success: function () {
                // do nothing by design
            },
            error: function (request, status, errorThrown) {
                // do nothing by design
            }
        });

        $("#slider").animate({
            opacity: 0
        }, 500, function () {
            $(this).addClass("hidden");
            $("#topcontainer .gMapContainer").css("opacity", 0).removeClass("startingPosition").animate({
                opacity: 1
            }, 500, function () {
                $('html, body').animate({
                    scrollTop: $(".navbar").offset().top
                }, 10);
            });
        });

        $(".logoImage").removeClass("lrg-logo")
        $(".logoImage").addClass("sml-logo")

        return false;

        var pnlMapId = $('#ctl00_ctl00_Body_header_banner_pnlMap');
        var pnlMapIdWidth = pnlMapId.width();
    };    
    
      $(document).ready()
    {
        var pnlMapId = $('#ctl00_ctl00_Body_header_banner_pnlMap');
        var pnlMapIdWidth = pnlMapId.width();                

        $("#topcontainer .gMapContainer").css("opacity", 0).removeClass("startingPosition").animate({
            opacity: 0
        }, 500, function () {
            $('html, body').animate({
                scrollTop: $(".navbar").offset().top
            }, 10);
        });
        

        $(".logoImage").removeClass("lrg-logo");
        $(".logoImage").addClass("sml-logo");
    }
</script>

