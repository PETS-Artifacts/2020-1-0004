    $(function () {

        var template = $('#video-item-template').html();

        $('.videos-listing').each(function () {
            if ( $(this).data('skip')) {
                return true;
            }

            var self = $(this);
            var rand = Math.floor(Math.random() * 5 );
            var item = $('.item', $(this)).eq(rand);
            var buttons = [];
            var ads = $('.a-container').size() ? 1 : 0;
            $.getJSON('https://porndoepremium.com/video/fake.json?v=' + (Math.random()), function (data) {
                if ( $.isEmptyObject(data)) {
                    return false;
                }

                // Store the current buttons
                $('.lot-upgrade-buttons-untoggled', self).each(function () {
                    buttons.push({
                        clone: $(this).clone(),
                        index: $(this).index()
                    });
                    $(this).remove();
                });

                var out = template;
                for ( i in data ) {
                    out = out.split('{{'+i+'}}').join(data[i]);
                }

                if ( $('#sitehome-index').size() || $('#sitevideo-video').size()) {
                    $('.item:last-child', self ).remove();
                }

                $(out).insertBefore(item);

                // Insert the buttons in their initial position
                for ( i in buttons ) {
                    self.children('.item:visible').eq(buttons[i].index - 1 - ads ).after(buttons[i].clone);
                }

            });
        });
    });
    
