            app.directive('ngRestrict', function() {
                return {
                    link: function ( $scope, $item ) {
                        $scope.restrict = {
                            show: false
                        };


                        var dispatcher = function (event) {
                            if ( $scope.service.hasOwnProperty('restrictions')) {
                                var $fields = $scope.service.restrictions;
                                var $object = Object.keys($fields);
                                if ( $object.length ) {
                                    event.preventDefault();
                                    $scope.restrict = { show: true };
                                    Object.assign ( $scope.restrict, $fields );
                                    $scope.$apply();
                                    window.scroll({top: 0, left: 0, behavior: 'smooth' });
                                }
                            }
                        };

                        $item.on('click touchstart', function (event) {
                            dispatcher(event);
                        });

                        document.addEventListener('ngRestrict', function (event) {
                            dispatcher(event);
                        }, false);

                    }
                };
            });

        })(window.angular);
