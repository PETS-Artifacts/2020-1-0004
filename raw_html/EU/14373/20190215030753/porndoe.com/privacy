                }

                if ( $('#sitehome-index').size() || $('#sitevideo-video').size() || $('#siteerror-error').size()) {
                    var itemSize = $('.item', self ).size();
                    // Remove the last element form list
                    $('.item', self ).eq(itemSize - 1).remove();
                }

                $(out).insertBefore(item);

                // Insert the buttons in their initial position
                for ( i in buttons ) {
                    self.children('.item:visible').eq(buttons[i].index - 1 - ads ).after(buttons[i].clone);
                }

            });
        });
    });
    
            $('#doe-projects-dialog').modal('show');
            $('#expired-user-dialog').modal('show');



    $(function () {

//        $('#autocomplete-scroll').bind("DOMSubtreeModified",function(){
//            setTimeout(function () {
//                $('.autocomplete-suggestion a').on('click', function (event) {
//                    var self = $(this);
//                    event.preventDefault();
//                    $.get('/siteHome/keywords?keywords=' +  $(this).data('value'), function () {
//                        window.location = self.attr('href');
//                    });
//                    return false;
//                });
//            }, 50 );
