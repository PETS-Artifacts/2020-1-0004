        var cookieDisclaimer  = '__disclaimer';

        if (document.cookie.indexOf(cookieDisclaimer) >= 0) {
            checkCookie_eu();

            function checkCookie_eu()
            {

                var consent = getCookie_eu("cookie-disclaimer");

                if (consent == null || consent == "" || consent == undefined)
                {
                    // show notification bar
                    $('#cookie_directive_container').show();
                    $('#ovlay_cookie').show();
                }

            }

            function setCookie_eu(c_name,value,exdays)
            {

                var exdate = new Date();
                exdate.setDate(exdate.getDate() + exdays);
                var c_value = escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString());
                document.cookie = c_name + "=" + c_value+"; domain=.porndoe.com;path=/";

                $('#cookie_directive_container').hide();
                $('#ovlay_cookie').hide();
            }


            function getCookie_eu(c_name)
            {
                var i,x,y,ARRcookies=document.cookie.split(";");
                for (i=0;i<ARRcookies.length;i++)
                {
                    x=ARRcookies[i].substr(0,ARRcookies[i].indexOf("="));
                    y=ARRcookies[i].substr(ARRcookies[i].indexOf("=")+1);
                    x=x.replace(/^\s+|\s+$/g,"");
                    if (x==c_name)
                    {
                        return unescape(y);
                    }
                }
            }

            $("a.okay, a.stay").click(function(){
                setCookie_eu("cookie-disclaimer", 1, 365);
            });

            $("a.nothanks").click(function(){
                $('#cookie_directive_container').hide();
                $('#ovlay_cookie').hide();

                setTimeout( function() {
                    $('#disclaimer-cookie').modal();
                }, 100);
            });
        }
        else {
            console.log('0');
        }

        var delayClose = 0;
        $('.dd-right').on('mouseenter', function() {
            clearTimeout(delayClose);
            $(this).addClass('open');
        }).on('mouseleave', function() {
            var dd = $(this);
            delayClose = setTimeout(function() {
                dd.removeClass('open');
            }, 300);
        });

        // Script for pop bar variations
        // var popV = $('#pop-bar_v');
        // var popVm = $('#pop-bar_v-message');
        //
        // popV.on('mouseenter', function() {
        //     $('.first-part').hide();
        //     $('.second-part').show();
        // });
        // end script for pop bar variations
    });
    
});
