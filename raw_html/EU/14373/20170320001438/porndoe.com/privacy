    function ajaxButtonWithCsrf(id, success) {
        var button = $(id);
        var data = {};
        var url = button.data('url');
        jQuery('body').on('click', id, function () {
            jQuery.ajax({
                'type': 'POST',
                'dataType': 'JSON',
                'url': url,
                'data': data,
                'success': success,
                'cache': false
            });
            return false;
        });
    }
