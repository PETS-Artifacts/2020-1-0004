function affiliate_text_toggler(){
  if($( '.mobile_logo_holder').is(":visible")){
      $( '.mobile_logo_holder').hide();
      $( '.mobile_menu_affiliate_holder').show();
  } else {
    $( '.mobile_logo_holder').show();
    $( '.mobile_menu_affiliate_holder').hide();
  }
}

function menu_toggler(){
    var windowWidth = window.innerWidth;

    if(windowWidth < 1024){ // mobil
        $('.menu_holder').hide();
        $('.mobile_menu_holder').show();
    } else {
      $('.mobile_menu_holder').hide();
      $('.menu_holder').show();
    }

    if(windowWidth < 1024){ // mobil
        $('.menu_holder').hide();
        $('.mobile_menu_holder').show();
    } else {
      $('.mobile_menu_holder').hide();
      $('.menu_holder').show();
    }

}
</script>
