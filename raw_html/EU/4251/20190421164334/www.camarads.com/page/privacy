    $('.sidebar_app_'+aptId+'_room_nr_'+camId).addClass('sidebar_room_active');
}

function activate_app(activeAppId, defaultStream){
    if (defaultStream !== undefined && defaultStream !== null) {
        change_video(defaultStream, 0);
    }

    $('.sidebar_rooms_holder').hide();
    $('.app_rooms_'+activeAppId).show();
    $('.sidebar_app_name_holder').removeClass('app_active');

            if(activeAppId == 32) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 29) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 35) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 38) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 37) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 3) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 28) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 5) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 34) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 33) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 27) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 36) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 31) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 14) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
            if(activeAppId == 9) {
            $('.appartment_link_'+activeAppId).addClass('app_active');
        }
    
    if (page == "replay") {
        $(".chart-buttons .mls").html(GLOBALSAPPS[activeAppId]['name']);
    }

    if (typeof(replayDefaultRooms) !== "undefined" ) {
        if (replayDefaultRooms[activeAppId] !== undefined) {
            var jsCam = activeAppId + '_' + replayDefaultRooms[activeAppId]['cam'];

            if (replayDefaultRooms[activeAppId]['moment'] != '') {
                change_video(jsCam, 'something', replayDefaultRooms[activeAppId]['moment'], 0);
            } else {
                change_video(jsCam, 'something');
            }
        }
    }

}

function activate_app_room(appNr, roomNr){
  $('.sidebar_room_name').removeClass('sidebar_room_active');
  $('.sidebar_app_'+appNr+'_room_nr_'+roomNr).addClass('sidebar_room_active');
}

function visitlink(link){
    if (link == 'replay') {
        if (typeof(playingAptAndCamera) !== 'undefined' && playingAptAndCamera != '') {
            $.get(GLOBALSURL+'api_time/get_moment_for_replay_start?apt='+playingAptAndCamera, function( data ) {
                var response = '';
                if (response = $.parseJSON(data)) {
                    if (response.time === null) {
                        window.location.href = "https://www.camarads.com/"+link;
                    } else {
                        window.location.href = "https://www.camarads.com/"+link+"/room/"+playingAptAndCamera+"?time="+response.time+"&deductSeconds=20";
                    }
                } else {
                    window.location.href = "https://www.camarads.com/"+link;
                }
            });
        } else {
            window.location.href = "https://www.camarads.com/"+link;
        }
    } else {
        window.location.href = "https://www.camarads.com/"+link;
    }
}

