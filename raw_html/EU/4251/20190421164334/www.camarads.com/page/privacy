function footer_redirect(name){
  if(name == 'facebook') window.open('https://www.facebook.com/CamaradsOfficial', '_blank');
  if(name == 'twitter') window.open('https://twitter.com/Camarads_com', '_blank');
  if(name == 'youtube') window.open('https://www.youtube.com/channel/UCK3nxqkDouiBg0FHSA74Fkg', '_blank');
}

function markAptAsActive(aptId, camId) {
    $('.sidebar_rooms_holder').hide();
    $('.app_rooms_'+aptId).show();
    $('.sidebar_app_name_holder').removeClass('app_active');

        if(aptId == 32) {
        $('.appartment_link_'+aptId).addClass('app_active');
    }
        if(aptId == 29) {
        $('.appartment_link_'+aptId).addClass('app_active');
    }
        if(aptId == 35) {
        $('.appartment_link_'+aptId).addClass('app_active');
    }
        if(aptId == 38) {
        $('.appartment_link_'+aptId).addClass('app_active');
    }
        if(aptId == 37) {
        $('.appartment_link_'+aptId).addClass('app_active');
    }
        if(aptId == 3) {
        $('.appartment_link_'+aptId).addClass('app_active');
    }
        if(aptId == 28) {
        $('.appartment_link_'+aptId).addClass('app_active');
    }
        if(aptId == 5) {
        $('.appartment_link_'+aptId).addClass('app_active');
    }
        if(aptId == 34) {
        $('.appartment_link_'+aptId).addClass('app_active');
    }
        if(aptId == 33) {
        $('.appartment_link_'+aptId).addClass('app_active');
