					        <script>
					        	function search (form, keyCode) {
					        		if (keyCode == '13' ) {
					        			var word = encodeURIComponent(jQuery('input[name=searchword]', form).val());
						        		window.location.href = '/search?searchword=' + word;
					        		}
					        	}
					        </script>
