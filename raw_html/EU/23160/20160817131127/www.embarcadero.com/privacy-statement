	<script>
		function update_language(code) {
			var d = new Date();
			d.setTime(d.getTime() + (365*24*60*60*1000));
			document.cookie = 'language='+ code +'; expires='+ d.toUTCString() +'; path=/; domain=.embarcadero.com';
		}
	</script>
