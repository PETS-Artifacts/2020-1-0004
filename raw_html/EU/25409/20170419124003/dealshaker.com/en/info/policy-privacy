                            <script>
        $(document).ready(function () {
            $('[data-preferred-country="preferred-country"]').select2({
                'width': '100%',
                dropdownAutoWidth: true
            }).change(function () {
                $.ajax({
                    method: "POST",
                    url: $('option:selected', this).data('uri'),
                    processData: false,
                    contentType: false,
                    cache: false
                })
                    .done(function (response, status, xhr) {
                        window.location.href = response.redirect;
                    })
                    .fail(function (response, status, xhr) {
                        if (response.status === 401) {
                            location.reload();
                        }
                    });
            });

            $(".preferred-countries").css({
                "display": "block"
            });
        });
    </script>
