    <script>
        $(function(){
            if($('#geo-location-tooltip').size()){
                $('#geo-location-tooltip').tooltip({ trigger: 'click' });
                            }

            $('.switch-btn :checkbox').change(function(evt){
                $(this).unbind('change').click(function(){
                    return false;
                })
                $.ajax({
                    method: "POST",
                    url: '/xhr/geo-locator/toggle',
                    cache: false
                })
                    .done(function (response, status, xhr) {
                        if(response.redirect){
                            document.location = response.redirect;
                        }
                    })
                    .fail(function (response, status, xhr) {
                        if (response.status === 401) {
                            location.reload();
                        }
                    });
            });

            $('#js-geo-tooltip-wrap').on('click', '#geo-locator-country-link', function(){
                $(this).unbind('click').click(function(){
                    return false;
                });
                $('#geo-location-tooltip').tooltip('hide');
                $.get($(this).attr('href'), function(response){
                   location.href = response.redirect;
                });
                return false;
            });
        });
    </script>
