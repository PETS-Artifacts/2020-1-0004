                    <td>
                        <table class='child bought'>
                            <tr>
                                <th>Bought</th>
                                <td class='<%= buy.total_value > 0 ? "pos" : (buy.total_value < 0) ?  "neg" : "" %>'><%- addCommas(buy.total_value, '$') %></td>
                            <tr>
                            <tr>
                                <th>Volume</th>
