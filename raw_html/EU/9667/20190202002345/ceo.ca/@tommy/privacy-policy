            <script type="text/html" id="mod_tools_ban_tmpl" class="template">
                <div class='big-buttons'>
    <%= handle %><% if (role == "admin") { %>/<%= email %><% } %><br><br>

    <% if (!giving_penalty && !making_silent && !banning) { %>
