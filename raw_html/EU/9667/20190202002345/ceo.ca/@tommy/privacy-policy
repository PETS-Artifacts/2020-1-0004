            <script type="text/html" id="short_positions_tmpl" class="template">
                <%
	function addCommas(nStr, appendStr) {
		if (!nStr) return 0;
	    nStr += '';
	    x = nStr.split('.');
	    x1 = x[0];
	    x2 = x.length > 1 ? '.' + x[1] : '';
	    var rgx = /(\d+)(\d{3})/;
	    while (rgx.test(x1)) {
	        x1 = x1.replace(rgx, '$1' + ',' + '$2');
	    }
	    return (appendStr || '') + x1 + x2;
	}
%>

