        <div class="close">&laquo; back</div>
    <% } else { %>
        <div class="see-all"><span class="icon icon-th-list"></span> browse all</div>
    <% } %>
</div>


            </script>
        

        <script type="text/javascript">

            window.templates = {}
            $('.template').each(function(e) {
                var id = $(this).attr('id');
                var content = $(this).get(0).innerHTML;
                templates[id] = microtemplates(content);
            });
            Template = function(id, data) {
                data.common = common;
                return templates[id](data);
            }

        </script>

        

