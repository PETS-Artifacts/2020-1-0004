            <script type="text/html" id="sidebar_channels_tmpl" class="template">
                

<% var channel_types = ['you', 'featured', 'private', 'following', 'recent', 'suggested']; %>

<%
    var sanitize_channels = function(channels) {
        channels.recent = channels.recent.filter(function(item){
            return !common.is_private_channel(item.channel);
        }).filter(function(item) {
            return (item.channel != "featured" && item.channel != "popular");
        });

        return channels;
    }

    channels = sanitize_channels(channels);
%>

<% for(var j=0; j<channel_types.length; j++) { %>
    <%
        var channel_type = channel_types[j];
        var _channels = channels[channel_type];

        // fix sortings
        if (channel_type == "featured") {
            var order = ['index', 'featured', 'popular', 'articles', 'newsroom', 'insiders', 'feedback'];
            _channels.sort(function(a, b) {
                if (order.indexOf(a.channel) < order.indexOf(b.channel)) return -1;
                if (order.indexOf(a.channel) > order.indexOf(b.channel)) return 1;
                return 0;
            });
        } else if(["private", "following", "recent"].indexOf(channel_type) != -1) {
            _channels.sort(function(a, b) {
                if (a.channel == current_channel) return -1;
                if (b.channel == current_channel) return 1;
                if (a.new_message_count > b.new_message_count) return -1;
                if (a.new_message_count < b.new_message_count) return 1;
                if (a.last_seen > b.last_seen) return -1;
                if (a.last_seen < b.last_seen) return 1;
                return 0;
            });
        }
    %>
    <div class="section channels <%= channel_type %>" data-type="<%= channel_type %>">
        <h3><%= channel_type %></h3>
        <div data-type="<%= channel_type %>" id="<%= channel_type %>-channels">
            <% if(_channels.length == 0) { %>
                <div class="none">none</div>
            <% } %>
            <% var limit = 4; %>
            <% var collapsable = ( _channels.length - _channels.slice(0,limit).length > 1 ) && (channel_type != 'recent') %>

            <% for(var i=0; i < (collapsed[channel_type] && collapsable ? _channels.slice(0,limit).length : _channels.length); i++) { %>
                <%
                    var item = _channels[i];
                    
                    var label='', classes='', icon='';
                    var icons = {
                        "index.html": '<span class="icon icon-home"></span>',
                        "featured": '<span class="icon icon-star-full"></span>',
                        "popular": '<span class="icon icon-thumbsup"></span>',
                        "newsroom": '<span class="icon icon-file-text"></span>',
                        "articles": '<span class="icon icon-pencil"></span>',
                        "insiders": '<span class="icon icon-file-text"></span>',
                        "feedback": '<span class="icon icon-bubble"></span>',
                    }

                    if(icons[item.channel]) {
                        label = item.channel;
                        classes = item.channel;
                        icon = icons[item.channel];
                    } else if(item.channel[0] == "@") {
                        label = item.channel;
                    } else {
                        label = "#" + item.channel;
                    }

                    if(channel_type == "private") {
                        icon = '<span class="icon-lock"></span>';
                        label = item.channel;
                    }

                    if(item.channel.toLowerCase() === current_channel.toLowerCase()) {
                        classes += " highlight";
                    }
                    if(channel_type == "private" && !item.last_seen) {
                        classes += " alert";
                    }
                %>
                 <div class="channel <%= classes %>" channel="<%= item.channel %>">
                    <div class="new-count">
                        <% if(item.new_message_count > 0) { %>
                            <% if(item.new_message_count >= 50) { %>
                                <%= Math.min(item.new_message_count, 50) + '+' %>
                            <% } else { %>
                                <%= item.new_message_count %>
                            <% } %>
                        <% } %>
                    </div><a href="/<%= item.channel %>"><%- icon %><%= label %></a><% if(["private", "recent", "following"].indexOf(channel_type) != -1) { %><div class="delete">&times;</div><% } %>
                </div>

            <% } %>

            <% if (_channels.length > limit && collapsed[channel_type] !== undefined && collapsable ) { %>
                <% if (!collapsed[channel_type]) { %>
                    <span class="sidebar-collapse sidebar-channel-display-control">▴ less</span>
                <% } %>
                <% if (collapsed[channel_type]) { %>
                    <span class="sidebar-reveal sidebar-channel-display-control">▾ <%= _channels.length - limit %> more</span>
                <% } %>
            <% } %>

        </div>
    </div>
    <% if(channel_type == "featured") { %>
        <% if(logged_in) { %>
            <div class='section actions'>
                <h3>write</h3>
                <div class="inner">
                    <button class="log-button open-my-articles" id="login">My Articles</button><br />
                </div>
            </div>
        <% } %>
    <% } %>

<% } %>


            </script>
        
