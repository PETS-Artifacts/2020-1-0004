                       <% if(push_enabled) { %> <td>push</td> <% } %>
                    </tr>   
                    <% for(i=0; i < sub_info.length; i++){ %>

                        <tr>
                            <td><span class="channel-name">#<%= sub_info[i].channel %></span></td>
                            <td>
