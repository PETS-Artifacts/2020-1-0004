<script type="text/javascript">
    try {
        var _gaq = _gaq || [];
        _gaq.push(['_setAccount', 'UA-7958280-1']);
        _gaq.push(['_trackPageview']);

        (function() {
            var ga = document.createElement('script');
            ga.type = 'text/javascript';
            ga.async = true;
            ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(ga, s);
        })();

        window.addEventListener("load", function() { //add new onload handler, so that the previous ones will not be overridden
            // Delay to allow the async Google Ads to load
            setTimeout(function () {
                var ad = document.querySelector('div[id*="ad_container"], div[id*="google_ads_iframe_"]'); // Get the first AdSense ad unit on the page
                if (!ad || ad.innerHTML.replace(/\s/g, "").length == 0) { // If the ads are not loaded, track the event
                    if (typeof ga !== 'undefined') {
                        // Log an event in Universal Analytics
                        // but without affecting overall bounce rate
                        ga('send', 'event', 'Adblock', 'Yes', {'nonInteraction': 1});
                    } else if (typeof _gaq !== 'undefined') {
                        // Log a non-interactive event in old Google Analytics
                        _gaq.push(['_trackEvent', 'Adblock', 'Yes', undefined, undefined, true]);
                    }
                }
            }, 2000); // Run ad block detection 2 seconds after page load
        });
    } catch(err) {
    }





</script>


<script type="text/javascript">

    (function(d, s, id) {
        var js, fjs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) {
            return;
        }
        js = d.createElement(s);
        js.id = id;
        js.src = "//connect.facebook.net/en_US/all.js#xfbml=1";
        fjs.parentNode.insertBefore(js, fjs);
    }(document, 'script', 'facebook-jssdk'));

</script>

<script>!function(d, s, id) {
    var js,fjs = d.getElementsByTagName(s)[0];
    if (!d.getElementById(id)) {
        js = d.createElement(s);
        js.id = id;
        js.src = "//platform.twitter.com/widgets.js";
        fjs.parentNode.insertBefore(js, fjs);
    }
}(document, "script", "twitter-wjs");</script>



<script type="text/javascript">



//download app popup, only on mobile browser
(function () {
    if(!hasText(getCookie("useApp"))  && hasText(1209600000) && (MobileEsp.DetectAndroid() || MobileEsp.DetectIos())){//for android and ios
        $('#useAppDialog').on('hidden.bs.modal', function () {
            setCookie("useApp", 1, 1209600000, false);
        });
            var appEl;
            if(MobileEsp.DetectAndroid()){
                appEl = $J('<a target="_blank" class="relative" href="https://play.google.com/store/apps/details?id=com.myfxbook.forex" style="margin-left:68px"><img src="http://static2.mfbcdn.net/images/transparent.png" class="sprite sprite-common sprite-myfxbook-android-app" style="border:1px solid #686767;margin-left:70px"/></a>');
            } else if(MobileEsp.DetectIos()){
                appEl = $J('<a target="_blank" class="relative" href="https://itunes.apple.com/il/app/forex-calendar-market-news/id985947299?mt=8"><img src="http://static2.mfbcdn.net/images/transparent.png" class="sprite sprite-common sprite-myfxbook_appstore_button" style="border:1px solid #686767;margin-left:70px"/></a>');
            }
            var appName;
            if(MobileEsp.DetectAndroid()){
                appName = 'Android';
            } else if(MobileEsp.DetectIos()){
                appName = 'iOS';
            }

        $('#useAppDialogDialog').width($( window ).width() - 20); /* todo - bootstrap - delete after change to bootstrap, bootstrap modal cant determine the width because page isnt wrap in bootstrap */

        $J('#useAppDialogBody')
            .append('<span>Have you tried out our free ' + appName + ' app? Click below to download it or continue browsing.</span></br></br>')
            .append(appEl)
            .append('</br>')

        $JD.open('#useAppDialog');
    }
}());

</script>

