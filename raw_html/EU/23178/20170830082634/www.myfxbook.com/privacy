<script type="text/javascript">
    var count = 0;

    function removeBugReportFile(id, type) {
        $PT(type + "reportFilesLink" + id).remove();
        $PT(type + "reportFileRemove" + id).remove();
        $PT(type + "reportFilesBr" + id).remove();
        if (count == 3)
            showElement('cabinetLabelBugReport');

        count--;
    }
    function startCallbackBR() {
        hideElement("uploadResponseBugReport");
        $PT('fileLoaderBugReport').style.display = "";
        return true;
    }

    function completeCallbackBR(response) {
        if (response.error == "false") {
            hideElement('fileLoaderBugReport');
            var id = response.id;
            var tempId = Math.floor(Math.random() * 99999);
            var link = "<a class='floatLeft marginL5 marginR10' target='_blank' href='" + response.path + "'  title='" + response.file + "' type='image'  name='reportFiles' id='" + response.type + "reportFilesLink" + tempId + "'>" + response.file + "</a> &nbsp;<a class='floatLeft' href='javascript:void(0)' name='bugReportFileRemove' class='font10 underline' id='" + response.type + "reportFileRemove" + tempId + "' onclick='removeBugReportFile(" + tempId + ",\"" + response.type + "\")'>Remove</a>&nbsp;<br id='" + response.type + "reportFilesBr" + tempId + "'/>&nbsp;";

            count++;

            new Insertion.Before($PT("uploadResponseBugReport"), link);
     //       Modalbox.resizeToContent();
        //    Modalbox.resizeToInclude(response.type + "reportFilesLink" + tempId, {});
            if (count > 2) {
                hideElement("cabinetLabelBugReport");
            }

        } else {
            hideElement('fileLoaderBugReport');
            inlineMsg('cabinetLabelBugReport', response.message, 6);
        }

    }

</script>


