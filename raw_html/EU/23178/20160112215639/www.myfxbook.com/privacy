//download app popup, only on mobile browser
(function () {
    if(!hasText(getCookie("useApp"))  && hasText(1209600000) && MobileEsp.DetectAndroid()){//for android only
        var options = {
            autoOpen: false,
            resizable: false,
            modal: true,
            autoReposition: true,    //This is the new autoReposition setting
            create: function( event, ui ) {
                var dialog = $J(this).closest(".ui-dialog");
                dialog.find(".ui-dialog-titlebar-close")
                        .appendTo(dialog)
                        .css({
                            position: "absolute",
                            top: 0,
                            right: 0,
                            margin: "3px"
                        });
                dialog.find(".ui-dialog-titlebar").remove();
                dialog.find(".ui-dialog-buttonset").attr("align", "center").removeClass("ui-dialog-buttonset");
            },
            open: function(){
                $J('.ui-dialog').css('z-index',1012);
                $J('.ui-widget-overlay').css('z-index',1011);
            },
            close: function (){
                $JD.close('#useAppDialog');
                setCookie("useApp", 1, 1209600000, false);
            },
            buttons:[
                {
                    text:"Continue Browsing",
                    click:function () {
                        $JD.close('#useAppDialog');
                    }
                }
                ,
                {
                    text: "Download App",
                    click: function() {
                        var go_to_url;
                        if(MobileEsp.DetectAndroid()){
                            go_to_url = 'https://play.google.com/store/apps/details?id=com.myfxbook.forex';
                        } else if(MobileEsp.DetectIos()){
                            go_to_url = 'https://itunes.apple.com/il/app/forex-calendar-market-news/id985947299?mt=8';
                        }
                        window.open(go_to_url, '_blank');
                        $JD.close('#useAppDialog');
                    }
                }
            ]
        };
            $JD.create('#useAppDialog', options);

        var appEl;
        if(MobileEsp.DetectAndroid()){
            appEl = $J('<a target="_blank" class="relative" href="https://play.google.com/store/apps/details?id=com.myfxbook.forex" style="margin-left:68px"><img src="http://static.mfbcdn.net/images/transparent.png" class="sprite sprite-common sprite-myfxbook-android-app" style="border:1px solid #686767"/></a>');
        } else if(MobileEsp.DetectIos()){
            appEl = $J('<a target="_blank" class="relative marginL20" href="https://itunes.apple.com/il/app/forex-calendar-market-news/id985947299?mt=8" style="margin-left:68px"><img src="http://static.mfbcdn.net/images/transparent.png" class="sprite sprite-common sprite-myfxbook-appstore-button" style="border:1px solid #686767"/></a>');
        }
            $J('#useAppDialog')
                .append('<span>Have you tried out our free Android app? Click below to download it or continue browsing.</span></br></br>')
                .append(appEl)
                .append('</br>')

            $JD.open('#useAppDialog');
    }
}());

//resize all open jquery dialogs
$J(window).resize(function() {
    $J(".ui-dialog-content:visible").each(function () {
        $J( this ).dialog("option","position",$J(this).dialog("option","position"));
    });
});

//scroll all open jquery dialogs
$J(document).ready(function () {
    $J(document).scroll(function(e) {
        if ($J(".ui-dialog").length) //the dialog has popped up in modal view
        {
            //get the current popup position of the dialog box
            var pos = $J(".ui-dialog").position();
            if(pos != undefined){
                var popupHeight = $J(".ui-dialog").height();
                var y = (window.innerHeight-popupHeight)/2;  //calculate y coordinate of the popup to put it at center of the window
                if(Math.abs(pos.top-y)>1 && pos.top+popupHeight<window.innerHeight){//if the popup is not positioned in the center, but inside the window's boundaries, assume it got dragged and don't center it
                   y=pos.top;
                }
                $J(".ui-dialog").css({position: 'fixed',top: y});
            }
        }
    });
});
</script>
</body>
</html>










