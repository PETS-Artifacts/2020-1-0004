  titles = $("#articles").find("h1, h2, h3, h4, h5"),
  totalTitles = titles.length;

 var criticalHit = false;
 $("[data-conversion-point='true'").waypoint({
   offset: "50%",
   handler: function() {
     if (!criticalHit) {
       criticalHit = true;
       var hitTime = Math.floor(Date.now() / 1000),
        timeOnPage = hitTime - startTime;
       var data = {
         "article" : {
           "id" : 10002865,
           "title" : "Privacy Policy",
           "slug" : "privacy-policy",
           "published" : "2015-04-15 10:19:51 -0600",
           "category" : window.location.pathname.replace("privacy-policy", ").replace(/[\/\\]/gi, ")
         },
         "author" : {
           "id" : 0,
           "name" : ""
         },
         "progress": {
           "tag" : "critical-hit",
           "time" : timeOnPage
         }
       }
        window.trackEvent("Article progress", "dc27ea40-806e-472f-a99d-1270c3ed8c43", data);
     }
   }
 })
</script>
