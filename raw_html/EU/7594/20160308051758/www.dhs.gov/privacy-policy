      <div id='boxes-box-field_box_widget' class='boxes-box'><div class="boxes-box-content"><div class="field field-name-field-widget field-type-node-reference field-label-hidden"><div class="field-items"><div class="field-item even"><article about="/widget/stopthinkconnect" typeof="sioc:Item foaf:Document" class="node node-widget node-published node-not-promoted node-not-sticky author-cathleentracy odd clearfix node-view-mode-sidebar" id="node-widget-7364">
        <span property="dc:title" content="Stop.Think.Connect." class="rdf-meta element-hidden"></span><span property="sioc:num_replies" content="0" datatype="xsd:integer" class="rdf-meta element-hidden"></span>    
  
