<script type="text/javascript">
  function showTextSearch(obj)
  {
    $('#txtSearch').focus();
    $('#show-search').addClass('opened');
    return false;
  }

  function blurredSearch()
  {
    $('#show-search').removeClass('opened');
  }
</script>          <div class="content-area">
