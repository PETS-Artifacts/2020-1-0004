    var submit_login_form = function (hrefl, isPmxx) {
        var username = $j("#username").val();
        var pw = $j("#password").val();
        var post_page = 'https://www.pond5.com' + P5._lpp + '/ajax/user/login/' + username;
        var post_data = {
                    username: username
        };
        try {
            document.cookie = 'pond_cookie2=; expires=Thu, 01-Jan-1970 00:00:01 GMT; path=/;';
            document.cookie = 'pond_cookie2=; expires=Thu, 01-Jan-1970 00:00:01 GMT; path=/; domain=pond5.com;';
            document.cookie = 'pond_cookie2=; expires=Thu, 01-Jan-1970 00:00:01 GMT; path=/; domain=.pond5.com;';
        }
        catch(e) {}

       if (P5._clicked_link_needs_login === "") {
            $j("#hrefl").val( hrefl );
        } else {
            $j("#hrefl").val( P5._clicked_link_needs_login );
        }
        P5.showWaiting("#p5_lightbox_content");
        $j.post( post_page, post_data, function(json) {
            if(json.js){ eval(json.js); }

            if (json.has_error) {
                P5.hideWaiting("#p5_lightbox_content");
                $j("#p5_login_error_message").fadeIn(300);
                $j("#password, #username").addClass("p5_error");
                return;
            }


                if(isPmxx)
                {
                    window.location.href = hrefl;
                }
                else
                {
                    if (P5._clicked_link_needs_login === "") {
                        setTimeout(function() {window.location.reload(true);}, 500);
                    } else {
                        setTimeout(function() {window.location = P5._clicked_link_needs_login;}, 500);
                    }
                }
                return;

        }, "json")
        .fail(function() {
            $j("#loginForm").attr("action", "https://www.pond5.com/?page=login&ps2=1").submit();
        });
    }

    var submit_remember_form = function () {
        var username = $j("#a_username").val();
        var post_page = P5._lpp + '/index.php?page=ajax_forgot_pwd&what=reqpwd';
        var post_data = {
            username: username
        };
        P5.showWaiting("#p5_lightbox_content");
        $j.post( post_page, post_data, function(json) {
            P5.hideWaiting("#p5_lightbox_content");
            if(json.js){eval(json.js);}

            if (json.has_error) {
                $j("#p5_remember_error_message").html(json.errstr).fadeIn(300);
                $j("#a_username").addClass("p5_error");
                return;
            }

            if(json.ok == 1)
            {
                $j('#p5_lightbox .p5_content').html('<div class="h1">You’ve got email.</div><div style="padding: 15px 0;">Please check your registered email account for a link to reset your password. If you don’t see it, try checking your spam filter or contact our <a href="https://help.pond5.com/hc/en-us/requests/new" target="_blank">customer service</a> for further assistance.</div>');
                $j('#p5_lightbox .p5_orange_flat_button').html('Ok').attr('onclick', "$j('.p5_lightbox_close').trigger('click')" );
                return;
            }

            $j("#p5_remember_error_message").fadeIn(300);
            $j("#a_username").addClass("p5_error");
            var ttt = setTimeout(function(){window.location.reload(true);}, 2000);

        }, "json");

    }
    $j("#p5_lightbox .p5_formitem:not(.has_error) input.p5_error").live("keydown",function(){
