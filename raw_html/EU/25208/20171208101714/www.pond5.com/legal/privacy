        })
        .done(function (result) {
            try {
                json = eval(result);
                if (typeof json == 'undefined') {
                    throw "JSON didn't evaluate as expected";
                }

                if (json.has_error) {
                    P5.hideWaiting("#p5_lightbox_content");
                    P5.messageTT(json.errstr);
