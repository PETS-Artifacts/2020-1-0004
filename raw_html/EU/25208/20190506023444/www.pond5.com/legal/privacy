<script>
  var cookieName = "gdpr_approved";

  // Check if cookie exist
  var match = document.cookie.match(new RegExp(cookieName + '=([^;]+)'));
  if (!match) {
    var bannerElement = document.getElementById("cookieBanner");
    bannerElement.classList.remove("cookieBanner-hidden");
    document.getElementById("cookieBannerApprove").addEventListener("click", function () {
      bannerElement.classList.add("cookieBanner-hidden");

      var cookieValue = 1;
      var cookieExpiration = new Date();
      cookieExpiration.setFullYear(cookieExpiration.getFullYear() + 2);
      var domain = (window.location.hostname.indexOf("p5staging") !== -1) ? ".p5staging.com" : ".pond5.com";
      // IE 11 not support template literals <3
      document.cookie =
        cookieName +
        "=" +
        cookieValue +
        ";expires=" +
        cookieExpiration.toGMTString() +
        ";path=/; domain=" + domain;
    });
  }
</script>
