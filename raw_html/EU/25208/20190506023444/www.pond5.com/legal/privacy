<script>
    var selectedMediaType = $j(".js-siteSearchMediaType.p5_selected");
    if (selectedMediaType.length === 0) {
        selectedMediaType = $j(".js-siteSearchMediaType").first();
    }
    P5._search_type = selectedMediaType.text().trim();
    P5._search_type_id = selectedMediaType.attr("id");
</script>
