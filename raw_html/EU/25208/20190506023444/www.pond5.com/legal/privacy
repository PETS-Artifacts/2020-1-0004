function pond_fb_login(signUp){
  if(P5.fbReturnPage)
  {
    var url = window.location.protocol + "//" + window.location.host + "/index.php?page=" + P5.fbReturnPage;
  } else {
    var url = window.location.toString();
    var hash = window.location.hash;
    url = url.replace(hash, "");

    if (signUp) {
        window.localStorage.setItem("p5_sign_up_fb", true);
    } else {
        url += ((url.indexOf("?") < 0) ? "?" : "&") + "fb=ret";
    }
    url += hash;

    P5.fbReturnPage = url;
  }
  var t=setTimeout(function(){
    window.location = url;
    }, 300);
}
