    P5.showWaiting("#p5_lightbox_content");

    var post_data = {
                email: $j("#signup_email").val(),
                username: $j("#signup_username").val(),
                password: $j("#signup_password").val(),
                retype_password: $j("#signup_confirm_password").val(),
