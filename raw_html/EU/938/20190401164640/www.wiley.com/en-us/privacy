document.addEventListener('DOMContentLoaded', function (event) {
	var header = document.getElementById("main-header-container");
	if (document.getElementById("privacy")) {
		if (window.innerWidth > 640) {
			header.style.height = "181px";
		} else {
			header.style.height = "105px";
			var navbarToggle = document.getElementsByClassName('navbar-toggle')[0];
			if (navbarToggle) {
				var disclaimer = document.getElementById('privacy');
				disclaimer.style.position = "relative";
				navbarToggle.addEventListener('click', function () {
					if (navbarToggle.className.indexOf('collapsed') === -1) {
						disclaimer.style.display = "block";
					} else {
						disclaimer.style.display = "none";
					}

				});
			}
			var searchIcon = document.getElementsByClassName('main-navigation-search-icon')[0];
			if (searchIcon) {
				var searchInput = document.getElementsByClassName('main-navigation-search')[0];
				searchInput.style.top = "44px";
				searchIcon.addEventListener('click', function () {
					if (searchIcon.className.indexOf('active') === -1) {
						searchInput.style.top = "104px";
					} else {
						searchInput.style.top = "44px";
					}
				});
			}
		}
	}
});
</script></div></div>
