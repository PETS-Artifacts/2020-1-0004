<script type="text/javascript" src="/_ui/addons/b2caddon/desktop/common/js/acc.common.js"></script>

<script type="text/javascript">
	/* This loader allow to load custom js from site theme folder */

var dynamicLoader = {
		 loadCSS : function(url) {
			 $('<link/>', {
				   rel: 'stylesheet',
				   type: 'text/css',
				   href: url
				}).appendTo('body');
		 },

		 loadJS : function(src) {
			 $('<script type="text/javascript" src="'+ src +'"><\/script>').appendTo('body');
		 },

		 getThemeJsPath : function() {
			 var scriptSrc = $('script[src*="changes.js"]').attr('src');
			 return scriptSrc.substring( 0, scriptSrc.indexOf('changes.js'));
		 },

		 getThemeJsLibPath : function() {
			var scriptSrc = this.getThemeJsPath();
			var delimiter = scriptSrc.substring(scriptSrc.length - 1);
			return scriptSrc + 'lib' + delimiter;
		 },

		 getThemeCssPath : function() {
			 var cssHref = $('link[href*="changes.css"]').attr('href');
			 return cssHref.substring( 0, cssHref.indexOf('changes.css'));
		 },

		 loadJsFromTheme : function(name){
			 var jsSrc = this.getThemeJsPath() + name;
			 this.loadJS(jsSrc);
		 },

		 loadJSLibFromTheme : function(name){
			var jsSrc = this.getThemeJsLibPath() + name;
			this.loadJS(jsSrc);
		 },

		 loadCssFromTheme : function(name){
			 var cssHref = this.getThemeCssPath() + name;
			 this.loadCSS(cssHref);
		 }
};ACC.autocomplete = {

	bindAll: function()
	{
        if ($( "#search").length > 0) {
            this.bindSearchAutocomplete();
        }
	},

	bindSearchAutocomplete: function()
	{
		$( "#search" ).autocomplete({
			source: function( request, response ) {
				$.getJSON(
						ACC.autocompleteUrl,
						{
							term : $('#search').val()
						},
						function(data) {
							var autoSearchData = [];
							if(data.suggestions != null){
								$.each(data.suggestions, function (i, obj) {
									autoSearchData.push(obj.term);
								});
							}
							response(autoSearchData);
						}
					);
			},
			minLength: 2,
			open: function() { $(".ui-menu").css("z-index", 10000); },
			close: function() { $(".ui-menu").css("z-index", -1); },
			focus: function() {return false;},
			select: function(event, ui) {
				if(ui.item) {
					$('#search').val($.trim(ui.item.value));
				}
				document.forms['search_form'].submit();
			},
			autoFocus: false

		});
	}

};

$(document).ready(function() {
	ACC.autocomplete.bindAll();
});
ACC.placeholder = {

	bindAll: function()
	{
		this.bindPlaceholderInNotSupportedBrowsers();
	},

	bindPlaceholderInNotSupportedBrowsers: function() {
		$('input#search').placeholder();
		$('input#newsSignUpGlobalEmail, input#newsSignUpEmail').placeholder();
	}
};

$(document).ready(function ()
{
	ACC.placeholder.bindAll();
});ACC.flashcookie = {
        COOKIE_NAME_PREFIX : 'FLASH_',
        flashCookies : [],

        populateFlashCookies : function() {
            for(var cookieName in $.cookie()) {
                if(cookieName && cookieName.indexOf && cookieName.indexOf(this.COOKIE_NAME_PREFIX) === 0) {
                    this.popFlashCookie(cookieName);
                }
            }
        },

        addFlashCookie : function(name, value) {
            var cookieName = this.COOKIE_NAME_PREFIX + name;
            $.cookie(cookieName, value, { path : "/" });
        },

        popFlashCookie : function(cookieName) {
            var value = $.cookie(cookieName);
            $.removeCookie(cookieName, { path : "/" });
            var name = cookieName.replace(this.COOKIE_NAME_PREFIX, "");
            this.flashCookies[name] = value;
        }
};

ACC.flashcookie.populateFlashCookies();var lazyComponentLoader = {

    loadedComponentsId : {},

    loadComponents: function(){
        var components = $(".lazyComponent");
        if (components.length) {
            var element = components.first();
            var componentId = $(element).data('componentId');
            if(lazyComponentLoader.isInView(element) && lazyComponentLoader.notYetLoaded(componentId)) {
                lazyComponentLoader.loadContent(element, componentId);
            }
        } else {
            $(document).unbind("scroll", this.loadComponents);
        }
    },

    isInView : function(element) {
        return $(window).scrollTop() + window.innerHeight + 50 > element.offset().top;
    },

    notYetLoaded : function(componentId){
        return !lazyComponentLoader.loadedComponentsId[componentId];
    },

    loadContent : function(element, componentId){
        lazyComponentLoader.loadedComponentsId[componentId] = true;
        $.get( "/lazy/" + componentId, function(data) {
            $(element).replaceWith(data);
        }).fail(function() {
            $(element).remove();
        }).always(function() {
            lazyComponentLoader.loadComponents();
        });
    }
};

$(window).load(function() {
    lazyComponentLoader.loadComponents();
    $(document).scroll(lazyComponentLoader.loadComponents);
});
</script>
