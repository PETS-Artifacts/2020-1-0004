<script type="text/javascript">
	var loadDeferredStyles = function() {
    $(".deferredCss").each(function(){
        this.media = $(this).data('media');
        $(this).attr("rel", "stylesheet");
    });
};
var removeInitialStyles = function() {
    $("#preloadStyles").remove();
};

window.addEventListener('DOMContentLoaded', loadDeferredStyles);
window.addEventListener('load', removeInitialStyles);
</script>

