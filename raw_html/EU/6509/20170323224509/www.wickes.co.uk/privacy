<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.7.2/jquery.min.js"></script>


	<!-- Start Google Analytics -->
<script type="text/javascript">
		/* Google Analytics */
		
		var googleAnalyticsTrackingId = 'UA-44725244-2';
		
		(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
		(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
		m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
		})(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
		
		ga('create', googleAnalyticsTrackingId);
		
		
		var freeTextSearchUnescaped = unescape('').replace(/\+/g,' ');
		
		
			ga('send', 'pageview');
			
		function bindFacetListeners(eventAction) {
			$('#searchFacetBlock :checkbox').on('click', function(){
				var checkbox = $(this);
				if (checkbox.is(':checked')) {
					trackFacets_google(eventAction, 'Shop by ' + checkbox.val());
			    }
			});
			
			$('#searchFacetBlock #spanFacet').on('click', function(){
				if($(this).prop("title")){
					trackFacets_google(eventAction, 'Shop by ' + $(this).prop("title"));
				}
			});
		}
		
		function trackAddToCart_google(productCode, quantityAdded) {
		    ga('send', 'event', 'Cart', 'AddToCart', productCode, quantityAdded); 
		}
		
		function trackUpdateCart(productCode, initialQuantity, newQuantity) {
			if (initialQuantity != newQuantity) {
				if (initialQuantity > newQuantity) {
		            ga('send', 'event', 'Cart', 'RemoveFromCart', productCode, initialQuantity - newQuantity); 
				} else {
		            ga('send', 'event', 'Cart', 'AddToCart', productCode, newQuantity - initialQuantity); 
				}
			}
		}
		
		function trackRemoveFromCart(productCode, initialQuantity) {
		    ga('send', 'event', 'Cart', 'RemoveFromCart', productCode, initialQuantity); 
		}
		
		function trackSiteNavigation_google(gaEvent, eventLabel) {
		    ga('send', 'event', 'SiteNavigation', gaEvent, eventLabel);
		}
		
		function trackOutOfStockPDP_google(eventAction, eventLabel) {
			 
		}
		
		function trackFacets_google(gaEvent, eventLabel) {
		    ga('send', 'event', 'Facets', gaEvent, eventLabel);
		}
		
		function trackCertonaWidgets_google(gaEvent, eventLabel) {
			ga('send', 'event', 'Certona Widget', gaEvent, eventLabel);
		}
		
		function trackSuggestionSelection_google(gaEvent, eventLabel) {
			ga('send', 'event', 'Search', gaEvent, eventLabel);
		}
		
		function isHowtoPage() {
			var pathComponents = document.location.pathname.toString().split('/');
			var pattern = /howto.+/i;
			return pattern.test(pathComponents[pathComponents.length-1]);
		}
		
		function extractYoutubeId(url) {
			var regExp = /^.*((youtu.be\/)|(v\/)|(\/u\/\w\/)|(embed\/)|(watch\?))\??v?=?([^#\&\?]*).*/;
			var match = url.match(regExp);
			if (match && match[7].length == 11) {
				return match[7];
			} else {
				console.log('Wrong format of youtube url');
				return undefined;
			}
		}
		
		function onYouTubePlayerAPIReady() {
			$('iframe[src*="www.youtube.com"]').each(function() {
				var videoId = extractYoutubeId($(this).attr('src'));
				var width = $(this).attr('width');
				var height = $(this).attr('height');
				var id = $(this).attr('id');
				if (!id) {
					id = 'player';
					do {
						id += "-frame";
					} while ($('#' + id).length != 0);
				}
				$(this).replaceWith('<div id="' + id + '"/>');
				new YT.Player(id, 
					{
						videoId: videoId,
						width: width,
						height: height,
						playerVars: { 'rel': 0 },
						events: {
							'onStateChange': function (event) {
								if(event.data == YT.PlayerState.PLAYING && !event.target.played) {
									event.target.played = true;
									ga('send', 'event', 'Video', 'Play', videoId);
								}
							}
						}
					}
				);
			});
		}
		
		function parseCodeFromLink(link) {
			return link.split('/').reverse()[0];
		}
		
