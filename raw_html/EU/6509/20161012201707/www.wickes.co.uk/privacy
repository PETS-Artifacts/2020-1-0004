				trackSiteNavigation_google('Checkout','AddToCartPopup');
			});
		
			$('#doCheckoutBut').click(function() {
				trackSiteNavigation_google('Checkout','HeaderCheckout');
			});
			
			$('#checkoutButtonTop').click(function() {
				trackSiteNavigation_google('Checkout','CartCheckoutTop');
			});
			
			$('#checkoutButtonBottom').click(function() {
				trackSiteNavigation_google('Checkout','CartCheckoutBottom');
			});
			
			$('.gotobasket').click(function() {
				trackSiteNavigation_google('Basket', 'ViewBasketLink');
			}); 
			
			$('.headerContent .search .button').click(function() {
				var searchString = $('#search').val();
				trackSiteNavigation_google('Search', searchString);
			});
			
			$('.footerBanner a').click(function() {
				var bannerTitle = $('.fb-heading', this).text();
				trackSiteNavigation_google('FooterBannerClick', bannerTitle);
			});
		
		    $('#all_tabs_strip a').on("click", function() {
		        trackSiteNavigation_google('PDPTabClick', $(this).text());
		    });
		    
		    $('.facebook, .twitter, .youtube').click(function(){
				trackSiteNavigation_google('Social CTAs', $(this).attr('class'));
			});
		    
		    $('.similarProdItemImg, .similarProdItemTitle').on('click', function(){
				var productSKU = parseCodeFromLink($(this).attr("href"));
				trackCertonaWidgets_google('Similar Products', productSKU);
			});
			
			$('.otherProdItemImg, .otherProdItemTitle').on('click', function(){
				var productSKU = parseCodeFromLink($(this).attr("href"));
				trackCertonaWidgets_google('Other Customers Also Bought', productSKU);
			});
			
			
			
			$('#homepage_slider .slides area, #homepage_slider .slides a').click(function() {
				var bannerUrl = $(this).attr('href');
				trackSiteNavigation_google('HomepageSliderBannerClick', bannerUrl);
			});
		
			$('.homepagePromoSlot a').click(function() {
				var bannerUrl = $(this).attr('href');
				trackSiteNavigation_google('HomepageStaticBannerClick', bannerUrl);
			});
			
			$('a[href$=".pdf"]').click(function() {
				if(isHowtoPage()) {
					var gilUrl=$(this).attr('href').split('/');
					var gilName=gilUrl[gilUrl.length-1];
					trackSiteNavigation_google('DownloadGIL', gilName);
				}
			});
		});
