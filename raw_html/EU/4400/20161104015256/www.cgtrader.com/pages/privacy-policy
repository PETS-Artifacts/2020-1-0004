  var item, itemPrice;

  itemPrice = function(prices) {
    var tabs;
    tabs = {
      '#tab-get-printed': prices.get_it_printed_price,
      '#tab-print': prices.streaming_price,
      '#tab-download': prices.download_price
    };
    return tabs[window.location.hash] || prices.first_available_price;
  };

  if (gon.gtm_view_item != null) {
    item = gon.gtm_view_item;
    item.price = itemPrice(item.price);
    customDataLayer.push({
      event: 'productDetailView',
      ecommerce: {
        detail: {
          products: [item]
        }
      }
    });
  }
</script>


