<script type="text/javascript">
	function insertHtml(id, string) {
		var el 	= document.getElementById(id),
			tag = el.getElementsByTagName('script');

		el.innerHTML = string;

		for (var i = 0; i < tag.length; i++) {
			eval(tag[i].text);
		}
	}

	var neymjlthtq4vm = 0;

	if (document.getElementsByClassName('adContainer').length) {
		var $tjChannelAndSearchParameters = '';
		var $tjImgData = varObj_widgetAdsJson.adUrl;

		if ($tjImgData) {
			var url = $tjImgData;

			if ($tjChannelAndSearchParameters) {
				var url = url + $tjChannelAndSearchParameters;
			}

			var a = new XMLHttpRequest();

			a.onreadystatechange = function () {
				if (a.readyState == 4 && a.status == 200) {
					var data = eval(a.responseText);

					for (var adsLoop = 0; adsLoop < data.length; adsLoop++) {
						if (document.getElementById(varObj_widgetAdsJson.putAd[varObj_widgetAdsJson.putAdZone[data[adsLoop].zone_id]])) {
							if (data[adsLoop].html !== undefined) {
								document.getElementById(varObj_widgetAdsJson.putAd[varObj_widgetAdsJson.putAdZone[data[adsLoop].zone_id]]).innerHTML = data[adsLoop].html;
								insertHtml(varObj_widgetAdsJson.putAd[varObj_widgetAdsJson.putAdZone[data[adsLoop].zone_id]], data[adsLoop].html);
							}
							else if (data[adsLoop].media_type == 'image') {
								document.getElementById(varObj_widgetAdsJson.putAd[varObj_widgetAdsJson.putAdZone[data[adsLoop].zone_id]]).innerHTML = '<a href="' + data[adsLoop].link + '" target="_blank" rel="nofollow"><img src="' + data[adsLoop].img_url + '" alt="TJ Ad" /></a>';
							}
							else if (data[adsLoop].media_type == 'video') {
								var random = Math.floor(Math.random() * (9999999 - 1000000) + 1000000);
								document.getElementById(varObj_widgetAdsJson.putAd[varObj_widgetAdsJson.putAdZone[data[adsLoop].zone_id]]).innerHTML = '<a class="mediaVideoType" href="' + data[adsLoop].link + '" target="_blank" rel="nofollow"><img class="mediaVideoType" src="' + data[adsLoop].logo_img_url + '" alt="TJ Ad" /><div><canvas width="100%" height="100%" id="JVmpegCanvas' + random + '"></canvas></div></a>';

								var canvas = document.getElementById('JVmpegCanvas' + random),
										player = new JVmpeg(data[adsLoop].video_url, {canvas: canvas, autoplay: true, loop: true});

								canvas.addEventListener('click', function() {
									player.play();
								}, false);
							}
							else {
								document.getElementById(varObj_widgetAdsJson.putAd[varObj_widgetAdsJson.putAdZone[data[adsLoop].zone_id]]).innerHTML = '<a href="' + data[adsLoop].link + '" target="_blank" rel="nofollow"><img src="' + data[adsLoop].img_url + '" alt="TJ Ad" /></a>';
							}

							if (neymjlthtq4vm) {
								document.getElementById(varObj_widgetAdsJson.putAd[varObj_widgetAdsJson.putAdZone[data[adsLoop].zone_id]]).removeAttribute('id');
							}
						}
					}
				}
			};

			a.open("GET", url, true);
			a.withCredentials = true;
			a.send();
		}
	}
</script>


