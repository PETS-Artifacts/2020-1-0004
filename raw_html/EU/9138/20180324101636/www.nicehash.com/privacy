						$(".hamburger").click(function() {
							$(".menu").show();
							$(".pushmenu-push").toggleClass("pushmenu-push-toright");
							$(".pushmenu-left").toggleClass("pushmenu-open");
						});
						
						$(".close").click(function() {
							$(".pushmenu-push").removeClass("pushmenu-push-toright");
							$(".pushmenu-left").removeClass("pushmenu-open");			
						});
						
						$("#buyers_button").click(function() {
							$("#sellers_dropdown").hide();
							$("#sellers_button").children(".divider").hide();
							$("#sellers_button").removeClass("more");
							$("#wallet_dropdown").hide();
							$("#wallet_button").children(".divider").hide();
							$("#wallet_button").removeClass("more");								
							$("#buyers_dropdown").toggle();
							$(this).children(".divider").toggle();
							$("#buyers_button").toggleClass("more");			
						});
						
						$("#sellers_button").click(function() {
							$("#buyers_dropdown").hide();
							$("#buyers_button").children(".divider").hide();
							$("#buyers_button").removeClass("more");
							$("#wallet_dropdown").hide();
							$("#wallet_button").children(".divider").hide();
							$("#wallet_button").removeClass("more");									
							$("#sellers_dropdown").toggle();
							$(this).children(".divider").toggle();
							$("#sellers_button").toggleClass("more");			
						});
						$("#wallet_button").click(function() {
							$("#buyers_dropdown").hide();
							$("#buyers_button").children(".divider").hide();
							$("#buyers_button").removeClass("more");
							$("#sellers_dropdown").hide();
							$("#sellers_button").children(".divider").hide();
							$("#sellers_button").removeClass("more");									
							$("#wallet_dropdown").toggle();
							$(this).children(".divider").toggle();
							$("#wallet_button").toggleClass("more");			
						});
				});
				
				$(function(){
					var utcoffset = (new Date().getTimezoneOffset()) * 60 * 1000; 
					$("time").each(function(index){
						var oldTime = Date.parse($(this).html());
						if(oldTime>0 && $(this).html().indexOf(" ") >= 0){
							var newdate = new Date(oldTime - utcoffset);
							newdate = newdate.getFullYear() + "-" + ("0" + (newdate.getMonth() + 1)).slice(-2) + "-" + ("0" + newdate.getDate()).slice(-2) + " " +  ("0" + newdate.getHours()).slice(-2) + ":" + ("0" + newdate.getMinutes()).slice(-2) + ":" + ("0" + newdate.getSeconds()).slice(-2);
							$(this).html(newdate);
						}
					});
				});
				</script>
