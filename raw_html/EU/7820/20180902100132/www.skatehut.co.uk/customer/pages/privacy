<script>
	window.addEventListener("load", function () {
		var googleLang = {};
		googleLang.holder = document.querySelector('.goog-te-menu-value span');
		googleLang.currentLanguage = googleLang.holder.innerHTML.replace(/[^A-Z0-9]/ig, "").toLowerCase();
		googleLang.holder.classList.add(googleLang.currentLanguage);
		googleLang.newLanguage = null;
		googleLang.observer = new MutationObserver(function (mutations) {
			googleLang.newLanguage = googleLang.holder.innerHTML.replace(/[^A-Z0-9]/ig, "").toLowerCase();
			if (googleLang.newLanguage !== googleLang.currentLanguage) {
				googleLang.holder.classList.add(googleLang.newLanguage);
				googleLang.holder.classList.remove(googleLang.currentLanguage);
			}
			googleLang.currentLanguage = googleLang.newLanguage;
		});
		googleLang.observerConfig = { subtree: true, characterData: true, childList: true };
		googleLang.observer.observe(googleLang.holder, googleLang.observerConfig);
	});
</script>
