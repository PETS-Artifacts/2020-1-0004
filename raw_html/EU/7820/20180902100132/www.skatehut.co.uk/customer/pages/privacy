<script type="text/javascript">
	
	window.addEvent('domready', function() {
		blucommerce.buildPage();

		Locale.use('en-GB');
	(function () {

		var currencySwitcher = $('js-currency-switcher-uid379395829');
		var toggler = currencySwitcher.getElement('.js-currency-switcher-toggler');
		var popup = currencySwitcher.getElement('.js-currency-switcher-popup');
		var popupCloser = currencySwitcher.getElement('.js-currency-switcher-popup-close');
		var open = false;

		$('popupOverlay').addEvent('click', function() {
			popup.removeClass('is-open');

			$$('html').removeClass('currencySwitcher-open');
			$$('html').removeClass('header-popup-open');
			open = false;
		});

		toggler.addEvent('click', function (event) {
			popup.toggleClass('is-open');
			open = !open;
			if (open) {
				$$('html').addClass('header-popup-open currencySwitcher-open');
			}
			else {
				$$('html').removeClass('currencySwitcher-open');
				$$('html').removeClass('header-popup-open');
				$$('.js-currency-switcher-popup').removeClass('is-open');
			}
});

		popupCloser.addEvent('click', function (event) {
			popup.removeClass('is-open');
			$$('html').removeClass('header-popup-open');
			$$('html').removeClass('currencySwitcher-open');

			open = false;
		});


	})();

	new QuickSearch(
		$('uid379395830'),
		{"headers":{"alternative":"Similar Searches","article":"Articles","box":"Site Content","hierarchy":"Categories","location":"Stores","more":"More","product":"Products","recent":"Recent Searches<button class=\"clear-history\">Clear history<\/button>","suggestion":"Suggestions","trending":"Trending Searches"},"maxRecentSearches":5,"queryUrl":"\/quicksearch","templates":{"recent":"<span class=\"title\">{TEXT}<\/span>","trending":"<span class=\"title\">{TEXT}<\/span>"},"trendingSearches":[]}	);


						window.addEvent('domready', function() {
							blucommerce.cart.addEvent('addedItem', function(event) {
								if ($$('.js-header-popup-minicart')[0]) {
									$$('.js-header-popup-minicart').addClass('is-open');
									$$('html').addClass('header-popup-open');
									// Let the world know that a popup has closed
									blucommerceChangePopupState('headerPopup', 'open');
								}
								if ($$('.minicart-toggler')[0]) {
									$$('.minicart-toggler')[0].addEvent('click', function(event) {
											// Close All Popups
											$$('.js-header-popup-minicart').removeClass('is-open');
											$$('html').removeClass('header-popup-open');
											$$('html').removeClass('currencySwitcher-open');
											// Let the world know that a popup has closed
											blucommerceChangePopupState('headerPopup', 'closed');
									});
								}

									$('popupOverlay').addEvent('click', function() {
											$$('.js-header-popup-minicart').removeClass('is-open');
											$$('html').removeClass('header-popup-open');
											// Let the world know that a popup has closed
											blucommerceChangePopupState('headerPopup', 'closed');
									});
							});
						});
					
									window.addEvent('load', function() {
										var req = new Request.HTML({
											method: 'get',
											url: '/productAZ',
											data: { raw: true },
											update: $('product-az-replace')
										}).send();
									});
									
		var checkWindowWidth = function() {
			var width = window.getWidth();
			var topbarheight = $$('.header_top')[0].getSize().y;
			$$('.header_main')[0].setStyle('margin-top', '' + topbarheight + 'px');
				if(width > 100) {
					new FixedSidebar({
						margin: topbarheight,
						marginPosition: 'top',
						element: 'nav-wrapper',
						startElement: 'static-hover-start',
						stopElement: 'static-hover-stop'
					});
				}
		};

		checkWindowWidth();

		window.addEvent('resize', function(){
		checkWindowWidth();
		});

	(function($) {
		$(document).ready(function() {
			$(".load-page > a").click(function(e) {
				var target = $(this).attr('href');
				var staticPage =  (target.indexOf('/pages/') !== -1);
				var faqPage = (target === '/customer');
				if (staticPage || faqPage) {
					e.preventDefault();
					var targetID = " #content-area";
					if (faqPage) {
						targetID = " #faq-content-area";
					}
					$(".load-page > a").not($(this)).removeClass('active');
					$(this).addClass('active');
					$('.js-main-content-area-wrapper').load(target + targetID);
					history.replaceState({}, "", target);
				}
			});
		});
	})(jQuery);

	(function() {
		document.getElementById('locationsSelect').addEventListener('change', function() {
			window.location = this.options[this.selectedIndex].value;
		}, false);
	})();

	(function () {

		var currencySwitcher = $('js-currency-switcher-uid379395831');
		var toggler = currencySwitcher.getElement('.js-currency-switcher-toggler');
		var popup = currencySwitcher.getElement('.js-currency-switcher-popup');
		var popupCloser = currencySwitcher.getElement('.js-currency-switcher-popup-close');
		var open = false;

		$('popupOverlay').addEvent('click', function() {
			popup.removeClass('is-open');

			$$('html').removeClass('currencySwitcher-open');
			$$('html').removeClass('header-popup-open');
			open = false;
		});

		toggler.addEvent('click', function (event) {
			popup.toggleClass('is-open');
			open = !open;
			if (open) {
				$$('html').addClass('header-popup-open currencySwitcher-open');
			}
			else {
				$$('html').removeClass('currencySwitcher-open');
				$$('html').removeClass('header-popup-open');
				$$('.js-currency-switcher-popup').removeClass('is-open');
			}
});

		popupCloser.addEvent('click', function (event) {
			popup.removeClass('is-open');
			$$('html').removeClass('header-popup-open');
			$$('html').removeClass('currencySwitcher-open');

			open = false;
		});


	})();

		var newsletterAllowSubmit = true;
		(function () {

			var CONTENT_URL = "\/?task=maillist_form";

			var openMagnificPopup = function (response, responseScript) {

			var sharedOptions = {"tClose":"Close","removalDelay":0,"modal":false,"closeOnContentClick":false,"closeOnBgClick":true,"showCloseBtn":true,"mainClass":"magnific-popup","closeMarkup":"<a class=\"popup-close mfp-close-button\"><span class=\"popup-close__title\">%title%<\/span><\/a>"};

				var options = {
					items: {
						src: response,
						type: 'inline'
					},
					type: 'inline',
					mainClass: 'js-newsletterpopup magnific-popup is-triggered-by-user newsletter__popup',
					callbacks: {
						open: function() {
							attachEvents();

						}
					}
				};

				(function($){
					var mergedOptions = $.extend({}, sharedOptions, options );
					$.magnificPopup.open(mergedOptions, 0);
				})(jQuery);

				Browser.exec(responseScript);
			};

			var form = document.querySelector('#js-newsletter-form');

			form.addEventListener('submit', function (e) {
					e.preventDefault();

					var request = new Request.HTML(
						{
							url: CONTENT_URL,
							method: 'get',
							evalScripts: false,
							data: {
								format: 'popup'
							},
						}
					)

					var requestFinished = function (response) {

							// Get script
						var responseScript = "";
						request.response.text.stripScripts(function(script) {
							responseScript += script;
						});

						openMagnificPopup(response, responseScript);
					}

					request.addEvent('onSuccess', requestFinished.bind(this));

					request.send();
				}

			);
		})();
	//<![CDATA[
			if ($('nav-top-search')) {
				$('nav-top-search').focus();
			}
			new NavDelay();

			if (!Browser.ie6 && !Browser.ie7) {
							} else {
				if ($('product-details-tabs')) $('product-details-tabs').setStyle('display', 'none');
			}
	//]]>

;(function(){

		var CONTENT_URL = "\/?task=maillist_form";
	var DELAY = 2000;
	var MAGNIFIC_POPUP_SHARED_OPTIONS = {"tClose":"Close","removalDelay":0,"modal":false,"closeOnContentClick":false,"closeOnBgClick":true,"showCloseBtn":true,"mainClass":"magnific-popup","closeMarkup":"<a class=\"popup-close mfp-close-button\"><span class=\"popup-close__title\">%title%<\/span><\/a>"};
	var VISIT_REQUIRED = 0;
	var REPEAT_DAYS = 365;

	var visitCount = parseInt(Cookie.read('blu-visit') || 1);
	var popupShown = parseInt(Cookie.read('blu-popup-shown') || 0);

	if ((window.location.hash === "#newsletter") || (visitCount >= VISIT_REQUIRED && popupShown == 0)) {

		var openMagnificPopup = function (response, responseScript) {
			var options = {
				items: {
					src: response,
					type: 'inline'
				},
				type: 'inline',
				mainClass: 'js-newsletterpopup magnific-popup is-triggered-by-a-cookie only-email newsletter__popup',
				callbacks: {
					open: function() {
						attachEvents();
						newsletterAllowSubmit = false;
					}
				}
			};

			(function($){
				var mergedOptions = $.extend({}, MAGNIFIC_POPUP_SHARED_OPTIONS, options );
				$.magnificPopup.open(mergedOptions, 0);
			})(jQuery);

			Browser.exec(responseScript);

		};

		var loadPopup = function () {

						if ( document.querySelector('.is-triggered-by-user') ) {
				return false;
			}

			var request = new Request.HTML(
				{
					url: CONTENT_URL,
					method: 'get',
					evalScripts: false,
					data: {
						format: 'popup'
					},
				}
			)

			var requestFinished = function (response) {
				Cookie.write('blu-popup-shown', 1, {duration: REPEAT_DAYS});

				// Get script
				var responseScript = "";
				request.response.text.stripScripts(function(script) {
					responseScript += script;
				});

				// Fire an event to GTM/GA
				bluStats.trackEvent({
					category: 'Marketing Popup',
					action: 'Popup Shown',
					label: CONTENT_URL
				});

				openMagnificPopup(response, responseScript);
			}



			request.addEvent('onSuccess', requestFinished.bind(this));

			request.send();
		}

		// Delay
		setTimeout(function() {
			loadPopup();
		}, DELAY);
	}


	// Increase visit count
	Cookie.write('blu-visit', ++visitCount, {duration: REPEAT_DAYS});
})();
var attachEvents = function() {
			console.log('attaching events...')

			var nextStageButton = document.getElementsByClassName('js-next-stage')[0];
			var showMoreButton = document.getElementsByClassName('js-show-more-fields')[0];
			var optionalFields = document.getElementsByClassName('js-fields-optional')[0];
			var submitButton = document.getElementsByClassName('js-newsletter-submit')[0];
			var theform = document.getElementsByClassName('js-newsletter-form')[0];
			var continueShopping = document.getElementsByClassName('js-continue-shopping')[0];
			var emailField = document.getElementById('newsletteremail');
			document.getElementById('newsletteremail').value = document.getElementById('foot-news-email').value;
			showMoreButton.addEventListener('click', function(e) {
				e.preventDefault();
				optionalFields.classList.toggle('optional-fields--hide');
				showMoreButton.classList.toggle('is-active');
			});
			nextStageButton.addEventListener('click', function(e) {
					e.preventDefault();
					nextStageButton.classList.add('hidden');
					theform.classList.add('show-all');
					newsletterAllowSubmit = true;
			});
			emailField.addEventListener('keydown', function(e) {
				if ((e.keyCode === 13) && (!(newsletterAllowSubmit))) {
					e.preventDefault();
					nextStageButton.classList.add('hidden');
					theform.classList.add('show-all');
					newsletterAllowSubmit = true;

				}
			});
			continueShopping.addEventListener('click', function(e) {
				e.preventDefault();
				(function($) {
					$.magnificPopup.instance.close();
				})(jQuery);
			});
		}




	document.body.classList.add('is-loaded');


		blucommerce.destruct();
	});

	window.addEvent('load', function() {
			});

</script>



