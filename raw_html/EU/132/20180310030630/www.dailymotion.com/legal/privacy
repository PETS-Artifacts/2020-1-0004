                    {
                        document.getElementsByTagName("head")[0].appendChild(crtg_script);
                    }
                    //end added that

                    else if(document.getElementsByTagName("head").length>0)
                    {
                        document.getElementsByTagName("head")[0].appendChild(crtg_script);
