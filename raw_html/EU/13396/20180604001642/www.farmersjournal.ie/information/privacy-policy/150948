      function makeItPasswordSub()
      {
         document.getElementById("passcontainerSub")
            .innerHTML = "<input id=\"loginpassword\" name=\"loginpassword\" type=\"password\" class=\"register-field\" validate=\"required:true\" />";
         document.getElementById("loginpassword").focus();
      }
</script>

