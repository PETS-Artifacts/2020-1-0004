<script>
$(document).ready(function(){
  $(".logo-arrow").click(function(){
    $(".logo-arrow-nav").toggle();
  });
  
  
  $('#loginbox input').keydown(function(e) {
    if (e.keyCode == 13) {
        $( "#login-formbutSubmot" ).trigger( "click" );
    }
});
  
});
</script>

