      function makeItPasswordReg()
      {
         document.getElementById("passcontainerReg")
            .innerHTML = "<input id=\"regformdropdownPassword\" name=\"regformdropdownPassword\" type=\"password\" class=\"register-field\" validate=\"required:true\" />";
         document.getElementById("regformdropdownPassword").focus();
      }
</script>
