<!--Subscribe Form Code Start Here-->

<script type="text/javascript">
$(document).ready(function(){

	$(".btn-slide").click(function(){
		$("#loginpanel").css('display','none');	
		$("#panel").slideToggle("slow");
		//$(this).toggleClass("active"); return false;
		$('html, body').animate({ scrollTop: 0 }, 'slow');
		return false;
	});
	
	$(".loginbtn-slide").click(function(){
		$("#panel").css('display','none');	
		$("#loginpanel").slideToggle("slow");
		//$(this).toggleClass("active"); 
		$('html, body').animate({ scrollTop: 0 }, 'slow');
		return false;
		
	});
	
	
	$("#panel-close").click(function(){
		//$("#panel").slideToggle("slow");
		$("#panel").css('display','none');
		//$(this).toggleClass("active"); return false;
	});
	
	$("#register-formbutSubmot").click(function(){
		var regemail = $('#regformdropdownEmail').val();
		var regpassword = $('#regformdropdownPassword').val();
		
		regemail = regemail.trim();
		regpassword = regpassword.trim();
		
		if(regemail!='Email' && regpassword!='Password' && regemail!='' && regpassword!=''){
			
				var emailReg = /^[\w-']+(\.[\w-']+)*@([A-Za-z0-9-]+(\.[A-Za-z0-9-]+)*?\.[a-z]{2,6}|(\d{1,3}\.){3}\d{1,3})(:\d{4})?$/;		
				if(!emailReg.test(regemail) || regemail=='') {
					alert("Invalid Email Address");
				}else{
						$.post("includes/processregister.php", { username: regemail, password: regpassword, regurl:'//www.farmersjournal.ie/information/privacy-policy/150948' },
							function(result){
								//alert (result);
								if(result == 0){//its free / not registered
									$("#panelfinal").toggle();
									$("#panel").css('display','none');
								}else{//its already registered i.e. 1
									alert("Your email address entered is already registered, please sign in or reset your password");	
								}
						});
				
				}//end of the valid email check
				
		}else{
			//alert("defaults entered");
		}
    });
	
	
		
	
	$("#login-formbutSubmot").click(function(){
		var regemail = $('#loginemail').val();
		var regpassword = $('#loginpassword').val();
		
		regemail = regemail.trim();
		regpassword = regpassword.trim();
		
		if(regemail!='Email' && regpassword!='Password' && regemail!='' && regpassword!=''){
			
				var emailReg = /^[\w-']+(\.[\w-']+)*@([A-Za-z0-9-]+(\.[A-Za-z0-9-]+)*?\.[a-z]{2,6}|(\d{1,3}\.){3}\d{1,3})(:\d{4})?$/;		
				if(!emailReg.test(regemail) || regemail=='') {
					alert("Invalid Email Address");
				}else{
				
						$.post("includes/processlogin.php", { username: regemail, password: regpassword, regurl:'//www.farmersjournal.ie/information/privacy-policy/150948' },
							function(result){
								//alert(result);
								if(result == 0){//its free / not registered
									alert("Sorry, account details not found, please try again");
								}else if(result == 'FOUND'){//login
									alert("We found your email address, but password did not match. Please try again or reset your password below");
								}else if(result == 1){//login
									window.location.replace("//www.farmersjournal.ie/account.php");
								}else{
									//window.location.replace(result);//replace with last visited URL by regurl
									window.location.replace(result);
								}
						});
				
				}//end of the valid email check
				
		}else{
			//alert("defaults entered");
		}
    });
	
	
	
	
	$("#panel-closefinal").click(function(){
    $("#panelfinal").css('display','none');
	$("#panel").css('display','none');	
	});
	
	
	$("#loginpanel-closefinal").click(function(){
    $("#loginpanel").css('display','none');	
	});
	
		 
});


</script>

<!--Subscribe Form Code End Here-->



