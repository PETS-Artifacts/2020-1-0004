<script type="text/javascript">

    function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') {
                c = c.substring(1);
            }
            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length);
            }
        }
        return "";
    }
    $(document).ready(function(){
        var customerid = getCookie('custid');
        if(customerid == null || customerid == ""){
            $('.before-login-btn').css('display','block');
            $('.after-login-btn').css('display','none');
        } else {
            $('.before-login-btn').css('display','none');
            $('.after-login-btn').css('display','block');
        }
    })
    var custid = getCookie('custid');
    $.ajax({
        url: "http://www.farmersjournal.ie/api/apps/fj_checkcustsub.php?custid="+custid,
        success: function(data){
            if(data == 1) {
                
                if(custid == null || custid == ""){
                    $(".right-subs-but").attr("href", "subscribe.php");
                    $(".right-subs-but").text('Subscribe Now');
                } else {
                    $(".right-subs-but").attr("href", "http://edition.pagesuite-professional.co.uk//launch.aspx?pbid=76379fd5-fa8e-44c4-a34a-7784b5dcb297");
                    $(".right-subs-but").text('Digital Edition');
                }
            } else {
                $(".right-subs-but").attr("href", "subscribe.php");
                $(".right-subs-but").text('Subscribe Now');
            }
        }
    });
</script>

