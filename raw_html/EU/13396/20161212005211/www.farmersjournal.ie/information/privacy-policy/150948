<!--Functions for changing Password text to password ***-->
<script type="text/javascript">
      function makeItPasswordSub()
      {
         document.getElementById("passcontainerSub")
            .innerHTML = "<input id=\"loginpassword\" name=\"loginpassword\" type=\"password\" class=\"register-field\" validate=\"required:true\" />";
         document.getElementById("loginpassword").focus();
      }
</script>

<script type="text/javascript">
      function makeItPasswordReg()
      {
         document.getElementById("passcontainerReg")
            .innerHTML = "<input id=\"regformdropdownPassword\" name=\"regformdropdownPassword\" type=\"password\" class=\"register-field\" validate=\"required:true\" />";
         document.getElementById("regformdropdownPassword").focus();
      }
</script>
<!--End functions-->
   
