            <!--[if lte IE 9]>
            <script src="/js/gulp_corporate.min.uglify.js" type="text/javascript"></script>
            <script type="text/javascript">
                $(function () {
                    lazyLoad(true, false);
                    $('#megaMenu ul,#megaMenu ul li').removeAttr('role').removeAttr('aria-label').removeAttr('aria-haspopup');
                    megaMenu();
                    mainJs();
                    HistoryTimeLine();
                    if (typeof (bindHistoryTimeLine) == "function") {
                        bindHistoryTimeLine();
                    }
                    if (typeof (createChargePointMap) == "function") {
                        createChargePointMap();
                    }
                });
            </script>
            <![endif]-->
            <script type="text/javascript">
                var isTouch = false;
                if ("ontouchstart" in window || navigator.msMaxTouchPoints) {
                    isTouch = true;
                } else {
                    isTouch = false;
                }
                function downloadJS(scriptSrc, callback) {
                    'use strict';
                    var element = document.createElement("script");
                    element.src = scriptSrc;
                    element.onload = function () {
                        callback();
                    };
                    document.body.appendChild(element);
                }
                // Add a script element as a child of the body
                function initJS() {
                    'use strict';
                    //load jQuery
                    //if browser is ie9 or older then returm document.addEventListener
                    if (document.all && !window.atob) {
                        return;
                    }
                    downloadJS("/js/gulp_corporate.min.uglify.js", function () {
                        var jqTimer = setInterval(function () {
                            if (window.jQuery) {
                                if (typeof loadAdditionalMinifiedJS == 'function') {
                                    loadAdditionalMinifiedJS();
                                }
                                if (isTouch) {
                                    if (($('#heroBanner').length) || ($('#rotators').length) || ($('#tweetsRotator').length) || ($('#mediaBanner').length)) {
                                        downloadJS("/js/hammer.min.js", function () {
                                            $('#heroBanner, #rotators, #tweetsRotator, #mediaBanner, #timeLine-indicators').hammer().on('swipeleft', function () {
                                                $(this).carousel('next');
                                            })
                                            $('#heroBanner, #rotators, #tweetsRotator, #mediaBanner, #timeLine-indicators').hammer().on('swiperight', function () {
                                                $(this).carousel('prev');
                                            })

                                        });
                                    }
                                }
                                $(function () {
                                    onPageReady();
                                    megaMenu();
                                    mainJs();
                                    //custom code goes here
                                    if (typeof (createChargePointMap) == "function") {
                                        createChargePointMap();
                                    }
                                });
                                clearInterval(jqTimer);
                            }
                        }, 50);
                    });
                }
                // Check for browser support of event handling capability
                if (window.addEventListener) {
                    window.addEventListener("load", initJS, false);
                } else if (window.attachEvent) {
                    window.attachEvent("onload", initJS);
                } else {
                    window.onload = initJS;
                }
                function onPageReady() {

                    $('.glyphicon-info-sign').tooltip();

                    $('input').iCheck({
                        checkboxClass: 'icheckbox_square-blue',
                        radioClass: 'iradio_square-blue',
                        aria: false,
                        increaseArea: '20%' // optional
                    });

                    var desktop = true;
                    var mobile = false;
                    enquire.register("screen and (min-width: 641px) and (orientation: landscape)", {
                        match: function () {
                            desktop = true;
                            mobile = false;
                            lazyLoad(desktop, mobile);
                        },
                        unmatch: function () {
                            desktop = false;
                        }
                    });
                    enquire.register("screen and (min-width: 641px) and (orientation: portrait)", {
                        match: function () {
                            desktop = true;
                            mobile = false;
                            lazyLoad(desktop, mobile);
                        },
                        unmatch: function () {
                            desktop = false;
                        }
                    });
                    enquire.register("screen and (max-width: 640px) and (orientation: portrait)", {
                        match: function () {
                            mobile = true;
                            desktop = false;
                            lazyLoad(desktop, mobile);
                        },
                        unmatch: function () {
                            mobile = false;
                        }
                    });
                    enquire.register("screen and (max-width: 640px) and (orientation: landscape)", {
                        match: function () {
                            mobile = true;
                            desktop = false;
                            lazyLoad(desktop, mobile);
                        },
                        unmatch: function () {
                            mobile = false;
                        }
                    });

                }

                function lazyLoad(desktop, mobile) {
                    $('img[data-src]').each(function () {//images that are not made responsive
                        var imgSrc = $(this).attr('data-src');
                        $(this).attr('src', imgSrc);

                    });
                    $('iframe[data-src]').each(function () {//iframes that are not made responsive
                        var imgSrc = $(this).attr('data-src');
                        $(this).attr('src', imgSrc);

                    });

                    if (desktop == true) {
                        $('img[data-src-desktop]').each(function () {
                            var imgSrc = $(this).attr('data-src-desktop');
                            $(this).attr('src', imgSrc);
                            bgw();
                        });
                    }
                    if (mobile == true) {
                        $('img[data-src-mobile]').each(function () {
                            var imgSrc = $(this).attr('data-src-mobile');
                            $(this).attr('src', imgSrc);
                            bgw();
                        });
                    }
                    //for home page banner only
                    var imageUrl = $('#HomeRotatorBg').attr('src');
                    $('#mediaBanner').css('background-image', 'url(' + imageUrl + ')');
                }


                if (document.all && !window.atob) {//for IE9
                    $('img[data-src-desktop]').each(function () {
                        var imgSrc = $(this).attr('data-src-desktop');
                        $(this).attr('src', imgSrc);
                    });
                    //for home page banner only
                    var imageUrl = $('#HomeRotatorBg').attr('src');
                    $('#mediaBanner').css('background-image', 'url(' + imageUrl + ')');
                }
            </script>
        
        
        
    

    

