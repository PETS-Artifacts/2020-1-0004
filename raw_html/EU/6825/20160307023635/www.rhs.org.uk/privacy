<script>
if ($(window).width() < 667 ) {
    $('a.more').on("click", function(event){
        event.preventDefault();
        $(this).next('.show-more').toggle();

           $(this).children('i').toggle();
      // $(this).sibblings().toggle();
    }); 
}
else{
    $('a.more').on("click", function(event){
        event.preventDefault();
    });
}
