			function toggleSkinRollOut($item,setstatus){
				
				/*
				ishover = 1|0
				isclick = 1|0
				isiframe = 1|0
				iframecontainer = #id
				iframesrc = URL
				pushdown = 1|0
				overlay = 1|0
				*/	
				
				var opening = false;
				var closing = false;
				var donothing = false;
				
				if($(".rodsinside").hasClass("staticopen")){
					donothing = true;
					$(".rodsinside").removeClass("staticopen");
				}
				
				if(donothing){
					// do nothing!
				}
				else {
				
					if(parseInt($item.data("ishover")) == 1){
						if($(".rodsinside").hasClass("open")){
							$(".rodsinside").removeClass("open staticopen");
							closing = true;
						}
						else {
							$(".rodsinside").addClass("open");
							opening = true;
						}
					}
					
					if(parseInt($item.data("isclick")) == 1){
						if($(".rodsinside").hasClass("open")){
							$(".rodsinside").removeClass("open staticopen");
							closing = false;
						}
						else {
							$(".rodsinside").addClass("open");
							opening = true;
						}
					}
				
				
					if(opening){
						if(parseInt($item.data("pushdown")) == 1){
							$(".rodsinside").addClass("pushdown");
							$(".rolloutdownskin").css({"height":parseInt($(".rolloutdownskin").data("open"))+"px"});
						}
						if(parseInt($item.data("overlay")) == 1){
							$(".rodsinside").addClass("overlay");
							$(".rolloutdownskin").css({"height":(parseInt($(".rolloutdownskin").data("open"))-142)+"px"});
						}
					}
					else {
						if(parseInt($item.data("pushdown")) == 1){
							$(".rodsinside").removeClass("pushdown");
						}
						if(parseInt($item.data("overlay")) == 1){
							$(".rodsinside").removeClass("overlay");
						}
						$(".rolloutdownskin").css({"height":$(".rolloutdownskin").data("closed")+"px"});
					}
					
					if(parseInt($item.data("isiframe")) == 1){
						if(opening){
							$("#"+$item.data("iframecontainer")).attr('src', $item.data("iframesrc"));
							$("#"+$item.data("iframecontainer")).show();
						}
						if(closing){
							$("#"+$item.data("iframecontainer")).hide();
							$("#"+$item.data("iframecontainer")).attr('src', ''); 
						}
					}
				}
				
				if(opening && !setstatus){
					$(".rodsinside").addClass("staticopen");
				}
			}
			
			

