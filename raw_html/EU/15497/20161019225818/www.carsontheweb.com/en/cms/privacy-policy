<script>
    function collapsefunction() {
        var a = $('#bs-example-navbar-collapse-1').attr('class');
        if (a == 'collapse navbar-collapse') {
            $('#bs-example-navbar-collapse-1').addClass('in');
        }
        else if (a == 'collapse navbar-collapse in') {
            $('#bs-example-navbar-collapse-1').removeClass('in');
        }
    }
    var isUserLoggedIn = 'False';
    var adBlockBannerTitle = 'You&#39;re missing our best offers!';
