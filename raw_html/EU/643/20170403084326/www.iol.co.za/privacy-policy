                  })();</script><script type="text/javascript" data-reactid=".1vdz4799mo0.1.16">function adPlaceholder_init() {
   setTimeout(function () {

      // check leaderboards
      $('.adLeaderboardAd').each(function () {
         if ($(this).is(':hidden')) {
            $(this).parent().parent('.adLeaderboardContainer').hide();
            var iolC = $(this).parent().parent('.adLeaderboardContainer').siblings().closest('.iolContainer');
            iolC.css('display', 'table');
            var iolI = iolC.children('.iolInner');
            iolI.css('display', 'table-cell');
         }
      });

      // check mpu 300
      $('.mputopRight').each(function () {
         if ($(this).not('#ad-mpu-topRight2').is(':hidden')) {
            $(this).parent('.mputopRightContainer').hide();
            $(this).parent('.mputopRightContainer').siblings().closest('.iolContainer250').show();
         }
      });

      // check mpu 600
      $('#ad-mpu600-right').each(function () {
         if ($(this).is(':hidden')) {
            $(this).parent('#mpu600Container').hide();
            $(this).parent('#mpu600Container').siblings().closest('.iolContainer600').show();
         }
      });
   }, 3000);
}

function mini_inline_final_start() {
   console_log('mini_inline_final_start');
   try {
      adPlaceholder_init();
   } catch (err) {
      console_warn(err);
   }
}

if (!ion.redirected) {
   //mini_inline_final_start();
}
