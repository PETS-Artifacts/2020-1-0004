   $(".layout1Container #popularBoxAdCards #popularMPU300").insertAfter(".layout1Container .layout1 .verticalToggleContent1 div.colLarge.span3.left:nth-child(2)");
}

function mini_headNavDropDown_init() {
   $('#verticalMore').click(function () {
      var target = $('#' + $(this).attr('id') + 'Toggle');
      accordion_Down($(this), target);
   });
   $('.headNavDropToggle').click(function () {
      var target = $('#' + $(this).attr('id') + 'Toggle');
      accordion_Right($(this), target);
   });
}

function businessFinanceSubNav_init() {
   var urlItem = $('#subNavDropDown > li > a').first().attr("href");
   if (urlItem === '/business-report/companies' || urlItem === '/personal-finance/my-money') {
      $('#subNavDropDown').css('display', 'none');
   } else {
      $('#subNavDropDown').css('display', 'inline');
   }
}

function scrollTopInit() {
   $(window).scroll(function () {
      if ($(window).scrollTop() > 400) {
         $('#scrollTopChevron').show();
      } else {
         $('#scrollTopChevron').hide();
      }
   });
   $('#scrollTopChevron').click(function () {
      $('html, body').animate({
         scrollTop: 0
      }, 500);
   });
}

function mini_inline_start() {
   console_log('mini_inline_initial_start', location.pathname, navigator.userAgent);
   try {
      if (ion.screenWidth < 768) {
         verticalLayoutToggle_init();
         mobileTvBoxAd_init();
         mobileHighlightsAd_init();
      }
      if (ion.screenWidth > 767) {
         businessFinanceSubNav_init();
      }
      headNavMenuToggle();
      mini_headNavDropDown_init();
      scrollTopInit();
      searchNav_init();
      mini_subNavDropDown_init();
      mini_headNavMenuDropDown_init();
      mini_mobileStickyAdClose();
      insterstitial_init();
      if (location.pathname.match('/movie-guide')) {
         mini_movieGuide_init();
      }
      if (location.pathname.match('/services-guide')) {
         mini_servicesGuide_init();
      }
      if (location.pathname.match('/tv-guide')) {
         mini_tvGuide_init();
      }
   } catch (err) {
      console_warn(err);
   }
}

if (!ion.redirected) {
   mini_inline_start();
}
</script><script src="//cdnjs.cloudflare.com/ajax/libs/es5-shim/4.1.0/es5-shim.js" data-reactid=".1vdz4799mo0.1.f"></script><script src="//cdnjs.cloudflare.com/ajax/libs/es5-shim/4.1.0/es5-sham.js" data-reactid=".1vdz4799mo0.1.g"></script><script src="/assets/vendor/modernizr-3.0.0.min.js" data-reactid=".1vdz4799mo0.1.h"></script><script src="//cdnjs.cloudflare.com/ajax/libs/lodash.js/3.10.1/lodash.min.js" data-reactid=".1vdz4799mo0.1.i"></script><script type="text/javascript" data-reactid=".1vdz4799mo0.1.j">function mini_dfp() {
   var tagName;
   var adPrefix = 'ad';
   var tagPrefixDesktop = '/175217832/IOL/';
   var tagPrefixMobile = '/175217832/MOBILERESPONSIVE/';
   var slotIds = [];
   var slots = {};
   var startTime = new Date().getTime();
   ion.deviceType = 'mobile';
   if (ion.screenWidth < 768) {
   } else if (ion.screenWidth >= 768 && ion.screenWidth <= 1024) {
      ion.deviceType = 'tablet';
   } else {
      ion.deviceType = 'desktop'
   }
   var tagPrefix = tagPrefixDesktop;
   if (ion.screenWidth < 768) {
      tagPrefix = tagPrefixMobile;
   }
   if (location.pathname === '/') {
      if (ion.screenWidth < 768) {
         tagName = 'top';
      } else {
         tagName = 'homepage'; //tagName = 'test';
      }
   } else if (location.pathname === '/news') {
      tagName = 'news/homepage';
   } else if (location.pathname.match('olympics-rio-2016')) {
      tagName = 'olympics';
   } else if (location.pathname.match('services-guide')) {
      if (location.search.split('?z=')[1]) {
         tagName = 'services-guide/' + location.search.split('?z=')[1].split('+').slice(0, 2).join('/');
      } else if (/[0-9]{5}$/.test(location.pathname)) {
         tagName = 'services-guide/' + location.pathname.split('/')[1] + '/' + location.pathname.split('/')[3];
      } else {
         tagName = location.pathname.split('/')[1];
      }
   } else {
      tagName = location.pathname.substring(1);
   }

   function getAdSize(adType) {
      if (adType === 'wallpaper') {
         return [1, 1];
      } else if (adType === 'mpu') {
         return [[300, 250]];
      } else if (adType === 'mpu600') {
         return [[300, 600]];
      } else if (adType === 'mpuLoot') {
         return [300, 300];
      } else if (adType === 'inarticle') {
         return [300, 250];
      } else if (adType === 'leaderboard') {
         if (ion.deviceType === 'desktop') {
            return [[728, 90], [970, 90], [970, 250]];
         } else if (ion.deviceType === 'tablet') {
            return [728, 90];
         } else if (ion.deviceType === 'mobile') {
            if (ion.screenWidth < 320) {
               return [[300, 50], [320, 50]];
            }
         }
      } else if (adType === 'interstitial') {
         return [2, 2];
      } else if (adType === 'pushdown') {
         return [970, 30];
      } else if (adType === 'sticky') {
         return [320, 50];
      }
      return [320, 50];
   }

   function wallpaperListener(isPresent) {
      if (window.ion.wallpaperListener) {
         window.ion.wallpaperListener(isPresent);
      } else {
         window.ion.wallpaperPresent = isPresent;
      }
   }

   function interstitialListener(isPresent) {
      if (window.ion.interstitialListener) {
         window.ion.interstitialListener(isPresent);
      } else {
         window.ion.interstitialPresent = isPresent;
      }
   }

   function adStatus(slotId, status) {
      try {
         window.ion.ads.status[slotId] = 'ended';
         var i = 0;
         for (i = 0; i < window.ion.ads.listeners.length; i++) {
            if (typeof window.ion.ads.listeners[i] === 'function') {
               window.ion.ads.listeners[i](slotId, status);
            } else {
               console_warn('adStatus', slotId, status);
            }
         }
      } catch (err) {
         console_warn('adStatus', slotId, err);
      }
   }

   function slotRenderEnded(event) {
      var duration = 0;
      var nowTime = new Date().getTime();
      duration = nowTime - startTime;
      var slotId = event.slot.getSlotId().getDomId();
      console_log('init slotRenderEnded', slotId, {isEmpty: event.isEmpty, size: event.size, duration: duration});
      adStatus(slotId, 'ended');
      if (/wallpaper/.test(slotId)) {
         if (event.isEmpty === false) {
            // $('html').addClass('no-touchevents');
            $('body').addClass('dfp-take-over-ad');
         } else if (event.isEmpty === true) {
            $('body').removeClass('dfp-take-over-ad');
            $('#ad-wallpaper').hide();
         }
      }
      if (/interstitial/.test(slotId)) {
         if (event.isEmpty === false) {
            $('.interstitial-center-container').show();
            // $('.interstitial-center-container').addClass('closebtn-box');
            // $('#closebutton').show();
            // $('#ad-interstitial').show();
         } else if (event.isEmpty === true) {
            $('.interstitial-center-container').hide();
            // $('.interstitial-center-container').removeClass('ad-interstitial-container');
            // $('.interstitial-center-container').removeClass('closebtn-box');
            // $('#closebutton').hide();
            // $('#ad-interstitial').hide();
         }
      }
      if (/pushdown/.test(slotId)) {
         if (event.isEmpty === false) {
            $('#ad-pushdown').css('display', 'block');
            if ($('.dfp-take-over-ad').length !== 0) {
               $('#rightAsideColumn').css('margin-top', '31px');
            }
         } else if (event.isEmpty === true) {
            $('#ad-pushdown').css('display', 'none');
         }
      } else if (event.isEmpty) {
         if (slotId === 'ad-leaderboard-sticky') {
            $('#adLeaderboardStickyContainer').show();
         } else if (slotId.startsWith('ad-mpu600')) {
            $('#mpu600Container').show();
            adStatus(slotId, 'present');
         } else if (slotId.startsWith('ad-mpu-topRight')) {
            $('.mputopRightContainer').show();
         }
      } else if (event.size && event.size.length === 2) {
         var element = document.getElementById(slotId);
         if (!element) {
            console_warn('dfp element empty', slotId);
         } else {
            if (slotId.startsWith('ad-leaderboard-top') || slotId.startsWith('ad-leaderboard-bot')) {
               $('.adLeaderboardContainer').css('display', 'table');
               if (ion.screenWidth <= 980) {
                  $('#top1LeaderContainer').hide();
               }
            } else if (slotId.startsWith('ad-mpu600')) {
               $('#mpu600Container').show();
               adStatus(slotId, 'present');
            } else if (slotId.startsWith('ad-mpu-topRight')) {
               $('.mputopRightContainer').show();
            } else if (slotId === 'ad-leaderboard-sticky') {
               $('#adLeaderboardStickyContainer').show();
            }
            element.style.width = event.size[0];
            element.style.height = event.size[1];
         }
      }
   }

   function defineSlot(adType, position, target) {
      target = target || position; // e.g. bot vs bottom1
      var adSize = getAdSize(adType);
      var tag = tagPrefix + tagName;
      var slotId;
      if (position) {
         slotId = [adPrefix, adType, position].join('-');
      } else if (adType === 'wallpaper') {
         slotId = [adPrefix, adType].join('-');
      } else {
         slotId = [adPrefix, adType].join('-');
      }
      console_state('defineSlot', slotId, tag, adSize);
      if (adSize[0] > 1) {
         var element = document.getElementById(slotId);
         if (element) {
            element.style.width = adSize[0];
            element.style.height = adSize[1];
         }
      }
      var slot = googletag.defineSlot(tag, adSize, slotId);
      if (!slot) {
         console_warn('defineSlot empty', tag, adSize, slotId);
      } else {
         slot.addService(googletag.pubads());
         if (target) {
            slot.setTargeting(adType, target);
         } else if (position) {
            slot.setTargeting(adType, position);
         }
         slotIds.push(slotId);
         slots[slotId] = slot;
         console_log('defineSlot targetting', adType, target, slotId);
      }
      return slotId;
   }

   if (!window.hasOwnProperty('googletag')) {
      googletag = {cmd: []};
   }
   googletag.cmd.push(function () {
      console_log('dfp', googletag.apiReady);
      try {
         if (false) {
            var wallpaperElement = $('#ad-wallpaper').detach();
            $('body').prepend(wallpaperElement);
         }
         googletag.pubads().enableAsyncRendering();
         googletag.pubads().enableSingleRequest();
         googletag.pubads().collapseEmptyDivs(true);
         googletag.pubads().addEventListener('slotRenderEnded', slotRenderEnded);
         googletag.enableServices();
         // For table of what ads are placed where, see https://docs.google.com/a/saratoga.co.za/spreadsheets/d/1__8TIpt7uGyjsMXcMFL6axVXN5fX7oa7HLR-JOpqFbQ/edit?usp=sharing
         if (ion.screenWidth >= 1024) {         // Desktop wide screen
            defineSlot('wallpaper');
         }
         if (ion.screenWidth >= 970) {          // Desktop
            defineSlot('leaderboard', 'top');
            defineSlot('pushdown');
            defineSlot('interstitial');
            interstitialListener(false);
            if (!(/[0-9][0-9]{4}$/.test(location.pathname))) {
               defineSlot('leaderboard', 'top1');
            }
         }
         if (ion.screenWidth >= 768) {          // Tablet landscape
            defineSlot('leaderboard', 'bot');
            defineSlot('mpu', 'topRight');
            if (location.pathname === '/') {
               defineSlot('leaderboard', 'top1');
               defineSlot('leaderboard', 'top2');
               defineSlot('mpu', 'topRight1');
            }
            if (ion.screenWidth < 970) {
               defineSlot('mpu', 'topRight2');
            }
            if (/[0-9][0-9]{4}$/.test(location.pathname)) {
               defineSlot('mpuLoot', 'middle');
               defineSlot('inarticle', 'middle');
               defineSlot('mpu600', 'right');
            }
         } else {                               // Mobile
            defineSlot('mpu', 'topRight');
            defineSlot('leaderboard', 'sticky');
            if (location.pathname === '/') {
               defineSlot('mpu', 'topRight1');
            }
            if (/[0-9][0-9]{4}$/.test(location.pathname)) {
               defineSlot('inarticle', 'middle');
               defineSlot('mpuLoot', 'middle');
               defineSlot('mpu600', 'right');
            } else {
               defineSlot('mpu', 'topRight2');
            }
         }
         var i;
         for (i = 0; i < slotIds.length; i++) {
            googletag.display(slotIds[i]);
         }
      } catch (err) {
         console_warn(err);
      }
   });
}

function mini_inline_article_ad() {
   if (window.ion.articleId && ($("#article-more-body").find("p").length > 6) && ($("#article-more-body").find("img").length < 1)) {
      $("<div id='ad-inarticle-middle-container'><div id='ad-inarticle-middle' style='margin:0 auto;'/></div></div>").insertAfter("#article-more-body > p:nth-child(2)");
   }
   document.addEventListener("ctxAdsLoaded", function (e) {
      document.getElementById('ad-inarticle-middle-container').style.display = "none";
   });
}

function mini_dfp_start() {
   console_log('mini_dfp_start', location.pathname, navigator.userAgent);
   try {
      mini_inline_article_ad();
      mini_dfp();
   } catch (err) {
      console_warn(err);
   }
}

