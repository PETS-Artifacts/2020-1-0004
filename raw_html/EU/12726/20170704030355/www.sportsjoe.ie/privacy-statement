        var mapTopPage = googletag.sizeMapping()
            .addSize([980, 200], [[728, 90], [970, 250], [970, 90]])
            .build();

            // If we're on an article page, different set of targeting - ignore mobile
    var mapTopPage = googletag.sizeMapping()
        .addSize([758, 200], [728, 90])
        .addSize([980, 200], [[728, 90], [970, 250], [970, 90]])
        .build();


    if (window.matchMedia("only screen and (min-width: 990px)").matches) {
        //Post Top Mapping
        var mapPostTop = googletag.sizeMapping()
            .addSize([980, 200], [300, 250])
            .build();

        googletag.placement.article_top = googletag.defineSlot('/247211819/sportsjoe_article_top', [1, 1], 'div-gpt-article_top')
            .defineSizeMapping(mapPostTop)
            .addService(googletag.pubads());
    }

        
            googletag.placement.article_inline_video = googletag.defineOutOfPageSlot('/247211819/sportsjoe_article_inline_video', 'div-gpt-article_inline_video')
            .addService(googletag.pubads());
    
        
                    googletag.placement.top_page = googletag.defineSlot('/247211819/sportsjoe_top_page', [1, 1], 'div-gpt-top_page')
                .defineSizeMapping(mapTopPage)
                .addService(googletag.pubads());
        
        googletag.defineOutOfPageSlot('/247211819/sportsjoe_oop', 'div-gpt-oop')
            .addService(googletag.pubads());

