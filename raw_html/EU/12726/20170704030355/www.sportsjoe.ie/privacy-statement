        googletag.pubads().enableSingleRequest();
        // This will unlock the scroll for the doc once the top banner is rendered
        googletag.pubads().addEventListener('slotRenderEnded', function(event) {renderCallback(event);});
        googletag.enableServices();

                var renderCallback = function(event) {
            if (googletag.placement.top_page == event.slot) {
                setTimeout(function(){
                    if ($('html, body').hasClass('noscroll')) {
                        $('body,html').removeClass('noscroll');
                    }
                }, 1700);
                //Set Height to sticky spacer on top ad
                if ($('.visible-md').is(':visible') || $('.visible-lg').is(':visible') && _maximummedia.sitemode != 'joecouk') {
                    var topAdHeight = $('#div-gpt-top_page').height();
                    $('.sticky-spacer').css('height', (topAdHeight + 20));
                }
            } else if (typeof event.slot.l !== "undefined"
                && typeof event.slot.l._qqid_ !== "undefined"
                && event.isEmpty) {
                // Lazy loaded slot. If it's an empty banner, hide the ad strapline
                $('.div-gpt-article-middle-holder[data-google-query-id="'+event.slot.l._qqid_+'"]').parent().remove();
            }

            if (_maximummedia.sitemode != 'joecouk' && ($('.visible-md').is(':visible') || $('.visible-lg').is(':visible'))) {
                setSticktyTopAd();
            }
        }
    });

    function setSticktyTopAd ()
    {
        //Sticky Top Ad on desktop
        if ($('.visible-md').is(':visible') || $('.visible-lg').is(':visible')) {
            if (!$('.sticky-spacer').hasClass('no-sticky')) {
                $("#div-gpt-top_page").ready(function () {
                    $(".top-ad").stick_in_parent({
                        parent: ".main-wrapper",
                        offset_top: 90,
                        spacer: '.sticky-spacer',
                    });
                });
            }
        }
        // Control stick property of div
        $(document).ready(function () {
            if ($("#div-gpt-top_page").length > 0 && !$('.sticky-spacer').hasClass('no-sticky')) {
                $( window ).bind( "scroll", handler );
            }
        });
    }
    var scrollPosition;
    var lastPosition;
    var limit = 0;
    var handler = function() {
        if (limit == 0) {
            limit = getScrollLimit();
        }
        scrollPosition = document.documentElement.scrollTop || document.body.scrollTop;
        if (scrollPosition == 0 && lastPosition > limit) {
              $(".top-ad").css('position', 'fixed');
                $(".top-ad").css('top', '90px');
                $('#top-ad-desktop').addClass('is_stuck');
        }
        if (scrollPosition > limit) {
            if ($('#top-ad-desktop').hasClass('is_stuck')) {
                $('#top-ad-desktop').removeClass('is_stuck');
                $(".top-ad").css('position', 'absolute');
                $(".top-ad").css('top', limit-1);
            }
        } else if (scrollPosition < limit && scrollPosition > 2) {
            if (!$('#top-ad-desktop').hasClass('is_stuck')) {
                $(".top-ad").css('position', 'fixed');
                $(".top-ad").css('top', '90px');
                $('#top-ad-desktop').addClass('is_stuck');
            }
        }
        lastPosition = scrollPosition;
    };

    /**
     * This Function return the limit of top ad to get sticky
     *
     * @return  int Limit
     */
    function getScrollLimit()
    {
        let topAdbottom = $('.sticky-spacer').offset().top + $('.sticky-spacer').outerHeight(true);
        let divSelector = '.hidden-xs .col-md-4 > div:eq(1)';
        if ($('#article-aside').is(':visible')) {
            divSelector = '#article-aside > div:eq(1)';
        } else if ($(".featured-story.missing-holder .featured-story-title.uppercase").hasClass('limit-top-ad')) {
            divSelector = '.content-container .col-md-4 .section-header.top-space.uppercase';
        } else if ($('.featured-story-headline.uppercase.headline').hasClass('limit-top-ad')) {
            divSelector = '.section-header.top-space.uppercase';
        } else if ($('.main-wrapper .col-md-4 > div:eq(1)').hasClass('limit-top-ad')) {
            divSelector = '.limit-top-ad';
        }
        let sidebarAdTop = $(divSelector).offset().top - parseInt($(divSelector).css('margin-top'));
        // Plus 5 to give a bit of distance more
        return sidebarAdTop - topAdbottom + 5;
    }

</script>
 
