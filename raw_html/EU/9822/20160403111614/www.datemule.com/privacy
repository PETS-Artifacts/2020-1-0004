<script>
function ajaxGetRequestModal(title, rurl){
	$("#modal_body").html('<img style="display: block; margin-left: auto; margin-right: auto;" src="http://www.datemule.com/img/loading.gif"/>');
	$("#modal_title").html(title);
	$.ajax(
	{
		url : rurl,
		type: "GET",
		success:function(data, textStatus, jqXHR) 
		{
			$("#modal_body").hide().html(data).fadeIn("slow");
		},
		error: function(jqXHR, textStatus, errorThrown) 
		{
			$("#modal_body").hide().html('Unexpected Error').fadeIn("slow");
		}
	});
}
</script>
<script>
function editFormAjax(asset){
		var postData = $("#editFormAjax_"+asset).serialize();
		var formURL = $("#editFormAjax_"+asset).attr("action");
		var codeForm = $("#editFormAjax_"+asset).attr("codeForm");
	
		$("#modal_body").html('<img style="display: block; margin-left: auto; margin-right: auto;" src="http://www.datemule.com/img/loading.gif"/>');
	
		$.ajax(
		{
			url : formURL,
			type: "POST",
			data : postData,
			success:function(data, textStatus, jqXHR) 
			{
				$("#modal_body").hide().html(data).fadeIn("slow");
			},
			error: function(jqXHR, textStatus, errorThrown) 
			{
				$("#modal_body").hide().html('Unexpected Error').fadeIn("slow");
			}
		});
}
</script>
<script>
function insertSymbol(asset, symbol){
	 $("#"+asset).html(symbol);
}


</script>
<script>
function ActionRequest(title, content, url, hide){
		$("#unfollow_processing").html('<img style="width:15px;" src="http://www.datemule.com/img/loading.gif"/>');
		
		$("#modal").modal("hide");
		$("#modal_title").html(title);
		$.ajax(
		{
			url : url,
			type: "GET",
			success:function(data, textStatus, jqXHR) 
			{
			  var json = JSON.parse(data);
			  var status = json['status'];
			  var message = json['message'];
		  
			  if(status == 'success'){
				$("#modal_body").hide().html('<span style="color:green;" class="glyphicon glyphicon-ok-circle"></span>&nbsp;&nbsp;&nbsp;'+message).fadeIn("slow");
				$("#modal").modal("show");
				$("#processing_action_"+content).html('<span style="color:green;" class="glyphicon glyphicon-saved"></span>');
				if (hide == 'hide') {
				$("#hide_"+content).hide();
				}
			  } else {
				$("#modal_body").hide().html('<span style="color:red;" class="glyphicon glyphicon-info-sign"></span>&nbsp;&nbsp;&nbsp;'+message).fadeIn("slow");
				$("#modal").modal("show");
				$("#processing_action_"+content).html('');
			  }   
		
			},
			error: function(jqXHR, textStatus, errorThrown) 
			{
				$("#modal_body").hide().html('<span style="color:red;" class="glyphicon glyphicon-info-sign></span>&nbsp;&nbsp;&nbsp;Unexpected error').fadeIn("slow");
				$("#modal").modal("show");
				$("#processing_action_"+content).html('');
			}
		});
}

$( document ).ready(function() {
	$('.bannerAd').each(function( index ) {
		var widthd 		= $(this).width();
		var that 		= this;
		var ad_pos 		= $(this).attr('data-pos');
		var ad_shape 	= $(this).attr('data-shape');
		if ($(this).is(':visible')){	
			$.post( "http://www.datemule.com/bannerAd", { width: widthd, position: ad_pos, shape: ad_shape }, function( data ) {
				$(that).html(data);
			})
		}
  	});
});

</script>
<script language="javascript" type="text/javascript">
  function resizeIframe(obj) {
    obj.style.height = obj.contentWindow.document.body.scrollHeight + 'px';
  }
</script>
 
