  <script>
    EDRSurvey.noths = {
      hasTaken: null
    };
    EDRSurvey.Events.attach('layerShown', function(options){
      EDRSurvey.noths.hasTaken = false;
      dataLayer.push({'event': "edigital_survey_shown"});
    });
    EDRSurvey.Events.attach('layerExecuted', function(event){
      EDRSurvey.noths.hasTaken = true;
      dataLayer.push({'event': "edigital_survey_accepted"});
    });
    EDRSurvey.Events.attach('layerDestroyed', function(layer){
      if (EDRSurvey && EDRSurvey.noths && (EDRSurvey.noths.hasTaken === false)) {
        dataLayer.push({'event': "edigital_survey_declined"});
      }
    });
  </script>
