    <script type="text/javascript">
  var NOTHS = NOTHS || {};

  NOTHS.FeatureDetection = (function() {
      function cssTransitions() {
          var style = document.documentElement.style;
          var property = 'transition';
          var vendorPrefix = ['Moz', 'Webkit', 'Khtml', 'O', 'ms'];

          if (typeof style[property] == 'string') return true;

          // Tests for vendor specific prop
          property = property.charAt(0).toUpperCase() + property.substr(1);

          for(var i = 0; i < vendorPrefix.length; i++) {
            if (typeof style[vendorPrefix[i] + property] == 'string') return true;
          }

          return false;
      }
      return {
          touch:                !!navigator.userAgent.match(/ipad|iphone|ipod|android|iemobile|blackberry/i),
          inputPlaceholderText: 'placeholder' in document.createElement('input'),
          autoPlayVideo:        !navigator.userAgent.match(/ipad|iphone|ipod|android|iemobile|blackberry/i),
          cssTransitions:       cssTransitions()
      }
  })();

  document.body.className = document.body.className.replace('nojs','js');

  if (NOTHS.FeatureDetection.touch) {
      document.body.className = document.body.className.replace('no_touch', 'has_touch');
  }
</script>


