<script>
	var pushCustomAttributeToOptimizelyX = function(attributeName, attributeValue) {
		var jsonAttribute = {};
		jsonAttribute[attributeName] = attributeValue;

		window['optimizely'] = window['optimizely'] || [];
		window["optimizely"].push({
			"type": "user",
			"attributes": jsonAttribute
		});
	};

	window['optimizely'] = window['optimizely'] || []; 
	(function(){
		var meta_userid = document.getElementById('meta-userid');
		var userid = meta_userid ? meta_userid.getAttribute('data-userid') : null;
		if (userid){
			window['optimizely'].push(["customTag", "User ID", userid]); 
			pushCustomAttributeToOptimizelyX("user_id", userid);

			window['optimizely'].push(["customTag", "User status", "Logged in"]); 
			if ("" === "true") {
				pushCustomAttributeToOptimizelyX("user_status", "Semi logged in");
			} else {
				pushCustomAttributeToOptimizelyX("user_status", "Fully logged in");
			}
		} else {
			window['optimizely'].push(["customTag", "User status", "Logged out"]); 
			pushCustomAttributeToOptimizelyX("user_status", "Logged out");
		}
	})();

	window['optimizely'].push(["customTag", "Open Site Affiliate", "No"]); 
	pushCustomAttributeToOptimizelyX("open_site_affiliate", "No");

	pushCustomAttributeToOptimizelyX("device_type", "WEB");
</script>












