        var SE = SE || {};

        SE.mediaLinkPoll = function(count) {
            var count = count || 0;

            if ($ !== undefined && SE.initMediaLinkInHeader !== undefined) {
                SE.initMediaLinkInHeader();
            }
            else {
                if (count < 10) {
                    setTimeout(function() { SE.mediaLinkPoll(count + 1) }, 50);
                }
            }
        };
    </script>
    

