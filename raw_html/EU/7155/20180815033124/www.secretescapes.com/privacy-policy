				<script>
					(function(){
						var meta_userid = document.getElementById('meta-userid');
						var userid = meta_userid ? meta_userid.getAttribute('data-userid') : null;
						var intUserid = parseInt(userid, 10);
						if (intUserid) {
							var optimizely = window['optimizely'];
							var bucketNumber = intUserid % 3;
							console.log("bucket number: " + bucketNumber);
