        function oldTrident() {
            var rv = -1;
            var ua = navigator.userAgent;
            var re = new RegExp("Trident\/([0-9]{1,}[\.0-9]{0,})");
            if (re.exec(ua) != null) {
                rv = parseFloat(RegExp.$1);
            }

            // Trident 4.0 is the rendering engine of Internet Explorer 8

            // If the visitor is using a version of Trident older than 4,
            // we should prompt them to upgrade. If they are using 4+ or
            // not using Trident at all, no problem.

            return (rv < 4);
        }

        if (oldTrident()) {
            // Create the content script element
            var head = document.getElementsByTagName('head')[0];
            var contentScript = document.createElement('script');
            contentScript.type = "text/javascript";
            contentScript.src = "/_script/cc.browser-upgrade.js";

            head.appendChild(contentScript);
        }
    </script>

