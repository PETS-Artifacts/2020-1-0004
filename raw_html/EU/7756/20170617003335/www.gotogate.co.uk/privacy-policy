  function whenGlobalsReady(func) {
    if (__globalsReady) {
      func();
    } else {
      __callbacksToRunAfterGlobalsReady.push(func);
    }
  }

  function beforeGlobalsReady(func) {
    if (__globalsReady) {
      func();
    } else {
      __callbacksToRunBeforeGlobalsReady.push(func);
    }
  }

