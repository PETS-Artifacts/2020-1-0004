        <script type="text/javascript">

  var __popupContentCanBeDisplayed_13933 = true;

  

  $(function() {
    whenGlobalsReady(function() {
      require([
          "jquery"
        ],
        function($) {

          

          function openInPopup() {
            if (__popupContentCanBeDisplayed_13933) {
              openPopupWindow('/popup.action?componentId=13933', 600, 800);
            }
          }

          function openInPopunder() {
            if (__popupContentCanBeDisplayed_13933) {
              IbeLocalStore.set("SessionStillActive", false);
              openInPopUnderWindow('/popup.action?componentId=13933');
            }
          }

          function openInPopUnderWindow(url) {
            IbeLocalStore.remove("ClosePopUnder");
            IbeLocalStore.remove("PopUnderUrl");
            IbeLocalStore.set("PopUnderUrl", url);
          }

          function broadCastSessionStillActive() {
            IbeLocalStore.set("SessionStillActive", true);
            IbeLocalStore.set("PopUnderUrl", "");
          }

          function preventInPageOnLeave(ev, triggerOriginEventName) {
            if (!triggerOriginEventName) __popupContentCanBeDisplayed_13933 = false;
            setTimeout(function() {
              __popupContentCanBeDisplayed_13933 = true;
            }, 500);
          }

          
          broadCastSessionStillActive();

          $(document.body).click(preventInPageOnLeave);

          $(window).on("popstate", function(ev) {
            __popupContentCanBeDisplayed_13933 = false;
          });

          
          var popunderFunction = function() {
            if (!$("body").hasClass("agent-msie-8")) {
              $(this).attr('target', '_blank');
              $(window).bind("blur", function() {
                setTimeout(function(w) {
                  goToUrl("/popup.action?componentId=13933");
                }, 2000);
              });
            }
          };
          $('#airSubmitButtonId').parents("form").bind("submit", popunderFunction);
          
        });
    });
  });

