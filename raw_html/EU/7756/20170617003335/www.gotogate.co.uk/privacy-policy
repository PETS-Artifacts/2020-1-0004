var get = window.location.search.indexOf('chat=true');
if(get!==-1) {
  var script  = document.createElement('script');
  script.type = 'text/javascript';
  script.src  = '//www.gotochat.se/livechat/php/app.php?widget-init.js';
  $('body').append(script);

var sChatTimeout =  setInterval(function(){
     
     
    if($('#customer-chat-iframe').length) {         
        clearInterval(sChatTimeout);
         // Execute custom code when a visitor logs into the chat
        phpLiveChat.onloginsuccess = function() {
          localStorage.setItem('openedChat','true');
          dataLayer.push({"event": "chatUsed", "chatUsed":"true"});
        };
      
        setTimeout(function(){
            if($('#customer-chat-iframe').is(':visible')) {
              dataLayer.push({"event": "chatOpen", "chatOpen":"true"});
            }
        },4000);



    }
     
     
},500);

}

