<script type="text/javascript">
  function ensureMainSearchFormIs(doExpand) {
    $('.mainSearchForm > .header, .mainSearchForm > .headerRight').each(function(index) {
      var c = $(this).parent().find('.componentExpandCollapse');
      if (doExpand) {
        c.parent().parent().find(".expandable").show();
        if (!c.hasClass("expand")) {
          c.removeClass("collapse").addClass("expand");
        }
      } else {
        c.parent().parent().find(".expandable").hide();
        if (c.hasClass("expand")) {
          c.removeClass("expand").addClass("collapse");
        }
      }
    });
  }
</script>
