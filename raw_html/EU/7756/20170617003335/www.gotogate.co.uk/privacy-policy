<script>
$( document ).ready(function() {
    try {
		if( dataLayer[0].salesPhase === "Result" && dataLayer[0].applicationPath != "/transition.air" && $('#DEFAULT_PAGE_LEFT').length > 0 ){
			if( $('#DEFAULT_PAGE_LEFT .uwtTrust250Wrap').length === 0 ){
				$('.uwtTrust250Wrap').prependTo('#DEFAULT_PAGE_LEFT');
				$('.uwtTrust250Wrap').css('display','block');
			}
		}
		else if( dataLayer[0].salesPhase === "Book" && $('#DEFAULT_PAGE_RIGHT').length > 0 ){
			if( $('#DEFAULT_PAGE_RIGHT .uwtTrust250Wrap').length === 0 ){
				$('.uwtTrust250Wrap').appendTo('#DEFAULT_PAGE_RIGHT');
				$('.uwtTrust250Wrap').css('display','block');
			}
		}
		else if( dataLayer[0].salesPhase === "ShoppingBasket" && $('#DEFAULT_PAGE_RIGHT').length > 0 ){
			if( $('#DEFAULT_PAGE_RIGHT .uwtTrust250Wrap').length === 0 ){
				$('.uwtTrust250Wrap').appendTo('#DEFAULT_PAGE_RIGHT');
				$('.uwtTrust250Wrap').css('display','block');
			}
		}
		else if( dataLayer[0].salesPhase === "Payment" && $('#DEFAULT_PAGE_RIGHT').length > 0 ){
			
				$('.uwtTrust250Wrap').appendTo('#DEFAULT_PAGE_RIGHT');
				$('.uwtTrust250Wrap').css('display','block');
			
		}else{

		}
	}
	catch(e) {
		console.log(e);
    }
});
</script>
