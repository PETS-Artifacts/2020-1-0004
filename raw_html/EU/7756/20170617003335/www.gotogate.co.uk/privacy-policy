<script>
if(dataLayer[0].salesPhase === "Book"){
	var uwtOptInCheckTimeout =  setInterval(function(){
	if($('.travellerInputFormContainer').length && $('.travellerInputFormContainer').html().length > 0) {
			clearInterval(uwtOptInCheckTimeout);
			document.cookie = "uwtOptIn=true; expires=Thu, 18 Dec 2099 12:00:00 UTC; path=/";
			$('.newsletterCheckbox').on('click',function(){
				setTimeout(function(){
					if($('.newsletterCheckbox').hasClass('fa-check-square-o')) {
						//console.log('false');
						//sessionStorage.setItem('uwtOptIn', 'true');
						document.cookie = "uwtOptIn=false; expires=Thu, 18 Dec 2099 12:00:00 UTC; path=/";
					}
					else {
						//console.log('true');
						//sessionStorage.setItem('uwtOptIn', 'false');
						document.cookie = "uwtOptIn=true; expires=Thu, 18 Dec 2099 12:00:00 UTC; path=/";
					}
					},250);		
			});
		
		
			$('.newsletter__checkbox').on('click',function(){
				setTimeout(function(){
					if( $('.newsletter__checkbox').attr( 'value' ) === "true" ) {
						//console.log('false');
						//sessionStorage.setItem('uwtOptIn', 'true');
						document.cookie = "uwtOptIn=false; expires=Thu, 18 Dec 2099 12:00:00 UTC; path=/";
