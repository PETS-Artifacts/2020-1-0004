$( document ).ready(function() {
    try {
		if( dataLayer[0].salesPhase === "Book" ){
			var extraProductsTimeout =  setInterval(function(){    
			if($('.rebookingContainer').length && $('.rebookingContainer').html().length > 0) {
					clearInterval(extraProductsTimeout);
					$(".rebookingContainer").find(".texts").on("click", function(){
						return false;
					});   
				}   
			},500);
		}
	}
    catch(e) {
    console.log(e);
    }
});
	
