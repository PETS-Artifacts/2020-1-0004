				var uwtsPageURL = decodeURIComponent(window.location.search.substring(1)),
					uwtsURLVariables = uwtsPageURL.split('&'),
					uwtsParameterName,
					i;

				for (i = 0; i < uwtsURLVariables.length; i++) {
					uwtsParameterName = uwtsURLVariables[i].split('=');

					if (uwtsParameterName[0] === uwtsParam) {
						return uwtsParameterName[1] === undefined ? true : uwtsParameterName[1];
					}
				}
