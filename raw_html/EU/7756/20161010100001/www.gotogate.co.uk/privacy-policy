<script>

function uwtUpdateOpener() {
    if (window.opener) {
        window.opener.document.popunder_12208 = window;
	}
}
var uwtUOInterval = setInterval(function() {
	try {
		uwtUpdateOpener();
	}
	catch(e) {
		//console.log(e);
		clearInterval(uwtUOInterval);
	}
},500);

function uwtGetURLParameter(name) {
  return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null;
}

try {
	var uwtNSMG			= $("meta[name='NSMG']").attr('content');
	
	localStorage.setItem('uwtSalesPhase_'+uwtNSMG,dataLayer[0].salesPhase);
	
	var applicationPath = dataLayer[0].applicationPath;
	if(applicationPath==='/transition.air') {	
		var uwtSelectionKey = uwtGetURLParameter('selectionKey');		
		localStorage.setItem('selectionKey_'+uwtNSMG,uwtSelectionKey);
	}
}
catch(e) {
}

function uwtSetCookie(cname, cvalue, exdays) {
	var d = new Date();
	d.setTime(d.getTime() + (exdays*24*60*60*1000));
	var expires = "expires="+d.toUTCString();
	document.cookie = cname + "=" + cvalue + "; " + expires;
}

if(dataLayer[0].salesPhase==='Result' && dataLayer[0].applicationPath!='/transition.air') {
	if($("link[rel='canonical']")) {
		var uwtNSMG = $("meta[name='NSMG']").attr('content');
		uwtSetCookie(uwtNSMG+'_SearchUrl', $("link[rel='canonical']").attr('href'), 1);
	}
}

var uwtSalesPhase = dataLayer[0].salesPhase;
if(uwtSalesPhase==='Receipt' || uwtSalesPhase==='Order') {
	var uwtNSMG = $("meta[name='NSMG']").attr('content');
	$.getJSON(
	  '//www.supersavertravel.se/ext-apps/popunder/confirm/?NSMG='+uwtNSMG,
	  function(r){		
	  }
	);
}

try {
	var uwtNSMG = $("meta[name='NSMG']").attr('content');
	$.post(
	  '//www.supersavertravel.se/ext-apps/popunder/register/salesPhase/?NSMG='+uwtNSMG+'&salesPhase='+dataLayer[0].salesPhase,
	  function(r){
	  }
	);
}
catch(e) {
}


/******/





try {
	if(dataLayer[0].salesPhase === 'Result'){
		var airResultListTimeout =  setInterval(function(){
				  
			if($('#airResultContentWrapper #airResultContent #airResultListPlaceHolder').length && $('#airResultContentWrapper #airResultContent #airResultListPlaceHolder').html().length > 0) {
			
				clearInterval(airResultListTimeout); 
			
				var uwtNSMG             = $("meta[name='NSMG']").attr('content');
				var popunderHasBeenUsed = localStorage.getItem('popunderHasBeenUsed_'+uwtNSMG);
				if(!popunderHasBeenUsed) {    
					$(document).ready(function() {
						var activePopunder = localStorage.getItem('activePopunder');
						if(activePopunder!==location.host) {
							// Check if meta search + allowed partner
							if(dataLayer[0].entryType==='metasearch') {    
								// Selected is special
								$('#airResultContentWrapper #tripHighlightContainer .b-button').on('click',function(){
									
									try {
										var tripId = airResultApplicationModel.tripHighlightManagerArgs.model.selectedTripIds[0];                                
										for(var uwtTripId in tripList__0) {
											var uwtSelOutBounds    = $('.outBounds input[type="radio"]:checked').attr('id');
											var uwtSelInBounds    = $('.inBounds input[type="radio"]:checked').attr('id');
											var uwtOutBounds     = tripList__0[uwtTripId ].bounds[0].id;
											var uwtInBounds      = tripList__0[uwtTripId ].bounds[1].id;    
											if(uwtOutBounds == uwtSelOutBounds && uwtInBounds == uwtSelInBounds) {
												tripId = tripList__0[uwtTripId].tripId;
												break;
											}
										}
										var uwtAirSelectUrl = airResultApplicationModel.pageManagerArgs.airSelectUrl;    
											uwtAirSelectUrl = uwtAirSelectUrl.substr(1);                                
										var uwtPopunder = window.open(uwtAirSelectUrl+'?sss.k=%24%7Bsss.k%7D&sss.p=%24%7Bsss.p%7D&tripId='+tripId);
											//uwtPopunder.focus();
											
										location.href='/popup.action?componentId=13933';

										return false;
										
									}
									catch(e) {
										console.log(e);
									}
								});

								$('#airResultContentWrapper .tripItemBookButton .b-button').on('click', function(){
									var tripId = $(this).parent().attr('id').split('_')[1];
									var uwtAirSelectUrl = airResultApplicationModel.pageManagerArgs.airSelectUrl;    
										uwtAirSelectUrl = uwtAirSelectUrl.substr(1);    
									var uwtPopunder = window.open(uwtAirSelectUrl+'?sss.k=%24%7Bsss.k%7D&sss.p=%24%7Bsss.p%7D&tripId='+tripId, 'uwtPopup');
										uwtPopunder.focus();
									
									location.href='/popup.action?componentId=13933';

									return false;
								});    
								
							}        
						};   
					});
				}
				 
						
			}
			
		},500);
