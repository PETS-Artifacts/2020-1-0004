  <script type="text/javascript">

    require(
      [
        "Class",
        "jquery"
      ],
      function(Class, $) {

        $(function() {

          var SiteSearchManager = Class.extend(
            {
              init : function(args) {
                this.rpcPath = '/api.json.resource.search.action?q=';
                this.component = new IBEWebComponent({
                                                       id : "rsResults",
                                                       view : "rsView",
                                                       placeHolderId : "rsPlaceHolder"
                                                     });
              },
              search : function(args) {
                args = $.extend({}, args);
                var that = this;

                function displayNoResultsMessage() {
                  args.responseCallback([
                                          {label : ''}
                                        ]);
                }

                sendRequest(that.rpcPath + args.requestParameterValue, function(response) {
                  response.model ?
                    args.responseCallback(that.getResponse({model : response.model})) : displayNoResultsMessage();
                }, function(response, o, error) {
                  displayNoResultsMessage();
                });
              },
              getResponse : function(args) {
                args = $.extend({}, args);
                var that = this, model = args.model, results = [];
                that.component.setModel(model);
                that.component.render();

                results.push({label : '<div class="searchResultHeader">' + model.title + '</div>', value : 'title'});
                $.each($(that.component.getResultAsHtmlNode()).find('.searchResult'), function(i, item) {
                  var target = model.targets[i];
                  results.push({label : $(item), target : target, value : target.title});
                });

                return results;
              },
              select : function(args) {
                args = $.extend({}, args);
                var target = args.target;
                if (target && target.url) window.location = args.target.url;
              }
            });

          var siteSearchManager = undefined,
            searchField = $("#rsInputId"),
            closeIcon = $('#closeIconId');

          closeIcon.bind('click', function() {
            searchField.val("");
            closeIcon.hide();
          });

          searchField.bind('focus', function() {
            closeIcon.show();
          });

          searchField.bind('blur', function() {
            var val = $(this).val();
            if (!val) {
              closeIcon.hide();
            }
          });

          searchField.autocomplete({
                                     minLength : 2,
                                     source : function(request, response) {
                                       if (siteSearchManager === undefined) {
                                         siteSearchManager = new SiteSearchManager();
                                       }
                                       siteSearchManager.search({requestParameterValue : request.term, responseCallback : response});
                                     },
                                     select : function(event, ui) {
                                       siteSearchManager.select({target : ui.item.target});
                                     },
                                     focus : function() {
                                       return false;
                                     },
                                     open : function(event, ui) {
                                       // Does not work with latest jQuery UI.
                                       //$(this).autocomplete("move", "next");
                                     }
                                   }).data("ui-autocomplete")._renderItem = function(ul, item) {
            return $("<li></li>").data("item.autocomplete", item).append(item.label).appendTo(ul);
          };

        });

      });

  </script><script type="text/javascript">
