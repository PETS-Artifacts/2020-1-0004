      <script>
    function closeLssky_v1() {
        $('#lssky_v1_shader').remove();
    }
    function couldBeBetterBtn() {
        $('.lsskyFormWhyWrapper').slideDown();
        $('.lsskyFail').css({background:'#ffbfb6',cursor:'default'});
        $('.lsskyOk').css({border:'1px solid transparent',cursor:'default'});
        $('.lsskyOk .fa, .lsskyOk span').css({opacity:'0.2'});
        $('.lsskyOk').addClass('lsskyOkDisabled');
        $('.lsskyOk').prop('onclick',null).off('click');
    }
    function everythingIsGreateBtn() {
        $('.lsskyOk').addClass('lsskyBoxSubsActive');
        $('.lsskyFormWhyWrapper, .lsskyFail').fadeOut();
        $('.lsskyOk').css({background:'#016b0b',color:'#fff',cursor:'default'});
        $('.lsskyOk span').html('Thank you!');
        submitLsskyForm();
    }
    function submitLsskyForm() {
        if(typeof dataLayer !== 'undefined') {
            uwt_test =  typeof dataLayer[0].uwt_test === 'undefined' || dataLayer[0].uwt_test === null
                        ? false
                        : dataLayer[0].uwt_test;
        }
        else {
            uwt_test = false;
        }

        var survey_comment = $('.lsskyFormSTextArea textarea').val();
        var answerkey      = $('.lsskyFormBtnsWrapper .fa-dot-circle-o');

        if(
            dataLayer[0].domain==='momondo'
            ||
            dataLayer[0].domain==='cheapflights'
            ||
            dataLayer[0].domain==='travelsupermarket') {
            var survey_sales_phase = dataLayer[0].salesPhase + '/' + dataLayer[0].domain + '/' + uwt_test.toString();
        }
        else {
            var survey_sales_phase = '';
        }


        if(answerkey.length) {
            answerkey = answerkey.parent();
        }
        else {
            answerkey = $('.lsskyFormBtnsWrapper .lsskyBoxSubsActive');
        }

        if(answerkey.length) {
            answerkey = answerkey.data('answerkey');
        }
        else {
            answerkey = -1;
        }

        if(answerkey===-1 && survey_comment==='') {
            alert('Please choose an option or leave a comment.');
        }
        else {
            $.post(
                'ext-apps/survey/collect.php',
                {
                    survey_sales_phase	: survey_sales_phase,
                    answerkey			: answerkey,
                    survey_comment		: survey_comment
                },
                function(resp) {
                    console.log(resp);
                }
            );

            $('.lsskyFormWhyWrapper').slideUp(function(){$('.lsskyFormWhyWrapper, .lsskyFail').fadeOut();});
            $('.lsskyOk, .lsskyOk .fa.fa-thumbs-up, .lsskyOk span').removeAttr('style');
            $('.lsskyOk').css({background:'#016b0b',color:'#fff',cursor:'default'});
            $('.lsskyOk span').html('Thank you!');
        }
    }
    function _readCookie(name) {
        var nameEQ = name + "=";
        var ca = document.cookie.split(';');
        for(var i=0;i < ca.length;i++) {
            var c = ca[i];
            while (c.charAt(0)==' ') {
                c = c.substring(1,c.length);
            }
            if (c.indexOf(nameEQ) == 0) {
                return c.substring(nameEQ.length,c.length);
            }
        }
        return null;
    }
    function _createCookie(name,value,days) {
        if (days) {
            var date = new Date();
            date.setTime(date.getTime()+(days*24*60*60*1000));
            var expires = "; expires="+date.toGMTString();
        }
        else {
            var expires = "";
        }
        document.cookie = name+"="+value+expires+"; path=/";
    }
