<script type="text/javascript">
    //<![CDATA[
    var progressDialogData =
    {"picture":"/v/494/109636/layout/a/image/animation/loading_progress.gif","text":"We are searching for our lowest price to your destination.","title":"Please wait...","height":"auto","width":"300","copiedContentCss":"undefined","textFirst":true,"showCloseIcon":true,"copiedContentStyle":"undefined"} ||
    undefined;
    (function() {
      var p;
      if (progressDialogData) {
        for (p in progressDialogData) {
          if (p === "width") {progressDialogData[p] = parseAutoInt(progressDialogData[p])}
          if (p === "height") {progressDialogData[p] = parseAutoInt(progressDialogData[p])}
          if (progressDialogData[p] === "undefined") {progressDialogData[p] = undefined;}
          if (progressDialogData[p] === "null") {progressDialogData[p] = null;}
        }
        if (progressDialogData.picture && cacheImage) {
          cacheImage(progressDialogData.picture);
        }
      }
      function parseAutoInt(val) {/* jquery dialog can take a number or "auto" */
        return val === "auto" ? "auto" : parseInt(val);
      }
    }());
    //]]>
  </script><script type="text/javascript">
    (function() {
      var animationDuration = 100;

      function submitTopLoginForm() {
        var email = $('#topLoginEmail').val(), password = $('#topLoginPassword').val();

        if (emptyString(email)) {
          setEnterEmailErrorMessage();
          return;
        }
        if (emptyString(password)) {
          setTopLoginErrorMessage(UiText.get('User.Profile.ValidationMessage.EnterPassword'));
          return;
        }
        setInnerHtmlToSmallLoadingAnimation('topLoginLoadingIconContainer');
        setTopLoginMessage('&nbsp;'); // Clear message when submitting
        setRememberMe($('#topRememberMe').is(':checked'));
        $("#topLoginForm").submit();
      }

      function setRememberMe(checked) {
        var email = $('#topLoginEmail').val(),
          value = (checked + '|' + (checked ? email : '').replace('@', '(at)'));
        setCookie('ibe.user.rememberMe', value, 30 * 18);
      }

      function submitForgotPasswordForm() {
        var email = $('#topfForgotPasswordEmail').val(),
          parameters,
          url;

        if (emptyString(email)) {
          setEnterEmailErrorMessage();
          return;
        }
        setInnerHtmlToSmallLoadingAnimation('forgotPasswordLoadingIconContainer');
        setTopLoginMessage('&nbsp;'); // Clear message when submitting

        parameters = new RequestParameters();
        parameters.addParameter("email", email);
        url = '/ajax.user.forgotpassword.do.action?' + parameters.toString();

        sendRequest(url, function(response) {
          clearInnerHTML('forgotPasswordLoadingIconContainer');
          setTopLoginMessage(response.message);
        }, function(response, obj, error) {
          clearInnerHTML('forgotPasswordLoadingIconContainer');
          setTopLoginMessage(response.message);
        }, {
          disableAlerts : true
        });
      }

      function setTopLoginMessage(text) {
        $('#topLoginMessageRow').html(text).fadeIn(siteConfigManager.getAnimationSpeed().fast);
      }

      function setTopLoginErrorMessage(text) {
        setTopLoginMessage(text);
      }

      function setEnterEmailErrorMessage() {
        setTopLoginErrorMessage(UiText.get('User.Profile.ValidationMessage.EnterEmail'));
      }

      function setIpadAdjustedInputSize(element, val) {
        element.attr('size', val);
      }

      function setIpadAdjustedInputType(element, typeVal) {
        element.get(0).type = typeVal;
      }

      function setIpadAdjustmentsOnElement(element, val1, val2) {
        if (val1) {
          setIpadAdjustedInputSize(element, val1);
        }
        if (val2) {
          setIpadAdjustedInputType(element, val2);
        }
      }

      // Submit form if the enter key is clicked when focus is on forgot password textfield.
      $('#topfForgotPasswordEmail').keydown(function(ev) {
        if (ev.keyCode === 13) {
          ev.preventDefault();
          submitForgotPasswordForm();
        }
      });

      // Submit form if the enter key is clicked when focus is on login email or login password textfield.
      $.each([$('#topLoginEmail'), $('#topLoginPassword')], function() {
        $(this).keydown(function(ev) {
          if (ev.keyCode === 13) {
            ev.preventDefault();
            submitTopLoginForm();
          }
        });
      });

      $('.loginTypeLink').click(function() {
        var parameters = new RequestParameters();
        parameters.addParameter("organisationId", $(this).attr("id").split("_")[1]);
        var url = '/ajax.user.setorganisation.do.action?' + parameters.toString();
        sendRequest(url, function(response) {
          ibelogs("response A", response);
          reloadPage();

        }, function(response, obj, error) {
          ibelogs("response B", response);
        }, {});
      });

      $('.topLoginLink').click(function() {
        $('#loginBox').css('left', -$('#loginBox').width() + $('.topLoginLink').width());
        $('#loginBox').slideToggle(siteConfigManager.getAnimationSpeed().fast, function() {
          $('#topLoginEmail').focus();
        });
      });

      $('#aForgotPwd').click(function() {
        $('#topfLoginForgotPassword').slideToggle(siteConfigManager.getAnimationSpeed().fast, function() {
          $('#topfForgotPasswordEmail').focus();
        });
      });

      $('#topLoginCustomerButton').click(function() {
        submitTopLoginForm();
      });

      $('#topForgotPasswordButton').click(function() {
        submitForgotPasswordForm();
      });

      $('#topRememberMe').click(function() {
        setRememberMe($(this).is(':checked'));
      });

      $('#topLogOutCustomerButton').click(function() {
        logoutCustomer();
      });

      $(".businessRadioButtonContainer").click(function(e) {
        $('#loginAsBusiness').attr('checked', 'checked');
      });

      $(".privateRadioButtonContainer").click(function(e) {
        $('#loginAsPrivate').attr('checked', 'checked');
      });

      $("#loginBox .closebutton").click(function(e) {
        $('#loginBox').slideToggle(siteConfigManager.getAnimationSpeed().fast, function() {
        });
      });

      

    })();
