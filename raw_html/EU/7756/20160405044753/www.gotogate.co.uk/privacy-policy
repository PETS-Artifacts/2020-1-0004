    require(
      [
        "jquery",
        /* jQuery plugins */
//        "jquery-ui" TODO: Remove this destorys autocomplete in search form. Why???
      ],
      function() {

        $(function() {

          function getItemValue(uiItem, property) {
            var objs = ['city', 'item'], value = "";
            for (var i = 0; i < objs.length; i++) {
              var obj = uiItem[objs[i]];
              if (obj !== undefined && property !== undefined) {
                value = obj[property];
              } else {
                value = uiItem[property];
              }
            }
            return value;
          }

          
          $("#top_content_fromCity").bind("keydown", function(ev) {
            if (charCodeIsChar(ev.which) || ev.which == KEY_BACKSPACE) {
              
              $('#cityIata_1125033173').val("");
              
              $('#top_content_fromCityId').val("");
              
            }
          });
          

          $('#top_content_fromCity').autocomplete(
            {
              // Setup source has RPC call which fetches the data from server
              source : function(autocompleteRequest, autocompleteResponse) {
                var param = "?citySearch=" + encodeURI(autocompleteRequest.term);

                

                sendRequest('/rpc.air.origin.search.do.action' + param, function(response) {
                  $("top_content_fromCityLoadingAnimation").hide();
                  if (response.model && response.model.length) {
                    autocompleteResponse(response.model);
                  } else {
                    var data = [
                      {label : 'No Cities Found!!'}
                    ];
                    autocompleteResponse(data);
                  }
                  $("#top_content_fromCityLoadingAnimation").hide();
                }, function(response) {
                  $("#top_content_fromCityLoadingAnimation").hide();
                });
              },
              // When new item is focused, nothing should happen.
              focus : function(event, ui) {
                return false;
              },
              // When autocompleter menu is opened, select the first option automatically.
              open : function(event, ui) {
                // Does not work with latest jQuery UI.
                //$(this).autocomplete("move", "next");
              },
              // Same behavior when selecting.
              select : function(event, ui) {
                var item = ui.item;
                $('#top_content_fromCity').val(getItemValue(item, "name"));

                
                $('#cityIata_1125033173').val(getItemValue(item, "iataCode"));
                
                $('#top_content_fromCityId').val(getItemValue(item, "id"));
                

                return false;
              },
              // Other settings
              minLength :3,
              autoFocus : true,
              delay : 500 // TODO: Set by property, one is already available?
            }).data("ui-autocomplete")._renderItem = function(ul, item) {
            return $("<li></li>").data("item.autocomplete", item).append("<a>" + item.label + "</a>").appendTo(ul);
          };

          $('#top_content_fromCityLoadingAnimation').css("height", $('#top_content_fromCity').css("height"));

        });

      });

  </script><script type="text/javascript">
