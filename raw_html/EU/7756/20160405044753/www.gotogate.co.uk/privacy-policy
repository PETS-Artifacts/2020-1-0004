    require([
              "jquery",
              "jquery-ui"
            ],
            function($) {
              (function(jsonDialogueWindows) {
                if (jsonDialogueWindows) {
                  for (var i = 0; i < jsonDialogueWindows.length; i++) {
                    var aWindow = jsonDialogueWindows[i];
                    if (aWindow && aWindow.title && aWindow.body) {
                      openDialogueBox({title : aWindow.title, text : aWindow.body, buttons : aWindow.buttonMap});
                    }
                  }
                }
              })([]);
            });
