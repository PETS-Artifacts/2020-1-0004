    $(function() {

      var submitButton = $("#airSubmitButtonId"), airSearchForm = undefined;

      function airFormSubmitted(form) {
        var formIsOk = undefined;

        try {
          formIsOk = validateIBEForm(form);
        } catch (e) {
          return true;
        }
        if (formIsOk) {
          formProgress(form, formIsOk);
        }
        return formIsOk;
      }

      if (airSearchForm === undefined) airSearchForm = getObj("airSearchFormId");
      airSearchForm.onsubmit = null; // clears the inlined 'onsubmit' attribute on the form

      submitButton.bind("click", function(ev) {
        ev.preventDefault();
        if (airFormSubmitted(airSearchForm)) {
          $(airSearchForm).submit();
        }
      });

      $('#comboSearchInAirButtonId').click(function() {
        $('#' + 'airSearchFormId').attr("action", "/combo.from-air-search.do.action").submit();
      });

      var $serviceClassRadioButtons = $('input[type=radio]').filter('[name=serviceClass]');

      $serviceClassRadioButtons.on("change", function() {
        $serviceClassRadioButtons.each(function() {
          if ($(this).prop("checked")) {
            $(this).addClass("checked");
            $(this).siblings("label").addClass("checked");
          }
          else {
            $(this).removeClass("checked");
            $(this).siblings("label").removeClass("checked");
          }
        });
      });
    });
  </script><script type="text/javascript">

    function airIsOnewaySearch(returnDateRowId, onewayId, partialHotelId, hotelDates) {
      var onewayCheckbox = $("#" + onewayId),
        partialHotelCheckbox = $("#" + partialHotelId),
        returnDateDiv = $("#" + returnDateRowId),
        returnDateTimeDiv = $("#" + returnDateRowId + 'TimeOfDay');

      if (onewayCheckbox.is(":checked")) {
        returnDateDiv.hide();
        if (returnDateTimeDiv) returnDateTimeDiv.hide();
        if (partialHotelCheckbox) {
          partialHotelCheckbox.attr('checked', true);
          toggleLayer(hotelDates, true);
        }
      } else {
        returnDateDiv.show();
        if (returnDateTimeDiv) returnDateTimeDiv.show();
        if (partialHotelCheckbox) {
          partialHotelCheckbox.attr('checked', false);
          toggleLayer(hotelDates, false);
        }
      }
    }

    function airOnewaySearch(returnDateRowId, onewayId, partialHotelId, hotelDatesId) {
      var onewayCheckbox = $('#' + onewayId),
        partialHotelCheckbox = $('#' + partialHotelId),
        returnDateDiv = $('#' + returnDateRowId),
        returnDateTimeDiv = $("#" + returnDateRowId + 'TimeOfDay'),
        animationSpeed = siteConfigManager.getAnimationSpeed().fast;

      function fadeOut(el) {
        el.fadeTo(animationSpeed, 0, function() {
          $(this).css('visibility', 'hidden');
        });
      }

      function fadeIn(el) {
        el.css('visibility', 'visible').fadeTo(animationSpeed, 1);
      }

      if (onewayCheckbox.is(':checked')) {
        fadeOut(returnDateDiv);
        if (returnDateTimeDiv) fadeOut(returnDateTimeDiv);
        if (partialHotelCheckbox) {
          partialHotelCheckbox.attr('checked', true);
          fadeIn($("#" + hotelDatesId));
        }
      } else {
        fadeIn(returnDateDiv);
        if (returnDateTimeDiv) fadeIn(returnDateTimeDiv);
        if (partialHotelCheckbox) {
          partialHotelCheckbox.attr('checked', false);
          fadeOut($("#" + hotelDatesId));
        }
      }
    }

    airIsOnewaySearch('component_topSearchForm_item1_content_returnDateRow', 'component_topSearchForm_item1_content_oneway', 'top_content_hotel_partialHotelDate', 'component_topSearchForm_item1_content_airOnewaySearchScriptComponent_hotelDates');

    $(function() {
      $('.advanceSearchLink').click(function() {
        airIsOnewaySearch('component_topSearchForm_item1_content_returnDateRow', 'component_topSearchForm_item1_content_oneway', 'top_content_hotel_partialHotelDate', 'component_topSearchForm_item1_content_airOnewaySearchScriptComponent_hotelDates');
      });
    });

