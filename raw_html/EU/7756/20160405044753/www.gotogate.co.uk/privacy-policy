<script type="text/javascript">

  var pageEventType;
  var globalsApplication;
  var sessionManager;
  var siteConfigManager;
  var metricsManager;
  var pageEventManager;
  var siteHeaderManager;
  var notificationsManager;
  var paymentStateManager;

  var __callbacksToRunBeforeGlobalsReady = [];
  var __callbacksToRunAfterGlobalsReady = [];
  var __globalsReady = false;

  function whenGlobalsReady(func) {
    if (__globalsReady) {
      func();
    } else {
      __callbacksToRunAfterGlobalsReady.push(func);
    }
  }

  function beforeGlobalsReady(func) {
    if (__globalsReady) {
      func();
    } else {
      __callbacksToRunBeforeGlobalsReady.push(func);
    }
  }

  function __runAllWhenGlobalsReady() {
    __globalsReady = true;
    for (var i = 0; i < __callbacksToRunAfterGlobalsReady.length; i++) {
      var func = __callbacksToRunAfterGlobalsReady[i];
      func();
    }
  }
  function __runAllBeforeGlobalsReady() {
    __globalsReady = true;
    for (var i = 0; i < __callbacksToRunBeforeGlobalsReady.length; i++) {
      var func = __callbacksToRunBeforeGlobalsReady[i];
      func();
    }
  }

  var globalsModel = {};

  globalsModel.paymentStateManagerArgs = {
    cartState : {"paymentInProgress":false,"state":"UNAVAILABLE"},
    receiptClickUrl : "https://www.gotogate.co.uk/your-receipt",
