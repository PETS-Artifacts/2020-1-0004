      "PaymentStartedSiteHeaderText" : 'Payment in progress' +' ' + '<img src="/v/494/102125/system/image/animation/loading_transparent_bars_small.gif" class="paymentNotificationLoadingAnimation" alt="Loading..." title="Loading..." hspace="0" vspace="0" >',
      "PaymentStartedSubject" : 'Payment in progress...',
      "PaymentStartedBody" : 'Please wait until your payment at Gotogate is done. You will be notified when payment is complete.' +"<br/>" + '<div class="animationContainer loadingBars pt10"><img src="/v/494/103658/layout/a/image/animation/loading_transparent_bars.gif" class="pt10" alt="Loading..." title="Loading..." hspace="0" vspace="0" ></div>',
      "PaymentSuccessSubject" : 'Payment successful!',
      "PaymentSuccessBody" : 'Your payment is now completed, you will soon receive an e-mail with the receipt.' +"<br/><br/>" + "<p class='em pointer'>" + 'Go to receipt' +"&nbsp;&raquo;</p>",
      "PaymentFailureSubject" : 'Payment failed!',
      "PaymentFailureBody" : 'We could not complete the payment. You can try again.' +"<br/><br/>" + "<p class='em pointer'>" + 'Go to payment page' +"&nbsp;&raquo;</p>",
