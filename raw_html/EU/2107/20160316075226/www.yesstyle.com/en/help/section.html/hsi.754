<script type="text/javascript" id="com-yesasia-yesstyle-web-wicket-panel-signinalert-SignInAlert-0">
// TODO move to header.html
function showSignInAlert(e){
    // remove first each time
    var pop=$("#popcon");
    pop.click(function(event){event.stopImmediatePropagation()});
    pop.remove();
    
    // construct the popup
    var container=$(".signInAlertContainer").html();
    $(e).before("<span id='popcon'><div class='shadow'></div><div class='explanation'><div id='speaker'></div>"+container+"</div></span>");
    $(e).poplocation({
        container:"#popcon",
        obj:".explanation",
        shadow:".shadow",
        speaker:"#speaker",
        direction:2
    });    
    return false;
}
</script>

