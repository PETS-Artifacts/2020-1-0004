<script type="text/javascript" id="com-yesasia-yesstyle-web-wicket-panel-footer-resolutionprompt-ResolutionPrompt-0">
$(document).ready(function(){
  // Detect resolution requirement
  if (screen.width < 1280 && $("#resolution").length){  
    $("body").addClass("modal-open");
    $("#resolution, .modal-overlay").fadeIn();
    
    if (isUserAgentMobile()){
        $("#resolution").addClass("modal-mobile");
    }
    $("#resolution .modalclose").click(function(){
        $("body").removeClass("modal-open");
        $("#resolution, .modal-overlay").fadeOut();
    });
  }
});
</script>


