window.ux.eldorado._onDelayedJSLoaded = function _onDelayedJSLoaded(err) {
  var fn;
  while (fn = window.ux.eldorado._onDelayedJSLoadedFns.shift()) {
    try {
      fn(err);
    } catch(e) {
    }
  }
};
window.addEventListener('load', function loadDeferredScripts() {
  var el, els = [], numScriptsRequested = 5;
  function createScript(src) {
    var el = document.createElement('script');
    el.async = false;
    el.onload = checkIfLoadComplete;
    el.onerror = handleLoadError;
    el.src = src;
    return el;
  }
  function checkIfLoadComplete() {
    if (!--numScriptsRequested) {
      window.ux.eldorado._onDelayedJSLoaded();
    }
  }
  function handleLoadError() {
    if (!this) return;
    var parent;
    if (this.parentNode) {
      parent = this.parentNode;
      this.parentNode.removeChild(this);
    }
    this.onerror = this.onload = null;

    if (!this.isRetry && parent) {
      // Retry once
      var el = createScript(this.src);
      el.isRetry = true;
      parent.appendChild(el);
    } else {
      var errorMessage = 'Error: failed to load "' + this.src + '"';
      window.ux.eldorado._onDelayedJSLoaded(new Error(errorMessage));
    }
  }
  els.push(createScript('https://img1.wsimg.com/wrhs-assets/7e80e0a2d23ded6f5646d5947d84ddc8/tcc.min.js'));
  els.push(createScript('https://img1.wsimg.com/poly/v2/polyfill.min.js?features=Promise,Promise.prototype.finally,Intl.~locale.nb-NO&rum=0&unknown=polyfill&flags=gated'));
  els.push(createScript('https://img1.wsimg.com/wrhs-assets/83be82dadb83c33d0be7345c3c63d565/vendor.min.js'));
  els.push(createScript('https://img1.wsimg.com/wrhs-assets/1ba9c4bfff5195c4136a8074647b2c10/uxcore2.min.js'));
  els.push(createScript('https://img1.wsimg.com/wrhs-assets/7bc0d7abecd80e5d7fcca39632c3aeb3/salesheader.min.js'));
  var scriptsParent = document.getElementsByTagName('script')[0].parentNode;
  while (el = els.shift()) scriptsParent.appendChild(el);
});
</script>
