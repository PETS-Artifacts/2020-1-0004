  jQuery(document).ready(function(){
    $j("[href='#full_privacy']").click(function() {
      $j('#full_privacy').fadeToggle();
    });

    if (window.location.hash.startsWith('#full_privacy'))
      $j('#full_privacy').fadeToggle();
    else if (window.location.hash.length > 1 && $j('#full_privacy ' + window.location.hash).length > 0)
      $j('#full_privacy').fadeToggle();
  });
</script>

