      <script>

        Geni.ready(function(){
          new RadioMenu('search_types',function(link,val){
            var form = $('basic_search_form');
            var input = $('search_box');
            if(input.value == input.getAttribute('label')) input.value = "";
            input.setAttribute('label',link.getAttribute('title'));
            if(window.peopleTagger_search) {
              if (val != "people") {
                peopleTagger_search.disable()
              } else {
                peopleTagger_search.enable()
              }
            }
            var action,name;
            switch(val) {
              case "projects":
                action = "/project/search";
                name = "q";
                break;
              case "discussions":
                action = "/discussions/search";
                name = "q";
                break;
              case "surnames":
                action = "/surnames/search_surnames";
                name = "id";
                break;
              case "people":
              default:
                action = "/search";
                name = "names";
            }

            form.setAttribute('action',action);
            input.setAttribute('name',name);

            labels_init();
          })
        })
      </script>

    </form>
  </div>
