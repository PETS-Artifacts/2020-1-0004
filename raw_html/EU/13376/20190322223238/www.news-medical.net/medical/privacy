            </div>
            <div class="col-lg-3 col-md-3 col-xs-6 cb-container">
                <button type="button" onclick="return BlogEngine.onAcceptAllCookiesClick();" id="cb-accept-btn"><span class="icon icon-checkmark"></span> Accept All Cookies</button>
            </div>
            <div id="cookie-notice-close">
                <a class="no-transition" onclick="return BlogEngine.closeCookieNotice(true);"><span class="icon icon-cross"></span></a>
            </div>
        </div>
    
    

                

    
        
