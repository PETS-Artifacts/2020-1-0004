<section class="wdg_page page-legal-text grid">
    <h1 class="box-title">Privacy Policy</h1>
    
    <section id="wdg_static_pages" class="wdg_static_pages legal-text box-content"
             data-props="{&quot;allowed_origins&quot;:[&quot;https://staging-sdk.toons.tv&quot;]}">
        <style>
.wdg_page{font-size: 16px!important; text-align: left;}
.wdg_page h1{font-size: 26px!important;}
.wdg_page h2{font-size: 21px!important;}
.wdg_breadcrumb{font-size: 14px!important;}
.grid{padding: 1.5em!important;}
