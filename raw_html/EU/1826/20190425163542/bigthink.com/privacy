function addSectionName() {
  var $this, wSection, widgets = document.querySelectorAll('.widget:not(.loaded-section__name)');
  for (var i = 0; i < widgets.length; i++) {
	$this = widgets[i];
	wSection = $this.querySelector('.widget__section') ? $this.querySelector('.widget__section').innerText : '';

     $this.className += " loaded-section__name";
     if ($this.querySelector('.widget__section') != null) {
      $this.className += " section__" + wSection ? wSection.toLowerCase().replace(/&\s/g, '').replace(/\s/g, '-') : '';
      }
  }
}
addSectionName();
window.addEventListener('load-more-event', addSectionName);
</script><script type="text/lazy-javascript" priority="6">
        rblms.require(['jquery', 'core/event_dispatcher'], function($, eventDispatcher) {
            eventDispatcher.on('load-more:sync', function() {
                addSectionName();
            });
        });
</script><!-- End User Code --><!-- User Code --><script>
(function(){

var navbarItems = document.querySelectorAll('.navbar-sections__item');

