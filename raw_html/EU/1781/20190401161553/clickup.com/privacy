    ct.src = capterra_prefix + '/capterra_tracker.js?vid=' + capterra_vid + '&vkey=' + capterra_vkey;
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ct, s);
  })();
</script>

