  <!--Scripts-->
  
  <script src="/landing/js/app.min.f999c85e.js"></script>
  <script>
    $(document).ready(function () {

     $('.toc-menu').perfectScrollbar();
     $(".scroll-menu").sticky({topSpacing:175});


     var div = $(".scroll-menu"); 
     var winHeight = $(window).height();
     var ftrOffsetTop = $('.bottom-menu').offset().top + 70;

     function stickyDiv() {

      var scrollTop = $(window).scrollTop();

      if ((winHeight + scrollTop) >= ftrOffsetTop) {
        div.unstick().css({
          position: 'absolute',
          bottom: '0'
        });;
      } 

      else {
       div.sticky({topSpacing:175});
       div.css({
        // position: 'fixed',
        bottom: 'auto'
      });
     }

   }

   $(document).scroll(function () {
    stickyDiv();
  }); 




   var nav = $('.toc-menu'),
   sections = $('.toc-section'),
   nav_height = nav.outerHeight();

   $(window).on('scroll', function () {


    var cur_pos = $(this).scrollTop();
    sections.each(function() {
      var top = $(this).offset().top - nav_height,
      bottom = top + $(this).outerHeight();

      if (cur_pos >= top && cur_pos <= bottom) {
        $('.toc-menu').find('a').removeClass('active');
        sections.removeClass('active');

        $(this).addClass('active');
        $('.toc-menu').find('a[href="#'+$(this).attr('id')+'"]').addClass('active');
      }
    });


    if ($(window).scrollTop() > $(".block-privacy").offset().top) {
      $("#toc-nav").removeClass("not-sticky").addClass("sticky");
    }
    else {
      $("#toc-nav").removeClass('not-closed').addClass("closed").addClass("not-sticky").removeClass("sticky");
    }
  });



   var nav_link = $('.toc-menu a');
   nav_link.on('click', function () {
    var $el = $(this) ,
    diff = $('.is-sticky').length ? 70 : 130 ,
    id = $el.attr('href');

    $('html, body').animate({
      scrollTop: $(id).offset().top - diff
    }, 1000, 'swing');

    return false;
  });


 });


    $(".back-to-top").click(function (e) {
      e.preventDefault();
      $('#toc-nav').removeClass("not-sticky").addClass("sticky");
      $("html, body").animate({scrollTop: 0}, 1000);
    });

  </script>
  <script src="//calendly.com/assets/external/widget.js"></script>

