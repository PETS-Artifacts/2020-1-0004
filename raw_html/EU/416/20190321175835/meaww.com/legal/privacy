<script>
	window.onscroll = function() {alignHeader()};
	function alignHeader(){
		if (document.body.scrollTop > 70 || document.documentElement.scrollTop > 70) {

			let alignableItems = document.getElementsByClassName('alignableItem');
			for (let i = 0; i < alignableItems.length; i++){
				alignableItems[i].style.transform="translateY(-70px)";
				alignableItems[i].style.transform="-webkit-transform: translateY(-70px)";
			}

			let fadingItems = document.getElementsByClassName('fadingItem');
			for (let i = 0; i < fadingItems.length; i++)
  			fadingItems[i].style.opacity = "0";

			let fixedItems = document.getElementsByClassName('fixedItem');
			for (let i = 0; i < fixedItems.length; i++){
				fixedItems[i].style.transform="translateY(60px)";
				fixedItems[i].style.transform="-webkit-transform: translateY(60px)";
			}

			let sidebarItems = document.getElementsByClassName('realsidebar');
			for (let i = 0; i < sidebarItems.length; i++){
				sidebarItems[i].style.transform="translateY(70px)";
				sidebarItems[i].style.transform="-webkit-transform: translateY(70px)";
			}
		}

		else{
			let alignableItems = document.getElementsByClassName('alignableItem');
			for (let i = 0; i < alignableItems.length; i++){
					alignableItems[i].style.transform="translateY(0px)";
					alignableItems[i].style.transform="-webkit-transform: translateY(0px)";
			}

			let fadingItems = document.getElementsByClassName('fadingItem');
			for (let i = 0; i < fadingItems.length; i++)
  			fadingItems[i].style.opacity = "1";

			let fixedItems = document.getElementsByClassName('fixedItem');
			for (let i = 0; i < fixedItems.length; i++){
				fixedItems[i].style.transform="translateY(0px)";
				fixedItems[i].style.transform="-webkit-transform: translateY(0px)";
			}

			let sidebarItems = document.getElementsByClassName('realsidebar');
			for (let i = 0; i < sidebarItems.length; i++){
				sidebarItems[i].style.transform="translateY(0px)";
				sidebarItems[i].style.transform="-webkit-transform: translateY(0px)";
		}
	}
}
</script>

