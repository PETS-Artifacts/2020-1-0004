    function GoToResults(selector) {
        var textKeyword = document.querySelector(selector);
        if (textKeyword.value != "") {
            var url = '/search-results?indexCatalogue=MainEnglish' + "&searchQuery=" + textKeyword.value;
            window.location.href = url;
        }
    }

