	<script>
		/* Sets cookie for 390 days */
		var cookieValue = 'cookie_notice_version=' + window['cookie-popup'].dataset.cookieVersion;

		var cookieExpiry = new Date();
		cookieExpiry.setTime(cookieExpiry.getTime() + (390 * 24 * 60 * 60 * 1000));
		cookieExpiry = 'expires=' + cookieExpiry.toGMTString();

		var cookiePath = 'path=/';

		document.cookie = cookieValue + '; ' + cookieExpiry + ' ;' + cookiePath;

		/* Closes box */
		window['cookie-popup'].querySelector('.btn').addEventListener('click', function () {
		  window["cookie-popup"].parentElement.removeChild(window["cookie-popup"]);
		});
	</script>




