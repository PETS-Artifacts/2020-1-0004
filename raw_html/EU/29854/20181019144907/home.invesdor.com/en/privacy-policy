<!-- check whether user has logged in on www.invesdor.com and change header links accordingly -->
<script async defer>
    document.addEventListener("DOMContentLoaded", function() {
       var authenticatedLinks = document.querySelector(".header-authenticated-links");
       var guestLinks = document.querySelector(".header-guest-links");
       
       var xhr = new XMLHttpRequest();
       xhr.onreadystatechange = function() {
           if (xhr.readyState == 4) {
               if (xhr.responseText == "true") {
                   guestLinks.classList.add("hidden");
                   authenticatedLinks.classList.remove("hidden");
                } else {
                    guestLinks.classList.remove("hidden");
                    authenticatedLinks.classList.add("hidden");
                }
            }
        };
       xhr.open("GET", "https://www.invesdor.com/authenticated");
       xhr.withCredentials = true;
       xhr.send();
    });
</script>

