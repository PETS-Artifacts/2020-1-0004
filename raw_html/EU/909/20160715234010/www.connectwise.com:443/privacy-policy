<script type="text/javascript">
    function startTriggerClientEvent(eventName) {
        if (eventName === "Sales Chat") {
            eventName = "CW Sales Chat Engagement";
            triggerClientEvent(eventName);
        }
        else if (eventName === "Support Chat Link") {
            eventName = "CW Support Chat";
            triggerClientEvent(eventName);
        }
    }
</script>

