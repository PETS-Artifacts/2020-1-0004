    <script type="text/javascript">
        function triggerClientEvent(eventName) {
            //example:
            //if(eventname === "Dynamic Event Name Produced by Code")
            //  eventname = "EventName expected by Sitecore"
            if (eventName === "CW Social Footer Blog")
                eventName = "CW Click to Blog";
            else if (eventName === "CW Social Footer Facebook")
                eventName = "CW Facebook Click";
            else if (eventName === "CW Social Footer Google+")
                eventName = "CW Google Plus Click";
            else if (eventName === "CW Social Footer Linkedin")
                eventName = "CW LinkedIn Click";
            else if (eventName === "CW Social Footer Twitter")
                eventName = "CW Twitter Click";
            else if (eventName === "CW Social Footer YouTube")
                eventName = "CW YouTube Click";

            var event = new AnalyticsPageEvent(eventName);
            event.trigger();
        }
    </script>
</body>
<script type="text/javascript">
    var getQueryParameters = function (str) {
        return (str || document.location.search).replace(/(^\?)/, '').split("&").map(function (n) { return n = n.split("="), this[n[0]] = n[1], this }.bind({}))[0];
    }

    $(document).ready(function () {
        var search = function (e) {
            var triggerId = e.target.id;
            var inputId = triggerId.split('-')[0] + '-search_input';
            var searchText = document.getElementById(inputId).value;
            var searchUrl = '/search' + '?q=' + encodeURIComponent(searchText);
            window.location.href = searchUrl;
            return false;
        }

        if (window.location.pathname == '') {
            $('.main-content .search-box').val(decodeURIComponent(getQueryParameters()['q']).replace('+', ' '));
        }

        $('.btn-search').click(search);
        $('.search-box').keydown(function (e) {
            if (e.keyCode == 13) {
                e.preventDefault();
                search(e);
                return false;
            }
        });
    });
</script>
</html>
