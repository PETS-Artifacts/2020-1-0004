<script type="text/javascript">
        var timerId = null, timeout = 5, cookieGuid;
</script>

<script type="text/javascript">
    //SCRIPT SECTION 3:  Push Eloqua Customer ID required for repost from external page
    function WaitUntilCustomerGUIDIsRetrieved() {
        if (!!(timerId)) {
            if (timeout == 0) {
                if (navigator.userAgent.search("Safari") >= 0 && navigator.userAgent.search("Chrome") < 0) 
                {
                    if (typeof this.PrePopElqContent === 'function') {
                        PrePopElqContent();
                    }
                }
                return;
            }
            if (typeof this.GetElqCustomerGUID === 'function') {
                cookieGuid = GetElqCustomerGUID();
                window["elqCustomerGuid"] = cookieGuid;
                var forms = document.forms;
                for (var i = 0; i < forms.length; i++){
                    if (forms[i].elements["elqCustomerGUID"] != undefined)
                        forms[i].elements["elqCustomerGUID"].value = cookieGuid;
                }

                if (typeof PrePopElqContent === 'function'){
                    PrePopElqContent();
                }

                return;
            }
            timeout -= 1;
        }
        timerId = setTimeout("WaitUntilCustomerGUIDIsRetrieved()", 500);
        return;
    }
    window.onload = WaitUntilCustomerGUIDIsRetrieved();
    _elqQ.push(['elqGetCustomerGUID']);
</script>

<script>
    $('form#form'+ 274 + ' input[type="submit"]').removeAttr('value');
    $('form#form'+ 274 + ' input[type="submit"]').attr('value', '');


    var form = $('form#form'+ 274);
    var buttonId;

    form.submit(function (data) {
        buttonId = $(data.currentTarget).attr('id');
        $.ajax({
            type: 'POST',
            url: '/api/sitecore/subscribe/PostFormToSpecificEndpoints',
            data: form.serialize(),
            datatype: 'json',
            success: function(data){
                if (data.redirect){
                    window.location.href = data.url;
                }
                else{
                    form.parents().closest(".opt-in").replaceWith("<div class='opt-in'><label>You're Subscribed! Watch your inbox for our next monthly newsletter.</label></div>");
                }
            }
        });
        return false;
    });
</script>

