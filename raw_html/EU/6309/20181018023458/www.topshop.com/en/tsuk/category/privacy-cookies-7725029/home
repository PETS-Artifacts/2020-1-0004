      </div>
</div><script type="text/javascript">
//<![CDATA[
   var _ArcQ = _ArcQ || [];
                
                
                
                
                function inner () {
                        $(".inner").live("click", function (e) {
                                // prevent default click action
                                e.preventDefault();

                                // get the current location and anything after the hash
                                var currHashLoc = window.location.hash;
                                var currHref = window.location.href;
                                var newHashLoc = $(this).attr("href");


                                // if there is a hash value, replace the existing hash value with the new one from link, and reload
                                if (currHashLoc!="") {

                                        fullCurrFrag = currHashLoc.split("tab-");

                                        // if it's this format "#fragment-X"
                                        if(fullCurrFrag != "") {
                                                
                                                currFrag = currHashLoc.split("-");
                                                currFrag = currFrag[1].replace("tab","");

                                                nextFrag = newHashLoc.split("-");
                                                nextFrag = nextFrag[2].replace("tab","");

                                                currHashLoc = "#fragment-"+currFrag+"tab-"+currFrag+"-1";

                                                if (currFrag !== nextFrag) {
                                                        // If it's going into another fragment
                                                        window.location.href = window.location.href.replace(window.location.hash,newHashLoc);
                                                        window.location.reload();

                                                } else {
                                                        // If it's staying withing the same fragment

                                                        currTab = currHashLoc.split("tab");
                                                        currTab = currTab[1];
                                                        fullCurrTab = "tab"+currTab;

                                                        nextTab = newHashLoc.split("tab");
                                                        nextTab = nextTab[1];
                                                        nextTab = "tab"+nextTab;

                                                        // Hide all tabs and content
                                                        $(".tab_content .fragment-"+currFrag+" li.txt_bg").removeClass("tabs-selected");
                                                        $(".tab_content .fragment-"+currFrag+" .tabs-container").addClass("tabs-hide");


                                                        // To new one
                                                        $(".tab_content .fragment-"+currFrag+" li a."+nextTab).parent().addClass("tabs-selected");
                                                        $(".tab_content .fragment-"+currFrag+" #"+nextTab).removeClass("tabs-hide");

                                                }
                                        } 

                                // if there isn't a hash value, add the hash value from the link to the url, and reload
                                } else {
                                        window.location.href = currHref+newHashLoc;
                                        window.location.reload();
                                }
                        })
                };

    function tabInit () {
        var test1 = new Arcadia.UI.Tabs({
            target: ".content-container"
        });
    };
    
                function infoToggle () {
                        var thisToggle = new Arcadia.UI.InfoToggle({
                                target : $('.tabs')
                        })
                };

                function countriesMenu () {
                        var dd = new Arcadia.UI.dynamicDropdown({
                            target : $('ul#countryList'),
                            child: 'li' 
                        });

                        var dd1 = new Arcadia.UI.dynamicDropdown({
                            target : $('ul#countryList1'),
                            child: 'li' 
                        });
                };

                function activateLink () {
                        $('.external_link').unbind('click');
                };

                function imageChange() {
                        $(".menu-nav li.image a").each(function(){
                                if ($(this).hasClass("active")) {
                                        $(this).find("img").css({"margin-top": "-0"});
                                }
                        });
                        $(".menu-nav li.image a").click(function(){
                                $(".menu-nav li.image a img").css({"margin-top": "0"});
                                $(this).find("img").css({"margin-top": "-0"});
                        });
                }
                
                
                                
                    _ArcQ.push([inner],  [tabInit],  [infoToggle], [countriesMenu], [activateLink], [imageChange]);
                
                                
//]]>
</script>
