        <!-- START Trustev -->
                    <script type="text/javascript">
    (function(global, $, ENV, Raven, trustevCallbacks) {
        function includeTrustEv(url) {

            if (!ENV.features.get('trustev-load')) {
                return;
            }

            $.ajax({
                url: url,
                dataType: 'script',
                method: 'GET',
                timeout: 5000,
                cache: false,
                success: function () {
                    if(trustevCallbacks) {
                        _.forEach(trustevCallbacks, function (fn) {
                            try {
                                fn();
                            } catch (e) {
                                var err = new Error(
                                        'Trustev Callback Error: ' + e.message
                                );
                                Raven.captureException(err);
                            }
                        });
                    }
                },
                error: function (err) {
                    var e = new Error('Trustev Load Error');
                    e.statusText = err.statusText;
                    Raven.captureException(e);
                }
            });
        }

        $(global).on('load', function () {
            includeTrustEv(
                'https://app.trustev.com/api/v2.0/TrustevJS?key=63ae22f80a8b4d87b4e855c41a61955f'
            );
        });

    })(this, this.$, this.ENV, this.Raven, this.trustevCallbacks);
</script>
                <!-- END Trustev -->

                    <script>
$.cachedScript("//cdn.ravenjs.com/1.1.16/raven.min.js").done(function() {
    var dsn = ENV.SENTRY_DSN;
    if (!dsn) {
        return;
    }

    var options = {
        whitelistUrls: [
            /hautelook\.com/,
            /nordstromrack\.com/,
            /optimizely\.com/
        ]
    };

    Raven.config(dsn, options).install()
