                var sChannelSection;
                var sSubSection;

                jQuery(document).ready(function () {
                    sChannelSection = window.location.pathname.split('/')[1];
                    if (sChannelSection == undefined || sChannelSection == null || sChannelSection == "/" || sChannelSection.trim() == "") {
                        sChannelSection = "";
                    }

                    sSubSection = window.location.pathname.split('/')[2];
                    if (sSubSection == undefined || sSubSection == null || sSubSection == "/" || sSubSection.trim() == "") {
                        sSubSection = "";
                    }
                });
