        var queuedErrors = [];
        window.onerror = function (message, source, line, column, error) {

            function logError(message) {
                if (typeof eventTracker == 'undefined') {
                    queuedErrors.push([message])
                } else {
                    eventTracker.logError(message);
                }
            }

            logError(message + ': ' + source + '@' + line + ':' + column);
            console.log(message + ': ' + source + '@' + line + ':' + column);
            return true;
        };
