<!-- BEGIN EGain Chat -->
<script language=javascript>
    /*eGain Chat server */
    var egainDockChat = egainDockChat  || {};
    egainDockChat.serverURL = "https://customerservice.wiggle.com/system";
        /*eGain Chat Entry Point*/
        egainDockChat.EntryPointId  = "1002";
        /*eGain Chat Locale*/
        egainDockChat.Locale  = "en-US";
        /*eGain template name*/
        egainDockChat.Template = "wiggle";
        /*Set to true to enable posting attributes to templates*/
        egainDockChat.PostChatAttributes = false;
        /*eGain video chat parameters */
        egainDockChat.VChatParams = '';
        /*Set to true if custom button is used to launch docked chat */
        egainDockChat.UseCustomButton=true;

/*Wiggle custom lanaguage*/
egainDockChat.WiggleLanguage  = "en";

        /*Method to set customer Parameters. To be called by client website. All the parameters specified in application-chat-defaults must be set here.*/
        egainDockChat.SetCustomerParameters = function(egainAttributeName, attributeValue)
        {
            if (!egainDockChat.SetParameter)
            {
                egainDockChat.CallQueue = egainDockChat.CallQueue || [];
                egainDockChat.CallQueue.push({ name: 'SetParameter', args:[egainAttributeName, attributeValue]});
            }
            else
            {
                egainDockChat.SetParameter(egainAttributeName, attributeValue);
            }
        };
        /* Method to be called on click of custom button to launch chat in docked mode */
        egainDockChat.openHelp = function()
        {
            egainDockChat.IsChatLaunched = true;
            startChat();
        }
        if(!egainDockChat.UseCustomButton){
            startChat();
        }
        function startChat()
        {
            if (!egainDockChat.launchChat)
            {
                egainDockChat.CallQueue = egainDockChat.CallQueue || [];
                egainDockChat.CallQueue.push({ name: 'launchChat', args:[]});
            }
            else
            {
                egainDockChat.launchChat();
            }
        }
        </script>

<!-- END EGain Chat -->
    <a id="egain-live-chat" href="javascript:void(0)" onClick="egainDockChat.openHelp()" style="position: fixed; right: 25px; bottom: 0; width: 135px; height: 35px; display: none; z-index: 10000">
        <img src="https://www.wigglestatic.com/images/livechat/buttons/onlinebuttons/online_en.png" id="eGainChatImage" alt="Chat is online" style="margin: 0px; padding: 0px; border-style: none; border-width: 0px; font-style: normal; font-weight: normal; font-variant: normal; list-style: none outside none; letter-spacing: normal; line-height: normal; text-decoration: none; vertical-align: baseline; white-space: normal; word-spacing: normal; position: absolute; top: 0px; left: -10px; z-index: 600;">
    </a>
    <script>
        (function () {
            var eGainChatImageRoot = 'https://www.wigglestatic.com/images/livechat/buttons/';
            function setLiveChatAvailable() {
                $('#eGainChatImage').attr('src', eGainChatImageRoot + 'onlinebuttons/online_' + egainDockChat.WiggleLanguage + '.png');
                $('#egain-live-chat').show();
            }

            function setLiveChatBusy() {
                $('#eGainChatImage').attr('src', eGainChatImageRoot + 'BusyButtons/busy_' + egainDockChat.WiggleLanguage + '.png');
                $('#egain-live-chat').show();
            }

            function setLiveChatUnavailable() {
                $('#eGainChatImage').attr('src', eGainChatImageRoot + 'OfflineButtons/offline_' + egainDockChat.WiggleLanguage + '.png');
                $('#egain-live-chat').show();
            }

            function checkEGainChatAvailablity() {
                try {
                    var agent_domain = egainDockChat.serverURL;
                    var agent_availability_uri = "/egain/chat/entrypoint/agentAvailability";
                    var agent_capacity_uri = "/egain/chat/entrypoint/capacity";
                    var eligibility_uri = "/egain/chat/entrypoint/checkEligibility";
                    var entryPointId = egainDockChat.EntryPointId;
                    var eglvchathandle = null;
                    var availability_url = agent_domain + '/' + agent_availability_uri + '/' + entryPointId;
                    var capacity_url = agent_domain + '/' + agent_capacity_uri + '/' + entryPointId;
                    var eligibility_url = agent_domain + '/' + eligibility_uri + '/' + entryPointId;

                    $.ajax({
                        type: 'GET',
                        url: eligibility_url,
                        dataType: 'xml',
                        success: function (xml) {
                            var responseType = $(xml).find('checkEligibility').attr('responseType');

                            if (responseType === "0") {
                                setLiveChatAvailable();
                            } else if (responseType === "1") {
                                setLiveChatUnavailable();
                            } else if (responseType === "2") {
                                setLiveChatBusy();
                            }
                        }
                    });

                } catch (err) {
                    console.log('Live Chat API error');
                    ga('send', 'exception', {
                        'exDescription': err.message,
                        'exFatal': false
                    });
                }
            }

            $(document).ready(function () {
                checkEGainChatAvailablity();
            });
        })();

    </script>


</body>
</html>
