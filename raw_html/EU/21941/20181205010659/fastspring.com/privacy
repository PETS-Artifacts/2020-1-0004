<script>
  window.addEventListener('DOMContentLoaded', function() {
    (function($) {
      jQuery('.toggle-slideout').on('click', function(event) {
        event.preventDefault();
        window.location.href = "/sign-up/";
      });

      jQuery(".fs-disable-clicks").on("click", function(event) {
        if ( jQuery(event.target).parent().hasClass("fs-disable-clicks") ) {
          event.preventDefault();
          event.stopPropagation();
          event.stopImmediatePropagation();
        }; 
      });

      jQuery("img.fs-lazy").one("inview", function() {
        jQuery(this).attr("src", $(this).data("src") );
      });
      jQuery(".fs-lazybg").one("inview", function() {
        jQuery(this).css("background-image", "url(" + $(this).data("bgsrc") );
      });

      jQuery(window).load(function() {
        jQuery(".hide-load").show();
      });

      $('.facebook-share').on('click', function() {
        var $url = $(this).attr('data-url');
        window.open('https://www.facebook.com/sharer/sharer.php?u=' + $url, "facebookWindow", "height=380,width=660,resizable=0,toolbar=0,menubar=0,status=0,location=0,scrollbars=0");
        return false;
      });

      $('.twitter-share').on('click', function() {
        var $this = $(this),
        $url = $this.attr('data-url'),
        $title = $this.attr('data-title');
        window.open('http://twitter.com/intent/tweet?text=' + $title + ' ' + $url, "twitterWindow", "height=380,width=660,resizable=0,toolbar=0,menubar=0,status=0,location=0,scrollbars=0");
        return false;
      });
      $('.linkedin-share').on('click', function() {
        var $this = $(this),
        $url = $this.attr('data-url'),
        $title = $this.attr('data-title'),
        $desc = $this.attr('data-desc');
        window.open('http://www.linkedin.com/shareArticle?mini=true&url=' + $url + '&title=' + $title + '&summary=' + $desc, "linkedInWindow", "height=380,width=660,resizable=0,toolbar=0,menubar=0,status=0,location=0,scrollbars=0");
        return false;
  });




    })(jQuery); 
    var fsdeferred = document.createElement('link');
    fsdeferred.rel = 'stylesheet';
    fsdeferred.href = 'https://fsmktingcdn-a82e.kxcdn.com/wp/wp-content/themes/jupiter-child' + '/plugin_js_composer.min.css';
    fsdeferred.type = 'text/css';
    var godefer = document.getElementsByTagName('link')[0];
    //godefer.parentNode.insertBefore(fsdeferred, godefer);
    jQuery("head").prepend(fsdeferred);
  });
 </script>
