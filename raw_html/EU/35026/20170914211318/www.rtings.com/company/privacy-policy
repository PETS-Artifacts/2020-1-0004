  <script type="text/javascript">installEventHandlers()</script>
    <script>
      var stickyTOC = {
        stickyTOC_after: 80,
        init: function() {
          this.scroll();
          this.events();
        },
        scroll: function() {
          var toc = document.getElementById("toc");
          if (toc == null) { return; }
          if (window.scrollY > this.stickyTOC_after) {
            if (toc.className != "toc-fixed") { toc.className = "toc-fixed"; }
          } else {
            if (toc.className != "") { toc.className = ""; }
          }
        },
        events: function() {
          window.addEventListener("scroll", this.scroll.bind(this));
        }
      };

    document.addEventListener("DOMContentLoaded", stickyTOC.init.bind(stickyTOC));
    init_toc();

    </script>
  </body>
</html>
