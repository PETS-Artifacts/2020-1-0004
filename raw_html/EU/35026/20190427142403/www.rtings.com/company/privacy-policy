    <script src="/assets/packs/application-c4002914aa49cc3792f8.js"></script>
    <script>

  // ==========================================================================
  // ARTICLE PAGE
  // ==========================================================================

  function ArticlePage (el) {
    this.el = el;
    this.els = {
      distributionCharts : this.el.find(".distribution_chart"),
      scrollingCarousels : el.find(".scrolling_carousel")
    };
    this.distributionCharts = this.bindDistributionCharts()

    this.scrollingCarousels = [];
    var self = this;
    this.els.scrollingCarousels.each(function (i, el) {
      self.scrollingCarousels.push(new ScrollingCarousel($(el)))
    });
  }

  ArticlePage.prototype.bindDistributionCharts = function () {
    var charts = [];
    for (var i=0; i < this.els.distributionCharts.length; i += 1){
      var el = this.els.distributionCharts[i];
      charts.push(new DistributionChart($(el), undefined, {
        showWhenNoReference: true,
        hideTitle: true,
        isInteractive: true,
        barGap: 2,
      }));
    }
    return charts;
  }

  var article_page = new ArticlePage($(".article_page"));
  if(RTINGS_CONFIG.silo && RTINGS_CONFIG.testBench){
    API.loadSiloData(RTINGS_CONFIG.silo, RTINGS_CONFIG.testBench);
  }
</script>
    <script>
      document.getElementById("stores-select").value = STATE.get("store");
      storeHandler.changeStore(RTINGS_CONFIG.silo);
    </script>


