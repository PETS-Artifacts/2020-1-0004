    window.addEventListener('beforeinstallprompt', function (e) {
        e.userChoice.then(function (choiceResult) {
            if (choiceResult.outcome == 'dismissed') {
                _gaq.push(['_trackEvent', 'WebInstall', 'InstallPromptResponse', 'Cancel']);
            }
            else {
                _gaq.push(['_trackEvent', 'WebInstall', 'InstallPromptResponse', 'Added']);
            }
        });
    });
</script>

