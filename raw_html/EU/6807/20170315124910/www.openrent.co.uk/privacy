<script type="text/javascript">

    var menuVisible = false;
    var originalHeight = 0;
    function toggleMobileMenu() {

        if ($("#mobileMenuContainer").hasClass("show")) {
            $("#mobileMenuContainer").removeClass("show");
            $("#mobileMenuOverlay").hide();
            $("body").removeClass("lock");
        }
        else
        {
            $("#mobileMenuContainer").addClass("show");
            $("#mobileMenuOverlay").show();
            $("body").addClass("lock");
            $(".mobile-menu").find(".menu-active").parent(".mobile-menu__subblock").show();
            $(".mobile-menu").find(".menu-active").parent(".mobile-menu__subblock").siblings(".navHeaderToggle").addClass("active");
        }

        calculateSizesAndAdjustMenu();
    }

    $(window).resize(function () {
        if ($("#mobileMenuContainer").hasClass("show")) {
            $("#mobileMenuContainer").removeClass("show");
            $("#mobileMenuOverlay").hide();
            $("body").removeClass("lock");
        }
    });

    function calculateSizesAndAdjustMenu() {
        if (!menuVisible) {
            $("#mobileMenuContainer .userDetailsMenu").css("height", $("#mobileMenuContainer .userDetailsMenu").height());
        }
        else {
            $("#mobileMenuContainer .userDetailsMenu").css("height", originalHeight);
        }

        menuVisible = !menuVisible;
    }


    $(document).ready(function () {
        $("#menuShowbutton, #mobileMenuOverlay").click(function () {
            toggleMobileMenu();
        });
    });

    $('#mobileMenuOverlay').bind('touchmove', function (e) {
        e.stopPropagation();
        e.preventDefault();
    });

    var currentStatusHidden = false;

    $(function () {
        $('.navHeaderToggle').click(function () {
            $(this).siblings("ul").slideToggle();
            $(this).toggleClass("active");
        });
    });

    
    
    if ('ontouchstart' in window) {

        var is_keyboard = false;
        var is_landscape = false;
        var initial_screen_size = window.innerHeight;

        /* Android */
        window.addEventListener("resize", function () {
            is_keyboard = (window.innerHeight < initial_screen_size);
            is_landscape = (screen.height < screen.width);
            updateViews();
        }, false);

        

        function updateViews() {
            if (is_keyboard) {
                $(".floatingTabBar").css("position", "static");
            }
            else {
                $(".floatingTabBar").css("position", "");
            }
        }
    }
</script>

<script type="text/javascript">
    String.prototype.format = function () {
        var formatted = this;
        for (var i = 0; i < arguments.length; i++) {
            var regexp = new RegExp('\\{' + i + '\\}', 'gi');
            formatted = formatted.replace(regexp, arguments[i]);
        }
        return formatted;
    };

    Date.prototype.getRealYear = function () {
        if (this.getFullYear)
            return this.getFullYear();
        else
            return this.getYear() + 1900;
    };

    function createCookie(name, value, days) {
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
            var expires = "; expires=" + date.toGMTString();
        }
        else var expires = "";
        document.cookie = name + "=" + value + expires + "; path=/";
    }
    function getCookie(c_name) {
        if (document.cookie.length > 0) {
            c_start = document.cookie.indexOf(c_name + "=");
            if (c_start != -1) {
                c_start = c_start + c_name.length + 1;
                c_end = document.cookie.indexOf(";", c_start);
                if (c_end == -1) {
                    c_end = document.cookie.length;
                }
                return unescape(document.cookie.substring(c_start, c_end));
            }
        }
        return "";
    }

    $(function () {
        // Site wide script to stop submit buttons being pressed twice.
        function PreventDoubleSubmission() {
            $(document.body).on('submit', 'form.stop-double-submission', function (e) {
                var $form = $(this);

                if ($form.data('submitted') === true) {
                    // Previously submitted - don't submit again
                    if (e.preventDefault) {
                        e.preventDefault();
                    }
                    else {
                        e.returnValue = false;
                    }
                    return false;
                } else if (
                    !e.isDefaultPrevented()
                    && ((typeof $form.valid === 'undefined') || $form.valid())
                    ) {
                    // Mark it so that the next submit can be ignored
                    $form.data('submitted', true);
                }
            });

            $(document.body).on('click', 'button.stop-double-submission', function (e) {
                var $form = $(this);

                if ($form.data('submitted') === true) {
                    // Previously submitted - don't submit again
                    if (e.preventDefault) {
                        e.preventDefault();
                    }
                    else {
                        e.returnValue = false;
                    }
                    return false;
                } else if (
                    !e.isDefaultPrevented()
                    && ((typeof $form.valid === 'undefined') || $form.valid())
                    ) {
                    // Mark it so that the next submit can be ignored
                    $form.data('submitted', true);
                }
            });

            // Keep chainability
            return this;
        };

        PreventDoubleSubmission();
    });

    function ReEnableSubmission(jqueryObject) {
        jqueryObject.data('submitted', false);
    }

    function ReEnableSubmissionOnAllForms() {
        $("form.stop-double-submission").each(function (index) {
            ReEnableSubmission($(this));
        });
    }

</script>

