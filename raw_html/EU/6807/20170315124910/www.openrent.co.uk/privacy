    var urlToGoTo = "/";
    function goToURLAfterDelay(url) {
        olark('api.box.shrink');
        urlToGoTo = url;
        timer = setTimeout("endpause()", 0); // 3 secs
        return false;
    }

    function endpause() {
        location.href = urlToGoTo;
        return false;
    }


