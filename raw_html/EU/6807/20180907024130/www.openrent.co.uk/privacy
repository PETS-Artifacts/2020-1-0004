    <script type="text/javascript">
        

        function DisableLink(linkToDisable)
        {
            linkToDisable.bind('click.disableHandler', function (e) {
                e.preventDefault();
                return false;
            });
        }

        function EnableLink(linkToDisable) {
            linkToDisable.unbind('click.disableHandler');
        }

        $(function () {
            if (document.getElementById("signInWidgetFallBackButton")) {

                DisableLink($("#signInWidgetFallBackButton"));

                document.getElementById("signInWidgetFallBackButton").href = "/account/simplelogon";

                setTimeout(function () {
                    EnableLink($("#signInWidgetFallBackButton"));
                }, 3000);

            }
        });

    </script>


