<script type="text/javascript">

    var menuVisible = false;
    var originalHeight = 0;

    function toggleMobileMenu() {

        if ($("#mobileMenuContainer").hasClass("show")) {
            $("#mobileMenuContainer").removeClass("show");
            $("#mobileMenuOverlay").hide();
            $("body").removeClass("lock");
        }
        else
        {
            $("#mobileMenuContainer").addClass("show");
            $("#mobileMenuOverlay").show();
            $("body").addClass("lock");

            $(".side-nav-list").find(".menu-active").parents(".sub-nav").show();
            $(".side-nav-list").find(".menu-active").parents(".sub-nav").siblings(".navHeaderToggle").addClass("active");
        }

        calculateSizesAndAdjustMenu();
    }

    $(window).resize(function () {
        if ($("#mobileMenuContainer").hasClass("show")) {
            $("#mobileMenuContainer").removeClass("show");
            $("#mobileMenuOverlay").hide();
            $("body").removeClass("lock");
        }
    });

    function calculateSizesAndAdjustMenu() {
        if (!menuVisible) {
            $("#mobileMenuContainer .userDetailsMenu").css("height", $("#mobileMenuContainer .userDetailsMenu").height());
        }
        else {
            $("#mobileMenuContainer .userDetailsMenu").css("height", originalHeight);
        }

        menuVisible = !menuVisible;
    }


    $(document).ready(function () {

        function isTouchDevice() {
            return "ontouchstart" in document.documentElement;
        }

        if (isTouchDevice()) {
            $("li.has-sub").each(function (index, elem) {
                $(this).find("a").first().click(function (event) {
                    event.preventDefault();
                });
            });
        }

        if ($("#mobileMenuContainer .menu-active")[0]) {
            $("#mobileMenuContainer").scrollTop($("#mobileMenuContainer").scrollTop() + ($(".side-nav-list").find(".menu-active").position().top - 50));
        }

        $("#menuShowbutton, #mobileMenuOverlay, .side-nav-close-btn").click(function () {
            toggleMobileMenu();
        });
        $(".side-nav-list .navHeaderToggle").click(function (e) {
            var nextSub = $(this).next('.sub-nav');
            nextSub.slideToggle();
            $(this).toggleClass('active');
        });
    });

    $('#mobileMenuOverlay').bind('touchmove', function (e) {
        e.stopPropagation();
        e.preventDefault();
    });

    var currentStatusHidden = false;

    
    if ('ontouchstart' in window) {

        var is_keyboard = false;
        var is_landscape = false;
        var initial_screen_size = window.innerHeight;

        /* Android */
        window.addEventListener("resize", function () {
            is_keyboard = (window.innerHeight < initial_screen_size);
            is_landscape = (screen.height < screen.width);
            updateViews();
        }, false);

        

        function updateViews() {
            if (is_keyboard) { // if small screen (caused by pinch / zoom or keyboard out)
                $(".main-nav").css({
                    "position" : "static",
                    "margin-top" : "-60px"
                });
            }
            else {
                $(".main-nav").css({
                    "position" : "",
                    "margin-top" : "0px"
                });
            }
        }
    }
</script>

<script type="text/javascript">
    $(function () {
        // Site wide script to stop submit buttons being pressed twice.
        function PreventDoubleSubmission() {
            $(document.body).on('submit', 'form.stop-double-submission', function (e) {
                var $form = $(this);

                if ($form.data('submitted') === true) {
                    // Previously submitted - don't submit again
                    if (e.preventDefault) {
                        e.preventDefault();
                    }
                    else {
                        e.returnValue = false;
                    }
                    return false;
                } else if (
                    !e.isDefaultPrevented()
                    && ((typeof $form.valid === 'undefined') || $form.valid())
                    ) {
                    // Mark it so that the next submit can be ignored
                    $form.data('submitted', true);
                }
            });

            $(document.body).on('click', 'button.stop-double-submission', function (e) {
                var $form = $(this);

                if ($form.data('submitted') === true) {
                    // Previously submitted - don't submit again
                    if (e.preventDefault) {
                        e.preventDefault();
                    }
                    else {
                        e.returnValue = false;
                    }
                    return false;
                } else if (
                    !e.isDefaultPrevented()
                    && ((typeof $form.valid === 'undefined') || $form.valid())
                    ) {
                    // Mark it so that the next submit can be ignored
                    $form.data('submitted', true);
                }
            });

            // Keep chainability
            return this;
        };

        PreventDoubleSubmission();
    });

    function ReEnableSubmission(jqueryObject) {
        jqueryObject.data('submitted', false);
    }

    function ReEnableSubmissionOnAllForms() {
        $("form.stop-double-submission").each(function (index) {
            ReEnableSubmission($(this));
        });
    }

</script>

<script type="text/javascript">"use strict";(function(n){if("serviceWorker"in n.navigator){var t=n.openrent.detectedBrowserFlags.IsChrome,i=n.openrent.detectedBrowserFlags.IsMobile,r=n.location.origin==="https://www.openrent.co.uk";if(!t||!i||!r){n.navigator.serviceWorker.getRegistrations().then(function(n){for(var t=0;t<n.length;t++)n[t].unregister()});return}n.addEventListener("load",function(){n.navigator.serviceWorker.getRegistrations().then(function(n){if(n.length>1)for(var t=0;t<n.length;t++)n[t].unregister()}).then(function(){n.navigator.serviceWorker.register("https://www.openrent.co.uk/ServiceWorker/service-worker.js").catch(function(n){console.log("Service Worker Error: ",n)})});n.addEventListener("beforeinstallprompt",function(t){t.userChoice.then(function(t){t.outcome==="dismissed"?n.ga("send","event","WebInstall","InstallPromptResponse","Cancel"):n.ga("send","event","WebInstall","InstallPromptResponse","Added")})})})}})(window,document)</script>


