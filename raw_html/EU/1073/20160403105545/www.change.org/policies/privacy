        if (window.bootstrapClient) {
          // If mergedAssets was executed first, call into it with the data.
          window.bootstrapClient(appData, bootstrapData)
        } else {
          // If this executed first, store the data for use when mergedAssets
          // loads.
          window.appData = appData
          window.bootstrapData = bootstrapData
        }
      })();
    </script>

