	function openLoginPopup() {
		
			$('#myModal').modal('show');
			var iframe = $('<div style="background: white" id="loaderframe"><img src="https://cwsmgmt.corsair.com/_ui/responsive/common/img/pdp/zoom_loader.gif"/></div><iframe id="frameframe" style="display:none" src="https://signin.corsair.com/arms/applications/access?id=69e5ff83-c24f-451e-ae2a-b832622b0695" frameborder="0"></iframe>');
			$('#myModal .modal-content').html(iframe);
			iframe.on('load', function() {
				setTimeout(
						function()
						{
							$('#loaderframe').hide();
							$('#frameframe').fadeIn();
						}, 500);
			});
			
	}
</script>

<script>
		$(document).ready(function () {
			$(".account-button-myaccount").click(function () {
				window.location.href = "/my-account";
			});

			$(".logout-button").click(function () {
				var iframe = $("#logoutFrame");
				iframe.attr("src", iframe.data("src"));

				$('#logoutFrame').load(function(){
					var logoutAnchor = $(".logout-js-corsair");
					window.location.href = "/logout";
				});
			});
		});
	</script>

	<script>
		$(document).ready(function() {
			$(".account-button").click(function() {
				openLoginPopup();
			});

			$(".checkoutLoginBtn").click(function() {
				previousPage = "/checkout"
				openLoginPopup();
			});

			$(".expressCheckoutButton").click(function() {
				previousPage = "/checkout/multi/express"
				openLoginPopup();
			});
		});
	</script>

