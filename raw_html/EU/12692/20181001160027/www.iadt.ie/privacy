    <div class="row">
        <section class="main_content push-up">
            <div id="share" class="dropdown">
	<button id="share-button" type="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false"></button>		
		

	<div id="share-links" class="dropdown-menu">
