};
        var sanitizeUrl = function sanitizeUrl(url) {
    if (url.match(/shr(\w{14})/)) {
        var replacedUrl = url.replace(/shr\w{14}/g, function (match) {
            return match.slice(0, 7) + '**********';
        });
        url = replacedUrl;
    }
    if (url.match(/auth\/passwordReset/)) {
        url = url.replace(/(auth\/passwordReset\/)(.*)$/, '$1*');
    }
    url = url.replace(/(\?.*)$/, function (queryString) {
        return sanitizeQueryStringByReplacingQueryValuesWithStar(queryString);
    });
    return url;
};
        var sanitizeQueryStringByReplacingQueryValuesWithStar = function sanitizeQueryStringByReplacingQueryValuesWithStar(queryString) {
    var sanitizedQueryString = queryString.replace(/(=)([^&]+)(&)/g, '$1*$3').replace(/(=)([^&]+)$/, '$1*');
    return sanitizedQueryString;
};
