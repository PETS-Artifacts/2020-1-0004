<script id="cartSummaryTemplate" type="text/x-jQuery-tmpl">

    <div class="cart-summary-item">
        <div class="row">
            <div class="remove-button" onclick="window.shopping.getShoppingCart().showRemoveProductModal(${productPK})">x</div>
            <div class="col-xs-3">
                <img src="${imageUrl}" />
            </div>
            <div class="col-xs-7">
                <span>${productName}</span>
            </div>
        </div>
        <div class="row cart-summary-item-info">
            <div class="col-xs-4">
                <div class="row">
                    <div class="col-xs-12">
                        <span class=" cms cms-translation" data-content="Price" data-name="price" data-state="shoppingCart">Price</span>
                    </div>
                    <div class="col-xs-12">
                        <span class="bold">${currencySymbol}${price.toFixed(2)}</span>
                    </div>
                    {{if priceWarning}}
                    <div class="col-xs-12">
                        <span>${priceWarning}</span>
                    </div>
                    {{/if}}
                </div>
            </div>
            <div class="col-xs-3">
                {{if limitOne}}
                <input name="quantity" type="number" class="number-picker" id="limit-one" value="${quantity}" onchange="window.shopping.getShoppingCart().updateProductQuantity(${productPK}, value)" disabled />
                {{else}}
                <input name="quantity" type="number" class="number-picker" value="${quantity}" onchange="window.shopping.getShoppingCart().updateProductQuantity(${productPK}, value)" />
                {{/if}}
            </div>
            <div class="col-xs-5">
                <div class="row align-right">
                    <div class="col-xs-12">
                        <span class=" cms cms-translation" data-content="Total" data-name="total" data-state="shoppingCart">Total</span>
                    </div>
                    <div class="col-xs-12">
                        <span class="bold">${currencySymbol}${(price * quantity).toFixed(2)}</span>
                    </div>
                </div>
            </div>
        </div>
    </div>

</script>
