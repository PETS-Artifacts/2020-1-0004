	<script>
	function formReset() {
		$('form').each(function() {
			$(this)[0].reset();
		});
	}
	function formResetLogged() {
		$('.form-message').each(function() {
			$(this).val('');
		});
	}
	function enableBtnContact() {
		$('.form-submit').each(function() {
			$(this).removeAttr('disabled').removeClass('disabled');
		});
	}
	function disableBtnContact() {
		$('.form-submit').each(function() {
			$(this).addClass('disabled').attr('disabled');
		});
	}
	function captchaCallback(captchalink) {
		captchalink = captchalink.replace('#contact', 'captcha');
		var $captchalink = $('#'+captchalink);
		if ( $captchalink.length > 0 && $captchalink.children().length == 0) {
			disableBtnContact();
			grecaptcha.render(captchalink, {'sitekey': '6LetTAITAAAAAFTKt8rQL-RDEMD6nGqPQ1FaibbQ', 'callback': enableBtnContact, 'expired-callback': disableBtnContact, 'data-size': 'compact' });
		}
	}
	if ( $('body').is('.user-not-logged') ) {
		$.getScript('https://www.google.com/recaptcha/api.js?render=explicit&hl=en');
		
		$('.capchabox').fancybox({
			openEffect: 'fade',
			closeEffect: 'fade',
			autoSize: false,
			width: '100%',
			height: '100%',
			fitToView: true,
			padding: 0,
			margin: [55, 0, 0, 0],
			beforeLoad: function() {
				$('body').addClass('gallery-open');
			},
			afterLoad: function() {
				formReset();
				captchaCallback( $(this.element).attr('href') );
			},
			afterClose: function() {
				$('body').removeClass('gallery-open');
			}
		});
		}
	$(document).on('submit', '.contact-form', function(e) {
		if (!$(this).is('.sent')) {
			$(this).addClass('sent').children('input[type="submit"]').addClass('disabled').attr('disabled');
			$.ajax({
				type: "POST",
				url: $(this).attr('action'),
				data: $(this).serialize(),
				complete: function(data) {
					window.location.href = 'https://www.kimovil.com/en/info/thanks';
				}
			});
			e.preventDefault();
			return false;
		} else {
			e.preventDefault();
			return false;
		}
	});
	</script>
