<script>
	function captchaKey() {
		var config = document.getElementsByName('const')[0];
		var jsonConfig = JSON.parse(config.getAttribute('content'));
		return jsonConfig['recapchaKey'];
	}
	function enableFormButton() {
		var element = document.getElementById('ajax-wrapper').getElementsByClassName('form-submit')[0];
		element.classList.remove('disabled');
		element.removeAttribute('disabled');
	}
	function disableFormButton() {
		var element = document.getElementById('ajax-wrapper').getElementsByClassName('form-submit')[0];
		element.classList.add('disabled');
		element.addAttribute('disabled');
	}
	function loadCapcha(document, tag) {
		var scriptTag = document.createElement(tag),
			firstScriptTag = document.getElementsByTagName(tag)[0];
		scriptTag.src = 'https://www.google.com/recaptcha/api.js?render=explicit&hl='+document.documentElement.lang;
		firstScriptTag.parentNode.insertBefore(scriptTag, firstScriptTag);
	}
	if (userData === 'undefined' || !userData || userData.user === 'undefined' || !userData.user) {
		loadCapcha(document, 'script');
		var captchaKey = captchaKey();
		window.addEventListener('captcha_add', function(e) {
			var captchaWrapper = document.querySelector('#ajax-wrapper .captcha-wrapper');
			grecaptcha.render(captchaWrapper, {'sitekey': captchaKey, 'callback': enableFormButton, 'expired-callback': disableFormButton, 'data-size': 'compact' });
		});
	}
</script>
