$(window).load(function() {
  
  if (document.cookie.indexOf("UIPATH_GDPR") === -1){
   $('body').addClass('is-walled');
  } else {
    $('body').css('overflow', 'scroll');
  }
  
  //Control x-default url
  var en_url = $('link[hreflang="en"]');
  
  if(en_url[0]){
  	var xDefault_url = en_url[0].href;
    $('link[hreflang="x-default"]')[0].href = xDefault_url;
  }
  
  
});
  
