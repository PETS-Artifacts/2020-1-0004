<script>
  
  // TitleDropdown functionality
  $('.TitleDropdown-titleButton').on('click', function() {
    var $this = $(this);
    var $parent = $this.parent('.TitleDropdown');

    if ($parent.hasClass('is-active')) {
      $parent.removeClass('is-active');
      console.log('no');
    } else {
      $parent.addClass('is-active');
      console.log('yes');
    }
  });
  
  // scroll to privacy content
function scrollToPrivacyContent($id) {
  return function() {
    var $privacyContent = $("#content-" + $id);
    $('.TitleDropdown').removeClass('is-active');

    if (!$($privacyContent).hasClass("is-active")) {
      $($privacyContent).addClass("is-active");
    }

    $([document.documentElement, document.body]).animate(
      {
        scrollTop: $($privacyContent).offset().top - $(".MainHeader").height() 
      },
      1000
    );
  };
}

$("#link-content-1").on("click", function() {
  scrollToPrivacyContent(1)();
});
$("#link-content-2").on("click", function() {
  scrollToPrivacyContent(2)();
});
$("#link-content-3").on("click", function() {
  scrollToPrivacyContent(3)();
});
