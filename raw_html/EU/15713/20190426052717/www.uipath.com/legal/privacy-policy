// jump from select list to privacy content
function jumpToPrivacyContent() {
  var $id = $(".privacy-content-sections").val();
  $('.TitleDropdown').removeClass('is-active');

  return function() {
    var $privacyContent = $("#content-" + $id);

    if (!$($privacyContent).hasClass("is-active")) {
      $($privacyContent).addClass("is-active");
    }

    $([document.documentElement, document.body]).animate(
      {
        scrollTop: $($privacyContent).offset().top - $(".MainHeader").height()
      },
      1000
    );

  };
}

$(".privacy-content-sections").on("change", function() {
  jumpToPrivacyContent()();
});

</script>

    
