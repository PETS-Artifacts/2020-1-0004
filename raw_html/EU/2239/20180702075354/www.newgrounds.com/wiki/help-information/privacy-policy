	// <![CDATA[ */
		jQuery(function($) {
			$('a.expand_list').click(function () {
				var my_li = $(this).closest('li');
				if (my_li.hasClass('collapsed')) {
					my_li.removeClass('collapsed');
					my_li.addClass('expanded');
				} else if (my_li.hasClass('expanded')) {
					my_li.addClass('collapsed');
					my_li.removeClass('expanded');
				}

				return false;
			});
		});
	// ]]>
</script>
