	<script>
		var scroll = 0;
		$(window).scroll(function (event) {
		    scroll = $(window).scrollTop();
		});
	
	    $("a.gotoMousePos").click(function() {
		 	   var $this = $(this);       
		 	   var _href = $this.attr("href"); 
		 	   var _target = $this.attr("target");
		 	   if (_target !='_blank' && scroll>0) {
		 	   		$this.attr("href", _href + '#pos'+scroll);
		 	   }	 	   
		 });
</script>		
