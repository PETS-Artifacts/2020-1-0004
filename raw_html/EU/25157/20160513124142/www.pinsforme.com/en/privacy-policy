	<script type="text/javascript">
			(function($){
			var chF;
			var validable=$("form[name=inline_login_form]");
			var errorDiv_inline_login_form = $("#inline_login_form .errors_wrap");
			// Try find errors_wrap inside form in case multi-forms
			if ($(validable).parent().find(".errors_wrap").length > 0) {
				errorDiv_inline_login_form = $(validable).parent().find(".errors_wrap");
			}
			var responseTargets = $("form[name=\"inline_login_form\"] .select").add("form[name=\"inline_login_form\"] select").add("form[name=\"inline_login_form\"] input[type=text]").add("form[name=\"inline_login_form\"] textarea").add("form[name=\"inline_login_form\"] input[type=password]").add("form[name=\"inline_login_form\"] input[type=checkbox]");
		var validator_inline_login_form = new FormValidator(
			"inline_login_form",
			[
			{
				name: "identity",
				display: "Your e-mail",
				rules: "trim|required|valid_email|max_length[100]"
			},
			
			{
				name: "password",
				display: "Password",
				rules: "trim|required|max_length[20]"
			}
			], function(errors, event) {
				var errDiv = errorDiv_inline_login_form;
				var stop_submit = $("#inline_login_form").hasClass("js_validation_stop_submit");
				if (stop_submit === true && typeof event !== "undefined" && event.type === "submit")
				{
					event.preventDefault();
				}
				
				if (errors.length > 0) {
					$(".js_validation_at_element").text("");
					if (errors.length > 0) {
						if($(validable).attr("style")!=undefined && $(validable).attr("style").indexOf("height")!==-1) {
							$(validable).css({height:"auto"});
						}
						responseTargets.removeClass("failed").addClass("passed");
						responseTargets.trigger("addClassPassed");
						var errorString = "<div class=\"error\">";
						errorString += "<ul>";
						var errorsCount = 0;
						for (var i = 0, errorLength = errors.length; i < errorLength; i++){
							
							if ($("#" + errors[i].id).hasClass("skip_js_validation")) {
								continue;
							}
							
							errorsCount++;
							
							errorString += "<li>" + errors[i].message + "</li>";
							
							var el = $("#" + errors[i].id);
							el.removeClass("passed").addClass("failed");
							el.trigger("addClassFailed");

							// For the forms that are not using Select2 for dropdowns - take down "failed" class from select element
							if (el.parent(".select").length !== 0) {
								el.add(el.parent(".select"));
								el.parent().find("select").removeClass("failed");
							}
							
							// Set each error near its input
							$(".js_validation_" + errors[i].id).text(errors[i].message);
						}
						errorString += "</ul></div>";
						
						if (errorsCount === 0) {
							errDiv.find(".error").html("").remove();
							responseTargets.removeClass("failed").addClass("passed");
							$(validable).css({height:"auto"});
							$(validable).find(".cancel").prop("data-height",$(validable).height());
							$(".js_validation_at_element").text("");
							
							return true;
						}
						
						if (errDiv.find(".error").is(":visible")) {
							errDiv.html(errorString).find(".error").show();
							$(validable).css({height:$(validable).height()});
							$(validable).find(".cancel").prop("data-height",$(validable).height());
						}
						else {
							errDiv.html(errorString);
							errDiv.find(".error").hide().slideToggle(300,function(){
								$(validable).css({height:$(validable).height()});
								$(validable).find(".cancel").prop("data-height",$(validable).height());
							});
						}
						if(chF==null) chF=responseTargets.add("select").change(function(e){
							validator_inline_login_form._validateForm(e);
						});
						
						// Scroll to first failed field only on submit
						if (typeof event !== "undefined" && event.type === "submit") {
							$("html, body").animate({
								scrollTop: $(validable).find(".failed").offset().top
							}, 300);
						}
					}
				} else {
					errDiv.find(".error").html("").remove();
					responseTargets.removeClass("failed").addClass("passed");
					$(validable).css({height:"auto"});
					$(validable).find(".cancel").prop("data-height",$(validable).height());
					$(".js_validation_at_element").text("");
					
					return true;
				}
			}
		);for (var lang_key in validate_messages) {
			validator_inline_login_form.setMessage(lang_key.substring("js_validate_".length), validate_messages[lang_key]);
		}$(document).ready(function() {
			CUSTOM_EVENTS.run("after_validation_initialised", validator_inline_login_form);
		});})(jQuery);
		</script><script type="text/javascript">
			(function($){
			var chF;
