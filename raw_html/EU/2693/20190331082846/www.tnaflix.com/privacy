	<script type="text/javascript">
$.ready(function() {
	$('.cms_poll button').on('click', function(){
		var params = new Object();
		params.cms_request = 'poll';
		inputs = $('.cms_poll input');
		for (i = 0; i<inputs.length; i++) {
			if (inputs[i].checked) {
				params.value = inputs[i].value;
			}
			if (inputs[i].type == 'hidden') {
				params.oid = inputs[i].value;
			}
		}
		$.request('POST', '/request.php', params)
		.then(function(html) {
			flixMessage.show(html, 0);
		});
	});

	$('.cms_form button').on('click', function(){
		$.request('POST', '/request.php', $(this).up().values())
		.then(function(html) {
			flixMessage.show(html, 0);
		});
	});
	
	processLocalChanges = function() {
		$(".ajax_content.main").addAfter($("footer"))	
	}
});
</script>
