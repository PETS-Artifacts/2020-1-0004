    function updateJasmine() {
        $.request('get', 'https://pt.protoawe.com/api/video-promotion/v1/list?psid=tnanetwork&pstool=421_1&campaign_id=41674&accessKey=770fd2621c12b79cc291ad188e26b2a5&ms_notrack=1&campaign=41674&type=&sexualOrientation=straight&forcedPerformers=&limit=24&primaryColor=05D5FF&labelColor=000000&clientIp=127.0.0.1')
            .then(function (json) {
                json = $.parseJSON(json);
                $('.improveADS').each(function (item, index) {
                    $(item).set('%link',json.data.videos[index].targetUrl);
                    $(item).set('%vid',json.data.videos[index].id);
                    $(item).set('%nk',json.data.videos[index].id);
                    $(item).set('%vk',json.data.videos[index].id);
                    $(item).set('%vn',5);
                    $(item).set('%date',1553698227);
                    $(item).set('%th',123);
                    $(item).set('%time',json.data.videos[index].duration);
                    $(item).set('%name',json.data.videos[index].title);

                    $(item).add(HTML('<a class="thumb no_ajax" data-title="'+encodeURI(json.data.videos[index].title)+'" data-link="' + json.data.videos[index].targetUrl + '" data-avatar="'+encodeURI(json.data.videos[index].profileImage)+'" data-category="'+json.data.videos[index].tags.join(',')+'" data-width="0"><img style="max-height:160px;" class="lazy" src="' + json.data.videos[index].previewImages[2] + '" data-original="' + json.data.videos[index].previewImages[2] + '" alt="' + json.data.videos[index].title + '"><div class="videoDuration">' + (parseInt(json.data.videos[index].duration/60)) + ':'+ (parseInt(json.data.videos[index].duration%60)) +'</div><div class="hdIcon">1080p</div><div class="watchedInfo" style="display:none">Watched</div></a>'));
                    $(item).add(HTML('<a style="display: block; font-size: 14px;margin: 5px 0;height: 17px;overflow: hidden;text-overflow: ellipsis;white-space: nowrap;" class="nvtHide" data-id="' + json.data.videos[index].id + '" data-link="' + json.data.videos[index].targetUrl + '" href="#" >' + json.data.videos[index].title + '</a>'));
                    $(item).add(HTML('<div class="thumbHidenBlock">' +
                        '                  <div class="thumbAdditionalInfo">' +
                        '                    <div class="tai clear">' +
                        '                      <div class="floatLeft">few hours ago</div>' +
                        '                      <div class="floatRight"><i class="icon-remove-red-eye"></i>0</div></div>' +
                        '                    <div class="ntlTagsCats">' +
                        '                      <span class="simpleTag" data-href="/hd-videos">#hd</span> <span class="simpleTag" data-href="/' + json.data.videos[index].category + '">#' + json.data.videos[index].category + '</span></div></div></div>'));
                    $(item).add(HTML('<div data-tooltip="Watch later" class="mdtOutOther tooltip-left"><button class="wlWatch"></button></div>'));
                    $(item).add(HTML('<div data-tooltip="Open in new window" class="mdtOutOther mdtOutOtherSec tooltip-left"><button class="wlNewWind"></button></div>'));

                });
                $('.improveADS').select('a').on('click', function(e,item) {
                    thumbClick(null,$('.thumb').only(16));
                    thumbClick(null,this);
                });
            });
    }
