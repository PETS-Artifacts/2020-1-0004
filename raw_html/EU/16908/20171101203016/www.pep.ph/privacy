			$(window).scroll(function() {
			   	var hT = $('#more-posts-2').offset().top,
			       hH = $('#more-posts-2').outerHeight(),
			       wH = $(window).height(),
			       wS = $(this).scrollTop();
			   	if (wS > (hT+hH-wH)){
			   		if($('#more-posts-2').attr('data-count') < 6){
			       		$('#more-posts-2').trigger('click');
			       	}
			   }
			});
		</script>
	</div>
