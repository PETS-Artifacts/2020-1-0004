<script type="text/javascript">
        //sns-button update for pep
                    $(document).on('click', '.topemailarticle, .botemailarticle', function(){
                            $('span.emailtrigger > a.emailtrigger > span.ra1-pw-icon-email').trigger('click');
                            clearTimeout();
                     });
$(document).on('click', '[data-sns]', function(){
                    var
                    data = $(this).data(),
                    sns = data.sns,
                    url = data.url.trim(),
                    image = data.image,
                    desc = data.desc,
                    caption = ((data.caption) ? data.caption : 'PEP.PH') + ((data.author) ? ' | by ' + data.author : ''),
                    title = data.title,
                    settings = 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600';


                switch(sns) {
                    case 'facebook':
                        var desc = desc.replace(/'/g, "%27");

                        window.open('https://www.facebook.com/sharer/sharer.php?u='+decodeURIComponent(url)+'&title='+decodeEntities(title)+'&description='+decodeEntities(desc)+'&caption='+decodeEntities(caption)+'&picture='+image, 'facebookwindow', settings);
                    break;
                    case 'twitter':
                        window.open('http://twitter.com/intent/tweet?url='+url+'&text='+decodeEntities(title)+'&via=Pepalerts', 'twitterwindow', settings);
                    break;
                    case 'whatsapp':
                        var desc = desc.replace(/'/g, "%27");
                        window.open('whatsapp://send?text=='+desc+'Check The Link Here'+url, 'whatsapp', settings);
                    break;
                    case 'googleplus':
                        window.open('https://plus.google.com/share?url='+url, 'googlepluswindow', settings);
                    break;
                    case 'pinterest':
                        window.open('//pinterest.com/pin/create/button/?url='+url+'&media='+image+'&description='+decodeEntities(title), 'pinterestwindow', settings);
                    break;
                }
            });
            </script>
