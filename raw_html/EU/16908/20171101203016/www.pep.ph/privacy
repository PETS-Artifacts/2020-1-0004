        <script>
            var gptAdSlots = [];
            var adsNum = 31;

            function loadAd($placeholder, dataID, type, size, keywords) {
                if (dataID == adsNum) {
                    $placeholder.append('<div id="div-gpt-ad-dynamic-' + adsNum + '" class="dfp_ad"></div>');
                    googletag.cmd.push(function() {
                        if (typeof keywords !== "undefined" && keywords.length != 0) {
                            console.log(keywords);
                            gptAdSlots[adsNum - 1] = googletag.defineSlot("/1015428/" + type, size, "div-gpt-ad-dynamic-" + adsNum).addService(googletag.pubads()).setTargeting("Tag", keywords).setSafeFrameConfig({allowPushExpansion: true});
                        } else {
                            gptAdSlots[adsNum - 1] = googletag.defineSlot("/1015428/" + type, size, "div-gpt-ad-dynamic-" + adsNum).addService(googletag.pubads()).setTargeting("Tag", "").setSafeFrameConfig({allowPushExpansion: true});
                        }
                        googletag.cmd.push(function(){ googletag.display('div-gpt-ad-dynamic-' + adsNum); });
                        googletag.pubads().refresh([gptAdSlots[adsNum - 1]]);
                        googletag.pubads().collapseEmptyDivs();
                    });
                    adsNum++;
                };
            }
        </script>
