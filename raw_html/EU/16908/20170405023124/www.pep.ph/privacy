			loadJS("http://contents.pep.ph/res/js/branded.min.gz", function(){
				var display_branded = setInterval(function(){

		        
	            	var s = $('.latest .row');

		    		if( typeof s !== "undefined" ) {
		    			clearInterval(display_branded);
		    		}

		    		var ref = (initialData.ID) ? 'article_featured' : 'featured';
                    $.BrandedContent({
                        flashboxAPI         : 'http://www.pep.ph/api/getBranded/flashbox',
                        ref                 : ref,
                        targetDiv           : 'top-stories-article'
                    });

	            		    				var ad_section = 'static';
	            	
		    		$.ajax({
		    			url : "http://data.pep.ph/pep-channels",
		    			type : "get",
		    			dataType : "json",
		    			success: function( response , status ){
		    				if( status === "success" ) {
				    			$.each(response , function( key , value ){
				    				/**************************************
				    				*
				    				* 	Sub-section / Sub-channel
				    				*
				    				**************************************/
				    				if( typeof is_subsection !== 'undefined' && is_subsection === true && typeof value.childs !== 'undefined' ) {

				    					$.each(value.childs , function( k , v ){
						    				if( v.channels_name == ad_section || v.channels_slug == ad_section ) {

						    					$.BrandedContent({
									            	sectionFeedAPI      : 'http://www.pep.ph/api/getBranded/sectionfeed/' + v.channels_id,
									            	selectedChannel     : v.channels_id
										        });

						    				}
				    					});

				    				} else {
					    				if( value.channels_name == ad_section || value.channels_slug == ad_section ) {

					    					$.BrandedContent({
								            	sectionFeedAPI      : 'http://www.pep.ph/api/getBranded/sectionfeed/' + value.channels_id,
								            	selectedChannel     : value.channels_id
									        });

					    				}
					    			}
				    			});
			    			}
		    			}
		    		})

	            
            }, 100);
			});
