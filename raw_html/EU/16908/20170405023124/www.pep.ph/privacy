		//include geo_ip function
		

		var base_url = 'http://www.pep.ph/';
		var xhr = new XMLHttpRequest();

		/*
		 * Geo location AJAX call
		 */
		function get_geo()
		{

			/*
			 * ajax call listener
			 */
			xhr.addEventListener('load', function(data){
				if (typeof data.target.responseText !== 'undefined') {
					var jData = JSON.parse(data.target.responseText);

					// set 'fullgeo' key in localStorage if not exists or not equal to ajax result
					if (window.localStorage.getItem('fullgeo') !== jData.ip) window.localStorage.setItem('fullgeo', data.target.responseText);
				}
			});

			/*
			 * fetch geo information
			 *
			 * @access public
			 * @query_string 	'_' 	random strings 	// always set this to bypass the caching
			 */
			xhr.open('GET', base_url + 'get_geo.php?_=' + Math.random());
			xhr.send();
		}


		// geo location call request
		get_geo();
		var	geo = JSON.parse(localStorage.getItem('fullgeo'));

		


