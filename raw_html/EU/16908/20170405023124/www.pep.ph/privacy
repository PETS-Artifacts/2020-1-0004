        		<script type="text/delayscript">
        //sns-button update for pep
             $(document).on('click', '.topemailarticle, .botemailarticle', function(){
                $('span.emailtrigger > a.emailtrigger > span.ra1-pw-icon-email').trigger('click');
                clearTimeout();
            });

            // $(document).on('click', '.topwhatsapparticle, .botwhatsapparticle', function(){
            //     $('span.whatsapptrigger > a.whatsapptrigger > span.ra1-pw-icon-whatsapp').trigger('click');
            //     clearTimeout();
            // });

            $(document).on('click', '.sns-control', function(){
                $(this).parents('.figure').toggleClass('sns-active');
            });

            var decodeEntities = (function() {
                // this prevents any overhead from creating the object each time
                var element = document.createElement('div');

                function decodeHTMLEntities (str) {
                  if(str && typeof str === 'string') {
                    // strip script/html tags
                    str = str.replace(/<script[^>]*>([\S\s]*?)<\/script>/gmi, '');
                    str = str.replace(/<\/?\w(?:[^"'>]|"[^"]*"|'[^']*')*>/gmi, '');
                    element.innerHTML = str;
                    str = element.textContent;
                    element.textContent = '';
                  }

                  return str;
                }

                return decodeHTMLEntities;
            })();

            $(document).on('click', '[data-sns]', function(){
                    var
                    data = $(this).data(),
                    sns = data.sns,
                    url = data.url.trim(),
                    image = data.image,
                    desc = data.desc,
                    caption = (data.caption) ? data.caption : 'PEP.PH',
                    title = data.title,
                    settings = 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600';

                    // if(data.author){
                    // 	caption = caption + ' | by ' + data.author;
                    // }


                switch(sns) {
                    case 'facebook':
                        var desc = desc.replace(/'/g, "%27");

                        window.open('https://www.facebook.com/sharer/sharer.php?u='+decodeURIComponent(url)+'&title='+decodeEntities(title)+'&description='+decodeEntities(desc)+'&caption='+decodeEntities(caption)+'&picture='+image, 'facebookwindow', settings);
                    break;
                    case 'twitter':
                        window.open('http://twitter.com/intent/tweet?url='+url+'&text='+decodeEntities(title)+'&via=Pepalerts', 'twitterwindow', settings);
                    break;
                    case 'whatsapp':
                        var desc = desc.replace(/'/g, "%27");
                        window.open('whatsapp://send?text=='+desc+'Check The Link Here'+url, 'whatsapp', settings);
                    break;
                    case 'googleplus':
                        window.open('https://plus.google.com/share?url='+url, 'googlepluswindow', settings);
                    break;
                    case 'pinterest':
                        window.open('//pinterest.com/pin/create/button/?url='+url+'&media='+image+'&description='+decodeEntities(title), 'pinterestwindow', settings);
                    break;
                }
            });


        //end sns update

			$(document).ready(function(){

				$('iframe[src*="youtube"]').wrap("<section class='pep-youtube-embed'></section>");

				$('iframe').each(function(){
					var iFrm = $(this),
					src  = iFrm.attr('src'),
					postType;
					if(typeof src !== 'undefined'){
						if(src.match("post.php"))
						{
							postType = 'fb-post';
						}
						else if(src.match("video.php"))
						{
							postType = 'fb-video';
						}

						else
						{
							return;
						}
					
					url = getUrlVars(src);

					iFrm.replaceWith($('<div>',{'class':postType, 'data-href': decodeURIComponent(url.href),'data-width': $("#main-content").width()}));
					}
				});

				$('#main-content').autoResizeFbPost();
				$(".news-footer-ad").addClass("gtm-footerAdvertise");
				$(".ra1-pw-icon-facebook").addClass("gtm-feedFbShare");
				$(".ra1-pw-icon-twitter").addClass("gtm-feedTwitterShare");

			})
			var lmcounter = 0;
			$(document).on("click", ".more-posts", function( event ){
				var kid = $( event.target ).children();

				$( "div.more-posts > p" ).removeClass("gtm-feedView"+lmcounter);
				$('#more-posts').removeClass("gtm-feedView"+(lmcounter));
				kid.removeClass("gtm-feedView"+lmcounter);
				$( "div.more-posts > p" ).addClass("gtm-feedView"+(lmcounter+1));
				$('#more-posts').addClass("gtm-feedView"+(lmcounter+1));
				kid.addClass("gtm-feedView"+(lmcounter+1));
				lmcounter++;

			});


			function onElementHeightChange(elm, callback){
			    var lastHeight = elm.clientHeight, newHeight;
			    (function run(){
			        newHeight = elm.clientHeight;
			        if( lastHeight != newHeight )
			            callback();
			        lastHeight = newHeight;

			        if( elm.onElementHeightChangeTimer )
			            clearTimeout(elm.onElementHeightChangeTimer);

			        elm.onElementHeightChangeTimer = setTimeout(run, 200);
			    })();
			}


			onElementHeightChange(document.body, function(){

			    $(".ra1-pw-icon-facebook").addClass("gtm-feedFbShare");
				$(".ra1-pw-icon-twitter").addClass("gtm-feedTwitterShare");

				$('.topfblike').children().addClass('gtm-articleFbLikeA');
				$('.botfblike').children().addClass('gtm-articleFbLikeB');

				$('.topfbarticle').children().removeClass('gtm-feedFbShare');
				$('.toptwitterarticle').children().removeClass('gtm-feedTwitterShare');
				$('.palertsfb').children().removeClass('gtm-feedFbShare');
				$('.palertstwitter').children().removeClass('gtm-feedTwitterShare');

				$('.topfbarticle').children().addClass('gtm-articleFbShareA');
				$('.toptwitterarticle').children().addClass('gtm-articleTwitterShareA');
				$('.palertsfb').children().addClass('gtm-alertsFbShare');
				$('.palertstwitter').children().addClass('gtm-alertsTwitterShare');

				$('.palertsemail').children().addClass('gtm-alertsEmail');
				$('.palertswhatsapp').children().addClass('gtm-alertsWhatsappA');

				$('.topemailarticle').children().addClass('gtm-articleEmailA');
				$('.topcommentarticle').addClass('gtm-articleCommentA');
				$('.topwhatsapparticle').children().addClass('gtm-articleWhatsappA');

				$('.botfbarticle').children().removeClass('gtm-feedFbShare');
				$('.bottwitterarticle').children().removeClass('gtm-feedTwitterShare');

				$('.botfbarticle').children().addClass('gtm-articleFbShareB');
				$('.bottwitterarticle').children().addClass('gtm-articleTwitterShareB');


				$('.botwhatsapparticle').children().addClass('gtm-articleWhatsappB');
				$('.botemailarticle').children().addClass('gtm-articleEmailB');
				$('.botcommentarticle').addClass('gtm-articleCommentB');
			});


			//helper
			function getUrlVars(url)
			{
				var vars = {}, hash;
				var hashes = url.slice(url.indexOf('?') + 1).split('&');

				for(var i = 0; i < hashes.length; i++)
				{
					hash = hashes[i].split('=');
					vars[hash[0]] = hash[1];
				}
				return vars;
			}

			(function($) {

				jQuery.fn.autoResizeFbPost = function() {

					var changeWidth = function($container, doParse) {
			            // default parameter.
			            doParse = typeof doParse == 'undefined' ? true : doParse;

			            var updatedWidth = $container.width();

						// update all div.fb-post with correct width of container
						$container
						.find('div.fb-post')
						.each(function() {
							$(this).attr('data-width', updatedWidth);
						});

			            // should we call FB.XFBML.parse? we don't want to do this at page load because it will happen automatically
			            if (doParse && FB && FB.XFBML && FB.XFBML.parse)
			            	FB.XFBML.parse();
			        };

			        return this.each(function() {
			        	var $container = $(this),
			        		$oldwidth = $container.width();

						// make sure there is a .fb-post element before we do anything.
						if ( ! $container.find('div.fb-post').length) {
							return false;
						}

						// execute once (document.ready) and do not call FB.XFBML.parse()
						changeWidth($container, false);

						// on window resize, update and fix..
						$(window).on('resize', function() {
							// only trigger changeWidth once after window has not been resized for 1 second

							if($oldwidth != $container.width())
								delayFireOnce(1000).done(function() {
									changeWidth($container);
								});

						});

						// helper function
						var	delayTimer;
						var delayFireOnce = function(timeout) {
							var d = $.Deferred();
							clearTimeout(delayTimer);

							t = setTimeout(function() {
								d.resolve();
							}, timeout);

							return d.promise();
						};

					});
			    };

			})(jQuery);

		</script>
	</body>
</html>
