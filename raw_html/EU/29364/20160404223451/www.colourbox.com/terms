<script>
$(function() {
	$('.js-colourboxFooter--header').on('click', 'a', function(e) {
		var action = '';
		if ($(e.target).is('#footer_newsmail > a')) {
			action = 'email subscribe';
		} else {
			if ($(e.currentTarget).hasClass('facebook_a')) {
				action = 'facebook';
			} else if ($(e.currentTarget).hasClass('twitter_a')) {
				action = 'twitter';
			} else if ($(e.currentTarget).hasClass('gplus_a')) {
				action = 'google+';
			} else if ($(e.currentTarget).hasClass('pinterest_a')) {
				action = 'pinterest';
			} else {
				return;
			}
		}
		GoogleTracker.report('footer', 'click', action);
	});
});
</script>
			
