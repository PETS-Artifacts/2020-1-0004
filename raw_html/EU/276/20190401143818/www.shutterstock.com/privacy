    var previousOnError = window.onerror;
    window.onerror = function(msg, u, line, column, err) {
      var url = u || 'untrusted external vendor script';
      var message = 'UncaughtException in ' + url + ' l:' + line + ' c:' + column + ' reason: ' +  message;

      if (!err || msg === 'Script error.') {
        var scriptError = new Error(message);
        scriptError.name = 'VendorScriptError';
        window.NREUM.noticeError(scriptError);
        return true;
      }
      if (previousOnError) return previousOnError(message, url, line, column, err);
      throw err;
    };

