    <script>
    function addParam(div_id, param) {
        $("#" + div_id + " a").each(function () {
            var href = $(this).attr('href');
            if (href.indexOf('?') == -1) {
                $(this).attr('href', href + '?lo=' + param);
            } else {
                $(this).attr('href', href + '&lo=' + param);
            }
        });
    }

    //Top utility bar
    addParam("utility-bar-z1", "ut_a1");
    addParam("utility-bar-z2", "ut_b1");
    addParam("utility-bar-z3", "ut_c1");
    addParam("utility-bar-z4", "ut_d1");
    addParam("utility-bar-z5", "ut_e1");
    //Secondary utility bar
    $("#utility-bar-secondary-div li").each(function (i, value) {
        $(value).find('a').each(function() {
            var href = $(this).attr('href');
            var param = "ut_" + String.fromCharCode(97+i) + "2";
            if (href.indexOf('?') == -1) {
                $(this).attr('href', href + '?lo=' + param);
            } else {
                $(this).attr('href', href + '&lo=' + param);
            }
        });
    });
</script>
<div class="super  fixed-top">
    <div class="super-inner">
        <div class="content layout-full">
            <section class="content-groupset pos-omega" >
                <div class="content-group ad ">
<div class="ad-slot flex horizontal " id="pol-01">
        </div>

</div>
</section>
