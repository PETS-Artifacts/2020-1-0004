    <script type="text/javascript">
        $(function(){
            if ($(".wiki_content").find("h1,h2,h3,h4,h5,h6").length > 0) {
                $("#toc").tableOfContents(
                    $(".wiki_content"),      // Scoped to div#wrapper
                    {
                        startLevel: 1,    // H2 and up
                        depth:      4,    // H2 through H4,
                        topLinks:   true, // Add "Top" Links to Each Header
                    }
                );
            } else {
                $(".toc_wrap").hide();
            }
            $(".toc_toggle").click(function(e){e.preventDefault();
                $(".toc_container").slideToggle();
                $(this).toggleClass("active");
            });
            $("a.toc-top-link").click(function(e) {e.preventDefault();
                $("html, body").animate({ scrollTop: 0 }, "slow");
                location.hash = "";
                return false;
            });
            $('#toc a').click(function(){
                $('html, body').animate({
                    scrollTop: $( $(this).attr('href') ).offset().top - 20
                }, 500);
                location.hash = $(this).attr("href");
                return false;
            });
        });
    </script>
