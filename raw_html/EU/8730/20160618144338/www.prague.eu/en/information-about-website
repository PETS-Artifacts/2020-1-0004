$(function(){
var puNlSubscribe_updateWidget = null;
    var puNlSubscribe_settings = {"suppressRedirect":true};
    var puNlSubscribe_enhanceWithPartialUpdate = function() {
        $("#puNlSubscribe").partialUpdate("http://www.prague.eu", "/qf/en/ramjet/nlSubscribe/nlSubscribeForm", puNlSubscribe_settings, null, puNlSubscribe_updateWidget);
        var triggerPU = function() {
            $(this).trigger("partialUpdate");
        };

        if ($.data(document, "partialUpdateContentsReplacedApplied_puNlSubscribe") !== true) {
            $(document).on("afterContentsReplaced", "#puNlSubscribe", function() { puNlSubscribe_enhanceWithPartialUpdate() });
            $.data(document, "partialUpdateContentsReplacedApplied_puNlSubscribe", true);
        }
    };
    if (jQuery.data(document, "partialUpdateContentsReplacedApplied_puNlSubscribe") !== true) {
        puNlSubscribe_enhanceWithPartialUpdate();
        jQuery(document).on("domRemoved", function() {
            if ($(this, "#puNlSubscribe").length > 0) {
                jQuery(document).off("afterContentsReplaced", "#puNlSubscribe");
                jQuery.data(document, "partialUpdateContentsReplacedApplied_puNlSubscribe", false);
            }
        });
