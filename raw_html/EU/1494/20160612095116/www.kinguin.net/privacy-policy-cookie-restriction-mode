    jQuery(document).ready(function() {
        if((jQuery("body").hasClass("homepage-index-index")) || (jQuery("body").hasClass("cms-index-index"))) {

            //top-banner
            jQuery('#top-banner').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Top Banner');
            });

            //Search
            jQuery('.form-search .button').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Search');
            });

            //Search Enter
            jQuery('.form-search').keydown(function (oEvent) {
                if (oEvent.keyCode == '13') {
                    gaTrackEvent('Homepage', 'Click', 'Search');
                }
            });

            //Search Suggestions
            jQuery('#search_autocomplete').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Search Suggestion');
            });

            //Hot Stuff
            jQuery('.hot_stuff .text a').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Hot Stuff');
            });

            //Genres
            jQuery('.genres_bg ul li').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Genres');
            });

            //Platform
            jQuery('.platform_bg ul li').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Platform');
            });

            //Skins
            jQuery('.selectedproducts_bg').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Skins');
            });

            //Sign In and Profile
            jQuery('.name a').click(function () {
                if(jQuery(".name a").attr("data-slbch")) {
                    gaTrackEvent('Homepage', 'Click', 'Sign In');
                }
                else {
                    gaTrackEvent('Homepage', 'Click', 'Profile');
                }
            });

            //Support
            jQuery('#help_menu .popup1 a').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Support');
            });

            //Login popup banner
            jQuery('#login_popup div a').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Login Banner');
            });

            //Big banner
            jQuery('#banner .top-banner a').click(function () {
                var find_div_with_id = jQuery(this).find('div').attr('id');
                if (typeof find_div_with_id == "undefined") {
                    gaTrackEvent('Homepage', 'Click', 'Big Banner - ' + (jQuery('#dg-container .dg-wrapper > a').index(this) + 1));
                }
                else if (find_div_with_id.indexOf('countdown_dashboard') > 0) {
                    gaTrackEvent('Homepage', 'Click', 'Big Banner - ' + (jQuery('#dg-container .dg-wrapper > a').index(this) + 1 + " - Special Banner"));
                }
            });

            //Small banner add attr rel
            jQuery('.jcarousel1 li').each(function () {
                jQuery(this).attr('rel', jQuery(this).index() + 1);
            });

            //Now trending
            jQuery('.feed-wrapper .visible .carousel .jCarouselFeed ul li a').click(function () {
                gaTrackEvent('Homepage', 'Click', 'Now Trending');
            });

                jQuery('.product-tabs-content .ajaxtabs div').click(function () {
                    var tab_id = jQuery(this).attr('id');
                        switch (tab_id) {
                            case "product_tabs_news_content":
                                gaTrackEvent('Homepage', 'Click', 'Tab New');
                                break;

                            case "product_tabs_bestsellers_content":
                                gaTrackEvent('Homepage', 'Click', 'Tab Bestsellers');
                                break;

                            case "product_tabs_special_content":
                                gaTrackEvent('Homepage', 'Click', 'Tab Prepaid');
                                break;

                            case "product_tabs_preorder_content":
                                gaTrackEvent('Homepage', 'Click', 'Tab Preorder');
                                break;

                            case "product_tabs_under30_content":
                                gaTrackEvent('Homepage', 'Click', 'Tab Top 30 EUR');
                                break;

                            case "product_tabs_under20_content":
                                gaTrackEvent('Homepage', 'Click', 'Tab Top 20 EUR');
                                break;

