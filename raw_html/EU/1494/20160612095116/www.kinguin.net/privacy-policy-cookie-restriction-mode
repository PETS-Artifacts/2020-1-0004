            jQuery(document).ready(function() {

                jQuery(document).on("click", "#js-language-switcher-toggle-button", function(e) {
                    e.preventDefault();
                    jQuery("#js-language-switcher-country_menu").toggle();
                    jQuery("#js-language-switcher-toggle-button").toggleClass("open");
                });

                jQuery(document).on("mouseup", "#js-language-switcher-country_menu", function() {
                    return false
                });

                jQuery(document).mouseup(function(e) {
                    if(jQuery(e.target).parent("#js-language-switcher-toggle-button").length==0) {
                        jQuery("#js-language-switcher-toggle-button").removeClass("open");
                        jQuery("#js-language-switcher-country_menu").hide();
                    }
                });
            });
        </script>
