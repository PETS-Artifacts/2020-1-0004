      jQuery('.popup1').stop(true, true).fadeIn('fast');
  }, 300));
}).live('mouseleave', function() {
  clearTimeout(jQuery.data(this, 'timer'));
  jQuery('.popup1').stop(true, true).fadeOut('fast');
