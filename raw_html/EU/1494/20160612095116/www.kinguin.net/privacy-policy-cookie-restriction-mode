<script type="text/javascript">
jQuery('.live_chat_footer').live('mouseenter', function() {
  jQuery.data(this, 'timer', setTimeout(function() {
      jQuery('.live_chat_footer_popup').stop(true, true).fadeIn('fast');
  }, 300));
}).live('mouseleave', function() {
  clearTimeout(jQuery.data(this, 'timer'));
  jQuery('.live_chat_footer_popup').stop(true, true).fadeOut('fast');
});

jQuery(document).ready(function() {
    
    var cookieName = 'livechat_cookie';
    var cookieValue = 1;
    var cookie = jQuery.cookie(cookieName,cookieValue);
    if (jQuery.cookie(cookieName)) {
        jQuery('.live_chat_footer_popup').css('display', 'block').delay(8000).fadeOut('slow');
    } else {
        jQuery('.live_chat_footer_popup').css('display', 'none')
    }
    
  });
</script>
