<script type="text/javascript">
    //<![CDATA[
        jQuery(document).ready(function($) {
            $(".signin1").click(function(e) {
				e.preventDefault();
                $("fieldset#login_menu").toggle();
				$(".signin1").toggleClass("menu-open");
            });

			$("fieldset#login_menu").mouseup(function() {
				return false
			});
			$(document).mouseup(function(e) {
				if($(e.target).parent("a.signin1").length==0) {
					$(".signin1").removeClass("menu-open");
					$("fieldset#login_menu").hide();
				}
			});
        });
        //]]>
</script>
