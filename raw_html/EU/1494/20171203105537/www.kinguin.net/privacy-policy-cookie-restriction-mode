<script type="text/javascript">
    jQuery('.genres_btn').live('mouseenter', function() {
        var element = jQuery(this);
        jQuery.data(this, 'timer', setTimeout(function() {
            element.find('.dropdown-content').stop(true, true).fadeIn('fast');
        }, 200));
    }).live('mouseleave', function() {
        var element = jQuery(this);
        clearTimeout(jQuery.data(this, 'timer'));
        element.find('.dropdown-content').stop(true, true).fadeOut('fast');
    });
</script>
