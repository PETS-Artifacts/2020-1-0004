<script type="text/javascript">
    // Overwrite jQuery sign
    jQuery(document).ready(function($) {
        // Change confirm state function
        var confirmAction = function() {
            $('html,body').animate({ scrollTop: 0 }, 'slow');
            $('div.fast2play').toggleClass('fast2play--active');
            $('body').toggleClass('js-information-popup-scrolling');

            // Add exception class if body height is bigger than container height
            if ( $(window).height() <= $('.fast2play__confirm').height() ) {
                $('.fast2play__confirm').addClass('fast2play__confirm--bexception');
            }
        }

        // Listeners
        $('body').on('click', 'li#js-fast2play > a, div#js-fast2play-panel, div#fast2play__confirm-btn-close', function() {
            confirmAction();
        });

        // Catch key events - to close confirm popup with escape
        $(document).keydown(function(e) {
            if ( e.keyCode == 27 && $('.fast2play--active').length ) {
                confirmAction();
            }
        });

        // Catch banner click event to activate fast2play confirm popup
        $('body').on('click', '#bottomBanner li, a#top-banner, #homebanners-section .block-content', function(e) {
            // Check if target is close btn on top-banner
            if ( $(e.target).attr("class") == 'top-banner-close' ) return false;

            // Get attr and check for #fast2play sign
            var hrefAttr = ( $(this).attr('href') ) ? $(this).attr('href') : $(this).find('a').attr('href');

            if ( hrefAttr.indexOf('#fast2play') != -1  ) {
                if( $('li#js-fast2play > a').length ) $('li#js-fast2play > a').click();
                else confirmAction();
            }
        });
    });
</script><script type="text/javascript">
    var displayPopupAfterRemoveItem = function(text) {
        if(Kingpopup === undefined) {
            console.error('Kingpopup not defined');
            return false;
        }

        Kingpopup.fill(
            'Cart',
            '<p>' + text + '</p>',
            '400px'
        );
        Kingpopup.addButton('OK', 'non-dont-remove-main-product', 'success');

        Kingpopup.show();
    };

    var displayPopupBeforeRemoveCartItem = function(data, id, isRefresh) {
        if(Kingpopup === undefined) {
            console.error('Kingpopup not defined');
            return false;
        }

        Kingpopup.fill(
            'Cart',
            '<p>' + data.data.popup_text + '</p>',
            '400px'
        );
        Kingpopup.addButton('Cancel', 'non-dont-remove-main-product');
        Kingpopup.addButton('OK', 'non-remove-main-product', 'success', function(event, popup){
            popup.close();
            removeCartItem(id, isRefresh);
            setTimeout(function(){
                displayPopupAfterRemoveItem(data.data.popup_after_removed_text);
            }, 1000);

        });

        Kingpopup.show();
    };

    var displayPopupBeforeRemoveMiniCartItem = function(data, id, isRefresh) {
        if(Kingpopup === undefined) {
            console.error('Kingpopup not defined');
            return false;
        }

        Kingpopup.fill(
            'Cart',
            '<p>' + data.data.popup_text + '</p>',
            '400px'
        );
        Kingpopup.addButton('Cancel', 'non-dont-remove-main-product');
        Kingpopup.addButton('OK', 'non-remove-main-product', 'success', function(event, popup){
            popup.close();
            removeMiniCartItem(id, isRefresh);
            setTimeout(function(){
                displayPopupAfterRemoveItem(data.data.popup_after_removed_text);
            }, 1000);
        });

        Kingpopup.show();
    }

    function showNoNRemoveMiniCartItemPopup(id, isRefresh) {
        checkShowNoNRemovePopup(id).then(function(data){
            if (data.data.popup_text) {
                displayPopupBeforeRemoveMiniCartItem(data, id, isRefresh);
            } else {
                removeMiniCartItem(id, isRefresh);
            }
        });
    };


    function cartShowMessage(message, isError) {
        isError = isError || false;

        cartClearMessage();

        var messages = jQuery('<ul/>').addClass('messages');
        var messageType = jQuery('<li/>').addClass(isError ? 'error-msg' : 'success-msg').appendTo(messages);
        var ul = jQuery('<ul/>').appendTo(messageType);

        if(Array.isArray(message)) {
            for(var i = 0; i < message.length; i++) {
                var li = jQuery('<li/>').appendTo(ul);
                jQuery('<span/>').text(message[i]).appendTo(li);
            }
        } else {
            var li = jQuery('<li/>').appendTo(ul);
            jQuery('<span/>').text(message).appendTo(li);
        }

        jQuery('.checkout-cart-index .cart .page-title').after(messages);
    }

    function cartClearMessage() {
        jQuery('.checkout-cart-index .cart ul.messages').remove();
    }

    function updateCart(form) {
        jQuery.ajax({
            url: 'https://www.kinguin.net/checkout/cart/updatePostAjax/',
            type: 'post',
            data: form.serialize(),
            dataType: 'json',
            success: function(json) {
//                if (json.redirect != undefined) {
//                    window.location = json.redirect;
//                }

                jQuery('#shopping-cart-totals-table').html(json.html);

                updateSlider(json.maxSpendBalance);

                if(json.message) {
                    cartShowMessage(json.message, json.error);
                    jQuery('.btn-proceed-checkout').hide();
                } else {
                    cartClearMessage();
                    jQuery('.btn-proceed-checkout').show();
                }
                if (form.isRefresh != undefined && form.isRefresh) {
                    removeMiniCartItem(null, false);
                }

                for(var x in json.cart) {
                    jQuery('#price-' + x).html(json.cart[x]['price']);
                    jQuery('#price-with-tax-' + x).html(json.cart[x]['priceWithTax']);
                }
            },
            error: function() {
                form.submit();
            }
        });
    }

    function updateSlider(maxValue) {
        if(jQuery('#balance-slider').length) {
            var currentValue = jQuery('#balance-slider').slider('value');
            jQuery('#balance-slider').slider('option', 'max', maxValue);
            jQuery('#balance-slider').slider('option', 'value', (currentValue > maxValue ? maxValue : currentValue));
            if(currentValue > maxValue) {
                jQuery("#redeem").val(maxValue);
                jQuery("#spend-balance input").val(maxValue.toFixed(2));
            }
        }
    }

    function updateMiniCart() {
        removeMiniCartItem(null, false);
    };

    function checkShowNoNRemovePopup(id) {
        return jQuery.ajax({
            url: 'https://www.kinguin.net/non/index/showNoNRemovePopup/',
            data: {
                id:id,
                form_key: window.APP_VARS.form_key
            },
            type : 'get',
            dataType: 'json',
            error:function(json) {
                console.log("failure", json.responseText.html);
            }
        });
    }

    function removeCartItem(id, isRefresh) {
        jQuery.ajax({
            url: 'https://www.kinguin.net/checkout/cart/cartUpdateAjax/',
            data: {
                id:id,
                form_key: window.APP_VARS.form_key
            },
            type: 'get',
            dataType: 'json',
            success: function(json) {
//                if (json.redirect != undefined) {
//                    window.location = json.redirect;
//                }
                if (!json.empty_cart) {
                    jQuery('#fieldset-shopping-cart-table').html(json.html);
                    taxRate = parseFloat('1');
                    addTaxes(taxRate, false, '.price.add-tax-rate');
                    var form = jQuery(this).closest('form');
                    form.isRefresh = isRefresh;
                    qtyAjaxAddEvent();
                    updateCart(form);
                } else {
                    
                    if (isRefresh != undefined && isRefresh) {
                        removeMiniCartItem(id, false);
                    }
                }
            },
            error: function(json) {
                console.log("failure", json.responseText);
            }
        });
    }



    function showNoNRemoveItemPopup(id, isRefresh) {
        checkShowNoNRemovePopup(id).then(function(data){
            if (data.data.popup_text) {
                displayPopupBeforeRemoveCartItem(data, id, isRefresh);
            } else {
                removeCartItem(id, isRefresh);
            }
        });
    };

    function updateCartAjax() {
        var form = jQuery(this).closest('form');
        form.isRefresh = true;
        updateCart(form);
    };

    function qtyAjaxAddEvent() {
        jQuery('input.qty').change(updateCartAjax);
        jQuery('button[name="update_cart_action"]').click(function(event) {
            event.preventDefault();
        });
    };

    jQuery(document).on("change", "input[type='checkbox']", function() {
        var form = jQuery(this).closest('form');
        form.isRefresh = true;
        updateCart(form);
    });

    qtyAjaxAddEvent();

    function removeMiniCartItem(id, isRefresh) {
        jQuery.ajax({
            url: 'https://www.kinguin.net/multivendor/minicart/cartUpdateAjax/',
            data: {
                id:id,
                form_key: window.APP_VARS.form_key
            },
            type : 'get',
            dataType: 'json',
            success: function(json) {
                jQuery('.cart-hover-area').parent().html(json.html);
                if (id != undefined && $$('.mini-cart.ajax')[0] != undefined) {
                    $$('.mini-cart.ajax')[0].show();
                }
                if (isRefresh != undefined && isRefresh) {
                    try {
                        removeCartItem(id, false);
                    } catch (err) {
                        console.log("info", err.message);
                    }
                }
            },
            error:function(json) {
                console.log("failure", json.responseText.html);
            }
        });
    };
</script><script type="text/javascript">
jQuery(document).ready(function() {
    taxRate = parseFloat('1');
    addTaxes(taxRate, false, '.price.add-tax-rate');
});

function addTaxes(taxRate, forceCalculation, elements) {
    if (taxRate > 1 || forceCalculation) {
        var zeroPrecision = '0';
        var buyerProtectionSub = '0';
        var includeTaxPriceComparision = '0';
        var JSONpriceFormat =  '{"pattern":"$%s","precision":2,"requiredPrecision":2,"decimalSymbol":".","groupSymbol":",","groupLength":3,"integerRequired":1}';
        var priceFormat = JSONpriceFormat.evalJSON(true);
        var separator = priceFormat.decimalSymbol;
        jQuery(elements).each(function(e) {
            noTaxPrice = parseFloat(jQuery(this).attr('data-no-tax-price'));
            bpSubVal = 0;
            if (buyerProtectionSub == 1 && jQuery(this).next('.bp-sub').length) {
                bpSubVal = parseFloat(jQuery(this).next('.bp-sub').val());
            }

            if (bpSubVal > 0) {
                noTaxPrice = noTaxPrice - bpSubVal;
            }

            if (0 == includeTaxPriceComparision && jQuery(this).parent().hasClass('external-vendor-price')) {
                // don't add tax to price comparision offers
                priceWithTax = noTaxPrice;
            } else {
                priceWithTax = noTaxPrice * taxRate;
            }

            if (bpSubVal > 0) {
                priceWithTax = priceWithTax + bpSubVal;
            }

            priceWithTax = Math.round(priceWithTax * 1000) / 1000;
            priceWithTax = Math.round(priceWithTax * 100) / 100;
            if (zeroPrecision == 1) {
                priceWithTax = Math.round(Math.round(priceWithTax * 10) / 10);
            }

            priceWithTax = formatCurrency(priceWithTax, priceFormat);
            if (zeroPrecision == 1) {
                priceWithTax = priceWithTax.replace(separator+'00', '');

            }

            priceWithTaxElement = (priceWithTax + "").split(separator);
            priceLeft = priceWithTaxElement[0];
            if (priceLeft === undefined) {
                priceLeft = "";
            }

            priceRight = priceWithTaxElement[1];
            if (priceRight === undefined) {
                priceRight = "";
            } else {
                priceRight = separator + priceRight;
            }
            if (zeroPrecision == 1) {
                priceText = priceLeft;
            } else {
                priceText = priceLeft + '<span class="super">' + priceRight + '</span>';
            }

            if (taxRate == 1) {
                taxTxt = '';
            } else {
                taxTxt = '';      /*   text remove Incl. Tax  */
            }

            priceText += '<span class="tax-info">' + taxTxt + '</span>';

            jQuery(this).html(priceText);
        });
        
        percentElements = jQuery('.percent-price');
        if (percentElements.size() > 0) {
            jQuery(percentElements).each(function(e) {
                firstPrice = parseFloat(jQuery(this).attr('data-first-price'));
                secondPrice = parseFloat(jQuery(this).attr('data-second-price')) * taxRate;
                percent = parseInt((secondPrice / firstPrice) * 100 - 100);
                jQuery(this).html('<span>' + percent + '%</span>')
            })
            
        } 

                
        
        
    }
    jQuery('.price.add-tax-rate').removeClass('hidden');
}
