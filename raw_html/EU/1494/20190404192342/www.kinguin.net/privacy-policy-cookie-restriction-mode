    var tfa = {
        cssSelector: {
            authBoxId: 'authenticationBox',
            tfaCustomerEmail: '#authenticationBox span.authenticationBox__customer_email',
        },
        helper: {
            showAuthBox: function () {
                $(tfa.cssSelector.authBoxId).show();

                return this;
            },
            hideAuthBox: function () {
                $(tfa.cssSelector.authBoxId).hide();

                return this;
            },
            setCustomerEmail: function (email) {
                $$(tfa.cssSelector.tfaCustomerEmail)[0].innerHTML = email;

                return this;
            }
        }
    }

        
