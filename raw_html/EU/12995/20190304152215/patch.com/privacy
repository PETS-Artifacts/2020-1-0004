 <!--Pre-GPT sync code-->
     <script>

            var TIMEOUT = 2000;

            //declare bidders
            var bidders = ['a9', 'prebid','moat'];
            
            // create a requestManager to keep track of bidder state to determine when to send ad server
            var requestManager = {
                adserverRequestSent: false,
            };

            //loop through bidder array and add the bidders to the request manager:
            bidders.forEach(function(bidder) {
                requestManager[bidder] = false;
              });
            // return true if all bidders have returned
            function allBiddersBack() {
                var enabledBidders = [];
                if(Drupal.settings.client_ads_dfp.prebid_enable){
                  enabledBidders.push('prebid');
                }
                if(Drupal.settings.client_ads_dfp.amazon_enable){
                  enabledBidders.push('a9');
                } /*if(Drupal.settings.client_ads_dfp.moat_enable){
                  enabledBidders.push('moat');
                }  */             
                var allBiddersBack = enabledBidders
                    // get the booleans from the object
                    .map(function(bidder) {
                        return requestManager[bidder];
                    })
                    // get rid of false values - indicates that the bidder has responded
                    .filter(Boolean)
                    // if length is equal to bidders, all bidders are back
                    .length === enabledBidders.length;
                return allBiddersBack;
            }

       // handler for header bidder responses
        function bidsBackForBidder(msg) {
              bidder=msg;
                // return early if request to adserver is already sent
                if (requestManager.adserverRequestSent === true) {
                    return;
                }
                // set the bidder targeting and flip bidder back flag
                if (bidder === 'a9') {
                    requestManager.a9 = true;
                } else if (bidder === 'prebid') {
                    requestManager.prebid = true;
                } /*else if(bidder === 'moat'){
                  requestManager.moat = true;
                }*/
                // if all bidders are back, send the request to the ad server
                if (allBiddersBack()) {
                    sendAdserverRequest();
                }
        }

               // actually get ads from DFP
            function sendAdserverRequest() {
                // return early if request already sent
                if ( requestManager.adserverRequestSent === true) {
                    return;
                }
                // flip the boolean that keeps track of whether the adserver request was sent
                requestManager.adserverRequestSent = true;
                // make ad request to DFP
                googletag.cmd.push(function() {
                    if(Drupal.settings.client_ads_dfp.prebid_enable && pbjs){
                          pbjs.setTargetingForGPTAsync();
                    }
                    googletag.pubads().refresh();
                });
                requestManager.adserverRequestSent = false;
            }

           



            var ad_media = 'desktop';
            if (mobile_detect.is_phone) {
                ad_media = 'mobile';
            }

  
            function fetchA9Bids(az_slots, timeoutA9) {
            if (az_slots.length) {
              
               googletag.cmd.push(function () {
                  apstag.fetchBids({
                    slots: az_slots,
                    timeout: timeoutA9
                  }, function(bids) {
                    apstag.setDisplayBids();
                    bidsBackForBidder("a9");
                  });
                });
              }
             
            }

            function constructPrebidAdUnits(ad_units){
              var maxTabletWidth = 992;
              var windowWidth = window.innerWidth
              || document.documentElement.clientWidth
              || document.body.clientWidth;
              var client = mobile_detect.is_phone || (mobile_detect.is_tablet && (maxTabletWidth > windowWidth))
              // Set client to mobile
              ? 'mobile'
              // Otherwise desktop
              : 'desktop';
              var adUnits = [];
              
              var ad_units_keys = Object.keys(ad_units);
              for(var j=0; j < ad_units_keys.length; j++){
                var slot_name= ad_units_keys[j];
                var indiv_unit = ad_units[slot_name];
         
                //only push mobile or desktop units based on mobiledetect (md) above
                if (indiv_unit.ad_media == client) {
                  var adUnit = {};
                  var bids = [];
                  //adUnit.code = unit_prefix + indiv_unit.ad_target;
                  adUnit.code = indiv_unit.ad_target;
                  adUnit.mediaTypes = {
                    banner: {
                      sizes: indiv_unit.ad_size_arr
                    }
                  };
                  //Openx
                  if (indiv_unit.hasOwnProperty('openx')) {
                    if (indiv_unit.openx.hasOwnProperty(slot_name)) {
                      var openx_unit = indiv_unit.openx[slot_name];
                      bids.push({
                        bidder: 'openx',
                        params: {
                          delDomain: openx_unit.del_domain,
                          unit: openx_unit.unit
                        }
                      });
                    } //if openx has slot
                  } //if openx unit
                  //APPNEXUS
                  if (indiv_unit.hasOwnProperty('appnexus')) {
                    if (indiv_unit.appnexus.hasOwnProperty(slot_name)) {
                      var appnexus_unit = indiv_unit.appnexus[slot_name];
                      bids.push({
                        bidder: 'appnexus',
                        params: {
                          placementId: appnexus_unit.placementId,
                        }
                      });
                    } //if appnexus has slot
                  } //if appnexus unit
                  //IndexExchange
                  if (indiv_unit.hasOwnProperty('index_exchange')) {
                    //loop through IX unit sizes:
                    if (indiv_unit.index_exchange.hasOwnProperty(slot_name)) {
                      var index_exchange_unit = indiv_unit.index_exchange[slot_name];
                      for (var i = 0; i < indiv_unit.ad_size_arr.length; i++) {
                        var bid = {
                          bidder: 'ix',
                          params: {
                            siteId: index_exchange_unit.siteId,
                            size: indiv_unit.ad_size_arr[i]
                          }
                        };
                        bids.push(bid);
                      }
                    } //if index_exchange has slot
                  } //if index_exchange unit
                  //Triplelift
                  if (indiv_unit.hasOwnProperty('triplelift')) {
                    if (indiv_unit.triplelift.hasOwnProperty(slot_name)) {
                      var triplelift_unit = indiv_unit.triplelift[slot_name];
                      bids.push({
                        bidder: 'triplelift',
                        params: {
                          inventoryCode: triplelift_unit.inventoryCode,
                        }
                      });
                    } //if triplelift has slot
                  } //if triplelift unit
                   //Yieldmo
                   if (indiv_unit.hasOwnProperty('yieldmo')) {
                    if (indiv_unit.yieldmo.hasOwnProperty(slot_name)) {
                      var yieldmo_unit = indiv_unit.yieldmo[slot_name];
                      bids.push({
                        bidder: 'yieldmo',
                        params: {
                          placementId: yieldmo_unit.placementId,
                        }
                      });
                    } //if yieldmo has slot
                  } //if yieldmo unit
                  //Crieo
                  if (indiv_unit.hasOwnProperty('criteo')) {
                    if (indiv_unit.criteo.hasOwnProperty(slot_name)) {
                      var criteo_unit = indiv_unit.criteo[slot_name];
                      bids.push({
                        bidder: 'criteo',
                        params: {
                          zoneId: criteo_unit.zoneId,
                        }
                      });
                    } //if criteo has slot
                  } //if criteo unit
                  //Audience Network
                  if (indiv_unit.hasOwnProperty('audience_network')) {
                    if (indiv_unit.audience_network.hasOwnProperty(slot_name)) {
                      var audience_network_unit = indiv_unit.audience_network[slot_name];
                      bids.push({
                        bidder: 'audienceNetwork',
                        params: {
                          placementId: audience_network_unit.placementId,
                        }
                      });
                    } //if audience_network has slot
                  } //if audience_network unit
                  //Pubmatic
                  if (indiv_unit.hasOwnProperty('pubmatic')) {
                    //loop through pubmatic unit sizes:
                    if (indiv_unit.pubmatic.hasOwnProperty(slot_name)) {
                      var pubmatic_unit = indiv_unit.pubmatic[slot_name];
                      bids.push({
                        bidder: 'pubmatic',
                        params: {
                          publisherId: pubmatic_unit.publisherId,
                          adSlot: pubmatic_unit.adSlot,
                        }
                      });
                    } //if pubmatic has slot
                  } //if pubmatic unit
                  // Sharethrough
                  if (indiv_unit.hasOwnProperty('share_through')) {
                    //loop through share_through unit sizes:
                    if (indiv_unit.share_through.hasOwnProperty(slot_name)) {
                      var share_through_unit = indiv_unit.share_through[slot_name];
                      bids.push({
                        bidder: 'sharethrough',
                        params: {
                          pkey: share_through_unit.pkey
                        }
                      });
                    } //if share_through has slot
                  } //if share_through unit
    
                  // teads
                  if (indiv_unit.hasOwnProperty('teads')) {
                    //loop through teads unit sizes:
                    if (indiv_unit.teads.hasOwnProperty(slot_name)) {
                      var teads_unit = indiv_unit.teads[slot_name];
                      bids.push({
                        bidder: 'teads',
                        params: {
                          pageId: teads_unit.pageId,
                          placementId: teads_unit.placementId
                        }
                      });
                    } //if teads has slot
                  } //if teads unit
    
                  adUnit.bids = bids;
                  adUnits.push(adUnit);
                } //if media is equal
              }
              return adUnits;
            }
          


            function fetchPrebidBids(PREBID_TIMEOUT, ad_units){
              pbjs.requestBids({
                adUnits: constructPrebidAdUnits(ad_units),
                bidsBackHandler: function (bidResponses) {
                  bidsBackForBidder("prebid");
                  if (getQueryVariable('pbjs_debug')) {
                    console.log(bidResponses);
                    console.log('pbjs_timeout: ' + PREBID_TIMEOUT);
                    // console.log('Are pbjs bids available? : ' + pbjs.allBidsAvailable());
                    console.log('pbjs_bidResponses');
                    for (var key in bidResponses) {
                      // skip loop if the property is from prototype
                      if (!bidResponses.hasOwnProperty(key)) continue;
    
                      var obj = bidResponses[key];
                      for (var prop in obj) {
                        // skip loop if the property is from prototype
                        if (!obj.hasOwnProperty(prop)) continue;
                        console.log('bids for ' + key + " = " + obj[prop].length);
                        console.log(obj[prop]);
                      }
                    }
                  }
                },
                timeout: +PREBID_TIMEOUT
              });
            }
            function getQueryVariable(variable) {
              var query = window.location.search.substring(1);
              var vars = query.split("&");
              for (var i = 0; i < vars.length; i++) {
                var pair = vars[i].split("=");
                if (pair[0] == variable) {
                  return pair[1];
                }
              }
              return false;
            }


            function refreshAdsOnPage(ad_units){
              console.log("****************refershAdsOnPage called *******************");
              var timeoutA9 = Number(Drupal.settings.fe_amazonads.timeout);
              var PREBID_TIMEOUT = Drupal.settings.client_ads_dfp.prebid_timeout;
              var az_slots = [];

              var ad_units_keys = Object.keys(ad_units);
              for(i = 0; i < ad_units_keys.length; i++){
                var name = ad_units_keys[i];
                var slot = ad_units[name];
                  if(slot.ad_media == ad_media){
                      var adobj = {};
                      adobj.slotID = name;
                      adobj.sizes = slot.ad_size_arr;
                      az_slots.push(adobj);
                  }
              }
              //set general targeting for the page
              // $.each(ad_units, function (name, slot) {
  
              //     if(slot.ad_media == ad_media){
              //         var adobj = {};
              //         adobj.slotID = name;
              //         adobj.sizes = slot.ad_size_arr;
              //         az_slots.push(adobj);
              //     }
              // });
  

              fetchPrebidBids(PREBID_TIMEOUT, ad_units);
              fetchA9Bids(az_slots, timeoutA9);
            }

     </script>
