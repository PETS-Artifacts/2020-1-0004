<script>
		function setCookie(name, value, expires, path, domain, secure) {
			var now = new Date();
var time = now.getTime();
time += 3600 * 1000 * 24 * 365;
now.setTime(time);
	document.cookie = name + "=" + escape(value) +
	"; expires=" + now.toGMTString() +
	((path) ? "; path=" + path : "") +
	((domain) ? "; domain=" + domain : "") +
	((secure) ? "; secure" : "");
}
function getCookie(name) {
	var cookie = " " + document.cookie;
	var search = " " + name + "=";
	var setStr = null;
	var offset = 0;
	var end = 0;
	if (cookie.length > 0) {
		offset = cookie.indexOf(search);
		if (offset != -1) {
			offset += search.length;
			end = cookie.indexOf(";", offset)
			if (end == -1) {
				end = cookie.length;
			}
			setStr = unescape(cookie.substring(offset, end));
		}
	}
	return(setStr);
}
		function blockPop() {
			$('#ageConfirmMask').hide();
			$('#ageConfirmWrap').hide();
			setCookie('_age-verification', '1');
			//checkCookie_m();
		}
		function showPop() {
			var pop = getCookie('_age-verification');
			if (pop){
			//checkCookie_m();
			  return; 
			}
			
				$.fn.pixels = function(property){
					return parseInt(this.css(property));
				};
				
				$.fn.center = function(){
					var w = $(window);
					return this.each(function(){
						$(this).css("position","absolute");
						$(this).css("top",200 + "px");
						$(this).css("left",((w.width() - $(this).width()) / 2) - (($(this).pixels('padding-left') + $(this).pixels('padding-right')) / 2) + w.scrollLeft() + "px");
					});
				};
				
				$('body').css('overflow','hidden');
				$('#ageConfirmMask').show();
				$('#ageConfirmWrap').show();
				$('#ageConfirmWrap').center();
				$(window).resize(function(){
					$('#ageConfirmWrap').center();
				});
			
				$('#doEnter1').click(function(){
					$('body').css('overflow','visible');				
					blockPop();
				})
				$('#doExit1').click(function(){
					//location.href = 'http://www.addictinggames.com/';
location.href = 'http://www.itshidden.eu/';
				})
		}
		$(document).ready(function() {
			showPop();
		});
