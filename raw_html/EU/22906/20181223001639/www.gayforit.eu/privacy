  <script type="text/javascript">

  $(document).ready(function(){

      $(".add_frnd").click(function(){
        //alert("sdjhfbsf");
        if (!confirm('Are You Sure?')) return false; //don't do anything
        var newVar = $(this).attr('data-ruid');
        var url= '/add_friend_new.php';
        $.ajax({
          url: url,
          data: {'ruid' : newVar, 'friend_accept' : true},
          success: function(result){
            console.log(result);
            $('.frnd_msg_'+newVar).html(result);
            $(this).val("Accepted");
          },
          beforeSend : function(){
            $(this).val("Please wait");
          },
        });
      });
      $(".remove_frnd").click(function(){
        if (!confirm('Are You Sure?')) return false; //don't do anything
        var newVar = $(this).attr('data-ruid');
        var url= '/add_friend_new.php';
        $.ajax({
          url: url,
          data: {'ruid' : newVar, 'friend_deny' : true},
          success: function(result){
            console.log(result);
            $('.frnd_msg_'+newVar).html(result);
            $(this).val("Denied");
          },
          beforeSend : function(){
            $(this).val("Please wait");
          },
        });
      });

  });
  </script>
 
