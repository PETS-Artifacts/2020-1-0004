$( "#search_form .s_for, #searchForm1 .s_for" ).bind( 'paste change input propertychange keypress',function(e){
      setTimeout( function() {
            var val = $("#search_form .s_for").val();            
            var newval = "" ;
            if(val.length > 0){
              for(i=0;i<val.length;i++){
               var keycode = val.charCodeAt(i);          
               if (!(keycode==8 || keycode==16 || keycode==32 )&&(keycode < 48 || keycode > 57 ) && (keycode < 97 || keycode > 122) && !(keycode >= 65 && keycode <= 90) && keycode!=16 && keycode!=32) {
                       
                }else {
                  newval += val.charAt(i); 
                  console.log(newval);
                  }             

             }
             $("#search_form .s_for").val(newval);
           }
        }, 100); 
});

$( "#searchForm1 .s_for" ).bind( 'paste change input propertychange keypress',function(e){
