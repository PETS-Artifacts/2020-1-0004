<script type="text/javascript">
  $('form#advanced_search_form').submit(function(e){
       e.preventDefault();
       var sKeyword = $('form #keywords').val();
       var error = '';
       if(sKeyword==''){
        error ="Please enter a keyword!";
       }else if(sKeyword.length <2){
        error ="Please enter at least two digit!";         
       }
       $('form#advanced_search_form .error').html(error);
       if(error==''){
        this.submit();        
       }
       
  });
   $('form#searchForm').submit(function(e){
     e.preventDefault();
       var sKeyword = $('#search_form .s_for').val();
       var error = '';
       if(sKeyword==''){
        error ="Please enter a keyword!";
       }else if(sKeyword.length <2){
        error ="Please enter at least two digit!";         
       }
       $('form#searchForm .error').html(error);
       if(error==''){
         window.location.replace("https://www.gayforit.eu/most-searched-gay-porn-videos/"+sKeyword);        
       }
  });
