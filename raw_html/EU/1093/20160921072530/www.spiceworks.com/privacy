  <script type="text/javascript">(function($) {
  $(window).load(function (){ 
    var timeout = 0;
    
    if ($('#cms-page-controls').is(':visible')) {
      timeout = 1000;
    }
    
    // to ensure our JS doesn't conflict with the CMS JS
    setTimeout(function() {
      User = window.User || {}; 
      
      /* --------- Helper Methods & General Page Setup ----------- */
      var $toggleSubmenu = $('.nav-toggle .products-submenu'),
      $productsSubmenu = $('#new-home-nav.nav-products .products-submenu');
      
      function getURLParameter(name) {
        return decodeURIComponent((new RegExp('[?|&]' + name + '=' + '([^&;]+?)(&|#|;|$)').exec(location.search)||[,""])[1].replace(/\+/g, '%20'))||null;
      }
      
      function userIsLoggedIn() {
        var fakeLoggedOut = getURLParameter('loggedOut');
        if (fakeLoggedOut && (fakeLoggedOut === 'true')) {
          return false;
        }
        return User && User.kind && (User.kind !== 'guest');
      }
      
      function equalheight(container, elements){  
        if ($(window).width() >= 768) {
          $(container).each(function(){  

            // Cache the highest
            var highestBox = 0;

            $(elements, this).height('auto');

            // Select and loop the elements you want to equalise
            $(elements, this).each(function(){

              // If this box is higher than the cached highest then store it
              if($(this).outerHeight() > highestBox) {
                highestBox = $(this).outerHeight(); 
              }

            });  

            // Set the height of all those children to whichever was highest 
            $(elements, this).height(highestBox);

          });
        }
      }
      
      function overrideHeadlines() {
        var headline = getURLParameter('sw_headline'),
            subHead = getURLParameter('sw_subheadline'),
            headlineElement = $('#headline').length ? $('#headline') : $('.hero').find('h1');

        if (!!headline) {
          headlineElement.text(headline);
        }

        if (!!subHead) {
          $('#subheadline').html(subHead);
        }
      }
      
      window.intentMap = {
        'app': 'APP',
        'help-desk': 'HD',
        'help-desk-cloud': 'HD (Cloud)',
        'help-desk-on-prem': 'HD (On-Prem)',
        'network-monitor': 'MONITOR',
        'inventory': 'INVENTORY',
        'troubleshooting': 'TROUBLESHOOT',
        'mdm': 'MDM',
        'community': 'COMMUNITY',
        'concierge': 'IT CONCIERGE',
        'help-desk-mobile': 'HD MOBILE'
      };
      
      if (userIsLoggedIn()) {
        $('[data-user-href*="/"]').each(function() {
          $(this).attr('href', $(this).data('userHref'));
        });
      } else {
      $('[data-user-href*="/"]').each(function() {
          $(this).removeData('userHref');
        });
      }
      
      overrideHeadlines();
      
      // Set equal height to some things found across the site
      equalheight('#products-row', '.product');
      equalheight('.equal-height', '.item');

      $(window).resize(function(){
        equalheight('#products-row', '.product');
        equalheight('.equal-height', '.item');
      });
      
      // Hide submenu by default
      $toggleSubmenu.attr('style', 'display: none !important');

      $('.products-dropdown').click(function() {
        if ($toggleSubmenu.hasClass('open')) {
          $toggleSubmenu.attr('style', 'display: none !important')
          .removeClass('open');
        } else {
          $toggleSubmenu.attr('style', 'display: block !important')
          .addClass('open');
        }
      });
      
      // Activate the current product submenu item on the right pages
      if (!!window.location.pathname.match(/help-desk/i)) {
        $productsSubmenu.find('#help-desk').addClass('active');
      } else if (!!window.location.pathname.match(/inventory/i)) {
        $productsSubmenu.find('#inventory').addClass('active');
      } else if (!!window.location.pathname.match(/monitor/i)) {
        $productsSubmenu.find('#monitor').addClass('active');
      } else if (!!window.location.pathname.match(/downloads/i)) {
        $productsSubmenu.find('#downloads').addClass('active');
      }
      
      /* Scroll menu smooth scroll */
      
      $('body').attr('data-spy', 'scroll');
      $('body').attr('data-target', '#page-nav');
            
      $('#page-nav').attr('data-offset-top', $('.hero').height() + $('#new-home-nav').height());
      $('body').scrollspy({target: '#page-nav'});

      $(".scroll-menu a.item, .smooth-scroll a").on('click', function(event){
        event.preventDefault();
        var hash = this.hash;

        $('html, body').animate({
          scrollTop: $(hash).offset().top
        }, 800, function(){
          window.location.hash = hash;
        });
      });

      /* ----------------- UFR Modal & Logic Code --------------------- */
      
