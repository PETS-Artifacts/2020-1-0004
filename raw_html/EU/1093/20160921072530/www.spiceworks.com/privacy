:1px solid #d3d3d3}#cms-content-regions h4,.cms-outside-content h4{font-weight:bold;margin-top:30px;margin-bottom:20px}@media (max-width: 767px){#cms-content-regions .pull-right,.cms-outside-content .pull-right{float:none !important}}
