        'margin-top': '30px'
      });
      
      $('.modal form#subForm').css({
        'padding-bottom': '30px'
      });
      
      /* ---------------- */
      var Cookie = {
        get: function( name ){
          var nameEQ = escape(name) + "=", ca = document.cookie.split(';');
          for (var i = 0, c; i < ca.length; i++) {
            c = ca[i];
            while (c.charAt(0) == ' ') { c = c.substring(1, c.length); }
            if (c.indexOf(nameEQ) === 0) { return c.substring(nameEQ.length, c.length); }
          }
          return null;
        },
        set: function( name, value, options ){
          options = (options || {});
          if ( options.expiresInOneYear ){
            var today = new Date();
            today.setFullYear(today.getFullYear()+1, today.getMonth, today.getDay());
            options.expires = today;
          }
          var curCookie = escape(name) + "=" + escape(value) +
              ((options.expires) ? "; expires=" + options.expires.toGMTString() : "") +
              ((options.path)    ? "; path="    + options.path : "") +
              ((options.domain)  ? "; domain="  + options.domain : "") +
              ((options.secure)  ? "; secure" : "");
          document.cookie = curCookie;
        },
        removeCookie: function ( key, options ) {
          options = (options || {});
          var date = new Date();
          date.setTime(date.getTime()-(1*24*60*60*1000));
          options.expires = date;
          this.set(key, '', options);
        },
        hasCookie: function( name ){
          return document.cookie.indexOf( escape(name) ) > -1;
        },
        checkSupport: function(){
          return this.hasCookie('compatibility_test');
        }
      };

      $(document).ready(function() {
        var page_intent = window.pageIntent || '';

        function setGADataIntent(e) {
          var eventIntent = (e && e.target) ? $(e.target).data('intent') : false,
              intent = eventIntent || window.intent || page_intent || false,
              GAIntent;

          GAIntent = window.intentMap[intent];
          window.dataLayer.push({'intent': GAIntent});
          window.intent = intent;
        }

        function setConversionData(e) {
          var eventIntent = (e && e.target) ? $(e.target).data('intent') : false,
              intent = eventIntent || window.intent || page_intent || false;

          if (e.target.href && e.target.href.indexOf('monitor') !== -1) {
            intent = 'network-monitor';
          }

          setGADataIntent(e);

          Cookie.removeCookie('conversion_page');
          Cookie.set('conversion_page', document.location.pathname,
                     {expiresInOneYear: true, path: '/', domain: '.spiceworks.com'});
          if (intent) {
            Cookie.removeCookie('conversion_intent');
            Cookie.set('conversion_intent', intent,
                       {expiresInOneYear: true, path: '/', domain: '.spiceworks.com'});
          }
        }
        
        if (!Cookie.hasCookie('landing_page')) {
          Cookie.set('landing_page', document.location.pathname,
            {expiresInOneYear: true, path: '/', domain: '.spiceworks.com'});
        }
        
        if (!Cookie.hasCookie('landing_category') && !!page_intent) {
          Cookie.set('landing_category', page_intent,
            {expiresInOneYear: true, path: '/', domain: '.spiceworks.com'});
        }
        
        if (!Cookie.hasCookie('referrer') && document.referrer) {
          Cookie.set('referrer', document.referrer,
            {expiresInOneYear: true, path: '/', domain: '.spiceworks.com'});
        }
        
        setGADataIntent();

        // Launch social login windows
        $('a[data-social-join]').on( 'click', function(e) {
          where = document.location;
          Cookie.set('social_auth_referer_url', where, {path: '/'});

          var social_network = $(this).attr('data-social-join');
          var window_opts = "width=760,height=430";
          window.open("//community.spiceworks.com/auth/" + social_network, "sw_social_join", window_opts);
          e.preventDefault();
        });

        $('a[href*="/download/"]').click(setConversionData);
        $('a[href*="play.google.com/store/apps/details?id=com.spiceworks"]').click(function(e) {
          page_intent = 'help-desk-mobile';
          setConversionData(e);
        });
        $('a[href*="//on.spiceworks.com/registers/new"]').click(function(e) {
          page_intent = 'help-desk-cloud';
          setConversionData(e);
        });
        $('a[data-target="#join"]').click(function(e) {
          page_intent = 'community';
          setConversionData(e);
        });
        $('#join_download_form').on('click', 'button[type="submit"]', function(e) {
          setConversionData(e);
        });
        $('#login_download_form').on('click', 'button[type="submit"]', function(e) {
          setConversionData(e);
        });

        // Set intent for UFR modal
        $('a[data-target="#join-login"]').click(function() {
          var tab = $(this).data('tab');
          window.dataLayer.push({ ufrTarget: tab });
        });

        $('a[href*="free-"], a[href*="download-free-"], a[href*="atyourservice"], a[href*="network-"]').click(function(e) {
          setGADataIntent(e);
        });   

        // When data-tab is present on a modal link set the active tabIndex
        $(document).on('click.modal.data-api', '[data-tab]', function (tab) {
          tab = $(this).data('tab');
          $(".tab-pane").removeClass("active");
          $("#" + tab).addClass("active in");
        });
      });
      
      
      /* ---------------- */

      $('#new_registration_referer').val(window.location.href);

      if (userIsLoggedIn()) {
        // Change Join modal toggle to community link
        $('.btn[data-target="#join"]').each(function() {
          $(this).attr('href', 'https://community.spiceworks.com');
          $(this).attr('data-target', null);
          $(this).attr('data-toggle', null);
        });
      }
        
      var downloadDestination;

      // ***** METHODS ***** //
      function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0)==' ') c = c.substring(1);
          if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
        }
        return "";
      }

      function getDownloadTrackingData() {
        var tracking = {
          'download_tracking': {}
        },
            landing_category = getCookie('landing_category'),
            landing_page = encodeURI(getCookie('landing_page')),
            referrer = getCookie('referrer');

        if (!!landing_category) tracking.download_tracking.landing_category = landing_category;
        if (!!landing_page) tracking.download_tracking.landing_page = landing_page;
        if (!!referrer) tracking.download_tracking.referrer = referrer;
        if (window.location.pathname === '/') {
          tracking.download_tracking.download_page = encodeURI(window.location.pathname);
        } else {
          tracking.download_tracking.download_page = encodeURI(window.location.pathname.substr(1));
        }
        return tracking;
      }

      function downloadButtonClick(evt) {
        var target = evt.target;

        downloadDestination = target.href;
        evt.preventDefault();

        if (userIsLoggedIn()) {
          if (!!window.intent && !!window.intentMap[window.intent]) {
            window.optimizely.push(["trackEvent", "Successful Download - " + window.intentMap[window.intent]]);
          }

          window.location = downloadDestination + '?user=true';

          $.ajax({url: 'https://community.spiceworks.com/login/download_tracking',
            data: getDownloadTrackingData(),
            dataType: 'jsonp'
          });

          return;
        }

        if (specialDest) {
          downloadDestination = specialDest;
        }

        $('.video-modal').modal('hide');
        $('#join_download.modal').modal('show');
        return false;
      }


      function validateEmail(email){
        var re = /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
        return !!re.test(email);
      }

      function validatePassword(password){
        var re = /^(?=.*[A-Z])[A-Za-z\d$@$!%*#?&^()-_+=`~|]{8,}$/;
        return !!re.test(password);
      }

      function enableSubmit(form) {
        var submit = $(form).find('button[type="submit"]');
        submit.removeAttr('disabled');
        submit.removeClass('disabled');
      }

      function disableSubmit(form) {
        var submit = $(form).find('button[type="submit"]');
        submit.attr('disabled');
        submit.addClass('disabled');
      }

      function parseErrorResponse(responseText) {
        try { return $.parseJSON(responseText); } catch(err) { return {errors: {}}; }
      }

      function formatErrorMessage(key, error) {
        var fragment = key + ' ' + error;
        return fragment.charAt(0).toUpperCase() + fragment.slice(1);
      }

      function handleErrors(form, jqxhr) {
        var errorMessages = [];
        var response = parseErrorResponse(jqxhr.responseText);
        if (response.errors) {
          _.each(response.errors, function(errors, key) {
            if (key == 'base') {
              errorMessages = errorMessages.concat(errors);
            } else {
              errors.forEach(function(error){ errorMessages.push(formatErrorMessage(key, error));});
              var inputSelector = 'input[name*="[' + key + ']"]';
              form.find(inputSelector).parents('.form-group').addClass('error');
              $('#registration_email_error').hide();
              $('#registration_password_error').hide();
            }
          });
        }
        if (errorMessages.length == 0) {
          errorMessages.push("Yikes, that didn't work. Please try again or <a href=\"mailto:support@spiceworks.com\" "
                             + "onClick=\"ga(\'send\', \'event\', \'Form\', \'Links\', \'Contact Support\');\">contact support</a>.");
          $('#registration_email_error').hide();
          $('#registration_email_success').hide();
          $('#registration_password_error').hide();
          $('#registration_password_success').hide();
        }
        form.find('.generic-error').html(errorMessages.join("<br/>")).slideDown(250);
      }

      function hideErrors(form) {
        form.find('.form-group').removeClass('error');
        form.find('.generic-error').slideUp(250);
      }

      // ***** GENERAL MODAL/FORM BEHAVIOR ***** //

      // Show the modal on button clicks
      var $specialDest = $('meta[name=specialdownloaddest]');
      var specialDest = false;
      if ($specialDest.length) {
        specialDest = $specialSignup.attr('content');
      }

      $(document).on('click', 'a[href*="/download/"]', function(evt) {
        downloadButtonClick(evt);
      });

      $(document).on('click', 'a[data-href*="/download/"]', function(evt) {
        downloadButtonClick(evt);
      });

      $(document).on('click', 'a[href*="/gns3/download/"]', function(evt) {
        downloadButtonClick(evt);
      });
      
      // Open other modals from within modals
      $(document).on('click', '.toggle-modal', function(e) {
        var currentModal = $(this).closest('.modal'),
            newModal = $(this).data('target');

        e.preventDefault();
        e.stopPropagation();

        $(currentModal).modal('hide');
        $(newModal).modal('show');
      });

      window.SW_DOWNLOAD_WAIT = false;


      // Focus on first field when user opens the modal, track which form was opened
      $('#join-login').on('shown.bs.modal', function () {
        var activeTab = $('.tab-pane.active'),
            activeTabId = activeTab.data('id'),
            message = '';

        if (activeTabId === '#join_download') {
          window.optimizely.push(['trackEvent', 'Register to Download - Opened']);
          // SWUFR.cmd.push(function() {
          //   SWUFR.ufr.modalOpen();
          // });
          message = 'Register to Download';
        } else if (activeTabId === '#join') {
          message = 'Register';
        } else if (activeTabId === '#login_download') {
          message = 'Login to Download';
        } else if (activeTabId === '#login') {
          message = 'Login';
        }

        ga('send', 'event', 'Form', message, 'Opened');
        activeTab.find('input').first().get(0).focus();
      });

      // Track when the user hides the modal
      $('#join-login').on('hidden.bs.modal', function () {
        if ( $('#join_download').hasClass('active') ) {
          ga('send', 'event', 'Form', 'Register to Download', 'Closed');
          if (typeof email !== 'undefined') {
            //SWUFR.cmd.push(function() { SWUFR.ufr.modalClose(email); });
          }
        } else if ( $('#login_download').hasClass('active') ) {
          ga('send', 'event', 'Form', 'Login to Download', 'Closed');
        } else if ( $('#join').hasClass('active') ) {
          ga('send', 'event', 'Form', 'Join Spiceworks', 'Closed');
        } else if ( $('#login').hasClass('active') ) {
          ga('send', 'event', 'Form', 'Login to Spiceworks', 'Closed');
        }
      });

      // Only enable the submit button if they've filled out the form
      $('form input').bind('change keyup',function() {
        var current_form = $(this).closest('form'),
            form_empty = false;
        $(current_form).find('input').each(function() {
          if ($(this).val() === '') {
            form_empty = true;
          }
        });

        if (form_empty === false) {
          enableSubmit(current_form);
        } else {
          disableSubmit(current_form);
        }
      });

      // ***** JOIN TO DOWNLOAD ***** //

      // Show password instructions when user starts creating password
      $('input#registration_password').bind('focus',function() {
        $('#registration_password_help').show();
      });

      $('input#registration_email').bind('change',function() {
        if (validateEmail($(this).val())) {
          $(this).removeClass('error');
          $('#registration_email_success').show();
          $('#registration_email_error').hide();
          $('#registration_email_help').slideUp(500);
          email = $(this).val();
          enableSubmit($(this).closest('form'));
        } else {
          $(this).addClass('error');
          $('#registration_email_success').hide();
          $('#registration_email_error').show();
          $('#registration_email_help').slideDown(500);
          disableSubmit($(this).closest('form'));
        }
      });

      $('input#registration_password').bind('change',function() {
        if ( validatePassword($(this).val()) ) {
          $(this).removeClass('error');
          $('#registration_password_success').show();
          $('#registration_password_error').hide();
          $('#registration_password_help').slideUp(500);
          enableSubmit($(this).closest('form'));
        } else {
          $(this).addClass('error');
          $('#registration_password_success').hide();
          $('#registration_password_error').show();
          disableSubmit($(this).closest('form'));
        }
      });

      $('#join_download_form').submit(function(evt) {
        evt.preventDefault();
        var self = $(this);

        // Success tracking message
        var successMessage = 'Successful Download';

        hideErrors(self);
        self.find('button').prop('disabled','disabled');
        var formDataString = self.serialize();
        formDataString += '&' + $.param({'registration': getDownloadTrackingData()});

        $.post('/community/join/register_for_download', formDataString,
          function(data){
            if (data.login) {
              ga('send', 'event', 'Form', 'Login to Download', successMessage);
              window.optimizely.push(["trackEvent", "Login to Download - Successful Download"]);
              setTimeout(function(){window.location.href = downloadDestination + '?user=true'}, 150);
            } else {
              ga('send', 'event', 'Form', 'Register to Download', successMessage);
              window.optimizely.push(["trackEvent", "Register to Download - Successful Download"]);
            }

            if (!!window.intent && !!window.intentMap[window.intent]) {
              window.optimizely.push(["trackEvent", "Successful Download - " + window.intentMap[window.intent]]);
            }

            setTimeout(function(){window.location.href = downloadDestination}, 150);
          }, 'json'
        ).fail(function(jqxhr){
          if (jqxhr.status == 401) {
            ga('send', 'event', 'Form', 'Register to Download', 'Email In Use');
            $('.email-use-error').slideDown(250);
          } else {
            ga('send', 'event', 'Form', 'Register to Download', 'Error');
            handleErrors(self, jqxhr);
          }
        }).always(function(){
          self.find('button').prop('disabled', false);
        });
      });

      // ***** LOG IN TO DOWNLOAD ***** //

      $('#login_download_form').submit(function(evt) {
        var self = $(this),
            successMessage = 'Successful Download',
            formDataString = self.serialize();

        evt.preventDefault();

        hideErrors(self);
        self.find('button').prop('disabled','disabled');
        formDataString += '&' + $.param({'login': getDownloadTrackingData()});

        $.post('/community/login/login_for_download', formDataString, function(){
          ga('send', 'event', 'Form', 'Login to Download', successMessage);
          window.optimizely.push(["trackEvent", "Login to Download - Successful Download"]);
          if (!!window.intent && !!window.intentMap[window.intent]) {
            window.optimizely.push(["trackEvent", "Successful Download - " + window.intentMap[window.intent]]);
          }
          setTimeout(function(){window.location.href = downloadDestination + '?user=true'}, 150);
        }, 'json').fail(function(jqxhr){
          ga('send', 'event', 'Form', 'Login to Download', 'Error');
          handleErrors(self, jqxhr);
        }).always(function(){
          self.find('button').prop('disabled',false);
        });

      });

      // ***** SOCIAL LOG IN ***** //


      // NEEDS FIXING
      $('.social-join').click(function(e) {
        var params = $.param(getDownloadTrackingData()),
            where = document.location,
            success = '//www.spiceworks.com/social-success',
            policy = 'www',
            social_network = $(this).attr('data-social-join'),
            window_opts = 'width=760,height=480',
            newUser = $(this).parents('.social_links').next('#join_download_form').length > 0,
            successLink,
            query;

        successLink = success + '?provider=' + social_network + '&newuser=' + 'false';
        ga('send', 'event', 'Buttons', 'Using '+ social_network +' to Download', 'Clicked for existing_user');

        Cookie.set('social_auth_referer_url', where, {path: '/'});
        query = '?policy=' + policy + '&success=' + success + '&' + params;
        window.open('http://accounts.spiceworks.com/auth/' + social_network + '/start' + query, 'sw_social_join', window_opts);
        e.preventDefault();
      });


      // ***** PASSWORD RESET ***** //

      $('#password-reset-form').submit(function(evt) {
        var self = $(this),
            endpoint = window.location.protocol + '//' + window.location.host + '/library/includes/identity/reset_password/',
            email = $('#reset-form_email').val(),
            callbackURL = window.location.toString();

        evt.preventDefault();

        hideErrors(self);
        self.find('.generic-error').hide();
        self.find('.generic-success').hide();
        self.find('button').prop('disabled','disabled');

        $.post(endpoint, {'email': email, 'url': callbackURL}, function(data) {
          if (data && data.status == 'success') {
            ga('send', 'event', 'Form', 'Reset Password', 'Email Sent');
            self.find('.generic-success').slideDown(250);
          } else if (data && data.status == 'error' && data.message) {
            ga('send', 'event', 'Form', 'Reset Password', 'Error');
            self.find('.generic-error').text(data.message).slideDown(250);
          } else {
            ga('send', 'event', 'Form', 'Reset Password', 'Error');
            self.find('.generic-error').text('Something went wrong. Please check your email and try again.').slideDown(250);
          }
        }, 'json').fail(function(jqxhr){
          ga('send', 'event', 'Form', 'Reset Password', 'Error');
          self.find('.generic-error').text('Something went wrong. Please check your email and try again.').slideDown(250);
        }).always(function(){
          self.find('button').prop('disabled',false);
        });
      });
      /* ------------------------------------------------ */
      
    }, timeout);
    
  });
  
})(jQuery);</script>
  <script type="text/javascript"></script>





      </div>
    </div>

    
  </body>
</html>
