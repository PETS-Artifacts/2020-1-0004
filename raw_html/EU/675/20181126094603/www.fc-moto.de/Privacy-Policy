  <script type="text/javascript">
  var ICPHoverOptions = {
    'activMinWidth':1200,   // minimum viewport width where the hover is available | 0 = disabled
    'activMaxWidth':0,      // maximum viewport width where the hover is available | 0 = disabled
    'delay': 1000,           // delay in milliseconds while you have to stay with the mouse cursor above the item
    'paddingSide': 0,       // size-offset of the hover-box left and right
    'paddingTop': 0,        // size-offset of the hover-box top
    //'initHeight': 444,      // inital height for the container | overwritten after the content was loaded | 0 = auto
    'initHeight': 404, //WP_4838 
    'refreshPrice': 0,      // determines whether the Script should return the variation price [0;1]
    'onLoad': showVariationProducts
  }

  function showVariationProducts() {
    // do something with the result
    var anchorNode = jq('#ICPListHoverAnchorID');
    var anchorWrap = jq('#ICPListHoverWrapID');
    var productID  = jq('.ICProductHoverContent',anchorWrap).attr('pid');
    var VarLeftArea = jq('#ICVariationAreaLeft',anchorWrap);
    var VarRightArea = jq('#ICVariationAreaRight',anchorWrap);
    var InfoArea = jq('.InfoArea',anchorWrap);

    // => implementation of the javascript from Super-Product-Presentation-Cartridge
    if((typeof ICPAttrSuperArray != 'undefined') && (ICPAttrSuperArray != null) && (typeof productID != 'undefined') && (typeof productID != null)) {
      var superArrayVal = ICPAttrSuperArray['PID_' + productID];
      
      // disable all buttons except for the first attribute
      var tmpAttributeHTMLObjects = null;
      
      if(superArrayVal.AttributeIDs.length === 1) {
        disableAttributeInputs(superArrayVal.AttributeIDs[0], false);
      }
      else {
        for (i = 1;  i < superArrayVal.AttributeIDs.length;  i++) {
          disableAttributeInputs(superArrayVal.AttributeIDs[i], false);
        }
      }
      
      // make basket button nice :)
      // jq('#basketForm_ICHover button').metaparse();
      
      //enabled default sub
      if(superArrayVal.IsSubProduct) {
        for (i = 0;  i < superArrayVal.AttributeIDs.length;  i++) {
          if(superArrayVal.AttributeIDs.length > 1) {
            var SelectedParentValue = getSelectedValue(superArrayVal.AttributeIDs[i-1]);
          }

          for (j = 0;  j < superArrayVal.SubProducts.length;  j++) {
            if(superArrayVal.SubProducts[j].IsAvailable === 1) {
              var possibleSub = false;
  
              if(superArrayVal.AttributeIDs.length > 1) {
                //check child/parent-combinations only if we have more than one attribute
                for (var key in superArrayVal.SubProducts[j].AttributeValues) {
                  if(superArrayVal.SubProducts[j].AttributeValues[key] === SelectedParentValue) {
                    possibleSub = true;
                    break;
                  }
                }
              }
              else {
                possibleSub = true;
              }
              
              if(possibleSub === true) {
                tmpAttributeHTMLObjects = jq('.icattribute[predefattrid='+superArrayVal.AttributeIDs[i]+']');
                  
                if((tmpAttributeHTMLObjects.length == 1) && (tmpAttributeHTMLObjects.prop('nodeName') === 'SELECT')){
                  jq('option[predefvalueid='+superArrayVal.SubProducts[j].AttributeValues[superArrayVal.AttributeIDs[i]]+']',tmpAttributeHTMLObjects).removeAttr('disabled');
                }
                else {
                  jq(tmpAttributeHTMLObjects).each( function() {
                    var elem = jq(this);
                    if(elem.attr('predefvalueid') == superArrayVal.SubProducts[j].AttributeValues[superArrayVal.AttributeIDs[i]]) {
                      elem.removeAttr('disabled').removeClass('disabled');
                    }
                  });
                }
              }
            }
          }
        }
      }
      else {
        if(superArrayVal.AttributeIDs.length > 0) {
          for (i = 0;  i < superArrayVal.AttributeIDs.length;  i++) {
            for (j = 0;  j < superArrayVal.SubProducts.length;  j++) {
              if(superArrayVal.SubProducts[j].IsAvailable === 1) {
                jq('.icattribute[predefvalueid=' + superArrayVal.SubProducts[j].AttributeValues[superArrayVal.AttributeIDs[i]] + ']').removeClass('disabled');
                jq('.icattribute[predefvalueid=' + superArrayVal.SubProducts[j].AttributeValues[superArrayVal.AttributeIDs[i]] + ']').parent().removeClass('disabled');
              }
            }
          }
        }
      }
      
      jq('button.icattribute,img.icattribute,span.icattribute').click( function() {
        if(!jq(this).hasClass('disabled')) {
          //no more ajax reload wanted by the customer
          //initEvents(jq(this),false,doAjaxReload);
          //console.log(jq(this));
          initEvents(jq(this),true);
        }
      });
      jq('select.icattribute').change( function() {
        initEvents(jq(this),false);
      });
    }    

    if(anchorWrap.height() > (anchorNode.height() + ICPHoverOptions.paddingTop)) {
      anchorNode.css({
        'height': anchorWrap.height()
      });
    }
    else {
      anchorWrap.css({
        'height': anchorNode.height()
      });      
    }

    //determine position for variation areas
    var offsetLeft = placeVariationItems(VarLeftArea,anchorWrap,'left');
    var offsetRight = placeVariationItems(VarRightArea,anchorWrap,'right');

    //place hover correctly
    InfoArea.css({
      'position':'absolute',
      'width':anchorNode.width(),
      'left':offsetLeft,
    });

    VarLeftArea.css({
      'width': offsetLeft,
      'left': 0
    });

    VarRightArea.css({
      'width': offsetRight,
      'right': 0
    });

    anchorNode.css({
      'width':(InfoArea.width() + offsetLeft + offsetRight),
      'left':(parseInt(anchorNode.css('left')) - offsetLeft + 1)
    });
    
    // old positioning
    
    /*if(anchorWrap.height() > (anchorNode.height() + ICPHoverOptions.paddingTop)) {
      anchorNode.animate({
        height: anchorWrap.height()
      });
    }*/
    
    // enable image hovering
    
    switchProductImage(anchorNode);
  }
  
  
  // this function calculates the positions of each li and each ul inside the variation area
  function placeVariationItems(jqSelector,anchorWrap,direction) {
    var colOffset = 0;     // col-offset is the return value of the complete left or right variation area
    var prevColWidth = 0;  // the previous col-width is the width of the previous ul to place the current ul at the right position
    
    jq('ul',jqSelector).each( function() {
      var $col = jq(this);
      var colWidth = $col.outerWidth();
      var wBorder = (parseInt($col.css('border-left-width')) + parseInt($col.css('border-right-width')));
      var items = jq('li',$col);
      var itemCount = items.length;
      
      var currentHeight = 0;
      var finalColWidth = colWidth;
      var itemTop = 0;
      var itemLeft = 0;
      jq(items).each(function(){
        jq(this).css({
          'left':itemLeft,
          'top':itemTop,
          'position':'absolute',
          'width':(colWidth - wBorder)
        });

        itemTop = itemTop + jq(this).outerHeight();

        if(itemTop > anchorWrap.height()) {
          finalColWidth = finalColWidth + colWidth;
          itemLeft = itemLeft + colWidth;
          itemTop = 0;
          
          jq(this).css({
            'left':itemLeft,
            'top':itemTop,
          });
          
          itemTop = itemTop + jq(this).outerHeight();
        }
      });
      
      $col.css({
        'width':(finalColWidth - wBorder),
        'position':'absolute',
        'left' : prevColWidth
      });
      
      prevColWidth = finalColWidth;
      
      if(direction === 'left') {
        jqSelector.css('left',0);
      }
      else {
        jqSelector.css('right',0);
      }
      
      colOffset = colOffset + finalColWidth;
    });
    
    return colOffset;
  }
  
  function doAjaxReload(clickedButton,FinalPath) {
    var LocalProduct = null;
    var $anchorNode  = jq('#ICPListHoverAnchorID');
    var $anchorWrap  = jq('#ICPListHoverWrapID');
    var productID    = $anchorWrap.attr('pid');
    
    var postUrl      = epConfig.baseUrl;
    var postData     = {};
        postData['ViewObjectPath'] = FinalPath;
        postData['ViewAction'] = 'ICGetProductHover';
    
    jq('.ICProductHoverContent',$anchorWrap).remove();
    
    //reset the hover size
    $anchorWrap.css({
      'width':jq.data(anchorNode,'origItemW'),
      'height':jq.data(anchorNode,'origItemH')
    });
    
    $anchorNode.css({
      'width':jq.data(anchorNode,'origItemW'),
      'height':jq.data(anchorNode,'origItemH'),
      'top':jq.data(anchorNode,'origItemT'),
      'left':jq.data(anchorNode,'origItemL')
    });
    
    //reload the hover content if variation is finally selected
    jq.ajax({
      type: 'GET',
      url: postUrl,
      cache: false,
      dataType: 'html',
      data: postData,
      success: function(response) {
        response = response.replace(/<\!--(.*)-->/g, '');
        response = response.replace(/\t/g, '');
        response = response.replace(/\n/g, '');

        $anchorWrap.html(response);
        eval(jq('#ICAjaxScriptBlock').html());
        
        if(typeof ICPHoverOptions.onLoad === 'function') {
          setTimeout(ICPHoverOptions.onLoad, 0);
        }
      },
      error: function(response) {
        anchorNode.hide();
        console.log(response);
      }
    });
  }
  
  function switchProductImage(anchorNode) {
    var $ICHotDealImage = jq('#ICHoverImageHT');
    
    
    jq('li.imageBtn',anchorNode).mouseover( function(ev) {
      var imgSrc = jq('img',jq(this)).attr('src');
      
      if((typeof imgSrc !== 'undefined') && (imgSrc !== '')) {
        $ICHotDealImage.attr('src',imgSrc);
      }
    });
  }
  
