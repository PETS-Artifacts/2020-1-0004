    function initSlider(slider) {
       var parentSlider = slider.parent(".ICDesignSlider");
       var sliderWidth = 230,
           itemsMax = 0;
       
       if(parentSlider.parent().hasClass("ManufacturerSlider")) {
           sliderWidth = 200; 
       } else if(parentSlider.parent().hasClass("AdvancedLinkSlider") || parentSlider.parent().hasClass("Slider")) {
           itemsMax = 1;
           sliderWidth = '100%';
       } else if(parentSlider.parent().hasClass("AccessorySlider")) {
           itemsMax = 2;
           sliderWidth = 400;
       }

      /* load the slider */
      slider.flexslider({
        slideshow: (slider.attr('slideshow') === 'true') ? true : false,
        touch: true,
        animation: (slider.attr('animation') === 'slide') ? 'slide' : 'fade',
        animationLoop: (slider.attr('animationloop') === 'false') ? false : true,
        itemWidth: sliderWidth,
        maxItems: itemsMax,
        move: 1,
        controlNav: (slider.attr('controlnav') === 'false') ? false : true,
        start: function() {
          var maxWidthSlideWrapper = slider.find('ul.slides li').size() * sliderWidth;
          slider.closest('.ICSliderWrapper').css('max-width', maxWidthSlideWrapper);
          //slider.find('ul.slides').css('max-width', maxWidthSlideWrapper);
          if(maxWidthSlideWrapper < jQuery('.ContentArea').width()) {
              slider.find('.flex-direction-nav li a').addClass('flex-disabled');
          }
        }
      });
    }
</script>
  <script type="text/javascript">  

  var JsonData  = '';
  var JsonDataUrl = '';  
  
  function GetPixiDeliveryPeriod(ProductID, WebPath, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, StockLevelAlert, MinOrder, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, ICProductIsAvailable, IsAvailable, IsVisible, IsNew, IsMaster, ICFormError){
    var BaseURL                       = "https://www.fc-moto.de";
    var AjaxURL                       = BaseURL + WebPath + "/stockstate_" + ProductID + ".json?PreventCache=" + Date.now();
    var JSONUrl                       = BaseURL + WebPath + "/stockstate_" + ProductID + ".json";
    var LowOnStockAction              = 3;
    var BContainer                    = jq('#BasketLink_' + ProductID);
    var BButton                       = jq('button[name=AddToBasket]', BContainer);
    var RButton                       = jq('button.ICBasketButton');
    var ICMinOrder                    = parseInt(MinOrder);

    if(ManufacturerDeliveryPeriod == ''){ return; } 
    if(ProductDeliveryPeriod == ''){ ProductDeliveryPeriod = '2 - 3'; }

    if(JsonDataUrl == JSONUrl){ 
      if(typeof BButton.attr('data-productid') === 'undefined') {
        ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, LowOnStockAction, ICProductIsAvailable, IsAvailable, IsNew, StockLevelAlert, ICMinOrder, JsonData);        
        ChangeTrafficLight(ProductID, JsonData.AvailableStock, StockLevelAlert, ICProductIsAvailable, ICMinOrder);  
        if(typeof RButton !== 'undefined' && RButton.length > 0) {
            ChangeRefreshButton(RButton, ProductID, ICProductIsAvailable, IsNew, JsonData.AvailableStock, LowOnStockAction, ICMinOrder);
        }
      } else {
        ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, LowOnStockAction, ICProductIsAvailable, IsAvailable, IsNew, StockLevelAlert, ICMinOrder, JsonData);        
        ChangeTrafficLight(ProductID, JsonData.AvailableStock, StockLevelAlert, ICProductIsAvailable, ICMinOrder);        
        ChangeMicroDataAvailability(ProductID, JsonData.AvailableStock, StockLevelAlert, ICMinOrder, LowOnStockAction, ICProductIsAvailable, IsNew);
        ChangeBasketButton(ProductID, LowOnStockAction, ICProductIsAvailable, IsNew, ICMinOrder, IsAvailable, IsVisible, IsMaster, 0, JsonData);
      }    
    
      return;
    }

    jQuery.ajax({      
      type: 'GET',
      url: AjaxURL,
      dataType: 'json',
      success: function(data) {
        JsonData = data;
        JsonDataUrl = JSONUrl;         
        
        if(typeof BButton.attr('data-productid') === 'undefined') {
          ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, LowOnStockAction, ICProductIsAvailable, IsAvailable, IsNew, StockLevelAlert, ICMinOrder, data);        
          ChangeTrafficLight(ProductID, data.AvailableStock, StockLevelAlert, ICProductIsAvailable, ICMinOrder);       
          if(typeof jq('span.Refresh') !== 'undefined' && RButton.length > 0) {
              ChangeRefreshButton(RButton, ProductID, ICProductIsAvailable, IsNew, data.AvailableStock, LowOnStockAction, ICMinOrder);
          }
        } else {
          ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, LowOnStockAction, ICProductIsAvailable, IsAvailable, IsNew, StockLevelAlert, ICMinOrder, data);        
          ChangeTrafficLight(ProductID, data.AvailableStock, StockLevelAlert, ICProductIsAvailable, ICMinOrder);        
          ChangeMicroDataAvailability(ProductID, data.AvailableStock, StockLevelAlert, ICMinOrder, LowOnStockAction, ICProductIsAvailable, IsNew);
          ChangeBasketButton(ProductID, LowOnStockAction, ICProductIsAvailable, IsNew, ICMinOrder, IsAvailable, IsVisible, IsMaster, 0, data);
        }
      }
    });
  
    return;
  }
  
  function ChangeRefreshButton(RButton, ProductID, ICProductIsAvailable, IsNew, AvailableStock, LowOnStockAction, MinOrder) {
    var Message                       = jq('#BasketForm .NotEnoughStock'),
        PayPal                        = jq('button[name="PayPalExpressCheckout"]');

    if((RButton).parent('.DoOrder').hasClass('Disabled')) {
        return;
    }
    if(typeof RButton.attr('dis-bc-of') === 'undefined') {
        RButton.attr('dis-bc-of', '');
    }
    if(AvailableStock < MinOrder) { 
      if(LowOnStockAction == '0') { 
          RButton.attr('dis-bc-of', RButton.attr('dis-bc-of').replace(ProductID, ''));                
      } else if(LowOnStockAction == '2') { 
            if (RButton.attr('dis-bc-of').indexOf(ProductID) < 0) {
                 RButton.addClass('Disabled');
                 RButton.attr('disabled', 'disabled');
                 Message.removeClass('Hidden');
                 if(PayPal.length > 0) {
                     PayPal.attr('disabled', 'disabled');
                 }
                 if(RButton.attr('dis-bc-of') !== undefined) {
                      RButton.attr('dis-bc-of', RButton.attr('dis-bc-of') + ProductID); 
                 } else {
                     RButton.attr('dis-bc-of', ProductID); 
                 }
            }          
      } else { 
        if(ICProductIsAvailable == 'Y') {
          RButton.attr('dis-bc-of', RButton.attr('dis-bc-of').replace(ProductID, ''));
        } else {
          if (RButton.attr('dis-bc-of').indexOf(ProductID) < 0) {
              RButton.addClass('Disabled');
              RButton.attr('disabled', 'disabled');
              Message.removeClass('Hidden');
              if(PayPal.length > 0) {
                  PayPal.attr('disabled', 'disabled');
              }
              if(RButton.attr('dis-bc-of') !== undefined) {
                   RButton.attr('dis-bc-of', RButton.attr('dis-bc-of') + ProductID); 
              } else {
                  RButton.attr('dis-bc-of', ProductID); 
              }
          }
        }              
      }        
    } else {
      RButton.attr('dis-bc-of', RButton.attr('dis-bc-of').replace(ProductID, ''));   
    }
    if (RButton.attr('dis-bc-of') === '') {
        RButton.removeClass('Disabled');
        RButton.removeAttr('disabled', 'disabled');
        Message.addClass('Hidden');
    }
  }
  
  function ChangeMicroDataAvailability(ProductID, ProductStock, StockLevelAlert, MinOrder, LowOnStockAction, ICProductIsAvailable, IsNew) {
    //var $MContainer  = jq('#MicroDataAvailability_' + ProductID); #FCM-148 | CS | Entfernen der ProductID, da diese im HTML-Code beim AJAX-Reload nicht ersetzt wird
    var $MContainer  = jq('#MicroDataAvailability');
    var Link        = 'https://schema.org/';

    if(ProductStock < MinOrder){
      if(LowOnStockAction == '0') {
          Link = Link + 'PreOrder';      
      } else if(LowOnStockAction == '2') {
        if(ICProductIsAvailable == 'Y') {
          Link = Link + 'InStock';  
        } else if(IsNew == '1') {
          Link = Link + 'PreOrder';  
        } else {
          Link = Link + 'OutOfStock'; 
        }           
      } else {
        if(ICProductIsAvailable == 'Y') {
          Link = Link + 'InStock';  
        } else if(IsNew == '1') {
          Link = Link + 'PreOrder';  
        } else {
          Link = Link + 'OutOfStock'; 
        }      
      }
    } else {
      Link = Link + 'InStock';    
    }
    
    //var $LinkObj = jq('<link>');
    //$LinkObj.attr('itemprop', 'availability');
    //$LinkObj.attr('href', Link);

    //$LinkObj.appendTo($MContainer);
    $MContainer.attr('href', Link);
  
    return;  
  }
  
  function ChangeBasketButton(ProductID, LowOnStockAction, ICProductIsAvailable, IsNew, MinOrder, IsAvailable, IsVisible, IsMaster, ToHide, Pixi) {
    var BContainer                    = jq('#BasketLink_' + ProductID);
    var BButton                       = jq('button[name=AddToBasket]', BContainer);       
    
    if(ToHide) {    
      if(IsMaster == '1') {
        BContainer.addClass('HideElement');      
      } else {
        if(IsAvailable == '1' && IsVisible == '1') {
          if(Pixi.AvailableStock < MinOrder) {
            if(LowOnStockAction == '0') {
                BContainer.removeClass('HideElement');                
            } else if(LowOnStockAction == '2') {
                if(ICProductIsAvailable == 'Y') {
                    BContainer.removeClass('HideElement'); 
                } else {
                    BContainer.addClass('HideElement');    
                }
            } else { 
              if(ICProductIsAvailable == 'Y') {
                BContainer.removeClass('HideElement');  
              } else {
                BContainer.addClass('HideElement');
              }              
            }        
          } else {
            BContainer.removeClass('HideElement');    
          }    
        } else {
          BContainer.addClass('HideElement'); 
        }
      }
    } else {
      if(IsMaster == '1') {
        BContainer.addClass('Disabled');
        BButton.addClass('Disabled');
        BButton.attr('disabled', 'disabled'); 
      } else {
        if(IsAvailable == '1' && IsVisible == '1') {
          if(Pixi.AvailableStock < MinOrder) {
            if(LowOnStockAction == '0') {
                BContainer.removeClass('Disabled');
                BButton.removeClass('Disabled');
                BButton.removeAttr('disabled');                     
            } else if(LowOnStockAction == '2') {
                if(ICProductIsAvailable == 'Y') {
                    BContainer.removeClass('Disabled');
                    BButton.removeClass('Disabled');
                    BButton.removeAttr('disabled');  
                } else {
                    BContainer.addClass('Disabled');
                    BButton.addClass('Disabled');
                    BButton.attr('disabled', 'disabled');    
                }                  
            } else { 
              if(ICProductIsAvailable == 'Y') {
                BContainer.removeClass('Disabled');
                BButton.removeClass('Disabled');
                BButton.removeAttr('disabled');     
              } else {
                BContainer.addClass('Disabled');
                BButton.addClass('Disabled'); 
                BButton.attr('disabled', 'disabled');   
              }              
            }        
          } else {
            BContainer.removeClass('Disabled');
            BButton.removeClass('Disabled');
            BButton.removeAttr('disabled');        
          }    
        } else {
          BContainer.addClass('Disabled'); 
          BButton.addClass('Disabled'); 
          BButton.attr('disabled', 'disabled');   
        }      
      }       
    }    
   
    return;  
  }
  
  function ChangeTrafficLight(ProductID, ProductStock, StockLevelAlert, ICProductIsAvailable, MinOrder){
    var TraficLight = "";
    var IContainer  = jq('#DeliverPeriodDisplay_' + ProductID + ' p i'); 
    
    if(ProductStock < MinOrder){ 
      if(ICProductIsAvailable == 'Y') {
          TraficLight = "ProductWarnStockIcon"; 
      } else {
        TraficLight = "ProductOutStockIcon"; 
      }
    }else if(StockLevelAlert != -1 && ProductStock < StockLevelAlert){
      TraficLight = "ProductWarnStockIcon";    
    }else{
      TraficLight = "ProductOnStockIcon";          
    } 
    
    IContainer.removeClass("ProductOutStockIcon");
    IContainer.removeClass("ProductWarnStockIcon"); 
    IContainer.removeClass("ProductOnStockIcon");
    IContainer.addClass(TraficLight);
    
    return;
  }  
        
  function ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, ProductStockAvailableText, ProductStockCriticalText, ProductLongerDeliveryTimeText, ProductNoAddToBasketText, ProductPreorderDataText, ProductNoStockDataText, LowOnStockAction, ICProductIsAvailable, IsAvailable, IsNew, StockLevelAlert, MinOrder, PixiData ){
    var PContainer    = jq('#DeliverPeriodDisplay_' + ProductID + ' p');
    var AvaibilyText  = '<i class="Icon"></i> ';

    if(typeof PixiData.AvailableStock === "undefined"){ return; }
    //alert(LowOnStockAction + " " + PixiData.AvailableStock + " " + MinOrder + " " + ProductID);
    
    if(ProductStockAvailableText.indexOf(ManufacturerDeliveryPeriod)) {
      //alert(ProductStockAvailableText + ManufacturerDeliveryPeriod+ProductDeliveryPeriod);
      ProductStockAvailableText = ProductStockAvailableText.replace(ManufacturerDeliveryPeriod, ProductDeliveryPeriod);      
    }    

    if(PixiData.AvailableStock < MinOrder){
      if(LowOnStockAction == '0') {
          if(IsAvailable == '0') {
              AvaibilyText += ProductNoAddToBasketText;
          } else {
              AvaibilyText += ProductLongerDeliveryTimeText;
          }          
      } else if(LowOnStockAction == '2') {
          if(ICProductIsAvailable == 'Y') {
              AvaibilyText += ProductLongerDeliveryTimeText; 
          } else if(IsNew == '1') {
              AvaibilyText += ProductPreorderDataText;
          } else {
              AvaibilyText += ProductNoAddToBasketText;   
          }                     
      } else { 
        if(ICProductIsAvailable == 'Y') {
          AvaibilyText += ProductStockAvailableText.replace(ProductDeliveryPeriod, ManufacturerDeliveryPeriod);   
        } else if(IsNew == '1') {
          AvaibilyText += ProductPreorderDataText;
        } else {
          AvaibilyText += ProductNoAddToBasketText;
        }              
      }   
    } else {
      if(PixiData.InternalStock >= MinOrder) {
        if(PixiData.AvailableStock <= StockLevelAlert) {
          AvaibilyText += ProductStockCriticalText;        
        } else {
          AvaibilyText += ProductStockAvailableText;
        }              
      } else {
        AvaibilyText += ProductStockAvailableText.replace(ProductDeliveryPeriod, ManufacturerDeliveryPeriod);      
      }
    }
    
    //alert(PixiData.InternalStock + ' _ ' + ProductStockAvailableText);
    
    PContainer.html(AvaibilyText);
    
    return;  
  }

</script>
