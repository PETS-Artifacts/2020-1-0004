    function initSlider(slider) {
       var parentSlider = slider.parent(".ICDesignSlider");
       var sliderWidth = 230,
           itemsMax = 0;
       
       if(parentSlider.parent().hasClass("ManufacturerSlider")) {
           sliderWidth = 200; 
       } else if(parentSlider.parent().hasClass("AdvancedLinkSlider") || parentSlider.parent().hasClass("Slider")) {
           itemsMax = 1;
           sliderWidth = '100%';
       } else if(parentSlider.parent().hasClass("AccessorySlider")) {
           itemsMax = 2;
           sliderWidth = 400;
       }

      /* load the slider */
      slider.flexslider({
        slideshow: (slider.attr('slideshow') === 'true') ? true : false,
        touch: true,
        animation: (slider.attr('animation') === 'slide') ? 'slide' : 'fade',
        animationLoop: (slider.attr('animationloop') === 'false') ? false : true,
        itemWidth: sliderWidth,
        maxItems: itemsMax,
        move: 1,
        controlNav: (slider.attr('controlnav') === 'false') ? false : true,
        start: function() {
          var maxWidthSlideWrapper = slider.find('ul.slides li').size() * sliderWidth;
          slider.closest('.ICSliderWrapper').css('max-width', maxWidthSlideWrapper);
          //slider.find('ul.slides').css('max-width', maxWidthSlideWrapper);
          if(maxWidthSlideWrapper < jQuery('.ContentArea').width()) {
              slider.find('.flex-direction-nav li a').addClass('flex-disabled');
          }
        }
      });
    }
</script>
    <script type="text/javascript">
  
  function GetPixiDeliveryPeriod(ProductID, WebPath, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, StockLevelAlert, MinOrder, ShopStockLevel, AvaibilyText, ProductIsAvailable){
    //var BaseURL         = "https://www.fc-moto.de";
    var BaseURL           = "http://www.fc-moto.de";
    var AjaxURL           = BaseURL + WebPath + "/stockstate_" + ProductID + ".json?PreventCache=" + Date.now();
    var LowOnStockAction  = 0;
    
    if(ShopStockLevel > 0){ return; }
    if(ManufacturerDeliveryPeriod == ''){ return; } 
    if(ProductDeliveryPeriod == ''){ return; }
    if(AvaibilyText == ''){ return; }
    
    jQuery.ajax({      
      type: 'GET',
      async: false,
      url: AjaxURL,
      dataType: 'json',
      success: function(data) {
        if(data.InternalStock <= 0 && data.ExternalStock > 0){
          ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, AvaibilyText);        
        }

        var TrafficLight = ChangeTrafficLight(ProductID, data.AvailableStock, StockLevelAlert, MinOrder);        

        ChangeMicroDataAvailability(ProductID, data.AvailableStock, StockLevelAlert, MinOrder, LowOnStockAction);
        
        if(LowOnStockAction == 2 && TrafficLight == "ProductOutStockIcon" && ProductIsAvailable) {
          ChangeBasketButton(ProductID, 0);        
        }else{
          ChangeBasketButton(ProductID, 1);        
        }
      }
    });
  
    return;
  }
  
  function ChangeMicroDataAvailability(ProductID, ProductStock, StockLevelAlert, MinOrder, LowOnStockAction)
  {
    var MContainer  = jq('#MicroDataAvailability_' + ProductID);
    var Link        = 'http://schema.org/';
    
    if(ProductStock < MinOrder){
      if(LowOnStockAction == '3'){
        Link = Link + 'OutOfStock';
      }else if(LowOnStockAction == '2'){
        Link = Link + 'OutOfStock';
      }else if(LowOnStockAction == '1'){
        Link = Link + 'PreOrder';        
      }else{
        Link = Link + 'PreOrder';
      }
    }else if(StockLevelAlert != -1 && ProductStock < StockLevelAlert){
      Link = Link + 'InStock';    
    }else{
      Link = Link + 'InStock';    
    }
    
    jq('#MicroDataAvailability_22004829').attr('href', Link);
  
    return;  
  }


  function ChangeBasketButton(ProductID, State){
    var BContainer  = jq('#BasketLink_' + ProductID);
    
    if(State == 0){
      BContainer.addClass('HideElement');      
    }else{
      BContainer.removeClass('HideElement');     
    }
    
    return;  
  }
  
  function ChangeTrafficLight(ProductID, ProductStock, StockLevelAlert, MinOrder){
    var TraficLight = "";
    var IContainer  = jq('#DeliverPeriodDisplay_' + ProductID + ' p i'); 
    
    if(ProductStock < MinOrder){ 
      TraficLight = "ProductOutStockIcon"; 
    }else if(StockLevelAlert != -1 && ProductStock < StockLevelAlert){
      TraficLight = "ProductWarnStockIcon";    
    }else{
      TraficLight = "ProductOnStockIcon";          
    } 
    
    IContainer.removeClass("ProductOutStockIcon");
    IContainer.removeClass("ProductWarnStockIcon"); 
    IContainer.removeClass("ProductOnStockIcon");
    IContainer.addClass(TraficLight);
    
    return TraficLight;
  }  
  
  function ChangeDeliveryPeriod(ProductID, ProductDeliveryPeriod, ManufacturerDeliveryPeriod, AvaibilyText){
    var PContainer      = jq('#DeliverPeriodDisplay_' + ProductID + ' p');
 
    if(ProductDeliveryPeriod.length > 0 && ManufacturerDeliveryPeriod.length > 0 && AvaibilyText.length > 1){
      AvaibilyText = AvaibilyText.replace(ProductDeliveryPeriod, ManufacturerDeliveryPeriod);
      AvaibilyText = '<i class="Icon"></i> ' + AvaibilyText;
      
      PContainer.html(AvaibilyText);          
    }    
    
    return;  
  }

</script>
    
    
    <script type="text/javascript">
   
jq( document ).ready(function() {
    transformFilterList();
    
    jq('.ICFilter').on("click",function(){
        setFilter(jq(this));
    });
});



/**
* -------------------------------------------------------------------
*
* Ajax Reload Product Information
*
* @author LK
* -------------------------------------------------------------------
*/
function getSubProduct(ProductID) {
    jQuery.ajax({
            url: epConfig.baseUrl+'?ViewAction=JSONGetSubProductPartially&ProductID='+ProductID,
            contentType: "text/plain",
            //preventCache: true,
            success: function (data) {
              var obj = JSON.parse(data),
                  el                = jq('.ic_Product .ProductDetails'),
                  ProductOrderArea  = el.find('.ICProductOrderArea'),
                  ProductGallery    = el.find('#ICProductGallery'),
                  AdditionalLinks   = el.find('.PriceAndOrderBox .AdditionalLinks li'),
                  DisplayPrice      = obj['displayprice'],
                  PriceSavings      = obj['pricesavings'],
                  ManufacturerPrice = obj['manufacturerprice'];


              //Product PriceSavings
              if (PriceSavings != "0") {
                  if(el.find('#ICProductGallery .HotPrice').length >= 1) {
                      el.find('#ICProductGallery .HotPrice').html("-" + PriceSavings);
                  } else {
                      jq('<span class="HotPrice">-' + PriceSavings + '</span>').appendTo(el.find('#ICProductGallery .ICEyeCatcher'));
                  }
                  ProductOrderArea.find('.HotPriceNoImg span').html(PriceSavings);
                  ProductOrderArea.find('.HotPriceNoImg').css({'display':'block'});
              } else {
                  ProductGallery.find('.ICEyeCatcher .HotPrice').remove();
                  ProductOrderArea.find('.HotPriceNoImg').css({'display':'none'});
              }

              //Product Prices
              el.find('.PriceArea .Price span').html(DisplayPrice);
              el.find('.PriceArea.InsteadOf .LineThrough').html(ManufacturerPrice);

              //New Icon
              if (obj['new']) {
                  if(ProductGallery.find('.NewProduct').length < 1) {
                      jq('<span class="NewProduct">New</span>').prependTo(el.find('#ICProductGallery .ICEyeCatcher'));
                  }
              } else {
                  if(ProductGallery.find('.NewProduct').length > 1) {
                      ProductGallery.find('.ICEyeCatcher .NewProduct').remove();
                  }
              }

              //Product Functions
              AdditionalLinks.find('.LinkProductSeenCheaper').attr('href', '?ObjectPath=' + obj['productpath'] + '&ViewAction=ViewSeenCheaper');
              AdditionalLinks.find('.LinkProductInquiry').attr('href', '?ObjectPath=' + obj['productpath'] + '&ViewAction=ViewInquiry');
              AdditionalLinks.find('.LinkProductRecommendation').attr('href', '?ObjectPath=' + obj['productpath'] + '&ViewAction=ViewRecommendation');
              AdditionalLinks.find('form[name="LinkAddShoppingList"] input[name="RefObjectID"]').attr('value', ProductID);
              var Shopping = AdditionalLinks.find('.LinkAddShoppingList');
              if(Shopping.length > 0) {
                  AdditionalLinks.find('.LinkAddShoppingList').attr('href', '?ObjectID=' + ProductID + '&ViewAction=ViewAddToShoppingList&RefObjectID=' + ProductID);
              } else {
                  jq('<li><a class="Link LinkIcon LinkAddShoppingList" rel="nofollow" href="?ObjectID=' + ProductID + '&ViewAction=ViewAddToShoppingList&RefObjectID=' + ProductID + '">Add to shopping list</li>').prependTo(el.find('.PriceAndOrderBox .AdditionalLinks:not(.BonusCampaign)'));
              }
              AdditionalLinks.find('.LinkCompareProduct').productcomparisonUiComparelink({productID:ProductID});

              //Basket Button
              el.find('#basketForm_standalone input[name="LastProductObjectID"], #basketForm_standalone input[name="ChangeObjectID"], #basketForm_standalone input[name="ViewObjectID"]').attr('value', ProductID);
              if(obj['available']) {
                  el.find('button[name="AddToBasket"]').removeClass('Disabled');
              } else {
                  el.find('button[name="AddToBasket"]').addClass('Disabled');
              }
              el.find('button[name="AddToBasket"]').attr('data-productid', ProductID).metaparse();

              //Shipping
              jQuery.ajax({
                      url: epConfig.baseUrl+'?ViewAction=ICPixiStockLevel&ProductID='+ProductID,
                      contentType: "text/plain",
                      //preventCache: true,
                      success: function (data) {
                        data = data.replace(/<\!--(.*)-->/g, '');				  	
                        data = data.replace(/\t/g, '');
                        data = data.replace(/\n/g, '');
                        data = jQuery.trim(data);
                              el.find('.PriceAndOrderBox .DeliveryPeriod').addClass('Hidden');
                              if(el.find('.PriceAndOrderBox .DeliveryPeriod').length > 0) {
                                  el.find('.PriceAndOrderBox .DeliveryPeriod').html(data);
                              } else {
                                  jq('.PriceAndOrderBox .DeliveryPeriod').remove();
                                  jq(data).appendTo('.PriceAndOrderBox .PriceArea:not(.InsteadOf)');
                              }
                              el.find('.PriceAndOrderBox .DeliveryPeriod').delay(500).removeClass('Hidden');
                      },
                      error: function(data) {
                              console.log(data);
                      }
              });

              //ListPrices and Discounts
              jQuery.ajax({
                      url: epConfig.baseUrl+'?ViewAction=ICSpecialPrices&ProductID='+ProductID,
                      contentType: "text/plain",
                      //preventCache: true,
                      success: function (data) {
                        data = data.replace(/<\!--(.*)-->/g, '');				  	
                        data = data.replace(/\t/g, '');
                        data = data.replace(/\n/g, '');
                        data = jQuery.trim(data);
                              jq('.ic_Product #ProductInfoTabs #tab-product-specialprices #Prices').replaceWith(data);
                              if (jq('.ic_Product #ProductInfoTabs #tab-product-specialprices #Prices .SpecialPricesPage').html()) {
                                 jq('.ic_Product #ProductInfoTabs li[aria-controls="tab-product-specialprices"]').removeClass('Hidden');
                              } else {
                                 jq('.ic_Product #ProductInfoTabs li[aria-controls="tab-product-specialprices"]').addClass('Hidden');
                                 if(jq('#tab-product-specialprices').attr('aria-hidden') === 'false') {
                                     jq(this).css({'display':'none'});
                                     jq('#ProductInfoTabs div.ui-tabs-panel:first-of-type').css({'display':'block'});
                                     jq('#ProductInfoTabs ul.ui-tabs-nav  li:first-of-type a').addClass('ui-tabs-active');
                                 }
                              }

                      },
                      error: function(data) {
                              console.log(data);
                      }
              });

            },
            error: function(data) {
                    console.log(data);
            }
    });
}

/**
* -------------------------------------------------------------------
*
* Show Filter in mobile Version only after clicking special Button
* @param {object} el clickedButton
* 
* @author LK
* -------------------------------------------------------------------
*/
function setFilter(el) {
      var nbl = jq('.NavBarLeft'),
          width = jq(window).width(),
          TriggerRes = 743;

        // Mobile Version
        if (width < TriggerRes && jq('.RemoteSearchFacet').size() > 0) {
            if (nbl.hasClass('ShowJS')) {
                nbl.removeClass('ShowJS');
                jq('.ICFilter').html('Show Filter');
                jq('.ICFilter.Remove').remove();
            } else {
                nbl.addClass('ShowJS');
                el.html('Hide Filter');
                jq("<span class='ICFilter Remove'>Hide Filter</span>").appendTo(nbl);
                jq('.ICFilter.Remove').on("click",function(){
                    setFilter(jq(this));
                });
            }
        // Desktop Version
        } else {
            nbl.removeClass('ShowJS');
            jq('.ICFilter').html('Show Filter');
            jq('.ICFilter.Remove').remove();
        }
}


/** 
 * show only x Elements in FilterList for Manufacturer and Size
 */
function transformFilterList() {
    var ShowAll = 'show all',
        size = 0,
        showElements = 6;

    size = jq('.RemoteSearchFacet.Manufacturer li').size();
    jq('.Middle .RemoteSearchFacetSelect.Manufacturer li.ManufacturerFilter').css({'margin-left':'0px'});
    
    if(size > showElements) {
        while (size > showElements) {
            jq(".RemoteSearchFacet.Manufacturer li:nth-child("+size+")").hide();
            size = size - 1;
        }
        jq('.RemoteSearchFacet.Manufacturer ul').append("<li class='ManufacturerFilter'><a href='#' onclick=showAllManufaturer(event)>"+ShowAll+"</a></li>");
    }

    size = jq(".RemoteSearchFacet.Size li").size(); 
    
    if(size > showElements) {
        while (size > showElements) {
            jq(".RemoteSearchFacet.Size li:nth-child("+size+")").hide();
            size = size - 1;
        }
        jq('.RemoteSearchFacet.Size ul').append("<li class='SizeFilter'><a href='#' onclick=showAllSizes(event)>"+ShowAll+"</a></li>");
    }
}

/** 
 * show all Elements in FilterList for Manufacturer
 */
function showAllManufaturer(ev) {
    ev.preventDefault();
    jq(".RemoteSearchFacet.Manufacturer li").each(function() {
        jq(this).show();
    });
    jq(".ManufacturerFilter").hide();
    jq(".RemoteSearchFacet.Manufacturer .scrollbar").css({"overflow-y":"scroll"});
    jq('.ManufacturerFilter').prev('li').css({'margin-left':'21px'});
}

/** 
 * show only x Elements in FilterList for Size
 */
function showAllSizes(ev) {
    ev.preventDefault();
    jq(".RemoteSearchFacet.Size li").each(function() {
        jq(this).show();
    });
    jq(".SizeFilter").hide();
    jq(".RemoteSearchFacet.Size .scrollbar").css({"overflow-y":"scroll"});
}
</script>


