		function initAuthentication() {
			if(!isAuthenticated){
				$.cookie('.ArchipassportToken', null, { domain: '.archiproducts.com', expires: -1, path: '/'});
				$.cookie('.IsAuthenticated', null, { domain: '.archiproducts.com', expires: -1, path: '/'});
            }

            // remove the old authentication cookie with domain archiproducts.com
            $.cookie('.AspNetCore.Identity.Application', null, { domain: '.archiproducts.com', expires: -1, path: '/' });

