				apiHost: archipassportDomain,
				culture: $('html').attr('lang'),
				countryCode: '',
				portal: portalType.Archiproducts,
				archipassportDomain: archipassportDomain,
				proxyPage: $.absuri('/Authentication/AuthProxy.ashx'),
