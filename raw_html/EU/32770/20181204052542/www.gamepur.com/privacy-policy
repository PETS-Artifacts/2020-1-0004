            /* * * CONFIGURATION VARIABLES: EDIT BEFORE PASTING INTO YOUR WEBPAGE * * */
            var disqus_shortname = 'gamepur';
            var disqus_config = function () {
                this.language = "en";
            };
    
            /* * * DON'T EDIT BELOW THIS LINE * * */
            (function () {
                var dsq = document.createElement('script'); dsq.type = 'text/javascript'; dsq.async = true;
                dsq.src = 'https://gamepur.disqus.com/embed.js';
                (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(dsq);
            })();
    
            /* * * Disqus Reset Function * * */
            //{"url" : "http://www.gamepur.com/node/85", "identifier" : "node/85"};
            var resetDisqus = function (identifier, title) {
                DISQUS.reset({
                    reload: true,
                    config: function () {
                        this.page.identifier = "node/" + identifier;
                        this.page.url = "http://www.gamepur.com/node/" + identifier;
                        this.page.title = title;
                        this.language = 'en';
                        this.page.disqus_container_id = "disqus_thread" + identifier;
                        this.disqus_container_id = "disqus_thread" + identifier;
                    }
                });
