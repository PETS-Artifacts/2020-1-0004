<script>
function toggleMobileMenu() {
    var mobileMenu = document.getElementById("header"),
    currentClass = mobileMenu.className;

    if ((currentClass).indexOf('dfx-mobile-menu-open') < 0) {
        currentClass = currentClass + ' dfx-mobile-menu-open';
    }
    else {
        currentClass = currentClass.replace(/(^| )dfx-mobile-menu-open/gi,'');
    }
    mobileMenu.className = currentClass;
}
</script>

