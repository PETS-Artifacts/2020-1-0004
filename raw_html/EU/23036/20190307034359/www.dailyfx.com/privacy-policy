<script>
    $(document).ready(function() {
        var bannerLink = "https://dailyfx.onelink.me/spAe?pid=web&c=banner&af_dp=dailyfx%3A%2F%2Fopen%2F&af_web_dp=https%3A%2F%2Fplay.google.com%2Fstore%2Fapps%2Fdetails%3Fid%3Dcom.ig.dailyfx";
        var siteCatalystInterval = setInterval(getSiteCatalystId, 1000);
        function getSiteCatalystId() {
            try {
                var scId = s.Util.cookieRead('s_vi');
                if (scId) {
                    scId = scId.replace('[CS]v1|', '').replace('[CE]', '');
                    bannerLink = bannerLink + '&af_sub4='+scId;
                    $('.banner-download').attr("href",bannerLink);
                    clearInterval(siteCatalystInterval);
                }
            } catch(e) {}
        }
        if($.cookie("download-app-banner-closed")){
            $("#download-app-banner").hide();
        }
    });
    $("#download-app-banner .close").click(function() {
        $("#download-app-banner").slideToggle();
        $.cookie("download-app-banner-closed", true);
    });
</script><script>
    //Start Fix MegaNavbar on scroll page
  $(document).ready(function() {
    var navHeight = $('#main_navbar').offset().top;
    FixNavbar(navHeight);
    $(window).bind('scroll', function () {
        FixNavbar(navHeight);
    });

    // update offest of navigation to height of #header
    $('.dfx-cookies-btn').on('click', function() {
        navHeight = $('#header').outerHeight(true);
        FixNavbar(navHeight);
    });

    function FixNavbar(navHeight) {
        if ($(window).scrollTop() > navHeight) {
            $('#main_navbar').addClass('navbar-fixed-top');
            $('body').css({'margin-top': $('#main_navbar').height() + 'px'});
        } else {
            $('#main_navbar').removeClass('navbar-fixed-top');
            $('body').css({'margin-top': ''});
        }
    }
  });
</script>
