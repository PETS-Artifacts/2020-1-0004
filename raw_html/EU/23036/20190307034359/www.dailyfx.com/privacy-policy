        $(document).ready(function () {
            geoLocationProcessor.addDisplayStrategy(function(currentCountry){
                if (currentCountry === countryPrefixes.unitedStates && currentLanguage === countryLanguages.english.toLowerCase()) {
                    $('#dfx-education-item').after('<li class=""><a href="/binary-options?ref=TopNav" data-ref="TopNav" data-title-short="Binaries" itemprop="url"><span itemprop="name">Binaries</span></a></li>');
                    $('#education-footer-item').after('<li class="footer-subtitle"><a href="/binary-options">BINARIES</a></li>');
                }
                return true;
            });
            fetchGeoLocationData();
        });
    </script>
