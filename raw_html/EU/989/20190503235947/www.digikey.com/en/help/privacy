var privacyMenuLink = document.getElementsByClassName("privacy-menu__link")[0];
var privacyMenuTrigger = document.querySelectorAll(".privacy-menu__has__submenu > a");

privacyMenuLink.addEventListener("click", function(e) {
	e.preventDefault();
	privacyMenuLink.classList.toggle("privacy-active");
	privacyMenu.classList.toggle("privacy-active");
});

function triggerSubmenu(privacyMenuTrigger) {
	privacyMenuTrigger.addEventListener("click", function(e) {
		e.preventDefault();
		privacyMenuTrigger.classList.toggle("privacy-active");
		privacyMenuTrigger.nextSibling.nextSibling.classList.toggle("privacy-active");
	});
}

for(var i=0; i < privacyMenuTrigger.length; i++) {
	triggerSubmenu(privacyMenuTrigger[i]);
}
