<script>
$(function() {
    var header = $(".is-scrolling");
    $(window).scroll(function() {
        var scroll = $(window).scrollTop();
        
        if (scroll >= 450) {
            $('#red-logo').removeClass("hidden");
            $('#white-logo').addClass("hidden");
        } else {
            $('#red-logo').addClass("hidden");
            $('#white-logo').removeClass("hidden");
        }
    });
});
</script>

