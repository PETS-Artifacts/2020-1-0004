	function contactFormOptions() {
	contactformoptions={showOneMessage: true, scroll: false, 
	onFieldFailure: function(field) {
	
	if(field)
	field.addClass('onerror');
	}, onFieldSuccess: function(field) {if(field) field.removeClass('onerror');}};
	
	
	}
	</script><script type="text/javascript">
jQuery(document).ready(function($) {
	//Contact Ajax Form
	jQuery('#general-contact-form').ajaxForm({
		success: function(msg) {
			if (msg.trim()!=""){
				ga('send','event','contatto','home');
				$("#general-contact-form").hide();
				$("#general-contact-form-exit").html(msg);
				console.log(msg);	
			}
		}
	});	
});	
</script>
