        <script type="text/javascript">
    function doClear(theText) {
        if (theText.value == theText.defaultValue) {
            theText.value = ""
        }
    }

    var addthis_config = {
        data_track_clickback: true,
        services_compact: 'print, email, facebook, twitter, linkedin, google, delicious, stumbleupon, messenger, digg, more'
    }

    window.onload = function()
    {
        if ( $('.cvsearchResultModule')[0] ){
            //we have results, dont focus keywords otherwise the page scrolls to top
        }else if ( document.search && document.search.q && document.search.q.attributes.nofocus === undefined )
        {
            document.search.q.focus();
            document.search.q.setSelectionRange(0,0);
        }
        else if ( document.startsession && document.startsession.login_email )
        {
            document.startsession.login_email.focus();
            document.startsession.login_email.setSelectionRange(0,0)();
        }
        else if ( document.search && document.search.keywords && document.search.keywords.attributes.nofocus === undefined )
        {
            document.search.keywords.focus();
            document.search.keywords.setSelectionRange(0,0);
        }

    };
    $(document).ready(function(){
        // Only if we dont have manually created toTop (admin side)
        if (!$('#toTop')[0]) {
            /* options */
            var show_to_top_from = 200; //at what amount of scroll should the 'to top' button show? (px)
            var ani_time = 300; //time in ms the scroll animation will take

            //create and add the to top & to bottom buttons to the page
            var to_top = $(document.createElement('div')).attr({'id':'to-top','title':'Go to top'}).append('<span></span>');
            $('body').append(to_top);

            var $to_top = $('#to-top');

            function scroll_to(){
                $('body,html').animate({scrollTop : 0 }, ani_time);
            };

            function check_scroll(){

                if($(window).scrollTop() > show_to_top_from){
                    $to_top.show();
                }else{
                    $to_top.hide();
                }

            };

            $to_top.click(scroll_to);

            //init on page load
            check_scroll();

            //init on scroll
            $(window).scroll(check_scroll);
        }
    });
</script>
