	$(function() {
		if ($('video').length == 1)
		{
			var title = document.title;	
		}	
		$('video').each(function() {
			$(this).bind('play', function(e) {
				if (typeof(title) == 'undefined')
				{
					if (typeof($(e.currentTarget).attr('data-video-title')) != 'undefined')
					{
						var title = $(e.currentTarget).attr('data-video-title');
					}
					else
					{
						var title = $(e.currentTarget).prop('currentSrc');
					}
				}
				ga('send', 'event', 'Video', 'Play', title);
			});
			$(this).bind('ended', function(e) {
				if (typeof(title) == 'undefined')
				{
					if (typeof($(e.currentTarget).attr('data-video-title')) != 'undefined')
					{
						var title = $(e.currentTarget).attr('data-video-title');
					}
					else
					{
						var title = $(e.currentTarget).prop('currentSrc');
					}
				}
				ga('send', 'event', 'Video', 'Finished', title);
			});
		});
	});
</script>

