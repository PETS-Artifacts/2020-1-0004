<script>$(document).ready(function () { 
  (function(win) {
    function ContactSlideOut(options){
      var wrapper = options.wrapper;
      var tab = options.tab;
      var duration = options.duration || .5;
      var ease = options.ease || "Sine.easeOut";
      var openClass = options.openClass;
      var closeClass = options.closeClass;
     var linkToContact = false;
      $(tab).data("open", false) 
$(tab).click(function() {
        var isOpen = $(this).data("open");
 
        if ($( window ).height() < 680 && isOpen !== true && isOpen !== "true"){
          window.open("/node/23", "_self")
        } else {
          if (isOpen == true || isOpen == "true"){
            //$(this._nDotArray[i]).data("index", i) 
            TweenMax.to($( wrapper), duration, {className:closeClass, ease:ease, onComplete:function(){$(tab).data("open", false) }});
            //$( wrapper).switchClass( "newClass", "close", {duration:1000, complete:function(){$(tab).data("open", false) }} );
          } else {
            $(tab).data("open", true)
            //$( wrapper).switchClass( "newClass", "open", 1000 );
            TweenMax.to($( wrapper), duration, {className:openClass, ease:ease});
          }
        }
            
          });
      $( window ).resize(applyBrowserResize);
      applyBrowserResize();
    }
    function applyBrowserResize(){
      
        //$( win ).width()
        //$( win ).height()
        var div = document.getElementById("contact-slide-out");
        //console.log("div: " + div)
        if (div){
          var wHeight = $( window ).height() - 90;
          var dHeight = 590;//$( div ).height();
          var y = (wHeight * .5) - (dHeight * .5);
            //console.log("dHeight: " + dHeight);
         //var y = (wHeight * .5) - (500 * .5)
          if (y < 0){
              y = 0;
          } 
          //y = Number(y + 20);
          y = Number(70);
          //console.log("y: " + y);
          div.style.top = y + "px"; 
        }
    }
    win.ContactSlideOut = ContactSlideOut;
  }(window));
  var cs = new ContactSlideOut({wrapper:"#contact-slide-out", tab:"#contact-slide-out-tab", duration:.5, ease:"Sine.easeOut", openClass:"open-contact", closeClass:"close-contact"})
 });</script>
