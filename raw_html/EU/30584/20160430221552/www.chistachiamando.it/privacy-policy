<script type="text/javascript" src="//ajax.googleapis.com/ajax/libs/jquery/2.0.3/jquery.min.js"></script>
  <script type="text/javascript">
    var stickyPanel=$('#stickyPanel');
    var stickyAdsense=$('#stickyAdsense');
    var bodyContainer=$('#body-container');
    var isSticky=false;
    var isBottom=false;
    function checkSticky() {
      if((!stickyPanel) || (!stickyAdsense) || (!bodyContainer))
        return;
      var stickyOffset=stickyPanel.offset();
      if(typeof(stickyOffset)==='undefined')
        return;
      var ofs=stickyOffset.top-$(window).scrollTop();
      var spaceToEnd=(bodyContainer.offset().top+bodyContainer.height())-$(window).scrollTop();
      var sh=stickyAdsense.height();
      if (spaceToEnd-sh<15) {
        if(isSticky) {stickyPanel.removeClass("stickyPanel");isSticky = false;}
        if(!isBottom) {stickyPanel.addClass("stickyBottomPanel");isBottom=true;}
      } else {
        if(isBottom) {stickyPanel.removeClass("stickyBottomPanel");isBottom=false;}
        if (ofs<15) {
          if (!isSticky) {stickyPanel.addClass("stickyPanel");isSticky=true;}
        } else {
          if(isSticky) {stickyPanel.removeClass("stickyPanel");isSticky = false;}
        }
      }
    }

    $(window).scroll(function(event) {
      checkSticky();
    });
    checkSticky();
  </script>

<script type="text/javascript">
  function setCookie(cname,cvalue,exdays)
  {
    if(exdays===undefined)
      exdays=100000;
    var d = new Date();
    d.setTime(d.getTime()+(exdays*24*60*60*1000));
    var expires = "expires="+d.toGMTString();
    document.cookie = cname + "=" + cvalue + "; " + expires+";path=/";
  }

  $( document ).ready(function() {
    var $elFooterPromo=$('#footerPromo');
    var rollEventSent=false;

    $(document).scroll(function() {
      var offset = $elFooterPromo.offset();
      var bottom = $(window).height()-(offset.top-$(window).scrollTop());
      if(bottom>0) {
        var h=Math.round($elFooterPromo.height()*0.7);
        var blurVal = 10 - (bottom / h * 10);
        var bv=( bottom / h);
        if(bv>1) bv=1;
        if((bv==1) && (!rollEventSent)) {
          ga('send', 'event', 'underPagePromo', 'view');
          rollEventSent=true;
        }
      }
    });

  });

  $('#cookie_gotit').click(function() {
    $('#cookie_statement').hide();
    setCookie('cookies_dontshow','1');
    return false;
  });
</script>

