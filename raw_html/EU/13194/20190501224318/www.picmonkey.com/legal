<script>
  !function() {
    var cookies = document.cookie ? document.cookie.split(';') : [];
    for (var i = 0; i < cookies.length; i++) {
      var splitted = cookies[i].split('=');
      var name = splitted[0];
      try {
        decodeURIComponent(name);
        decodeURIComponent(splitted[1]);
      } catch(e) {
        var cookie = name + '=;expires=Thu, 01 Jan 1970 00:00:01 GMT;path=/';
        document.cookie = cookie;
        document.cookie = cookie + ';domain=' + location.hostname.replace(/^.*\.picmonkey/, '.picmonkey');
      }
    }
  }();

  var consentLogic={queue:[],hasBeenExecuted:!1,pushToQueue:function(e){consentLogic.hasBeenExecuted?e():consentLogic.queue.push(e)},executeQueue:function(){consentLogic.hasBeenExecuted=!0,consentLogic.queue.forEach(function(e){e()}),consentLogic.queue=[]},scriptWrapper:function(e,t,n){var c=document.createElement("script");c.src=e,t&&(c.async=t),n&&(c.defer=n),document.body.appendChild(c)},iframeWrapper:function(e){var t=document.createElement("iframe");t.setAttribute("src",e),t.setAttribute("width","1"),t.setAttribute("height","1"),t.setAttribute("frameborder","0"),t.setAttribute("style","display:none"),document.body.appendChild(t)}};
</script>
<script>
    function supportsWebGL(label) {
      return label === undefined ? !!getWebGLDeviceLabel() : !!label;
    }

    function getWebGLDeviceLabel() {
      try {
        var canvas = document.createElement('canvas');
        var glsniff  = (!!window.WebGLRenderingContext &&
          (canvas.getContext('webgl', {'failIfMajorPerformanceCaveat': true}) || canvas.getContext('experimental-webgl')));
        var renderInfo = glsniff.getExtension('WEBGL_debug_renderer_info');
        if (renderInfo != null) {
          var umrenderer = glsniff.getParameter(renderInfo.UNMASKED_RENDERER_WEBGL);
          if (umrenderer.match(/ps_3_0/)){
            return null;
          }
        }
        var vendor = glsniff.getParameter(renderInfo.UNMASKED_VENDOR_WEBGL);
        var renderer = glsniff.getParameter(renderInfo.UNMASKED_RENDERER_WEBGL);
        var label = 'vendor: "' + vendor + '" renderer: "' + renderer + '" dpr: ' + window.devicePixelRatio;
        return glsniff === null ? null : label ;
      } catch(e) {
        return null;
      }
    };

    var webGlDeviceLabel = getWebGLDeviceLabel();
    document.cookie = "supports_webgl=" + supportsWebGL(webGlDeviceLabel);
    if (webGlDeviceLabel) window.webGlDeviceLabel = webGlDeviceLabel;
    var excludeFromPlanPageExperiment = !webGlDeviceLabel;

      !function() {
        var redirectUrl = '';
        var queryParams = {};
        var referrer = document.referrer;

        if (referrer) {
          queryParams.v2Referrer = referrer;
        }

        var route = window.location.hash
        var excludedRoutes = ['^$', 'collage', 'save', 'hub/collage/add', 'hub/collage/update', 'templates']
        var isExcludedRoute = new RegExp(excludedRoutes.join("|")).test(route)
        var isLegalRoute = window.location.pathname.indexOf('legal') >= 0
        var isRegisterRoute = window.location.hash.indexOf('register') >= 0
        var isDeepLinkRoute = window.location.hash.indexOf('go') >= 0
        var accessingPricing = location.pathname.split('/')[1] === 'pricing';
        var secondLevelPath = location.pathname.split('/')[1] !== '';
        var isChromeExtFlow = window.sessionStorage.getItem("chrome_extension")

        // If v3_only user is logged in and wants to access something that is not collage, throw user back to V3 home
        // We still have users without beta access, that is why we need to check for "registered_on_new_picmonkey"

        // All signed out users who try to deep link into the product should be redirected to V3

        if (redirectUrl) {
          function buildQueryString(queryParams) {
            var queryString = Object.keys(queryParams)
              .map(function(key) {
                return key + '=' + encodeURIComponent(queryParams[key]);
              })
              .join('&');

            return queryString ? ('?' + queryString) : '';
          }

          window.location.href = redirectUrl + buildQueryString(queryParams);
        }
      }();
</script>
