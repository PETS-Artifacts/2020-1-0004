} else {

$('#fb-optout').html('<p>Tracking via the Facebook Pixel on this website is activated.<a href="https://www.wirecard.com/privacy-protection/"" onclick="alert(\'You have successfully excluded yourself from data collection via Facebook Pixel.\');dataLayer.push({\'event\': \'fboptout\'});"> Click here to deactivate the tracking.</a></p>');

}

$('#GAOptOut').on('click', function(event) {
event.preventDefault();
gaOptout();
});

$('#et-optout').on('click', function(event){
event.preventDefault();
alert('You have successfully excluded yourself from data collection via etracker.');
_etracker.disableTracking();
});

</script>


	
