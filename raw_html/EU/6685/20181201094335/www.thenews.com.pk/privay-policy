  window.onload = function () {
    setTimeout(function () {
      document.getElementById("render-css").remove();
      var imagesInterval = setInterval(function () {
        if(typeof  $ !== "undefined"){
          clearInterval(imagesInterval);


         /* var header_html = $("#header_menu_after_load").html();
