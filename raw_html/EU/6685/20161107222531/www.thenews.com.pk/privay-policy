    $(document).ready(function(){

      $(window).bind('scroll', function() {

        var navHeight = 250; // custom nav height

        ($(window).scrollTop() > navHeight) ? $('nav').addClass('goToTop') : $('nav').removeClass('goToTop');

      });

      $('.bxslider').bxSlider({
        minSlides: 1,
        maxSlides: 4,
        slideWidth: 205,
        slideMargin: 10
      });

      function changeDivWidth()
      {
        jQuery('.sticky_clock').css( "width" , "30px");
      }
      function closeDivWidth()
      {
        jQuery('.divClockAds').toggle( "slow" );
        setTimeout(changeDivWidth,1000);
      }

      //setTimeout(closeDivWidth,18000);

      jQuery(".ramazanTimingBanner").click(function(){
          jQuery('.sticky_clock').css( "width" , "195px");

          if(jQuery('.divClockAds').css('display') != 'none')
          {
            setTimeout(changeDivWidth,1000);
          }
          jQuery('.divClockAds').toggle( "slow" );
      });

    });

    </script>
