jQuery(document).ready(function() {

	function orderNewsLetter(){
		var postiosoite = jQuery('#newsltremail').val();
		if ( (postiosoite !== '') && (isValidEmail(postiosoite)) ) 
		{
			var ajaxParams = {
				postita: 'true',
				email: postiosoite
			};
			jQuery.BlockAjax.load('newsletter-subscription-block',ajaxParams, function(data){  
				var obj = JSON.parse(data); 
				if(obj.success == true){ 
					jQuery('#newsltremail').val('');
					jQuery('#check-email-address').css('display', 'none');
					jQuery('#subscription-ok').css('display', 'inline-block');
				}
				else{
					jQuery('#check-email-address').css('display', 'inline-block');
					jQuery('#subscription-ok').css('display', 'none');
					jQuery('#newsletter-subscribe-errormsg').html(obj.msg);
				}
			});
		}
		else
		{
			jQuery('#check-email-address').css('display', 'inline-block');
			jQuery('#subscription-ok').css('display', 'none');
			jQuery('#newsletter-subscribe-errormsg').html('Invalid e-mail address format!');
		}
	}
	
	jQuery('#newsletter-order-button').on('click', function(event){ 
		event.preventDefault();
		orderNewsLetter();
	});

});

//autocomplete search
$(document).ready( function(){
	$('.lamnia_search_autocomplete').autocomplete({
		source: $.BlockAjax.getBlockAjaxUrl('sidebar-autocomplete'),
		minLength: 3,
		autofocus: true,
		focus: function( event, ui ) {
			event.preventDefault();
		},
		select: function( event, ui ){
			window.location = ui.item.url;
		},
		delay: 300,
		open : function(){
			//lisätään oma luokka menulle
			$(this).data('uiAutocomplete').menu.element.addClass('sidebar-autocomplete-menu');

		}
	});
});

jQuery(document).ready(function(e) {

	$('#lang-select-container-hidden-while-loading').show();

	$('#langselect').show();

	$('#langselect').val($('#langselect').find('option[selected]').val());

	$('#langselect').selectpicker();

	$('#langselect').change(function() {
		var params = {};
		params.active_rewrite_rule = 'page';
		params.query = '{"pid":["8"]}';
		params.isFrontPage = false;
		params.new_locale = $(this).val();
		$.BlockAjax.load('languageselect', params, function(data) {
			var jso = $.parseJSON( data );
			if( jso ){
				if( jso.url ){
					window.location = jso.url;
				}
			}
		});
	});

});

jQuery(document).ready(function(e) {


	$('#deliverycountry-container-hidden-while-loading').show();

	$('#sidebar-dropdown-deliverycountry').show();

	$('#sidebar-dropdown-deliverycountry').val($('#sidebar-dropdown-deliverycountry').find('option[selected]').val());

	$('#sidebar-dropdown-deliverycountry').selectpicker();

	$('#sidebar-dropdown-deliverycountry').change(function() {
		var uusi_dcid = $('#sidebar-dropdown-deliverycountry').val();

		var ajaxParams = {
			changed: 'true',
			new_dcid: uusi_dcid
		};
		$.BlockAjax.load('sidebar-deliverycountry-block',ajaxParams, function(data){
			var obj = JSON.parse(data);
			if(obj.success == true){
				$('#sidebar-dropdown-deliverycountry').val(obj.dcid);
				if (obj.reloadpage == true)
				{
					window.location.reload();
				}
			}
		});
	});
});


