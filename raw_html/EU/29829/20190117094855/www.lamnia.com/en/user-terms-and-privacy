jQuery(document).ready(function(e) {

	$('.lang-select-container-hidden-while-loading').show();

	$('#1446555218-langselect').show();

	$('#1446555218-langselect').val($('#1446555218-langselect').find('option[selected]').val());

	$('#1446555218-langselect').selectpicker();

		$('#1446555218-langselect').change(function() {
			var params = {};
			params.active_rewrite_rule = 'page';
			params.query = '{"pid":["8"]}';
			params.isFrontPage = false;
			params.new_locale = $(this).val();

			$.ajax({
				method: 'POST',
				url: '?langchange=1',
				data: params

				}).done(function(data) {
					var jso = $.parseJSON( data );
					window.location = jso.url;

				});
			});
		});


jQuery(document).ready(function(e) {

	$('.deliverycountry-container-hidden-while-loading').show();

	$('#1419322828-sidebar-dropdown-deliverycountry').show();

	$('#1419322828-sidebar-dropdown-deliverycountry').val($('#1419322828-sidebar-dropdown-deliverycountry').find('option[selected]').val());

	$('#1419322828-sidebar-dropdown-deliverycountry').selectpicker();

	$('#1419322828-sidebar-dropdown-deliverycountry').change(function() {
		var uusi_dcid = $('#1419322828-sidebar-dropdown-deliverycountry').val();

		var ajaxParams = {
			changed: 'true',
			new_dcid: uusi_dcid
		};

		$.ajax({
			method: 'POST',
			url: '?changeDelivCountry=1',
			data: ajaxParams

			}).done(function(data) {
				var obj = JSON.parse(data);
				if(obj.success == true){
					$('#1419322828-sidebar-dropdown-deliverycountry').val(obj.dcid);
					if (obj.reloadpage == true)
					{
						window.location.reload();
					}
				}
			});
		});

});


