$zopim(function() {
    $zopim.livechat.button.setPosition("tr");
    $zopim.livechat.window.setPosition("tr");
    $zopim.livechat.setOnConnected(function() {
        $zopim.livechat.setOnStatus(function () {
            var department_status = $zopim.livechat.departments.getDepartment("all");
            if (department_status.status == "offline") {
                $zopim.livechat.setStatus("offline");
            } else {
                $zopim.livechat.departments.filter("");
                $zopim.livechat.departments.setVisitorDepartment("all");
            }
        });
    });
    $zopim.livechat.setLanguage("en");
});


