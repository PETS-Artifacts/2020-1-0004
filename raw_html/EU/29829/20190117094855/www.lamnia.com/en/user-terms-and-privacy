jQuery(document).ready(function() {
 // kirjautuminen
    window.lilcart_logIn = function() {
        var nimi = jQuery('#login_username').val();
        var salasana = jQuery('#login_password').val();
        if(salasana !== '' && nimi !== ''){
            var ajaxParams = {
                login: 'true',
                username: nimi,
                password: salasana
            };
            jQuery.BlockAjax.load('lilcart-block',ajaxParams, function(data){
                var obj = JSON.parse(data);
                if(obj.success == true){
                    jQuery('#login_username').val('');
                    jQuery('#login_password').val('');

                    jQuery('.mk-login-register.mk-box-to-trigger').css('display', 'none');
                    jQuery('#logged-user').html(obj.etunimi+' '+obj.sukunimi);
                    jQuery('#login-register-container').css('display', 'none');
                    jQuery('#user-info').css('display', 'block');
                    jQuery('#check-username-password').css('display', 'none');
                    window.location.reload();
                }
                else{
                    jQuery('#check-username-password').css('display', 'inline-block');
                }
            });
        }
        else{
            jQuery('#check-username-password').css('display', 'inline-block');
        }
    }

    jQuery('#login').on('click', function(){
        lilcart_logIn();
    });
    jQuery('#mk_login_form').keypress(function (e) {
        if (e.which == 13) {
            lilcart_logIn();
        }
    });

    jQuery('#user-menu-logout').on('click', function(){
        var ajaxParams = {
                logout: 'true'
        }
        jQuery.BlockAjax.load('lilcart-block',ajaxParams, function(data){
            jQuery('#logged-user').html('');
            jQuery('#login-register-container').css('display', 'block');
            jQuery('#user-info').css('display', 'none');
            location.reload();
        });
    });

    // Näytä ostoskori
    jQuery('.showcart-wrapper #showcart').on('click', function(){
        location.href = 'https://www.lamnia.com/en/cart';
    });

    // Näytä toivelista
    jQuery('.showcart-wrapper #showwishlist').on('click', function(){
        location.href = 'https://www.lamnia.com/en/wishlist';
    });

    // Näytä vertailu
    jQuery('.showcart-wrapper #showcompare').on('click', function(){
        location.href = 'https://www.lamnia.com/en/product-comparison';
    });

    // Profiilinmuokkaus
    jQuery('#user-menu-myprofile').on('click', function(){
        location.href = 'https://www.lamnia.com/en/my-profile';
    });

    jQuery('#mk-header-login-button').on('click', function(){
        jQuery('.mk-login-register').css('display', 'block');
    });

    // kirjautumisikkunan sulkeminen, jos klikataan ulkopuolella

    jQuery('html').on('click',function(event) {
        var target = jQuery(event.target);
        if(jQuery('#lilcart_login').css('display') == 'block' && jQuery(target).parents('#lilcart_login').length == 0 && jQuery(target).attr('id') != 'mk-header-login-button'){
            jQuery('#lilcart_login').css('display', 'none');
            jQuery('.ask-user-email-container').css('display', 'none');
            jQuery('.reset-password-dialog-msg').html('');
            jQuery('#reset_pw_email').val('');
        }
    });

    jQuery('#closeLilcartLogin').on('click',function() {
        if(jQuery('#lilcart_login').css('display') == 'block'){
            jQuery('#lilcart_login').css('display', 'none');
            jQuery('.ask-user-email-container').css('display', 'none');
            jQuery('.reset-password-dialog-msg').html('');
            jQuery('#reset_pw_email').val('');
        }
    });


    // forgotten password feature
    jQuery('#forgottenPassword').on('click',function(event)
    {
        event.preventDefault();

        jQuery('.reset-password-dialog-msg').html('');
        jQuery('#reset-password-button').prop('disabled', false);

        if ($('#ask-user-email').is(':visible'))
        {
            console.log('click - hide');
            jQuery('.ask-user-email-container').hide();
        }
        else
        {
            console.log('click - show');
            jQuery('.ask-user-email-container').show();
            $('#sidebar').animate({
                scrollTop: $('#reset_pw_email').position().top + 80
            },1500);
        }

    });

    jQuery('#reset_pw_email').on('input',function()
    {
        jQuery('.reset-password-dialog-msg').html('');
        jQuery('#reset-password-button').prop('disabled', false);
    });

    jQuery('#reset_pw_email').keypress(function (e)
    {
        if (e.which == 13) {
            var email = jQuery('#reset_pw_email').val();
            if (email != '' && email != null)
            {
                jQuery('#reset-password-button').prop('disabled', true);
                forgottenPassword(email);
            }
        }
    });

    jQuery('#reset-password-button').on('click',function(event)
    {
        event.preventDefault();
        var email = jQuery('#reset_pw_email').val();
        if (email != '' && email != null)
        {
            jQuery('#reset-password-button').prop('disabled', true);
            forgottenPassword(email);
        }
    });

    function forgottenPassword(email)
    {
        if(email != '' && email != null)
        {
            var ajaxParams = {
                event: 'forgottenPassword',
                email: email
            }
            jQuery.BlockAjax.load('lilcart-block',ajaxParams, function()
            {
                jQuery('.reset-password-dialog-msg').html('<h4 style="color:green;">New password sent to your e-mail!</h4>');
                jQuery('#reset_pw_email').val('');
            });
        }
    }


});

$( document ).ready(function() {

	$('.sidebar-category-container').on('click','.main-level',function(event){

		if( $(window).width() <= 768 ){

			//mobiili, avaa puu
			event.preventDefault();

			var e = $(this);
			var next = e.next();

			if( next.hasClass('open') ){
				next.addClass('hidden');
				next.removeClass('open');
			}else{
				$('.sivu-menu-lista .open').addClass('hidden');
				$('.sivu-menu-lista .open').removeClass('open');
				next.addClass('open');
				next.removeClass('hidden');
			}
			return false;
		}else{
			//isompi kun mobiili, avaa linkki
			return true;
		}
	});
});

		window.ratingwidget_ToggleTrustPilot = function()
		{
			var currentwidth = window.innerWidth;

			if ( $('#ratingwidget-area-trustpilot').length > 0 )
			{
				if ( ( currentwidth >= 768 ) && ( currentwidth <= 991 ) )
				{
					$('#ratingwidget-area-trustpilot').hide();
				} else {
					$('#ratingwidget-area-trustpilot').show();
				}
			}
			return true;
		}

		$(document).ready(function()
		{
			ratingwidget_ToggleTrustPilot();

			$(window).on('resize',function()
			{
				ratingwidget_ToggleTrustPilot();
			});
		});
	
             jQuery(".respo-goto-cart").on("click", function()
             {
              location.href = "https://www.lamnia.com/en/cart";
             });
             jQuery(".respo-goto-wishlist").on("click", function()
             {
              location.href = "https://www.lamnia.com/en/wishlist";
             });
             jQuery(".respo-goto-compare").on("click", function()
             {
              location.href = "https://www.lamnia.com/en/product-comparison";
             });
            
                $("#respo-home-icon").on("click", function()
                {
                    window.location.href = "https://www.lamnia.com/en";
                });

                $("#responsive-cover-toggle").on("click", function(){
                    if($("#sidebar").hasClass("responsive-fixed")){
                        $("#sidebar").removeClass("responsive-fixed");
                    }
                    if($(".row.row-offcanvas.row-offcanvas-left.active")){
                        $(".row.row-offcanvas.row-offcanvas-left.active").removeClass("active");
                    }
                });
                $("#responsive-menu-toggle").on("click", function()
                {
                    $("#sidebar").toggleClass("responsive-fixed");

                    if($(".navbar-collapse.collapse.in"))
                    {
                        $(".navbar-collapse.collapse.in").removeClass("in");
                    }
                    if(!$("#responsive-cover-toggle").hasClass("collapsed"))
                    {
                        $("#responsive-cover-toggle").addClass("collapsed");
                    }
                });

            
$(document).ready( function()
{
	$('#searchbox').on('keyup',function(e){
		if(e.keyCode == 13) {
			e.preventDefault();
			var terms = $(this).val().split(',');
			for (index = 0; index < terms.length; ++index) {
				terms[index] = encodeURIComponent(terms[index].trim()).replace('%20','+');
			}
			terms = terms.join();
			window.location = $(this).parent().data('url')+'?s='+terms;
		}
	});
	$('#sidebar-search-submit').on('click',function(e){
		e.preventDefault();
		var terms = $('#searchbox').val().split(',');
		for (index = 0; index < terms.length; ++index) {
				terms[index] = encodeURIComponent(terms[index].trim()).replace('%20','+');
		}
		terms = terms.join();
		window.location = $(this).parent().data('url')+'?s='+terms;
	});
	if ($(window).width() > 1024) 
	{
		$('.lamnia_search_autocomplete').autocomplete({
			source: '?autocompletesearch=1',
			minLength: 3,
			autofocus: true,
			focus: function( event, ui ) 
			{
				event.preventDefault();
			},
			select: function( event, ui )
			{
				window.location = ui.item.url;
			},
			delay: 300,
			open : function()
			{
				$(this).data('uiAutocomplete').menu.element.addClass('sidebar-autocomplete-menu');
			}
		});
	}
});

