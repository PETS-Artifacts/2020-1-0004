    $('body').on('click','.cartaction-addtocart', function(event){
        event.preventDefault();

        var reload = false;
        if($(this).data('reload')){
            reload = true;
        }

        ajaxCartActions('add', $(this).data('prodid'), 1, false, reload);
        return false;
    });
