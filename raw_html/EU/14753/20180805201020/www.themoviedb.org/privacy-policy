  function scrollToDocument(id) {
    $('html, body').animate({ scrollTop: ($('#' + id).first().offset().top - 120) }, 1);
  }

  if (window.location.hash) {
    scrollToDocument(window.location.hash.slice(1))
  }

  $(document).ready(function() {

    $('ul.privacy_contents a').on('click', function(e) {
      e.preventDefault();
      scrollToDocument($(this).attr('data-id'))
    });

  });

</script>
