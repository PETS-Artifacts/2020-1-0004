	<script type="text/javascript">
		
										
				
			//$(".chosenselect").chosen({disable_search:true});
			$(document).ready(function(){
				var cookieview = $.cookie('ciview');				
				if(cookieview == 1){
					$('#cookie-notice').hide();
				};
				$('#cn-accept-cookie').click(function(){					
					$.cookie('ciview', 1, {expires: 30,path: '/'});
					$('#cookie-notice').hide();
				});
				
				$('input[placeholder], textarea[placeholder]').placeholderEnhanced();
				$(".showtiptip").tipTip({defaultPosition:'top',fadeIn:50,delay:50,attribute: 'alt'});
				//$(".selectize").selectize();
				$(".chosen-select").chosen();
				
				$('.datepicker').each(function(){
					var defaultDate = null;
					if ($(this).data('default-date')!=undefined)
						defaultDate = $(this).data('default-date');
					
					var minDate = null;
					if ($(this).data('min-date')!=undefined)
						minDate = $(this).data('min-date');
					
					if ($(this).attr('data-nice-format')=='true')
						$(this).datepicker({ dateFormat: "d M yy",defaultDate:defaultDate,minDate:minDate,changeYear: true,changeMonth: true });
					else
						$(this).datepicker({ dateFormat: "yy-mm-dd",defaultDate:defaultDate,minDate:minDate,changeYear: true,changeMonth: true });
				});
				$(".chosenselect").each(function(){
					$(this).wrap('<span class="selectbox"></span>');
				});
				
				$('.joinbutton').click(function(e){
					e.preventDefault();
					$('#modalwindow .boxcontents').html('<div class="loading"><img src="/img/loading.gif"/> Loading, please wait...</div>');
					showBox('#modalwindow',100,null,800);
					$('#modalwindow .boxcontents').load('/my-wedding/register/modal?redirect='+encodeURI(document.location.href));
				});
				
				                
                
                /*
                 The objective of below code is not to show the popupregistration modalbox
                 for the urls which is listed in the popupblocksurlparams. Other than that, 
                 the popup registration modalbox should display.
                 */
                var urlparams = window.location.href.split("/");
                var popupblocksurlparams = ['supplierhq','my-wedding','suppliers','venues','service-finder','venue-finder','venue-finder-lp2','offers','competitions'];
                var showpopupregistration = popupblocksurlparams.indexOf(urlparams[3]);                    
                    
				// create cookie
				var wolPV = $.cookie('wolPV',Number)!=undefined?$.cookie('wolPV',Number):0;
                //showpopupregistration flag indicates that, the current url is not in the popupblocksurlparams list
				if (window.location.hash=='' && !$.cookie('wolShowRegiModal',Boolean) && (wolPV >= 3) && (showpopupregistration == -1)) {                    
					setTimeout(function () {
						$('#modalwindow .boxcontents').load('/my-wedding/register/modal?shownothanks=1&redirect='+encodeURI(document.location.href),function(){
							showBox('#modalwindow',100,null,800);
						});
						$.cookie('wolShowRegiModal', false, {expires: 2,path: '/'});
					}, 10);
				} else {
					wolPV++;
					$.cookie('wolPV', wolPV, {expires: 1,path: '/'});
				}
							});
			
			//Commented this code due to new analytics code
			/*
			var _gaq = _gaq || [];
			var pluginUrl = '//www.google-analytics.com/plugins/ga/inpage_linkid.js';
			_gaq.push(['_require', 'inpage_linkid', pluginUrl]);
			_gaq.push(['_setAccount', 'UA-1302361-1']);
						_gaq.push(['_trackPageview']);


			(function() {
			var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
			ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
			var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
			})();
			*/
			//new analytics code
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
			m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
			})(window,document,'script','//www.google-analytics.com/analytics.js','ga');

			ga('create', 'UA-1302361-1', 'weddingsonline.ie');
			ga('send', 'pageview');			

