		<script data-cfasync="false" src="/cdn-cgi/scripts/5c5dd728/cloudflare-static/email-decode.min.js"></script><script>
		$(document).ready(function(){
			var cookieview = $.cookie('ciview');				
			if(cookieview == 1){
				$('#cookie-notice').hide();
			};
			$('#cn-accept-cookie').click(function(){					
				$.cookie('ciview', 1, { expires: 30,path: '/' } );
				$('#cookie-notice').hide();
			});
		});

		$('.datepicker').each(function(){
			var defaultDate = null;
			if ($(this).data('default-date')!=undefined)
				defaultDate = $(this).data('default-date');
			
			var minDate = null;
			if ($(this).data('min-date')!=undefined)
				minDate = $(this).data('min-date');
			
			if ($(this).attr('data-nice-format')=='true')
				$(this).datepicker({ dateFormat: "d M yy",defaultDate:defaultDate,minDate:minDate,changeYear: true,changeMonth: true });
			else
				$(this).datepicker({ dateFormat: "yy-mm-dd",defaultDate:defaultDate,minDate:minDate,changeYear: true,changeMonth: true });
		});

		$('.join-button').click(function(e){
			e.preventDefault();
			$('#modalwindow .modalcontents').load('/my-wedding/register/modal?redirect='+encodeURI(document.location.href),function(){
				showModal('#modalwindow');
			});
		});
		$('.login-button').click(function(e){
			e.preventDefault();
			$('#modalwindow .modalcontents').load('/new/ajax/_loginform.php?redirect='+encodeURI(document.location.href),function(){
				showModal('#modalwindow');
			});
		});

		$('.newsletterformsubmit').click(function(){
			if ($(this).hasClass('disabled')) return false;
			var formCheck = true;
			var thisButton = $(this);
			var currentText = thisButton.text();
			var thisForm = $(this).closest('.newsletter-form');
			thisForm.find('.required').each(function(){
				if (!$(this).errorCheck())
					formCheck = false;
			});

			if (formCheck)
			{
				thisButton.text('Please wait...').addClass('disabled');
				$.post('/new/ajax/_newsletter.php',thisForm.serialize(),function(data){
					var result = data.split('|');
					if (result[0]=='true')
						thisForm.html('<p>Thank you, please check your email for a confirmation email.</p>');
					else
						thisForm.html('<p>There was an error while subscribing you to our newsletter: '+result[1]+'</p>');

					thisButton.text(currentText).removeClass('disabled');
				});
			}
		});

		
	</script>
	
