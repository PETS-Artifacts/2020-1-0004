  <script async="async" src="//consent.truste.com/notice?domain=familysearch.org&c=teconsent&js=bb&noticeType=bb&text=true&oc=1&fade=30000&language=false" crossorigin=""></script>



    


  
  

    


    <script>
    (function() {
      function getSegmentData(callback) {
        var url = '/home/bs/users/' + FS.User.getEffectiveId() + '/segment';
        var headers = [['Accept', 'application/json'], ['Authorization', 'Bearer ' + FS.Cookie.getCookie('fssessionid')]];

        FS.xhr("GET", url, headers, null, function(res) {
          callback(res);
        });
      }

      function handleSegmentData(res) {
        if (res.status === 200) {
          var segmentData = FS.JSON.parse(res.response);
          dtmData.visitor.treeLevel = segmentData.treeLevel;
          dtmData.visitor.userLevel = segmentData.userLevel;
        }
        else {
          console.log('error getting segment data', res);
        }
        trackPageView();
      }

      function trackPageView() {
        FS.Analytics._hfSetupDone();
        
          /* After setting up all the dtmData attributes, send data to DTM */
          FS.Analytics.trackPageView('page_view', dtmData);
        
      }

      window.dtmData = {
        site: {
          id: 'FamilySearch',
          type: 'adapative',
          language: "en"
        },
        page: {
          channel: "Home",
          subChannel: "",
          name: "Privacy Policy"
        }
      };

      dtmData.page.channelLong = dtmData.site.id + ": " + dtmData.page.channel + ": " + ((dtmData.page.subChannel) ? + dtmData.page.subChannel + " " : "");
      dtmData.page.nameLong = dtmData.page.channelLong + dtmData.page.name;

      
      
        trackPageView();
      

    })();
    </script>

  


  
  


  
  
  


  

  
