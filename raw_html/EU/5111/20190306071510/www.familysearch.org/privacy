  <script>
    FS.loadHTML = function(path, attribute) {
      var link = document.createElement('link');
      link.setAttribute('href', path);
      link.setAttribute('rel', "import")
      // set up loadHTML to handle optional object-key attribute type to match the asset-array method
      if (attribute) {
        link.setAttribute(attribute, 'true');
      }
      document.getElementsByTagName('head')[0].appendChild(link);
    }

    document.addEventListener('JSLoaded', function() {
      
    });
  </script>


    <script type="text/javascript">

  /* Translations */
  

  

  var walkme_get_language = function(){
    var locale = '';
    if(FS && FS.simpleLocale){
      locale = FS.simpleLocale();
    }
    if(locale === 'en'){
      locale = '';
    }
    return locale;
  }

  

  
</script>

