  FS.File = FS.File || {};
  FS.File.downloadHistory = {}; // Obj to store download history, keeps files from being re-downloaded
  FS.File.loadCSS = function(path, media) {
    if (path) {
      if (FS.File.downloadHistory[path]) {
        // already loaded this resource
        FS.File.downloadHistory[path]++;
        return true;
      }
      FS.File.downloadHistory[path] = 1;
      var fileref = document.createElement('link');
      fileref.setAttribute('rel', 'stylesheet');
      fileref.setAttribute('type', 'text/css');
      fileref.setAttribute('href', path);
      // set optional attrs
      if (media) {
        fileref.setAttribute('media', media);
      }
      document.getElementsByTagName('head')[0].appendChild(fileref);
    }
    return false;
  }
  FS.File.resolvePath = function(path) {
    // http://stackoverflow.com/questions/190852/how-can-i-get-file-extensions-with-javascript/1203361#1203361
    var ext = path.substr((~-path.lastIndexOf('.') >>> 0) + 2);
    var filePath = path;

    // the client manifest does not add the leading slash
    if (filePath[0] === '/') {
      filePath = filePath.substring(1);
    }

    // js files do not have an extension in the manifest due to Inject
    if (ext === 'js') {
      filePath = filePath.slice(0, -3);
    }

    if (window.manifest[filePath]) {
      return window.manifest[filePath];
    }

    return path;
  };
  
  
