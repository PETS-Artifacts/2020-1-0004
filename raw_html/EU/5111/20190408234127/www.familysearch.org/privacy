<script type="text/javascript">
  var walkme_get_language = function(){
    var locale = '';
    if(FS && FS.simpleLocale){
      locale = FS.simpleLocale();
    }
    if(locale === 'en'){
      locale = '';
    }
    return locale;
  }
</script>



    


  
