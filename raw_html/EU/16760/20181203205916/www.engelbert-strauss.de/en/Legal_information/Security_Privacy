        if (navigator.userAgent.indexOf('KioWareKioskDevice1080p') > -1 || document.cookie.indexOf('acceptedCookieNotice=true') > -1) return;

        var cookieBar = document.createElement('div');
        cookieBar.setAttribute('style', 'position: fixed; background: rgba(0,0,0,0.75); width: 1000px; font-size: 13px; color: #fff; bottom: 0; right: 0; left: 0; margin-right: auto; margin-left: auto; padding: 20px 60px 20px 20px; font-family: Verdana,Arial,Helvetica,sans-serif; z-index: 1100; ');

        var spanHeadline = document.createElement('span');
        spanHeadline.setAttribute('style', 'font-weight: bold;display: block;margin-bottom: 10px;');
        spanHeadline.insertAdjacentHTML('afterbegin', 'Information about cookies');

        var spanText = document.createElement('span');
        spanText.insertAdjacentHTML('afterbegin', 'engelbert strauss wants to make your online shopping experience as pleasant as possible. To this end, we save information about your visit in so-called cookies. If you want to continue using our webshop, please agree to the use of cookies.<br/><a target="_blank" style="color: #fff; text-decoration: underline;" href="https://www.engelbert-strauss.de/en/Legal_information/Security_Privacy">You will find more information about the cookies here. &gt;&gt;</a>');

        var spanConfirm = document.createElement('span');
        spanConfirm.setAttribute('class', 'cookiebar-confirm-button');
        spanConfirm.setAttribute('style', 'position: absolute;right: 15px;top: 50%;transform: translateY(-50%);');
        spanConfirm.insertAdjacentHTML('afterbegin', '<svg style="cursor: pointer; fill: #fff; width: 20px; height: 20px;" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" x="0px" y="0px" viewBox="0 0 50.8 50.5" style="enable-background:new 0 0 50.8 50.5;" xml:space="preserve"><polygon points="50.9,1.8 49,0.1 25.4,23.5 1.9,0 0,1.9 23.6,25.3 0.1,48.7 1.9,50.5 25.4,27.1 48.9,50.5 50.8,48.6 27.3,25.3 "/></svg>');

        cookieBar.appendChild(spanHeadline);
        cookieBar.appendChild(spanText);
        cookieBar.appendChild(spanConfirm);
        spanConfirm.addEventListener('click', function() {
            document.cookie = 'acceptedCookieNotice=true; domain=.engelbert-strauss.de; path=/; max-age=' + 60 * 60 * 24 * 365 * 10 + '; expires=Sun, 31 Dec 2028 23:59:59 GMT;';
            document.body.removeChild(cookieBar);
        });

        document.body.appendChild(cookieBar);
    }

    addCookieBar();
</script>


        </div>
        
        
