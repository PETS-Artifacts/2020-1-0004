  <script type="text/javascript">
    $(function() {
      var nav = new MobileNav();
      nav.initialise();

      // ***************************************
      // Site Search
      // ***************************************
      $('.site-search-toggle').click(function(event) {
        event.preventDefault();

        var navMain = $(this).closest('.nav-main');
        navMain.toggleClass('search-visible');

        if (navMain.hasClass('search-visible')) {
          $('.site-search-query').focus();
        }
      });
    });
  </script>
  <script type="text/javascript">
    $(function() {
      var topMenu = new ScrollableMenu($("#global-product-nav .top"), "horizontal");
      topMenu.scrollToActiveItem();

      var subMenu = new ScrollableMenu($("#global-product-nav .sub"), "horizontal");
      subMenu.scrollToActiveItem();
    });
  </script>
