		$(document).ready(function () {
            $('.info-tip').tooltipster({ animation: 'fade',
                delay: 200,
                theme: 'tooltipster-default',
                touchDevices: false,
                trigger: 'hover'});
            $('.tool-tip').tooltipster({ animation: 'fade',
                delay: 200,
                theme: 'tooltipster-default',
                touchDevices: false,
                trigger: 'hover'});
        });
        
        function scrollToId(id){
            $("html, body").animate({ scrollTop: $(id).offset().top }, 1000);
        }
        
        function showhelp(id) {
            id = '#' + id;
            $(id).show();
            scrollToId(id);
            return false;
        }
    </script>

    
    <script>
    $(function() {
    $('#scroll_to_bottom').click( function ()
        {
            $('html, body').animate({
                scrollTop: $("#accept").offset().top
                }, 2000);
            });
    });
</script></head>

