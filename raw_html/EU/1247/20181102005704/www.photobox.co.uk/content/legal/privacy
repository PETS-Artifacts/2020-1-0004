var dataLayer = [];

(function() {
  'use strict';
  var masonTemp;
  var pageName = "/content/legal/privacy" === '/studio' ? window.location.pathname : "/content/legal/privacy";

  //To build the dataLayer with good var
  dataLayer = [{
    // All pages
    'uac' : 'null' == 'null' ? null : "null",
    'session_status' : "session_defined",
    'page_name' : pageName,
    'page_category' : 'Generic',
    'country_code' :  "GB",
    'ua' : "UA-48172145-1",
    'current_domain' : window.location.host,
    'Cookie_Timeout' : "2592000000",
    'channel_id' : "1002",
    'member_info' : getCookie('pbx-member') || 'pbx-not-member',

    // Eulerian Infos
    'ea_from' : "photobox-uk",
    'ea_uri' : "pbox.photobox.co.uk"
  }];

  var _dlv = dataLayer[0];

  //To know if we are in PRODUCT or RANGE page and add the product category in this case
  masonTemp = "false";

  if (masonTemp === 'true') {
    _dlv.dataLayer_contains_Product_Page_Block = true;
    _dlv.product_category = "";
    _dlv.product_price = "";
    _dlv.product_id = "";
    _dlv.product_name = "";

    //To know if we are in PRODUCT page and add product Id / Product cat / Product amout
    masonTemp = "false";
    if (masonTemp === 'true') {
      _dlv.ecommerce = {
        'products': createArray([])
      };
    }
  } else {
    _dlv.dataLayer_contains_Product_Page_Block = false;
  }

  //To know if we are in error page
  //Equal to false on all pages except 404 and 500 error pages. When equal to true, the keys 'error_code' and 'error_page_path' have to be indicated.
  //error_code: Has to be implemented on all pages (ErrorDocument) that handle 404 and 500 errors.
  //error_page_path: Has to indicate the URI of the referral page that precede the 404 or 500 error.
  masonTemp = "false";

  if (masonTemp === 'true') {
    _dlv.dataLayer_contains_Error_Block = true;
    _dlv.error_code = "false";
    _dlv.error_page_path = window.location.href;
  } else {
    _dlv.dataLayer_contains_Error_Block = false;
  }

  //dataLayer_contains_User_Info_Block: Equal to true if the user is connected. otherwise false. When equal to true, the keys 'uid', 'profile' have to be indicated.
  //Profile : The values can be: 'New Member', 'New Customer', 'Old Customer'. * This nomenclature has to be confirmed with the BI Team.
  //uid : ID of the User.
  masonTemp = "false";

  if (masonTemp === 'true') {
    _dlv.dataLayer_contains_User_Info_Block = true;
    _dlv.uid = "";
    _dlv.profile = "";

    // Eulerian Infos
    _dlv.email = "";
   } else {
    _dlv.dataLayer_contains_User_Info_Block = false;
   }

  //To know if we are in upsell page and add the upsell product category in this case
  masonTemp = "false";

  if (masonTemp === 'true') {
    _dlv.dataLayer_contains_Upsell_Info_Block = true;
    _dlv.upsell_product_category = "" === "null" ? null : "";
    _dlv.upsell_product_name = "" === "null" ? null : "";
    _dlv.upsell_product_id = "" === "null" ? null : "";
    _dlv.upsell_product_upsell = ""== "null" ? null : "";
  } else {
    _dlv.dataLayer_contains_Upsell_Info_Block = false;
  }

  //To know if we are in landing page and add the landing product category in this case
  masonTemp = "false";
  if (masonTemp === 'true') {
    dataLayer[0].landing_product_id = "";
  }

  //The transaaction is here to get the context.
  //Equal to true on the confirmation pages, false elsewhere.
  //If true, the keys 'transaction_data', 'order_amount_ati ', 'order_amount_tf ', 'order_tax_amount ',  'order_shipping_cost_ati', 'order_shipping_cost_tf', 'order_shipping_tax_amount', 'order_promo_ati', 'order_promo_tf' have to be indicated.
  varMasonTemp = "false";

  if (varMasonTemp == 'true') {
    dataLayer[0].dataLayer_contains_Transaction_Block = true;
    dataLayer[0].order_amount_tf = "";
    dataLayer[0].order_amount_ati = "";
    dataLayer[0].order_tax_amount = "";
    dataLayer[0].order_shipping_cost_tf = "";
    dataLayer[0].order_shipping_cost_ati = "";
    dataLayer[0].order_shipping_tax_amount = '';
    dataLayer[0].order_promo_ati = "";
    dataLayer[0].order_promo_tf = "";
    dataLayer[0].estimated_despatch  = "";
    dataLayer[0].estimated_arrival  = "";

    // Universal Analytics Order
    dataLayer[0].Transaction_Data = transactionData();

    // Eulerian Infos
    dataLayer[0].ea_order_ref = "";
    dataLayer[0].ea_promo_code = "";
    dataLayer[0].ea_currency = "GBP";
    dataLayer[0].ea_payment = "";
    dataLayer[0].ea_sales_affiliation  = "";
    dataLayer[0].ea_commission = "";
    dataLayer[0].ea_post_code = "";
  }
  else {
    dataLayer[0].dataLayer_contains_Transaction_Block = false;
  }

  var hpDesk = "/";
  var hpMsite = "/msite/homepage";

  if (location.pathname === hpDesk || location.pathname === hpMsite) {
    dataLayer[0].platform = 'insurance';
  }

  //To know if we are in basket
  var varMasonTemp = "false";
  if (varMasonTemp == 'true') {
    dataLayer[0].dataLayer_contains_Basket_Block = true;
    dataLayer[0].ea_product_basket = productBasket();
    dataLayer[0].ea_amout_basket = "0.00";
    dataLayer[0].ea_currency = "GBP";
  }
  else {
    dataLayer[0].dataLayer_contains_Basket_Block = false;
  }

  //To build transaction Data
  function transactionData() {
    var transaction_id, product_id, product_name, product_sku, product_category, product_price, product_quantity, product_currency,
    result;

    transaction_id = "";
    product_id  = "".split(";");
    product_name = "".split(";");
    //product_sku = "".split(";");
    product_category = "".split(";");
    product_price = "".split(";");
    product_quantity = "".split(";");
    product_currency = "";
    result = [
      // Descriptif du [<Trans>]
      [
      transaction_id,
      "",
      "",
      //"",
      "",
      "",
      product_currency
      ]
    ];

    for (var i=0; i< product_id.length; i++){
      result.push(
        [
          transaction_id,
          decodeURIComponent(product_name[i]),
          product_id[i],
          decodeURIComponent(product_category[i]),
          product_price[i],
          product_quantity[i],
          product_currency
        ]
      );
    }
    return result;
  }

  function productBasket() {
     var arrayPush, arrayproductIds, arrayProductName, arrayProductPrice, arrayProductQuantity, arrayProductCat, arrayProductType;

     arrayproductIds = "".split(";");
     arrayProductName = "".split(";");
     arrayProductType = "".split(";");
     arrayProductCat = "".split(";");
     arrayProductPrice = "".split(";");
     arrayProductQuantity = "".split(";");
     arrayPush = [];

       for (var i=0; i < arrayproductIds.length; i++){
          arrayPush.push({
            'prdref' : arrayproductIds[i],
            'prdname' : decodeURIComponent(arrayProductName[i]),
            'prdamount' : arrayProductPrice[i],
            'prdquantity' : arrayProductQuantity[i],
            'prdgroup' : decodeURIComponent(arrayProductCat[i])
            //'prdgroup' : arrayProductType[i]
         });
        }

    return arrayPush;
  }
  /**
   * Transform a product string to an array
   * @param {string} products the string of the products create in the perl section.
   * @return {array}
   */
  function createArray(products) {
    var count = 0;
    var newArray = [];
    if (products && typeof products === "object") {
      for (var i = 0; i < products.length; i++) {
        var newObj = {};
        for (var key in products[i]) {
          products[i].hasOwnProperty(key) && products[i][key] !== '' && (newObj[key] = products[i][key], count++);
          //Price should be a string and not a number
          if (key === 'product_price' && newObj[key] === 0) {newObj[key] = '0.00';}
        }
        newArray.push(newObj)
      }
    }
    return count ? newArray : undefined;
  };
  function getCookie(cname) {
      var name = cname + "=";
      var ca = document.cookie.split(';');
      for(var i=0; i<ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0)==' ') c = c.substring(1);
          if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
      }
      return "";
  }
})();

function getGA() {
   var tid = "null";
   if (tid == "null")
     tid = null;
   return tid;
}
