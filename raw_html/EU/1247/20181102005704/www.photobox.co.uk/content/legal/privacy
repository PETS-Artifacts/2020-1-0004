(function() {
/*<!--*/
    var dataPath = "/content/legal/privacy";
    var serviceUrl = "www.photobox.co.uk";
    var marketingGroup = "photobox.co.uk";
    var channel_id = "1002";
    var language = "en";
    var maturity_id = "1";
/*-->*/

    var arrayLocale = marketingGroup.split('.');
    var locale;
//<![CDATA[
    for(i=0; i < arrayLocale.length; i++){
//]]>
        if((arrayLocale[i] != language) && !arrayLocale[i].match(/photobox/i)) {
            locale = arrayLocale[i];
        }
    }

    var html_node = document.getElementsByTagName("html")[0];
    html_node.setAttribute('data-path', dataPath);
    html_node.setAttribute('data-service-url', serviceUrl);
    html_node.setAttribute('data-marketing-group', marketingGroup);
    html_node.setAttribute('data-maturity', maturity_id);
    html_node.setAttribute('data-channel', channel_id);
    html_node.className += " " + language + " " + locale;
})();</script>

