    <script type="text/javascript">
      $(function(){
            var optCookieInfo = cookieF.get("optcookieinfo"),
            cookieText = "This site uses cookies. Continuing to use the site without changing your cookie settings means that you consent to those cookies",
            cookieTextBtn = "Okay, got it",
            cookieTextMore = "Find out about the <a href=\"/content/legal/privacy?cid=cookieprivacy#cookies-policy\">cookies</a>",
            cookieTextMoreShort = "Learn more",
            cookieLinkMore = "/content/legal/privacy?cid=cookieprivacy#cookies-policy",
            cookieLinkMoreMobile = "/msite/content/legal/terms-of-use?cid=cookieprivacy#privacy",
            cookieTextChange = "How to change your <a href=\"/content/legal/privacy?cid=cookiesettings#cookies-settings\">settings</a>";

            if (optCookieInfo != "true") {

                

                

                
                    if (window.location.pathname == "/") {
                        $('#pbx_frontpage_footer').append('<div id="cookies_banner"></div>');
                    }

                    
                    
                        else {
                            $('#pbx_footer_wrapper').append('<div id="cookies_banner" class="pbx_absolute"></div>');
                        }
                    
                 
                

                $('#cookies_banner')
                .html('<div class="cookies_wrapper"><p>'+cookieText+'</p><div class="cookies_info"><p>'+cookieTextMore+'&nbsp;|&nbsp;</p><p>'+ cookieTextChange+'</p>'+'</div><div class="pbx_spaccer"></div><a href="'+cookieLinkMore+'" class="cookies_info_mobile">'+cookieTextMoreShort+'</a><a href="#" class="cookies_btn">'+cookieTextBtn+'</a></div>')

                $('#cookies_banner_mobile')
                .html('<div class="cookies_wrapper"><p>'+cookieText+'</p><div class="cookies_info"><p>'+cookieTextMore+'&nbsp;|&nbsp;</p><p>'+ cookieTextChange+'</p>'+'</div><div class="pbx_spaccer"></div><a href="'+cookieLinkMoreMobile+'" class="cookies_info_mobile">'+cookieTextMoreShort+'</a><a href="#" class="cookies_btn">'+cookieTextBtn+'</a></div>')
                 


                $('.cookies_btn').bind('click', function(event) {
                    event.preventDefault();
                    $('#cookies_banner').fadeOut('400', function() {
                        cookieF.set("optcookieinfo", "true", 365*2);
                    });
                    $('#cookies_banner_mobile').fadeOut('400', function() {
                        cookieF.set("optcookieinfo", "true", 365*2);
                    });
                });
            };
        });
