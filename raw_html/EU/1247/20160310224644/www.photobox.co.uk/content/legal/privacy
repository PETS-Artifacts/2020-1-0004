        /* FUNCTIONS FOR MSITE URLS */
        function checkAlternateLinks(sAttribute, sValue){
          // Search for "alternate" link (media=handheld) to get the msiteUrl
          var headLinks = document.getElementsByTagName('head')[0].getElementsByTagName('link');
          for(i = 0; i < headLinks.length; i++){
              if(headLinks[i].getAttribute(sAttribute) == sValue){
                // targetPath = headLinks[i].getAttribute('href').replace('http://'+window.location.host, '');

                // Convert absolute paths to rel
              targetPath = '/'+ headLinks[i].getAttribute('href').replace(/^(?:\/\/|[^\/]+)*\//, "");
              }
          }
        }

        function checkQueryString(query){
          // Seach for "msitepage" parameter to get msiteUrl
          var msitePage = getQuerystring(query);

          if(window.location.pathname == (msitePage || '/'+msitePage) ){

          }else{
            if(msitePage.length > 0){
              targetPath = msitePage;

              if(targetPath.charAt(0) != '/'){
                targetPath = '/'+targetPath;
              }
            }
          }
          
        }

        function msiteRedirections(targetPath, urlParams){

          msiteUrl = targetPath + urlParams;

          var locationUrl   = window.location.pathname;
          var mobPopup    = getQuerystring('mobpopup');

          if(!mobPopup || mobPopup == ""){
            mobPopup = getQuerystring('mob_popup');
          }
                
          // if mobPopup url parameter is set to "yes", redirect to msite
          if (mobPopup == 'yes') {
              window.location.href = msiteUrl;
          }
          
          // if set to "no", set cookie to prevent redirections
          else if (mobPopup == 'no') {        
            function doNothing(){return false;}
            sCookieDomain = window.location.host.replace('www','');

            document.cookie = 'layout-preferences=classic;expires=0;path=/;domain='+sCookieDomain;
            doNothing();
          }

          
          else if (mobPopup == 0 || sitePrefNo == null) {
            if( mobDetect.phone()) { 


              //To know where customer from and To put a link in the footer after (except if he come from mobile
              if ((locationUrl.indexOf("mobile") == -1) || (locationUrl.indexOf("msite") == -1)){
                  setCookie('whereDesktop', locationUrl);
              }

              // get the cookie value of one we set on the landing page
              // layout-preferences = 'classic' : desktop page
              var sitePref = GetCookie("layout-preferences");
              var cookieNoRedirectApp = GetCookie("mobileNoRedirect");
              
              if(sitePref == null &&  locationUrl == "/" && cookieNoRedirectApp !== null ||  sitePref == null &&  locationUrl == "/home-v3" && cookieNoRedirectApp !== null ){
                  window.location.href = msiteUrl;
              }
              else if (sitePref == "mobile" && cookieNoRedirectApp !==  null) {
                  window.location.href = msiteUrl;
              }  

              else if (sitePref == null){
                if(mobDetect.os() == 'iOS'){
                  /*window.location.href = "http://www.photobox.fr/mobile/splash-app";*/
                  window.location.href = msiteUrl;
                }
                if (mobDetect.os() != 'iOS') {
                  /*window.location.href = "http://www.photobox.fr/mobile/android-app";   */  
                  window.location.href = msiteUrl;
                }
              }
            }
          }
        }

        // msiteUrl from "alternate" link (media=handheld)
        checkAlternateLinks('media', 'handheld');

        // msiteUrl from url parameter
        checkQueryString('msitepage');

        // Launch redirects
        if(!mSiteChannelParam){
          var mSiteChannelParam = '';
        }
        msiteRedirections(targetPath, urlParams);
      }
      //-->
    </script>
  






