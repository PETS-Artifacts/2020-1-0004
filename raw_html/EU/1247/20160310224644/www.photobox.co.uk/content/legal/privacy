<script type="text/javascript"><!--
(function($){$.fn.hoverIntent=function(handlerIn,handlerOut,selector){var cfg={interval:100,sensitivity:6,timeout:0};if(typeof handlerIn==="object"){cfg=$.extend(cfg,handlerIn)}else{if($.isFunction(handlerOut)){cfg=$.extend(cfg,{over:handlerIn,out:handlerOut,selector:selector})}else{cfg=$.extend(cfg,{over:handlerIn,out:handlerIn,selector:handlerOut})}}var cX,cY,pX,pY;var track=function(ev){cX=ev.pageX;cY=ev.pageY};var compare=function(ev,ob){ob.hoverIntent_t=clearTimeout(ob.hoverIntent_t);if(Math.sqrt((pX-cX)*(pX-cX)+(pY-cY)*(pY-cY))<cfg.sensitivity){$(ob).off("mousemove.hoverIntent",track);ob.hoverIntent_s=true;return cfg.over.apply(ob,[ev])}else{pX=cX;pY=cY;ob.hoverIntent_t=setTimeout(function(){compare(ev,ob)},cfg.interval)}};var delay=function(ev,ob){ob.hoverIntent_t=clearTimeout(ob.hoverIntent_t);ob.hoverIntent_s=false;return cfg.out.apply(ob,[ev])};var handleHover=function(e){var ev=$.extend({},e);var ob=this;if(ob.hoverIntent_t){ob.hoverIntent_t=clearTimeout(ob.hoverIntent_t)}if(e.type==="mouseenter"){pX=ev.pageX;pY=ev.pageY;$(ob).on("mousemove.hoverIntent",track);if(!ob.hoverIntent_s){ob.hoverIntent_t=setTimeout(function(){compare(ev,ob)},cfg.interval)}}else{$(ob).off("mousemove.hoverIntent",track);if(ob.hoverIntent_s){ob.hoverIntent_t=setTimeout(function(){delay(ev,ob)},cfg.timeout)}}};return this.on({"mouseenter.hoverIntent":handleHover,"mouseleave.hoverIntent":handleHover},cfg.selector)}})(jQuery);


$(document).ready(function(){

var iFadeSpeed      = 250;  //= 150;
var iTimeoutSpeed   = 250;  //= 250;
var bDisableClose   = 0;

var aArrayCategories = new Array();

$('.rebrand-menu > li').each(function(){
    aArrayCategories.push($(this).attr('id'));
});

if(mobDetect.tablet()){
    iFadeSpeed      = 50;
    iTimeoutSpeed   = 50;
}

$('.rebrand-menu .subHolder a').each(function(){
    
    if($(this).attr('title')){
        cleanTitle = $(this).attr('title').replace('&amp;', '&');
        $(this).attr('title', cleanTitle);
    }
});

/*Dropdown icon menu*/
function toggleDropdown(oEl, sTarget, sToggle, sFilter){
    /*if no sToggle when launching the function,
    the function sets it here.
    Necessary for the basket icon : the dropdown must not appear all the time,
    only when the number of items in the basket is not at 0 */

    if(!sToggle){
        if(oEl.hasClass('hovered')){
            sToggle = 'close';
        }else{
            sToggle = 'open';
        }
    }

    switch(sToggle){
        case 'open':

/*            if( !((oEl.find('.toggle-deploy').attr('id') == 'basket-icon') && ( (parseInt($('#pbxBubbleBasket').text()) == 0) && ($('#pbxBubbleBasket').text() == '') )) ){


                if(mobDetect.tablet() && mobDetect.os() == 'iOS'){
                    $('<div class="dropDown-overlay"/>').insertBefore(oEl.find('.dropdown'));
                }

                oEl.find(sTarget).fadeIn(iFadeSpeed, function(){
                    $(this).parent().addClass('hovered');
                });
            }

            if(mobDetect.tablet() && mobDetect.os() == 'iOS'){
                $('<div class="dropDown-overlay"/>').insertBefore(oEl.find('.dropdown'));
            }*/

            if( oEl.find('.toggle-deploy').attr('id') == 'basket-icon' && sFilter == 'mini' ){
                
                oEl.find(sTarget).addClass('mini');
            
                oEl.find(sTarget).fadeIn(iFadeSpeed, function(){
                    $(this).parent().addClass('hovered');
                });

            }
            else{
                oEl.find(sTarget).fadeIn(iFadeSpeed, function(){
                    $(this).parent().addClass('hovered');
                });
            }
        break;

        case 'close':
            oEl.find(sTarget).fadeOut(iFadeSpeed, function(){
                $(this).parent().removeClass('hovered');
                oEl.find('.dropDown-overlay').remove();
                if(sFilter == 'mini'){
                    oEl.find(sTarget).removeClass('mini');
                }
            });
        break;
    }

}


/*Show/Hide submenus (tablet)*/

// oEl : $('.rebrand-menu > li > a');
// sTarget : '.subHolder';
// sToggle : 'open' or 'close';
function toggleSubMenu(oEl, sTarget, sToggle){
        if(!sToggle){
        if(oEl.hasClass('hovered')){
            sToggle = 'close';
        }else{
            sToggle = 'open';
        }
    }

    $('.rebrand-menu > li').removeClass('hovered');

    // console.log(sToggle +' - '+ oEl.find('.category-name').text());

    switch(sToggle){
        case 'open':
            // test iPad
            if(mobDetect.tablet() && mobDetect.os() == 'iOS'){
                $('<div class="dropDown-overlay"/>').insertAfter(oEl.find('> a'));
            }

            if((oEl.attr('data-hovered')) || mobDetect.tablet() ){
                setTimeout(function(){
                    oEl.find(sTarget).fadeIn(iFadeSpeed, function(){
                        oEl.removeClass('hovered').siblings().removeClass('hovered');
                        $(this).closest('li').addClass('hovered');
                    });
                }, iTimeoutSpeed);
            }else{
                oEl.removeClass('hovered').siblings().removeClass('hovered');
            }
        break;

        case 'close':
            oEl.find(sTarget).css('display','none').closest('li').removeClass('hovered');
            oEl.find('.dropDown-overlay').remove();
        break;
    }
}

/*Add/Remove classes*/
function updateStatus(oEl, sClassAdd, sClassRemove){
    if(sClassAdd != ''){
        oEl.addClass(sClassAdd);
    }
    if(sClassRemove != ''){
        oEl.removeClass(sClassRemove);
    }
}

/*Close all dropdown menus*/
function closeAllDropDowns(){
    $('.toggle-deploy').each(function(){
        toggleDropdown($(this).parent(), '.dropdown', 'close');
    });
}

function closeAllSubMenus(){
    toggleSubMenu($('.rebrand-menu > li'), '.subHolder', 'close');
    mouseEvent = 1;
}

function closeAllInHeader(){
    closeAllSubMenus();
    closeAllDropDowns();
}



$('.toggle-deploy').click(function(event){
    if(($(this).attr('id') == 'basket-icon') && ( (parseInt($('#pbxBubbleBasket').text()) == 0) || ($('#pbxBubbleBasket').text() == '') )){
    
    }else{
        if(mobDetect.mobile()){
            event.preventDefault();
            
            closeAllInHeader()

            updateStatus($(this).parent().find('.dropdown'), '', 'mini');

            toggleDropdown($(this).parent(), '.dropdown');
        }
    }
});




// $(document).ready(function(){
/*for tablets/mobile, deactivate the link on the "account" icon (triggered a popup)*/
if(mobDetect.tablet()){
    $('#account-icon a').attr('href','#');
    $('#account-icon a').click(function(event){
        event.preventDefault();
    });

    $('.rebrand-menu').mouseleave(function(){
        bIsHovered = false;
        closeAllInHeader()

    });

}else{
    var mouseEvent = 1;

    $('.rebrand-menu').mousedown(function(event) {
        mouseEvent = event.which;
    }).mouseleave(function(){
        if($('html').hasClass('desktop') && mouseEvent != 3){
            bIsHovered = false;
            closeAllInHeader()
        }
    })
} 
// })



/*DESKTOP*/

if(!mobDetect.mobile()){
    /* LI Mouseleave : close all dropdowns (for all menu icons) */
    $('html .list-icons > li').mouseleave(function(){
        closeAllInHeader();
    });


    /* .toggle-deploy mouseenter : open the dropdown menu for this icon */
    $('html .toggle-deploy').mouseenter(function(){
        /*For the basket icon : remove class .mini that was hiding some elements in the dropdown (used when adding product to basket)*/
        updateStatus($(this).parent().parent().find('.dropdown'), '', 'mini');
        
        /*if it's the basket icon, let the function determine the sToggle to "open" or "close"*/
        if($(this).attr('id') == 'basket-icon' ){
            if($('#pbxBubbleBasket').text().length > 0 && (parseInt($('#pbxBubbleBasket').text()) > 0)){
                toggleDropdown($(this).parent(), '.dropdown', 'open');
            }else{
                // updateStatus($(this).parent(), 'hovered test');
                $(this).parent().addClass('hovered');
            }
        }
    
        /* if it's not the basket icon, set the sToggle to "open"*/
        else{
            toggleDropdown($(this).parent(), '.dropdown', 'open');
        }
    });

    /*on desktop, on hover -> submenus (navigation)*/
    
    function openMenu(){
        $(this).attr('data-hovered', true);
        $(this).siblings().find('subHolder').css('display','none');
        $(this).find('.subHolder').fadeIn(iFadeSpeed);
        $(this).addClass('hovered').siblings().removeClass('hovered');
    }

    function closeMenu(){
        $(this).removeAttr('data-hovered').removeClass('hovered').find('.subHolder').css('display','none');
    }

    if(!mobDetect.tablet()){
        $('.rebrand-menu > li').hoverIntent({
            over : openMenu, 
            out : closeMenu,
            timeout : iTimeoutSpeed
        });
    }
    /*$('.rebrand-menu > li').mouseenter(function(event){
        

        listItem = $(this).parent();
        listItemId = listItem.attr('id');

        listItem.attr('data-hovered', true);
        // toggleSubMenu(listItem, '.subHolder', 'open', event);

        for(i = 0; i< aArrayCategories.length; i++){
            if(aArrayCategories[i] == listItemId){               
                toggleSubMenu($('#'+aArrayCategories[i]), '.subHolder', 'open', event);
            }else{
                toggleSubMenu($('#'+aArrayCategories[i]), '.subHolder', 'close');
            }
        }
    
    }).mouseleave(function(){
        // closing category submenu
        $(this).removeAttr('data-hovered');
    });*/

    if($.browser.msie){
        $('.big-products li a img.product-image').click(function(){
            sTargetUrl = $(this).closest('a').attr('href');
            window.location = sTargetUrl;
        });
    }
}





/*TABLET*/

if(mobDetect.tablet()){

    var clickHandler = ('ontouchend' in document.documentElement ? "touchend" : "click");

    if(mobDetect.os() != 'iOS'){
        $("body").live("click", function(event){
            var oTarget = $(event.target);
            if(oTarget.closest('.toggle-deploy').html() == null){
                closeAllDropDowns();
            }

            if(oTarget.closest('.rebrand-menu > li').html() == null){
                closeAllSubMenus();
            }
        });
    }

    // on tablets : on click -> submenus (navigation)
    $('.rebrand-menu > li > a').click(function(event){
        // deactivate link on the main category (nav level 1)
        event.preventDefault();
        
        // if it's hovered, close all submenus
        if($(this).parent().hasClass('hovered')){
            closeAllInHeader();
        }
        // it not, close all submenus AND open the right one
        else{
            closeAllInHeader();
            toggleSubMenu($(this).parent(), '.subHolder', 'open');
        }
    });


    $('.dropDown-overlay').live(clickHandler, function(){
        closeAllInHeader();
    });



/*    $("body *").live(clickHandler, function(event){
        var oTarget = $(event.target);
        //event.stopPropagation();
        


        // alert(oTarget[0].tagName + " : " + oTarget[0].className );

        // Close dropdown menu (icons) when clicking outside of it
        if(oTarget.closest('.toggle-deploy').html() == null){
            // alert("closeAllDropDowns");
            closeAllDropDowns();
        }

        if(oTarget.closest('.rebrand-menu > li').html() == null){
            // alert("closeAllSubMenus");
            closeAllSubMenus();
        }
    });*/
}


/*MSITE*/

if(mobDetect.phone()){
    function closeMenuMobile(){
        $('#openMenu').attr("checked", false);
        // updateStatus($('.rebrand-header'), '', 'open');
    }

    // Show/Hide mobile menu
    function toggleMenuMobile(){
        if($('.rebrand-header').hasClass('open')){
            updateStatus($('.rebrand-header'), '', 'open')
        }else{
            updateStatus($('.rebrand-header'), 'open', '')
        }
    }

    if(!$.browser.msie){
        window.addEventListener("orientationchange", function() {
            closeMenuMobile();
        }, false);

        /*window.addEventListener("resize", function() {
            closeMenuMobile();
        }, false);*/
    }

/*    $('.toggle-menu-mobile').click(function(){
        toggleMenuMobile();
    });*/

    // msite nav : fold/deploy submenus
    $('.toggle .holder-list-header').click(function(){
        oHolderList = $(this).parent().find('.holder-list');
        oDeployList = oHolderList.find('.holder-list-menu');

        // fold the submenu
        if($(this).closest('.toggle').hasClass('deployed')){
            updateStatus($(this).closest('.toggle'), '', 'deployed');
            oHolderList.css('height', '0');
        }
        // deploy the submenu
        else{
            updateStatus($(this).closest('.toggle'), 'deployed', '');
            oHolderList.css('height', oDeployList.height() + "px");
        }
    });

    $('body').live('click tap', function(event){
        var oTarget = $(event.target);
        
        // Close mobile menu (on the side) when clicking outside of it
        if((oTarget.closest('.rebrand-header').html() == null) && ($('.rebrand-menu-mobile').length > 0) ){
            updateStatus($('.rebrand-header'), '', 'open');
        }
    });
}






/*basket icon menu :*/
var iNbItems = parseInt($('#pbxBubbleBasket').text());
var iBasketTextLength = $('#pbxBubbleBasket').text().length;

$('#pbxBubbleBasket').bind("DOMSubtreeModified propertychange",function(){

    // iNbItems = parseInt($('#pbxBubbleBasket').text());

    /*if there's a number on the basket icon*/
    // if (isNaN(iNbItems) == false){

    if(iBasketTextLength > 0){
        /*if it's more than 0*/
        if(iNbItems >= 0){
            /*open mini dropdown (show only the number of items, no buttons)*/
            toggleDropdown($('#basket-icon').parent(), '.dropdown', 'open', 'mini');

            if(iNbItems > 99){
                $('#pbxBubbleBasket').text('99+');
            }

            /*
            bDisableClose : 
            0 : allow dropdown autoclose
            1 : disable dropdown autoclose
            */
            
            // by default, allow autoclose
            bDisableClose = 0;

            // if mouse enters dropdown, disable autoclose
            $('#basket-icon').parent().find('.dropdown').mouseenter(function(){
                bDisableClose = 1;
            });

            //after 2500ms :
            setTimeout(function(){
                // if allowed, autoclose dropdown
                if(bDisableClose == 0){
                    toggleDropdown($('#basket-icon').parent(), '.dropdown', 'close', 'mini');
                }
                // if disabled, remove mode "mini" to show the buttons
                else{
                    $('#basket-icon').parent().find('.dropdown').removeClass('mini');
                }
            }, 2500);
        }
    }
    iNbItems = parseInt($('#pbxBubbleBasket').text());
    iBasketTextLength = $('#pbxBubbleBasket').text().length;
});


});

//-->

