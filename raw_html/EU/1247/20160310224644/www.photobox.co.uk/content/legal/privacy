$(window).load(function(){
    if (document.getElementById('pbxBubbleBasket') != null ) {
        if (typeof document.pbx_hex_decode == "undefined"){
            document.pbx_hex_decode = function(hex){
                var txt = '';
                for (var count = 0; count < hex.length; count += 2){
                    txt += '%' + hex.substr(count, 2);
                }
                return unescape(decodeURI(txt));
            };
        }

        if (typeof document.pbx_basket_cookie == "undefined") {
            document.pbx_basket_cookie = [];
            var cookie_data;
            
            if (null != document.cookie && (null != document.cookie.match(/pbx_basket=([^;]+)/)) && (cookie_data = document.cookie.match(/pbx_basket=([^;]+)/)[1]) ) {       
                document.pbx_basket_cookie = document.pbx_hex_decode(cookie_data).split(':');
            }
        }

        var value = document.pbx_basket_cookie[0];

        if (null != value){
            showedValue = parseInt(value);
            if (showedValue > 99){
                showedValue = '99+';
            }

            // document.getElementById('pbxBubbleBasket').innerHTML = value.toString().replace(/^(\d+)$/, "$1");
            document.getElementById('pbxBubbleBasket').innerHTML = showedValue;
            document.getElementById('basket-items').innerHTML = value.toString().replace(/^(\d+)$/, "$1");

