			// <!--
				/**
				 * Prevent users from clicking on <a onclick="pbx.something()"> before the pbx objects are laoded.
				 * Will be set to false at end of pbx.init().
				 */
				var pbx_loadInProgress = true;
				/**
				 * This section prevents user to click on pbx_ajax link in all the document before the jquery bindings are performed
				 * Fields having class pbx_field_switch are prevented to be changed too, but so far only works on checkboxes.
				 * TODO: comment it out on next test release
				 * TODO: try to skip events when an <a onclick="pbx.something()"> is called and the page is not fully loaded
				 * TODO: investigate onchange/onclick for pbx_field_switch to make it working with all input types
				 */
				function checkLoadInProgress(ev)
				{
					var pbx_target;
					if (!ev) var ev = window.event; //ie, our best friend!

					if (typeof ev.target == "undefined")
					{
						pbx_target = ev.srcElement;
					}
					else
					{
						pbx_target = ev.target;
					}
					if (pbx_loadInProgress)
					{
						if (pbx_target.className && pbx_target.className.match(/pbx_ajax|pbx_field_switch/))
						{
							return false;
						}
						// try with the parent
						if ((pbx_target = pbx_target.parentNode) && pbx_target.className && pbx_target.className.match(/pbx_ajax|pbx_field_switch/))
						{
							return false;
						}
					}
				}
				document.onclick = checkLoadInProgress;
			//--></script><script src="https://secure.photobox.com/assets/v/G2v3GIJHH_ChansP2vTuYtRjgsE.js" type="text/javascript"></script><link href="https://secure.photobox.com/assets/content_graphics/20/102920.png?1428934213" rel="shortcut icon" type="image/vnd.microsoft.icon" /><!-- Development CSS Files --><link href="https://secure.photobox.com/assets/v/xyAU2pklq2fxN1rFyyopDtnzxkI.css" rel="stylesheet" type="text/css" /><link href="https://secure.photobox.com/assets/v/ymKrHzvnzzKimzYXe8SkJBk3pGA.css" rel="stylesheet" type="text/css" /><!-- CMS ASSETS  --><link href="https://secure.photobox.com/assets/v/825aIeqsTpYO_QX76EJC0I4WV3s.css" comment="/css/product.css" rel="stylesheet" type="text/css" /><link href="https://secure.photobox.com/assets/v/bMWLbUidVj6HLJorZWkzhDd4eFc.css" comment="/css/content.css" rel="stylesheet" type="text/css" /><link href="https://secure.photobox.com/assets/v/DRyEB33Bj4VMgNhyH9LDilhhFQc.css" comment="/css/content_popup.css" rel="stylesheet" type="text/css" /><!-- START NEW CMS GROUP TAGS --><!-- END NEW CMS GROUP TAGS --><!-- Whitelabel --><!-- extra CSS, using shared method --><!-- EXTERNAL CSS --><!-- Print media --><link media="print" href="https://secure.photobox.com/assets/v/ovXv3DNp00W1KBuWzzrDULU35IE.css" rel="stylesheet" type="text/css" /><script type="text/javascript"><!--
