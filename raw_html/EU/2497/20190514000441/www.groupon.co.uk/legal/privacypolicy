          (function() {
            try {
              var nav = document.getElementById('ls-header-top-row');
              var fn = function(s) {
                Array.prototype.forEach.call(nav.getElementsByClassName(s), function(i) {
                  i.classList.remove(s)
                })
              };
              if (nav && document.cookie.match(/(^c=|\sc=)/g)) fn('show4C');
              var data = sessionStorage['verifiedUser'];
              if (nav && data) {
                fn('show4user');
                var user = nav.querySelector('.user-menu-action');
                user.classList.add('user-name');
                user.innerHTML = '<span class="icon-arrow-down-large">' + JSON.parse(data).nameTruncated + '</span>';
                document.getElementById('ls-user-signup').classList.add('ls-hide');
              }
            } catch (e) {}
          }());
