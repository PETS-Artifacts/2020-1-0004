        <script>
  (function() {
    var loc = document.cookie.match(/search_loc=%25.*?%257D;/g);
    if (!loc) {
      return;
    }
    try {
      var location = JSON.parse(decodeURIComponent(decodeURIComponent(loc[0].replace('search_loc=','').replace(/;$/, ''))));
      if (location && location.friendlyName) {
        document.getElementById('ls-location').value = location.friendlyName;
      }
    } catch (e) {}
  }())
</script>

