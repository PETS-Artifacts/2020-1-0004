		];
	
		var pbLabels = [];
		if(dc_ad_home) {
			pbLabels.push('home');
		} else {
			pbLabels.push('article');
		}
	
		var pbjsDone = false;
		pbjs.que.push(function() {
			pbjs.setConfig({
				priceGranularity: "high",
				sizeConfig: [
					{
						 mediaQuery: '(min-width: ' + String(dcRespMaxW + 1) + 'px)',
						 sizesSupported: [].concat(dailycaller_top_desktop_sizes, dailycaller_right1_desktop_sizes, dailycaller_right2_desktop_sizes),
						 labels: ["desktop"]
					}, {
						 mediaQuery: '(min-width: ' + String(dcRespMinW) + 'px) and (max-width: ' +String(dcRespMaxW) + 'px)',
						 sizesSupported: [].concat(dailycaller_top_tablet_sizes, dailycaller_inline_sizes),
						 labels: ["tablet"]
					}, {
						 mediaQuery: '(min-width: 0px) and (max-width: ' +String(dcRespMinW - 1) + 'px)',
						 sizesSupported: [].concat(dailycaller_top_mobile_sizes, dailycaller_inline_sizes),
						 labels: ["mobile"]
					}
				]
			});
			pbjs.addAdUnits(adUnits);
			pbjs.requestBids({
				bidsBackHandler: function(bids, timedOut) {
					console.log('[Prebid] Finished');
					pbjs.setTargetingForGPTAsync();
					pbjsDone = true;
					/* Disabled for coordination with Amazon */
					/* googletag.pubads().refresh(); */
				},
				timeout: PREBID_TIMEOUT,
				labels: pbLabels
			});
		});
	
		/* This is the final coordination between Amazon and Prebid */
		var dcIntervalStart = Date.now();
		function dcIntervalCheck() {
			var runTime = Date.now() - dcIntervalStart;
			if(FAILSAFE_TIMEOUT + 25 < runTime || (apsDone && pbjsDone)) {
				console.log('[Google] Submitting');
				clearInterval(dcInterval);
				googletag.pubads().refresh();
			}
		}
		var dcInterval = setInterval(dcIntervalCheck, 50);
	</script>

