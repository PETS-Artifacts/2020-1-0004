<script>
		jQuery(document).ready(function() {
			var is_logged_in = -1 != document.cookie.indexOf('dailycaller_logged_in=loggedin');
			if(is_logged_in) {
				var edit_divs = jQuery(".dc_edit_link");
				if(edit_divs) {
					var post_id = jQuery("article[dc_data]").attr('dc_data');
					var edit_base = "//dailycaller.com/wp-admin/post.php?action=edit&post=";
					edit_divs.html('<a href="' + edit_base + post_id +
					               '">Edit this</a>');
				}
			}
		});
	</script>

