<div class="dc-piano"><div id="piano-form"></div></div>
<script>
		var dcPianoAccountLinks = '<span class="piano-links piano-mobile-loggedin"><span class="piano-desktop-only"> | </span><a href="/subscribe/" class="piano-subscribe piano-loggedout">Subscribe</a><a style="display: none;" href="/account/" class="piano-myaccount piano-loggedin">My Account</a> | <a class="piano-login piano-loggedout">Log In</a><a style="display: none;" class="piano-logout piano-loggedin">Log Out</a></span>';
		var dcPianoOnOffButton = '<span class="piano-links piano-switch">Advertisements</span><span class="piano-links piano-switch"><a id="piano-on-off"><img src="//dailycaller.com/wp-content/uploads/2019/03/adsonoff.png" style="width: 80px; position: relative; top: 3px;"></img></a></span>';
	
		function dc_piano_is_logged_in(data = false) {
			console.log('[Piano] dc_piano_is_logged_in begins');
			tp.user.refreshAccessToken(true);
			if(data) {
				//console.log('[Piano] User ', data.user, ' logged in with token', data.token);
			} else {
				//console.log('[Piano] User is logged in');
			}
			if(dc_ad_free || dc_ad_light) {
				jQuery('.piano-switch').hide();
			}
			jQuery('.piano-loggedout').hide();
			jQuery('.piano-loggedin').show();
			jQuery('.search-hamburger .piano-mobile-loggedin, .piano-loggedin').show();
		}
		function dc_piano_is_logged_out() {
			console.log('[Piano] dc_piano_is_logged_out begins');
			document.cookie = '__tac=;expires=Thu, 01 Jan 1970 00:00:01 GMT;domain=.dailycaller.com;path=/;';
			jQuery('.search-hamburger .piano-mobile-loggedin, .piano-loggedin').hide();
			jQuery('.piano-loggedin').hide();
		}
		function dc_piano_do_login() {
			//console.log('[Piano] dc_piano_do_login begins');
			tp.pianoId.show({
				screen: 'login',
			});
	
		}
		function dc_piano_do_logout() {
			//console.log('[Piano] dc_piano_do_logout begins');
			tp.pianoId.logout();
		}
	
		tp = window["tp"] || [];
		tp.push(["setUsePianoIdUserProvider", true ]);
		tp.push(["init", function() {
			//console.log("[Piano] init begins");
			window.resetPianoCookie = false;
	
			tp.pianoId.init({
				displayMode: 'modal',
				width: 320,
				height: 460,
				containerSelector: '#piano-form',
				loggedIn: dc_piano_is_logged_in,
				loggedOut: dc_piano_is_logged_out
			});
			jQuery('.dc-piano').css('top', '100%');
			//console.log("[Piano] window.resetPianoCookie: " + window.resetPianoCookie);
			console.log("[Piano] isUserValid: " + JSON.stringify(tp.pianoId.isUserValid()));
			if(window.resetPianoCookie || !tp.pianoId.isUserValid()) {
				dc_piano_do_logout();
			}
		}]);
		jQuery(document).ready(function() {
			//console.log("[Piano] document.ready begins");
			jQuery('#social-box').append(dcPianoAccountLinks);
			jQuery('#search-box').empty().append(dcPianoOnOffButton);
			jQuery('.search-hamburger')
				.empty()
				.append(jQuery(dcPianoOnOffButton.replace('id="piano-on-off"', 'id="piano-on-off-hamburger"')))
				.append(jQuery(dcPianoAccountLinks));
			jQuery('.piano-links').css("font-weight", "bold");
			jQuery('.piano-links').css("font-size", "0.7em");
			jQuery('.piano-links a').css("cursor", "pointer");
			jQuery('.search-hamburger').css("margin-top", "8px");
			jQuery('.search-hamburger .piano-desktop-only').hide();
			jQuery('.search-hamburger .piano-links:first-child')
				.css("position", "relative")
				.css("display", "inline-block")
				.css("top", "-0.5em")
				.css("left", "-1em");
			jQuery('.piano-login').click(function() {
				dc_piano_do_login();
			});
			jQuery('.piano-logout').click(function() {
				dc_piano_do_logout();
			});
			jQuery('#piano-on-off, #piano-on-off-hamburger').click(function() {
				tp.offer.show({
					displayMode: "modal"
				});
			});
		});
	</script>
