  __dataLayerPayload__.user = {
      account_type: window.__session__.user && window.__session__.user.id ? 'plus' : 'free',
      subdomain_name: window.__session__.profile && window.__session__.profile.subdomain_name ? window.__session__.profile.subdomain_name : null,
      language: window.__session__.user && window.__session__.user.language
    };
}
// Preloaded transfer
if (window._preloaded_transfer_) {
  var transfer = window._preloaded_transfer_;
  // Download page data
  __dataLayerPayload__.page = {
    download: {
      id: transfer.id,
      state: transfer.state,
      receiver_id: transfer.recipient_id,
      security_hash: transfer.security_hash
    }
  };
  // VRL
  if (transfer.items && transfer.items.length) {
    __dataLayerPayload__.page.VRL = transfer.items.some(function(item) {
      return item.previewable;
    });
    __dataLayerPayload__.transfer = {
      spaceship_transfer: transfer.id.charAt(0) === 's',
      item_count: transfer.items.length,
      previewable_count: transfer.items.reduce(function(sum, item) {
        return item.previewable ? sum + 1 : sum;
      }, 0)
    }
  }
}

// Fake data layer because everytime dataLayer.push is invoked, it's one extra event
// As versus this approach, where the dataLayer is rather initialised with data
window.dataLayer = [__dataLayerPayload__];
</script>

