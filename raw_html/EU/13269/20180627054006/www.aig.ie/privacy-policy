  <div class="featured-content detail-template" ID="featuredContentImage">
    <div data-picture data-alt="Privacy Policy">
      <div data-src='/content/dam/global/images/fc-interior/monitors-interior-632-1139B.jpg/jcr:content/renditions/mobile.768.632.jpg' data-media="(min-width: 240px) and (max-width: 768px)"></div>
