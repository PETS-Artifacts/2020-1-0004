                this.frameworkMappings.push({scVar:"event17",cqVar:"eventdata.events.sc_error_event",resourceType:"global/components/pages/errorpage404"});
                this.frameworkMappings.push({scVar:"eVar18",cqVar:"eventdata.sc_numberOfSearchResults",resourceType:"global/components/pages/newshomepage"});
                this.frameworkMappings.push({scVar:"eVar34",cqVar:"eventdata.sc_searchFilter",resourceType:"global/components/pages/newshomepage"});
                this.frameworkMappings.push({scVar:"event1",cqVar:"eventdata.events.sc_searchResultPageLoad",resourceType:"global/components/pages/newshomepage"});
