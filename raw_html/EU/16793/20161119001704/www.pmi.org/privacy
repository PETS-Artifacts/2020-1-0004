    <!-- Main section -->
    <div class="row">
        <!-- Secondary Left Nav -Desktop only- -->
        <div class="col-md-2 hidden-xs hidden-sm">
            <div class="left-nav" role="navigation" itemscope itemtype="http://schema.org/SiteNavigationElement">
    
    <ul class="list-unstyled">
                <li class="" itemprop="name">
                    <a class="" href="http://www.pmi.org/terms" title="Terms of Use">Terms of Use</a>
                </li>
                <li class="" itemprop="name">
                    <a class="" href="http://www.pmi.org/linking" title="Linking Agreement">Linking Agreement</a>
