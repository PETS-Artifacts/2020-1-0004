<script>
  if (go_popup.length) {
    $(document).off('click.cuturls.goPopupClick');
    $(go_popup).off('submit.cut-urls.goPopupSubmit');
  }

  $('a.get-link, .skip-ad a').on('DOMSubtreeModified', function (event) {
    if (!$(this).hasClass('disabled')) {

      $(this).off();
      $(this)
        .attr('onclick', 'doFunction();')
        .attr('data-href', $(this).attr('href'))
        .attr('href', 'javascript: void(0)');
    }
  })

  function doFunction() {
    if(confirm('Do you want to leave this website ?')) {
      $('#go-popup').submit();
      setTimeout(function () {
        window.location.href = $('a.get-link, .skip-ad a').attr('data-href');
      }, 500);
    }
  }
</script>
