  (function () {
    var captchaWidgetID = {};
    var captchaWidgetOptions = {};

    window['captchaSiteKey'] = '6LfqtSATAAAAAMd_bCul5C2la5jBcmFJOWenBDGs';

    window['recaptchaInit'] = function (options) {
      options = options || {};
      Object.keys(options).forEach(function (captchaId) {
        if (!captchaWidgetOptions.hasOwnProperty(captchaId)) {
          captchaWidgetOptions[captchaId] = {
            theme: options[captchaId].theme,
            callback: options[captchaId].callback
          };
        }
      });
      if (typeof grecaptcha === 'undefined') {
        return;
      }
      var elements = document.querySelectorAll('.recaptcha');
      for (var i = 0; i < elements.length; i++) {
        var el = elements[i];
        var id = el.getAttribute('id');
        if (!(id in captchaWidgetID)) {
          captchaWidgetID[id] = grecaptcha.render(id, {
            'sitekey': el.getAttribute('data-sitekey'),
            'theme': captchaWidgetOptions[id] && captchaWidgetOptions[id].theme || 'light',
            'callback': captchaWidgetOptions[id] && captchaWidgetOptions[id].callback
          });
        } else {
          grecaptcha.reset(captchaWidgetID[id]);
        }
      }
    };
  })();
