									<li class="stock-message">{{ stockMessage }}</li>
									{{/if}}
								</ul>
								<!-- .header-nav-bag-item-details -->
							</div>
							<!-- .header-nav-bag-item-info -->
						</a>
					</li>
					<!-- .header-nav-bag-item -->
					{{/each}}
				</ul>
				<div class="header-nav-sub-total _shadow-wrapper-bottom">
					<span class="label">Sub Total</span> <span class="amount" dir="ltr">{{order-total}}<span class="price-label">
	</span></span>
				</div>
				<a class="header-nav-checkout" href="/us/en/checkout/single">Checkout</a>
				<a class="header-nav-bag-details" href="/us/en/cart">View Bag Details</a>

                <a class="header-nav-button-paypal" href="/us/en/checkout/paypal-express/paypal-payment-auth-init" >
					<span>Pay with</span>
					<i class="ico-paypal"></i>
				</a>
                
				{{else}}

                <p class="empty {{#userIsAuthenticated user}}empty-with-signin{{/userIsAuthenticated}}">Currently empty</p>
					{{#userIsAuthenticated user}}
						<div class="has-signin-bag-content">
							<div class="reference-to-create-account">Create an account for access to:</div>
