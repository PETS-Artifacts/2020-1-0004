                   break;
                case "Hong Kong":
                    hongkongCode();
                    break;
           //     case "United States":
             //      fullStackAcademyCode();
                  //  unitedStatesCode();
           //         break;
                default: 
                    defaultCode();
            }
        });
    

    }(jQuery));
</script>  </div>
</div>
