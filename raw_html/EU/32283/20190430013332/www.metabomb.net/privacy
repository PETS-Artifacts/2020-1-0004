00;display:block}.comment-count{font-weight:400;color:inherit}.comment-count:hover{color:inherit}.comment-count .fa{margin-right:.125rem}.avatar{position:relative;vertical-align:middle;background-size:contain}.section-container{margin:0 0 24px}.section-container .section-title{margin:0}.game-icon{width:2em;height:2em;line-height:2em;text-align:center;border-radius:100%;text-transform:uppercase;margin:0 auto;border:1px solid currentColor;font-size:.675rem}.basic-form label{font-size:1rem;margin:0;padding:0 0 4px}.basic-form input,.basic-form select,.basic-form textarea{display:block;width:100%;max-width:38rem;margin:0 0 16px;padding:8px;font-size:.8375rem;border:1px solid #B5B5B5}#cookie_gdpr_settings{border:1px solid #E1E9ED;padding:1rem}#cookie_gdpr_settings h2{font-size:1.5rem;color:#000}#cookie_gdpr_settings h3{font-size:1rem;color:#000}#cookie_gdpr_settings .col{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-box-orient:vertical;-webkit-flex-direction:column;-ms-flex-direction:column;flex-direction:column;padding:0 16px}#cookie_gdpr_settings .col.left{width:50%;display:inline-block;vertical-align:top;padding-left:0}#cookie_gdpr_settings .col.left h3{padding-bottom:.5em}#cookie_gdpr_settings .col.right{width:50%;display:inline-block;vertical-align:bottom;border-left:1px dashed #0465b0;padding-left:20px;text-align:center;padding-top:0;margin-top:0}#cookie_gdpr_settings .col.right p{padding-bottom:10px}#cookie_gdpr_settings .button.true{background-color:#0465b0;color:#fff}#cookie_gdpr_settings .gdpr_setting{display:-webkit-box;display:-webkit-flex;display:-ms-flexbox;display:flex;-webkit-box-pack:left;-webkit-justify-content:left;-ms-flex-pack:left;justify-content:left;border-top:1px solid #402A3F;padding:20px 0 0;margin:20px 0}#cookie_gdpr_settings .buttons .button{margin-right:1em;padding:.5em 1em;font-size:.9em;border:1px solid #402A3F}@media screen and (max-width:320px){#cookie_gdpr_settings .buttons .button{margin-bottom:1em}}