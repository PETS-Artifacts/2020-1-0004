<script>
	j(document).ready(function() {
		try { } catch(e) {
			yafaIt('[data-dfp-id]');
			var restyled = document.querySelectorAll('[data-yafa-style]');
			for (var i = 0; i < restyled.length; i++) {
				restyled[i].setAttribute('style', restyled[i].getAttribute('data-yafa-style'));
			}

		}

		device = "hd";

        //lazyload some ads
	    var lazyloading = true;
        if(device == "mobile"){
        	var lazyload_distance = window.innerHeight * 2;
        } else {
        	var lazyload_distance = window.innerHeight
        }
        function lazyloadAds() {
            lazy_mpus = jQuery('.mpu .lazyload').filter(':visible');
            if(lazy_mpus.length == 0) {
                lazyloading = false;
                clearInterval(lazyloadInterval);
            } else {
                for ( var i = 0; i < lazy_mpus.length; i++ ) {
                    mpu_pos = jQuery(lazy_mpus[i])[0].getBoundingClientRect();
                    if(mpu_pos['top'] < lazyload_distance && lazyloading == true){
                        
                        if(device == "mobile") {
	                        googletag.cmd.push(function () {
	                        	lazyloadSlot = googletag.defineSlot('/43340684/' + jQuery(lazy_mpus[i]).attr('data-dfp-id'), [[300,600],[300,250],[320,100],[320,50]], jQuery(lazy_mpus[i]).attr('id')).addService(googletag.pubads());
	                        });
	                    } else {
	                    	googletag.cmd.push(function () {
	                        	lazyloadSlot = googletag.defineSlot('/43340684/' + jQuery(lazy_mpus[i]).attr('data-dfp-id'), [[300,250]], jQuery(lazy_mpus[i]).attr('id')).addService(googletag.pubads());
	                        });
	                    }
                        googletag.pubads().refresh([lazyloadSlot]); 
                        jQuery(lazy_mpus[i]).removeClass('lazyload').addClass('lazyloaded');
                    }
                }
            }
        }
        lazy_mpus_check = jQuery('.mpu .lazyload').filter(':visible');
	    if(lazy_mpus_check.length > 0) {
	    	var lazyloadInterval = setInterval(lazyloadAds, 300);
	    }
	});
</script>

