			<script type="text/javascript">
				function yafaIt() {
					var dfp = document.querySelectorAll('[data-dfp-id]')
						,adList = document.querySelectorAll('[data-yafa-click]')
						,observerOptions = {attributes: true, attributeFilter: ['style'], attributeOldValue: true}
					;

					for (var i = 0; i < dfp.length; i++) {
						dfp[i].parentNode.removeChild(dfp[i]);
					}

					for (var i = 0; i < adList.length; i++) {
						var ad = adList[i]
							,img = ad.getAttribute('data-yafa-img')
							,link = ad.getAttribute('data-yafa-click')
							,styles = window.JSON.parse(ad.getAttribute('data-yafa-styles'))
							,targets = Object.keys(styles)
							,linkheight = ad.getAttribute('data-yafa-linkheight') ? ad.getAttribute('data-yafa-linkheight') : 0
						;

						for (var j = 0; j < targets.length; j++) {
							var target = document.querySelectorAll(targets[j]);

							if (targets.length) {
								target = target[0];

								if (typeof target != 'undefined') {
									var targetName = targets[j]
										,originalStyles = target.getAttribute('style') !== null ? target.getAttribute('style') : ''
									;

									break;
								}
							}
						}

						if (typeof target == 'undefined') {
							break;
						}

						function setYafaEvents(target, ad, linkheight) {
							target.addEventListener('click', function (e) {
								if (isHovering(target, e, linkheight)) {
									window.open(ad.getAttribute('data-yafa-click'), '_top');
								}
							});

							target.addEventListener('mousemove', function (e) {
								if (isHovering(target, e, linkheight)) {
									document.body.style.cursor = 'pointer';
								} else {
									document.body.style.cursor = 'auto';
								}
								e.stopPropagation();
							});
						}

						target.setAttribute('style', originalStyles + styles[targetName] + 'background-repeat: no-repeat; background-image: url("' + img + '");');

						setYafaEvents(target, ad, linkheight);
					}
				}

				function isHovering(target, event, linkheight) {
					return (target == event.target) && (linkheight == 0 || event.pageY <= linkheight);
				}
			</script>
		

