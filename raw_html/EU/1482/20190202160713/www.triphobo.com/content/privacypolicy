<script src="https://ak2.jogurucdn.com/resources_version/desktop/js/live/loaders/loader_funs.js?cache_bust=310120191822"></script>

    <script type="text/javascript">
      // -- Triphobo popunder global object
      var global_place_object = {
          id : '',
          type : '',
          latitude : '', 
          longitude : '',
          session_page : '',
          name : '',
          country : '',
          search_term : '',
          exit_intent_page : '',
          multi_th_spk : 1
      };
      
      var _global_place_object = null;
      // Update global_place_object 
        try {
            $.extend( global_place_object, _global_place_object );
            joguru.session_page = global_place_object.session_page;
             
            // Only For Mobile
            if( joguru.device_type != 'web' ){

                var intent_media_pages = ['MOB_HOME_PAGE','MOB_CR_ITI','MOB_CITY_ITI','MOB_MOB_PLA','MOB_MOB_PLC','MOB_MOB_DAY_TP','MOB_BL_V','MOB_MOB_CITY_TP','MOB_H_SEO','MOB_H_VIEW','MOB_HOL','MOB_FROM_TO_TP','MOB_MOB_TR'];
        
                if( ( 'session_page' in global_place_object ) 
                && intent_media_pages.indexOf(global_place_object.session_page) != -1 
                ){
                    // -- Prepare IntentMedia object need to open intent media
                    window.IntentMediaProperties = {  
                        site_name       : 'TRIPHOBO',
                        page_id         : '',
                        hotel_city      : '',
                        hotel_country   : '',
                        site_country    : 'US',
                        site_language   : 'EN',
                        site_currency   : 'USD',
                        site_reporting_value_01   : '',
                        site_reporting_value_02   : '',
                        site_reporting_value_03   : '',
                    };

                    switch(global_place_object.session_page) {
                        case 'MOB_HOME_PAGE':
                        case 'MOB_HOL':
                            window.IntentMediaProperties.page_id = 'package.home.mobile';
                            break;
                        case 'MOB_CR_ITI':
                        case 'MOB_CITY_ITI':
                            window.IntentMediaProperties.page_id = 'package.frontdoor.mobile';
                            break;   
                        case 'MOB_MOB_PLA':
                        case 'MOB_MOB_PLC':
                        case 'MOB_MOB_DAY_TP':
                        case 'MOB_H_SEO':
                        case 'MOB_H_VIEW':
                        case 'MOB_MOB_CITY_TP':
                        case 'MOB_FROM_TO_TP':
                        case 'MOB_MOB_TR':
                            window.IntentMediaProperties.page_id = 'content.details.mobile';
                            break;
                        case 'MOB_BL_V':
                            window.IntentMediaProperties.page_id = 'content.list.mobile';
                            break; 
                    }
                    
                     var intent_media_city =  global_place_object;
                     if( global_place_object.session_page == "MOB_BL_V" 
                      && typeof intent_media_city_array !== "undefined" 
                      && intent_media_city_array.length){
                        // it take random array value from intent_media_city_array 
                        intent_media_city = intent_media_city_array[Math.floor(Math.random()*intent_media_city_array.length)];

                     }else if(!jQuery.isEmptyObject(global_place_object.intent_media_city) ){
                       intent_media_city = global_place_object.intent_media_city;
                     }

                     if(intent_media_city.name){
                         window.IntentMediaProperties.hotel_city = intent_media_city.name.toLowerCase();   
                         window.IntentMediaProperties.site_reporting_value_02 = window.IntentMediaProperties.hotel_city;
                     }
                     if(intent_media_city.country){
                         window.IntentMediaProperties.hotel_country = intent_media_city.country;
                     }    


                    if( navigator.cookieEnabled ) {
                        // -- include intend Media script    
                        var intentMediaScript = document.createElement("script");
                        //intentMediaScript.src = '//a.cdn.intentmedia.net/javascripts/v1/intent_media_core.js';
                        intentMediaScript.src = '//compare.triphobo.com/javascripts/v1/p/alt_core.js';
                        intentMediaScript.async = true;
                        intentMediaScript.defer = true;
                        document.getElementsByTagName("head")[0].appendChild(intentMediaScript);
                    }
                }    
            } 
        }
        catch(err) { }

    </script>
