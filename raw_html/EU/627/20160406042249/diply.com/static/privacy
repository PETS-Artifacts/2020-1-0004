    function loadFacebookAPI() {
        (function (d, s, id) {
            var js, fjs = d.getElementsByTagName(s)[0];
            if (d.getElementById(id)) return;
            js = d.createElement(s); js.id = id;
            js.src = "//connect.facebook.net/en_US/sdk.js";
            fjs.parentNode.insertBefore(js, fjs);
        }(document, 'script', 'facebook-jssdk'));
    }

    window.onscroll = function () {
        var titleRect = document.getElementById("postTitle");
        if (titleRect) titleRect = titleRect.getBoundingClientRect();

        var nextRect = document.getElementsByClassName("controls-next")[0];
        if (nextRect) nextRect = nextRect.getBoundingClientRect();

        var commentsRect = document.getElementById('fbComments');
        if (commentsRect) commentsRect = commentsRect.getBoundingClientRect();

        if ((titleRect && titleRect.bottom < 0)
            || (nextRect && nextRect.top < window.innerHeight)
            || (commentsRect && commentsRect.top < window.innerHeight)) {
            loadFacebookAPI();
            window.onscroll = null;
        }
    }

