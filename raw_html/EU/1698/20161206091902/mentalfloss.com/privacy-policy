<script>

    jQuery('.hamburger-box').click(function() {
        if(jQuery('#in-subMenuBar-sandwich').css('left') == '0px'){
            jQuery('.hamburger--collapse').removeClass('is-active');
            jQuery('#in-subMenuBar-sandwich').css('left','-650px');
            jQuery('.menu_layout').css('left','-960px');
        }else{
            jQuery('.hamburger--collapse').addClass('is-active');
            jQuery('#in-subMenuBar-sandwich').css('left','0px');
            jQuery('.menu_layout').css('left','0px');
        }
    });
</script>

