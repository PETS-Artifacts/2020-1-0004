        <script>
            jQuery(document).ready(function() {
            	jQuery('#su-footer #newsletter-form').validate({
            		rules: {
            			email: {
            				required: true
            			   ,email: true
            			}
            		},
            		errorPlacement: function(error, element) {
            			error.appendTo( '.error' );
            		},
            		submitHandler: function(form) {
            			jQuery.ajax({
            				url: '/newsletter_signup/' + encodeURIComponent(jQuery('#su-footer #email').val()),
            				dataType: 'json',
            				jsonp: false,
            				complete: function(data) {
            					if(data['responseText'] == 'true') {
                                    jQuery('#su-footer #email-store').val(jQuery('#su-footer #email').val());
            						jQuery('#su-footer #su-button').hide();
                                    jQuery('#su-footer #newsletter-store-form').show();
            						jQuery('#su-footer #newsletter-form #email').hide();
            						jQuery('#su-footer #su-text').html('<span>Congratulations</span>, you\'re subscribed!<br><br>WOULD YOU ALSO LIKE TO RECEIVE EXCLUSIVE DEALS FROM THE<br> mental_floss store AND SAVE 15% ON YOUR FIRST ORDER?');
            					} else {
            						jQuery('#su-footer #su-text').html('Let\'s try your <span>email address</span> again!');
            						jQuery('#su-footer #email').val('OOOPS!').css('color','#f16566');
            					}
            				}
            			});
            		}
            	});
            });

        </script>

         <script>
                jQuery(document).ready(function() {     // second form for store newsletter
