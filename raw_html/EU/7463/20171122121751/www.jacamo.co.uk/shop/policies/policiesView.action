    <script>
        function calcSupersitesTabs() {
            var show = false;
            var $superHeight = $('#supersites ul').outerHeight();
            
              $('#supersites ul li').each(function() {
                 var $this = $(this);
                 show = Math.round($this.offset().top) > $superHeight;
                 $('ul.small-supersites-tabs li#'+ $this.attr('id')).css('display', (show) ? 'block' : 'none');
              });
              
            $('#supersites #more-tab').css('display', (show) ? 'block' : 'none' );             
        }
        
        (function(jquery) {
            $('div#supersites').prependTo('body');
            
            $('#more-tab, .superClose').unbind('click').bind('click', function(e) {
                $('#small-supersites').fadeToggle('300');
                e.stopPropagation();
            });
            
	            // Apply TABS to supersites links - except on Tablet, 'active' links or links who Title is empty    
	            $('.supersite:not(".active") a:not([title=""],[title=" "])').qtip({
