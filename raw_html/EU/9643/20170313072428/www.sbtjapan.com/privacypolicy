<script type="text/javascript">
    function udpate_me(my_register){
          if(my_register=='my_livechat'){
              $.ajax({
                type: "POST",
                url: "http://www.sbtjapan.com/user/reply_liveperson",
                data: '',
                success: function(msg) {
                  if(msg == true) $('#'+my_register).hide();
                  //else $('#my_content').html('omg');
                }
              });
              return false;
          }
          else if(my_register=='my_register'){
              $.ajax({
                type: "POST",
                url: "http://www.sbtjapan.com/user/reply_register",
                data: '',
                success: function(msg) {
                  if(msg == true) $('#'+my_register).hide();
                  //else $('#my_content').html('omg');
                }
              });
              return false;
          }
      }
</script>

