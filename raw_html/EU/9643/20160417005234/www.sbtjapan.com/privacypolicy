}
else{
return monthArray[month] + " " + day + ", " +  " " + hr + ":" + min + "(JST)"
//return hr + ":" + min
}
}
function worldClockZone(){
    document.getElementById("TokyoTime").innerHTML = worldClock(9, "Tokyo");
    setTimeout("worldClockZone()", 1000);
}

