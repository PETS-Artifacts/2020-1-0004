<script type="text/javascript">
    function Usearch(obj) {
        var btn = $(obj);
        var keyword = encodeURIComponent(btn.next().val());
        var url = "/en/searchresult";
        //if (language == "zh")
        //{
        //    language = "cn";
        //}
        //language = language.toLowerCase();
        //if (language == "zh") language = "cn";
        //if (language == "cn" || language == "zh" || language == "en") {
        //    window.open("http://www.huawei.com/" + language + "/group-search/index.htm?ssUserText=" + keyword);
        //}
        //else {
        if ($("#psirtId").val() != undefined && $("#psirtId").val() == "psirtId") {
            window.open(url + "?keywords=" + keyword + "&contentTypes=" + "{9704BDD0-A1CF-4706-BDFC-210721E028F2}");
        }
        else
        {
            window.open(url + "?keywords=" + keyword);
        }
        //}
    }

    function Usearchs(obj) {
        var btn = $(obj);
        var keyword = encodeURIComponent(btn.val());
        var url = "/en/searchresult";
        //if (language == "zh")
        //{
        //    language = "cn";
        //}
        //language = language.toLowerCase();
        //if (language == "zh") language = "cn";
        //if (language == "cn" || language == "zh" || language == "en") {
        //    window.open("http://www.huawei.com/" + language + "/group-search/index.htm?ssUserText=" + keyword);
        //}
        //else {
        window.open(url + "?keywords=" + keyword);
        //}
    }
    function UsearchMobile() {

        var warn = "Please type a keyword for your search";
        var KeyText = $('#search-box-h-mobile').attr("value");
        var url = "/en/searchresult";
        if (KeyText == "") {
            alert(warn);
            return false;
        } else {
            if (KeyText.indexOf("@") > 0) {
                KeyText = KeyText.replace("@", "%40");
            }
            window.open(url + "?keywords=" + encodeURIComponent(encodeURIComponent(KeyText))) ;
        }
    }
</script>
