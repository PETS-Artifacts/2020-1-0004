<script>
$(function () {
    $("#hw1_global_nav .nav-contact > ul > li > a").each(function (i) {
        var hrefs = $(this).attr("href");
        if (hrefs == "javascript:;") {
            $(this).removeAttr("target");
        }
    })


$.each($("a[href^='https://www.huawei']"), function(n, v){
	$(v).attr("href", $(v).attr("href").replace("https:", ""));
});


});
</script>


