        function gPlusLoginOrRegister() {
          var params = {
            'clientid': '485119388126.apps.googleusercontent.com',
            'cookiepolicy': 'http://revzilla.com',
            'scope': 'profile email',
            'callback': function(response) {
              if ((response['status']) && (response['status']['signed_in']) && (response['status']['method'] != 'AUTO')) {
                $("social_type").value = "GooglePlusGraph";
                $("social_access_token").value = response.id_token;
                $("social_login_fieldset_form").submit();
                GOOGLE_ANALYTICS.track_event('SocialLogin','GooglePlus',window.location.pathname);
              }
            }
          }
          gapi.auth.signIn(params);
        }
