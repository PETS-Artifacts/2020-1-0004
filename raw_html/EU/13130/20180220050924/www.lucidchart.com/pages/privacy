<script>
(function($) {
    $(".responsive-menu").on('click', function(e) {
        $(this).toggleClass('mobile-open');
        if ($(this).hasClass('mobile-open')) {
            $("#navigation").slideDown();
        }
        else {
            $("#navigation").slideUp();
        }
    });

	$('.popout').on('click', function(e) {
        e.preventDefault();
        e.stopPropagation();

        var id = $(this).attr('href');
		if ($(id).hasClass('hidden')) {
			$(id).removeClass('hidden');
		}
		else {
			$(id).addClass('hidden');
			}
			$('.flyout').not(id).addClass('hidden');

        $(document).one('click', function(e){
            if($(id).has(e.target).length === 0){
                $(id).addClass('hidden');
            }
        });
    });
	$('#dd-button').on('click', function(e) {
        e.preventDefault();
        e.stopPropagation();

        var id = $('.menu-container').attr('href');
		if ($('.menu-container').hasClass('hidden')) {
			$('.menu-container').removeClass('hidden');
		}
		else {
			$('.menu-container').addClass('hidden');
		}

        $(document).one('click', function(e){
            if($('.menu-container').has(e.target).length === 0){
                $('.menu-container').addClass('hidden');
            }
        });
    });
}(window.jQuery));
</script><!-- /block -->

