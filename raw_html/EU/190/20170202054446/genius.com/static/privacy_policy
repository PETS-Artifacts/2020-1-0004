  <script type="text/javascript">
  var googletag = googletag || {};
  googletag.cmd = googletag.cmd || [];

  googletag.helpers = (function() {
    var set_targeting = function(obj, kv) {
      for (var key in kv) {
        if (kv[key] != null) {
          obj.setTargeting(key, kv[key].toString());
        }
      }
    };

    var annotation_ad_data, annotation_slot;
    var annotation_ad_loading_for_first_time = true;

    var manually_loading_ads = false;
    var unloaded_defined_slots = [];

    return {
      enable_manual_loading: function() {
        manually_loading_ads = true;
        googletag.pubads().disableInitialLoad();
      },

      define: function(data) {
        var slot = googletag.defineSlot(data.path, data.sizes, data.id);
        set_targeting(slot, data.kv);
        slot.addService(googletag.pubads());

        if (manually_loading_ads) {
          unloaded_defined_slots = unloaded_defined_slots.concat(slot);
        }

        return slot;
      },

      render: function(slot) {
        googletag.display(slot.getSlotElementId());

        var is_unloaded = unloaded_defined_slots.some(function(unloaded_slot) {
          return unloaded_slot === slot;
        });

        if (is_unloaded) {
          googletag.pubads().refresh([slot]);
          unloaded_defined_slots = unloaded_defined_slots.filter(function(unloaded_slot) {
            return unloaded_slot !== slot;
          });
        }
      },

      load_pending_ads: function() {
        googletag.pubads().refresh(unloaded_defined_slots);
        unloaded_defined_slots = [];
      },

      configure: function(kv) {
        var pubads = googletag.pubads();
        set_targeting(pubads, kv);
        pubads.enableSingleRequest();
        pubads.collapseEmptyDivs(true);
        googletag.enableServices();
      },

      set_annotation_ad_data: function(data) {
        annotation_ad_data = data;
        annotation_slot = this.define(annotation_ad_data);
      },

      annotation_ad_slot: function() {
        return new Promise(function(resolve) {
          googletag.cmd.push(function() {
            if (annotation_ad_loading_for_first_time) {
              resolve({refresh: function() { googletag.cmd.push(function() { googletag.display(annotation_ad_data.id) }); }});
              annotation_ad_loading_for_first_time = false;
            } else {
              resolve({refresh: function() { googletag.pubads().refresh([annotation_slot]); }});
            }
          });
        });
      }
    };
  })();
</script>


