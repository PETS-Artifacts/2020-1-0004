<script type="text/javascript">
(function ($, window, document, undefined)
{

    // Plugin object constructor
    $.pltSearch = function (element, options)
    {
        var
        properties = {
            $nav: null,
            $container: null,
            $results: null,
            $button: null,
            $loadingIcon: null,
            $overlay: null,
            $input: null,
            $closeIcon: null,
            $placeholder: null,
            $url: null
        },
        methods = {
            init: function ()
            {
                // Set elements
                properties.$nav = $(element);
                properties.$closeIcon = properties.$nav.find(options.closeIcon);
                properties.$container = $(options.container);
                properties.$results = $(options.results);
                properties.$button = $(options.button);
                properties.$loadingIcon = $(options.loadingIcon);
                properties.$overlay = $(options.overlay);
                properties.$input = $(options.inputField);
                properties.$placeholder = $(options.placeholder);
                properties.$url = options.url;

                console.log('urls...');
                console.log(properties.$url);
                console.log('search initialized');

                properties.$input.on('click',function(){
                    properties.$nav.addClass('focus');
                });

                properties.$overlay.on('click',function(){
                    properties.$nav.removeClass('focus');
                });

                properties.$closeIcon.on(
                    'click',
                    methods.nav.resetSearch
                );

                properties.$input.keyup(function(){
                    methods.nav.userInput();
                });
            },
            nav: {
                userInput: function () {
                    if(properties.$input.val().length >= 1){

                        properties.$results.hide();
                        properties.$placeholder.hide();
                        properties.$loadingIcon.show();
                        properties.$closeIcon.show();
                        methods.ajax.load();
                    }else{
                        methods.nav.resetSearch();
                    }
                },
                resetSearch: function () {
                    properties.$input.val("");
                    properties.$results.hide();
                    properties.$placeholder.show();
                    properties.$loadingIcon.hide();
                    properties.$closeIcon.hide();
                },

                buildHtml: function (data) {

                    console.log(data);

                    var htmlSource = jQuery('#template-plt-search').html();

                    var myTemplate = Handlebars.compile(htmlSource);

                    var myData = data;

                    var myHtml = myTemplate(myData);

                    properties.$results.html(myHtml);

                }
            },
            ajax: {
                load: function () {

                    //var url = 'https://www.prettylittlething.local/merchandiser/test/search';

                    var url = properties.$url.searchUrl;
                    console.log(url);

                    $.ajax({
                        url: url,
                        method: 'post',
                        error: methods.ajax.failed,
                        success: methods.ajax.added,
                        complete: methods.ajax.reset
                    });
                },
                failed: function ()
                {
                    alert('An error occurred, please try again.');
                },
                added: function (data, status, xhr)
                {
                    if (status !== 'success') {
                        return methods.ajax.failed();
                    }
                    // alert('added');
                    //var data = JSON.parse(data);
                    // console.log(data);
                    methods.nav.buildHtml(data);
                    properties.$results.show();
                },
                reset: function ()
                {
                    // alert('reset');
                    properties.$loadingIcon.hide();
                }
            }
        };

        methods.init();
    };

    // Global options
    $.pltSearch.defaultOptions = {
        nav: '.top-navigation',
        container: '.search-wrap',
        results: '#fredhopper-results',
        button: '.btn-search',
        loadingIcon: '#search-loading',
        overlay: '#search-overlay',
        inputField: '#searchbox',
        closeIcon: '.fa-close',
        placeholder: '#search-placeholder',
        url: {
            searchUrl: ''
        }
    };

    $.fn.pltSearch = function (options)
    {
        var options = $.extend(true, {}, $.pltSearch.defaultOptions, options);
        return new $.pltSearch(this, options);
    };

})(jQuery, window, document);



jQuery(document).ready(function()
{

});
</script>


