        function pltUpdateDataLayer() {
            window.dataLayer = window.dataLayer || [];
            dataLayer.push({
                "event": "beforeSnippet",
                "page": {
                    "pageCategory": "Other",
                    "subCategory": selectedCategory,
                    "url": "https://www.prettylittlething.com/privacy-policy",
                    "pageReferrer": document.referrer
                },
                "user": {
                    "userEmailAddress": dataLayerEmail
                }
            });
        }
        pltUpdateDataLayer();
