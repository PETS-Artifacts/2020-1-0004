<script id='trackingloader' type='txt/template'>

  <div id='trackingloader'>
  <img src="https://cdn-skin.prettylittlething.com/frontend/prettylittlething/plt-theme/images/opc-ajax-loader.gif"
       alt="Please wait..."
       title="Please wait..."
       class="v-middle"/> Please wait...</img>
  </div>

</script>

<script>
jQuery(function() {

  jQuery('#trackmyorder').on('click',function(){

    var html = jQuery('#trackingloader').html();

    // Set spinner
    jQuery('#trackingloader').before('#trackmyorder').show();

    var orderNo = jQuery('#orderRef').val();

    jQuery.ajax({
        method: 'POST',
        url: "/customer/tracking/gettrackingurl",
        data:{ 'orderNo':orderNo },
        dataType:'json'
      }).complete(function(data) {

          data=data.responseJSON;

          if (data.status==0) {

            var url = data.redirectUrl;
            window.location = url;

          } else {

            // remove spinner
            jQuery('#trackingloader').hide();

            // show error message
            var msg = data.msg;

            jQuery('#tracking-error').remove(0);

            jQuery('#trackmyorder').before('<div id="tracking-error">'+msg+'</div>');

            jQuery('#tracking-error').delay(5000).hide(0);

          }

      });
  });
});
</script>

