        if (document.getElementById(id)) return;
        var js = document.createElement('script'); js.id = id;
        js.src = subdomain + '.scarabresearch.com/js/15839D67E28C23C3/scarab-v2.js';
        var fs = document.getElementsByTagName('script')[0];
        fs.parentNode.insertBefore(js, fs);
    })('https:' == document.location.protocol ? 'https://recommender' : 'http://cdn', 'scarab-js-api');

    jQuery(document).ready(function () {

        var customerEmail = false;
        if (typeof dataLayerEmail!='undefined' && dataLayerEmail!=false) {
            customerEmail = dataLayerEmail;
        }else if(typeof plt_customer_email != 'undefined') {
            customerEmail = plt_customer_email;
        }
        if (typeof mobileEmail!='undefined') {
            customerEmail = mobileEmail;
        }
        if (typeof customerEmail!='undefined' && customerEmail!=false) {
            ScarabQueue.push(['setEmail', customerEmail]);
        }

        var datalayerBasket=false;
        if (typeof datalayerCart['basket']!='undefined') {
            datalayerBasket=datalayerCart['basket'];
        }
        if (typeof mobileCart!='undefined') {
            datalayerBasket=mobileCart['basket'];
        }

        var amasysCart = [];
        if (typeof datalayerBasket!='undefined' && datalayerBasket!=false) {
            jQuery.each(datalayerBasket,function(k,v){
                if (typeof v['price'] != 'undefined' &&
                    typeof v['price'] != 'undefined' &&
                    typeof v['price'] != 'undefined') {
                    try {
                        var i = v['simpleId'];
                        var p = parseFloat(v['price']);
                        var q = parseFloat(v['quantity']);
                        amasysCart.push({item:i,price:p,quantity:q});
                    } catch (e) {}
                }
            });
        }
        ScarabQueue.push(['cart', amasysCart]);

        if (jQuery('body').hasClass('catalog-product-view')) {
            if (typeof pageProduct!='undefined') {
                ScarabQueue.push(['view', pageProduct['sku']]);
            }
        }

        if (jQuery('body').hasClass('catalog-category-view')) {
            ScarabQueue.push(['category', '']);
        }

        if (jQuery('body').hasClass('catalogsearch-result-index')) {
            ScarabQueue.push(['searchTerm', '']);
        }

        if (jQuery('body').hasClass('checkout-onepage-success')) {
            if (typeof pltPurchase !== 'undefined') {
                ScarabQueue.push(['purchase', pltPurchase]);
            }
        }
        ScarabQueue.push(['go']);
    });

</script>
