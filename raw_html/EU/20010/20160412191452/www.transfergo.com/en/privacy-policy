<script type="text/javascript">window._trackJs = { token: "116e3399496f485885980875b58bfd80", application: "landing", callback: { enabled: true, bindStack: true }};</script>
<script type="text/javascript" src="https://d2zah9y47r7bi2.cloudfront.net/releases/current/tracker.js"></script>

<script src="//cdn.optimizely.com/js/204820032.js"></script>
<script>(function () {
    'use strict';

    /// Private functions --------------------------------------------------

    function getUrlParts(url) {

        var a = document.createElement('a');
        a.href = url;
        return {
            hostname: a.hostname,
            pathname: a.pathname
        };
    }

    /// Adapted from https://github.com/js-cookie/js-cookie/blob/master/src/js.cookie.js
    function getCookie(key) {

        var cookies = document.cookie ? document.cookie.split('; ') : [];
        var rdecode = /(%[0-9A-Z]{2})+/g;
        var i = 0;

        for (; i < cookies.length; i++) {

            var parts = cookies[i].split('=');
            var name = parts[0].replace(rdecode, decodeURIComponent);

            if (key === name) {

                var cookie = parts.slice(1).join('=');
                if (cookie.charAt(0) === '"') {
                    cookie = cookie.slice(1, -1);
                }
                return cookie.replace(rdecode, decodeURIComponent);
            }
        }
    }

    /// Adapted from https://github.com/js-cookie/js-cookie/blob/master/src/js.cookie.js
    function setCookie(key, value, secondsToExpireIn) {

        var path = '/';
        var domain = '.' + document.location.hostname.split('.').slice(-2).join('.');
        var expires = new Date(+new Date + secondsToExpireIn * 1000);

        key = (encodeURIComponent(String(key))
            .replace(/%(23|24|26|2B|5E|60|7C)/g, decodeURIComponent)
            .replace(/\(/g, '%28')
            .replace(/\)/g, '%29')
        );

        try {
            var result = JSON.stringify(value);
            if (/^[\{\[]/.test(result)) {
                value = result;
            }
        } catch (e) {}

        value = (encodeURIComponent(String(value))
            .replace(/%(23|24|26|2B|3A|3C|3E|3D|2F|3F|40|5B|5D|5E|60|7B|7D|7C)/g, decodeURIComponent)
        );

        return (document.cookie = [
                key, '=', value,
                '; expires=' + expires.toUTCString(),
                '; path=' + path,
                '; domain=' + domain,
            ].join('')
        );
    }

    function getUtmVars() {

        var result = {};
        var pairs = document.location.search.slice(1).split('&');
        for (var i=0; i<pairs.length; i++) {
            var parts = pairs[i].split('=');
            var key = parts.shift();
            if (key.indexOf('utm_') === 0 || key === 'gclid') {
                result[key] = decodeURIComponent(parts.join('='));
            }
        }
        return result;
    }

    function getSearchEngine(hostname) {

        var patterns = {
            yahoo: /^yahoo\.cn|[\w\.]*search\.yahoo\.(com?(\.\w\w)?)$/,
            ask: /^teoma\.com|(www|\w\w|[\w.]*search)\.ask\.com$/,
            google: /^www\.google\.(cat|com|com\.\w\w|\w\w|\w\w\.\w\w)$/,
            bing: /^((\w\w|images|m|search|ssl|www)\.)?bing.com$/,
            yandex: /^(www\.)?yandex\.(\w\w|com|com\.\w\w)$/,
        };

        for (var name in patterns) {
            if (hostname.match(patterns[name])) {
                return name;
            }
        }
        return null;
    }

    /// Main ---------------------------------------------------------------

    var COOKIE_EXPIRY = 30 * 60; // 30 min.

    var excludeHosts = [
        'my.transfergo.com',
        'transfergo.com',
        'www.transfergo.com',
        'marlin.firstatlanticcommerce.com',
        'accounts.google.com',
        'my.tgv3.dev',
        'my.tgstatic.com',
        'tgstatic.com',
        'www.tgstatic.com',
        'qa-my.tgstatic.com'
    ];

    var domainParts = window.location.hostname.split('.');
    var pathParts = window.location.pathname.split('/');

    /// Get UTM data from URL:
    var utm = getUtmVars();

    var campaign = null;

    if (utm.gclid) {
        campaign = {
            source: 'google',
            medium: 'cpc',
        };
        /// URL based Auto-tagging:
        if (domainParts[0] === 'www' && pathParts[2] === 'personal') {
            campaign.name = pathParts[3];
            campaign.term = pathParts[4];
            campaign.content = pathParts[5];
        }
    } else if (utm.utm_source && utm.utm_medium) {
        campaign = {
            source: utm.utm_source,
            medium: utm.utm_medium,
            name: utm.utm_campaign,
            content: utm.utm_content,
            term: utm.utm_term,
        };
    } else if (document.referrer) {

        var referrerParts = getUrlParts(document.referrer);
        var searchEngine = getSearchEngine(referrerParts.hostname);

        if (searchEngine) {
            campaign = {
                source: searchEngine,
                medium: 'organic',
            };
        } else if (-1 === excludeHosts.indexOf(referrerParts.hostname)) {
            campaign = {
                source: referrerParts.hostname,
                medium: 'referral',
                content: document.referrer,
            };
        }
    }

    if (!campaign && !getCookie('tg-campaign')) {
        campaign = {
            source: '(direct)',
            medium: '(none)',
        };
    }

    if (campaign) {
        setCookie('tg-campaign', campaign, COOKIE_EXPIRY);
    } else {
        setCookie('tg-campaign', getCookie('tg-campaign'), COOKIE_EXPIRY);
    }

    /// Extend expiry time every 5 minutes.
    setInterval(function(){
        setCookie('tg-campaign', getCookie('tg-campaign'), COOKIE_EXPIRY);
    }, 5 * 60e3);
        

    /// First visit cookie: ------------------------------------------------

    if (!getCookie('tg-first-visit')) {
        var oldUtma = getCookie('__utma');
        if (oldUtma) {
            setCookie('__utma', '');
            var firstTimestamp = oldUtma.slice(4, -6);
        } else {
            var firstTimestamp = Math.floor(+new Date/1000);
        }
        setCookie('tg-first-visit', firstTimestamp, 720 * 24 * 3600);
    }
}) ();
</script>
