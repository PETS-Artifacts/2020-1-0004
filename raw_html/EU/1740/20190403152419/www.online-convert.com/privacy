        <script>
            var ocBrowserExtension = (function () {
                var supportedBrowser = true;
                var browser = '';

                var init = function () {
                    _start();
                    _buttons();
                    _setCookie();
                };

                var _start = function () {
                    var isOpera = !!window.opera || navigator.userAgent.indexOf(' OPR/') >= 0;

                    
                    if (!!window.chrome && !isOpera) {
                        browser = 'chrome';
                        $('.extension-browser').show();
                        $('#extension-chrome-logo').show();
                        $('#extension-chrome-text').show();
                        $('.btn-extension-install').attr('href', 'https://chrome.google.com/webstore/detail/file-converter-by-online/dicgkflojhbopmagcacdklcpdfdcnhko');

                        return;
                    }

                    
                    if (typeof InstallTrigger !== 'undefined') {
                        browser = 'firefox';
                        $('.extension-browser').show();
                        $('#extension-firefox-logo').show();
                        $('#extension-firefox-text').show();
                        $('.btn-extension-install').attr('href', 'https://addons.mozilla.org/firefox/addon/online-convert/');

                        return;
                    }

                    supportedBrowser = false;
                };

                var _log = function(message) {
                    try {
                        qgLogger.log(message);
                    } catch(e) {
                        // do nothing
                    }
                };

                var _buttons = function () {
                    if (supportedBrowser === false) {
                        return;
                    }

                    _log('extension' + browser + 'shown');

                    $('.btn-extension-cancel').click(function () {
                        $('.extension-browser').fadeOut();

                        _log('extension' + browser + 'cancel');
                    });

                    $('.btn-extension-install').click(function () {
                        $('.extension-browser').fadeOut();

                        _log('extension' + browser + 'install');
                    });
                };

                var _setCookie = function () {
                    var maxAge = 31557600;

                    if (supportedBrowser) {
                        maxAge = 3888000;
                    }
                    document.cookie = "qgExtension=true; max-age=" + maxAge + "; path=/;domain=.online-convert.com";
                };

                return {
                    init: init
                };
            })();

            ocBrowserExtension.init();
        </script>
    
