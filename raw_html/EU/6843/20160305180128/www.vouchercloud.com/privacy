                addText = "Add to favourites",
                existingFavourites = [],
                searchBrand = jQuery("#search-brand"),
                searchBrand2 = jQuery("#search-brand2");

            (function(){

                // Brand Search in Header and Profile > Favourites
                var algolia = new AlgoliaSearch('LGJV9YLQAJ', 'f27959c1a225181828f2e7c8bdac5894'),
                    algoliaIndex = algolia.initIndex('Merchants'),
                    countryCode = 'countryCode:GB';

                function offerTitle(s, p, i){
                    if (i == 0) return '';
                    return i.toString() + ' ' + (i == 1 ? s : p);
                }

                function generateTrackingLink(current) {
                    return '/search-vouchers-at-vouchercloud?searchTerm=' + encodeURIComponent(current);
                }

                function attachTypeAhead(elem){

                    elem.each(function(){

                        var $this = jQuery(this),
                            isFavouriteSearch = $this.data("favourites") == true,
                            isSimple = $this.data("simple") == true;

                        elem.typeahead(null, {
                            source: algoliaIndex.ttAdapter({ hitsPerPage: 7, facets: '*', facetFilters: ['status:1', countryCode, 'excludedPlatforms:-1'] }),
                            displayKey: 'tradingName',
                            templates: {
                                suggestion: function(obj) {

                                    if(isFavouriteSearch){
                                        if(existingFavourites.indexOf(Number(obj.objectID)) > -1){
                                            return '<div class="image"><img src="' + obj.mediaPath.replace("{{options}}", "w_83,q_60,") + '" /></div><div class="title"><p>' + obj._highlightResult.tradingName.value + '</p><p class="offers">' + offerTitle('offer', 'offers', obj.totalOffers) + '</p></div><a href="#" class="login-button action-favourites btn-favorite selected" data-endpoint="/account/updatefavouritemerchant" data-merchant="' + obj.objectID + '" data-favourite-link="searchedmerchant" data-add="false"><span class="added">Added to favourites</span><span class="add">Add to favourites</span></a>';
                                        } else {
                                            return '<div class="image"><img src="' + obj.mediaPath.replace("{{options}}", "w_83,q_60,") + '" /></div><div class="title"><p>' + obj._highlightResult.tradingName.value + '</p><p class="offers">' + offerTitle('offer', 'offers', obj.totalOffers) + '</p></div><a href="#" class="login-button action-favourites btn-favorite" data-endpoint="/account/updatefavouritemerchant" data-merchant="' + obj.objectID + '" data-favourite-link="searchedmerchant" data-add="true"><span class="added">Added to favourites</span><span class="add">Add to favourites</span></a>';
                                        }
                                    } else if (isSimple) {
                                        return '<div class="title"><p>' + obj._highlightResult.tradingName.value + '</p></div>';
                                    }
                                    else {
                                        return '<a href="#"><div class="image"><img src="' + obj.mediaPath.replace("{{options}}", "w_83,q_60,") + '" /></div><div class="title"><p>' + obj._highlightResult.tradingName.value + '</p><p class="offers">' + offerTitle('offer', 'offers', obj.totalOffers) + '</p></div></a>';
                                    }
                                }
                            }
                        }).on('typeahead:selected', function (e, data) {

                            _gaq.push(['_trackPageview', generateTrackingLink(data.tradingName)]);

                            var form = $this.parents('form').first(),
                                isFavouriteSearch = $this.data("favourites") == true;

                            if(!isFavouriteSearch && !isSimple){
                                form.submit();
                            }

                        }).keypress(function(e){
                            if(isFavouriteSearch){
                                if (e.which == 13){
                                    return false;
                                }
                            }
                        });
                    });
                }

                attachTypeAhead(searchBrand);
                attachTypeAhead(searchBrand2);

            })();</script></body></html>