<script>

    var fileManifest = {"fonts.css" : "fonts.a13fac52ae.css", "styles.css" : "styles.d7f337dead.css", "stash.js" : "stash.cshtml", "core.js" : "core.4d28699f24.js", "modules.js" : "modules.a65c98e653.js", "placeholder.polyfill.js" : "placeholder.polyfill.95ff7948d9.js", "fastclick.polyfill.js" : "fastclick.polyfill.a0004c80bf.js", "picturefill.polyfill.js" : "picturefill.polyfill.f11949a034.js", "vendor.js" : "vendor.987f925d41.js", "maps.module.js" : "maps.module.8e4c8011cb.js", },
        fileName = '//a4-static-v3.vouchercloud.com/content/dist/' + fileManifest['fonts.css'],
        key = 'VcAssets',
        cache;

    function insertFont(content) {
        var style = document.createElement('style');
        style.innerHTML = content;
        document.head.appendChild(style);
    }

    try {
        cache = window.localStorage.getItem(key);
        if (cache) {
            cache = JSON.parse(cache);
            if (cache.fileName == fileName) {
                insertFont(cache.content);
            } else {
                window.localStorage.removeItem(key);
                cache = null;
            }
        }
    } catch (e) {
    }

    if (!cache) {
        var xhttp = new XMLHttpRequest();
        xhttp.onreadystatechange = function() {
            if (xhttp.readyState === 4 && xhttp.status === 200) {
                try {
                    insertFont(xhttp.responseText);
                    window.localStorage.setItem(key, JSON.stringify({
                        id: 'fonts',
                        fileName: fileName,
                        content: xhttp.responseText
                    }));
                } catch (e) {
                }
            }
        };
        xhttp.open('GET', fileName, true);
        xhttp.send();
    }

</script>
    
    

    
    

