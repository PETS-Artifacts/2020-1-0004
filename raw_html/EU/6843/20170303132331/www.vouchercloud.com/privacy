    var Stash=function(){this._args=Array.prototype.slice.call(arguments),this._argsLength=this._args.length,this._holdingArray=[],this._holdingArrayLength=null,this._masterItems=[],this._masterItemsLength=null,this._masterCallback=null,this._masterItemTemplate={url:null,content:null,before:null,ready:null,extension:null,xhr:null},this.config={debug:!1,timeout:1e4},this.config=this._is.undefined(Stash.config)?this.config:this._clone(this.config,Stash.config),this._init()};Stash.prototype._is={string:function(t){return"string"==typeof t},undefined:function(t){return"undefined"==typeof t},"function":function(t){return"function"==typeof t},array:function(t){return"[object Array]"===Object.prototype.toString.call(t)},object:function(t){return"[object Object]"===Object.prototype.toString.call(t)}},Stash.prototype._init=function(){if(this._is.undefined(this._windowUrl)&&(Stash.prototype._windowUrl=this._getWindowUrl()),this._argsLength){for(var t=0;t<this._argsLength;t++)this._prepareList(this._args[t]);this._masterItemsLength=this._masterItems.length,this._processForAjax()}},Stash.prototype._prepareList=function(t){if(this._is.string(t))this._prepareItem(t,{url:t});else if(this._is.object(t))this._prepareItem(t,t);else if(this._is.array(t))for(var e=0,n=t.length;n>e;e++)this._prepareList(t[e]);else null===this._masterCallback&&this._is["function"](t)&&(this._masterCallback=t)},Stash.prototype._prepareItem=function(t,e){t=this._clone(this._masterItemTemplate,e),this.config.debug?t.xhr=!1:t.xhr=!!(t.xhr||t.url.indexOf(this._windowUrl)>-1),this._masterItems.push(t)},Stash.prototype._processForAjax=function(){for(var t=0;t<this._masterItemsLength;t++)this._masterItems[t].i=t,this._masterItems[t].xhr?this._ajax(this._masterItems[t]):this._inject(this._masterItems[t])},Stash.prototype._ajax=function(t){var e=this,n=new XMLHttpRequest,s=!1;setTimeout(function(){s||(s=!0,e._inject(t))},e.config.timeout),n.onreadystatechange=function(){4==n.readyState&&(200==n.status&&(t.content=n.responseText),s||(s=!0,e._inject(t)))},n.open("GET",t.url,!0),n.send()},Stash.prototype._inject=function(t){if(this._holdingArray.push(t),this._holdingArrayLength=this._holdingArray.length,this._holdingArrayLength===this._masterItemsLength){this._holdingArray.sort(function(t,e){return parseFloat(t.i)-parseFloat(e.i)});for(var e=0;e<this._holdingArrayLength;e++)this._performInjection(this._holdingArray[e]);this._masterCallback&&this._masterCallback()}},Stash.prototype._performInjection=function(t){var e;if(t.extension=t.extension||this._getExtensionType(t.url),t.before&&t.before(),t.xhr){if(t.xhr&&t.content){"js"===t.extension?(e=document.createElement("script"),e.type="text/javascript"):"css"===t.extension&&(e=document.createElement("style"));try{e.appendChild(document.createTextNode(t.content))}catch(n){e.text=t.content}}}else"js"===t.extension?(e=document.createElement("script"),e.type="text/javascript",e.async=!1,t.ready&&(e.onload=t.ready),e.src=t.url):"css"===t.extension&&(e=document.createElement("link"),e.type="text/css",e.rel="stylesheet",e.href=t.url);document.getElementsByTagName("head")[0].appendChild(e),t.xhr&&t.ready&&t.ready()},Stash.prototype._clone=function(t,e){var n,s=t.constructor();for(n in t)e&&e.hasOwnProperty(n)?s[n]=e[n]:s[n]=t[n];return s},Stash.prototype._getExtensionType=function(t){return t.replace(t.substring(0,t.lastIndexOf(".")+1),"")},Stash.prototype._getWindowUrl=function(){var t=window.location.href,e=t.length;return t="https://"==t.substring(0,8)?t.substring(8,e):t,e=t.length,t="http://"==t.substring(0,7)?t.substring(7,e):t,e=t.length,t.substring(0,t.indexOf("/"))};

