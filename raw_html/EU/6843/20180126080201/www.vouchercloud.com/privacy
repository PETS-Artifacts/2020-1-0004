<script>

        var fileManifest = {"snowfall.js":"snowfall-b7b38d3f.js","placeholder.polyfill.js":"placeholder.polyfill-70edb006.js","fastclick.polyfill.js":"fastclick.polyfill-4f7e6017.js","picturefill.polyfill.js":"picturefill.polyfill-a896e945.js","core.js":"core-145e8f53.js","vendor.js":"vendor-c2a4bc9f.js","modules.js":"modules-437edba4.js","maps.module.js":"maps.module-d0e2e2fc.js","fonts.css":"fonts-0ef0d4a2.css","sourcesanspro-bold.ttf":"sourcesanspro-bold.ttf","sourcesanspro-light.ttf":"sourcesanspro-light.ttf","sourcesanspro-regular.ttf":"sourcesanspro-regular.ttf","vouchercloud.ttf":"vouchercloud.ttf","styles.css":"styles-9d6a8dc0.css","serviceworker.js":"serviceworker-13c724f6.js"},
            fileName = '//a4-static-v3.vouchercloud.com/content/dist/' + fileManifest['fonts.css'],
            key = 'VcAssets',
            cache;

        function insertFont(content) {
            var style = document.createElement('style');
            style.innerHTML = content;
            document.head.appendChild(style);
        }

        try {
            cache = window.localStorage.getItem(key);
            if (cache) {
                cache = JSON.parse(cache);
                if (cache.fileName === fileName) {
                    insertFont(cache.content);
                } else {
                    window.localStorage.removeItem(key);
                    cache = null;
                }
            }
        } catch (e) {
        }

        if (!cache) {
            var xhttp = new XMLHttpRequest();
            xhttp.onreadystatechange = function() {
                if (xhttp.readyState === 4 && xhttp.status === 200) {
                    try {
                        insertFont(xhttp.responseText);
                        window.localStorage.setItem(key,
                            JSON.stringify({
                                id: 'fonts',
                                fileName: fileName,
                                content: xhttp.responseText
                            }));
                    } catch (e) {
                    }
                }
            };
            xhttp.open('GET', fileName, true);
            xhttp.send();
        }

</script>
    
    

    
    

