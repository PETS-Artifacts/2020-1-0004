<script>

    var Stash = function(){

   this._args = Array.prototype.slice.call(arguments);
   this._argsLength = this._args.length;
   this._orderedItems = false;
   this._orderedGroup = null;
   this._orderedIndex = 1;
   this._holdingArray = [];
   this._holdingArrayLength = null;
   this._masterItems = [];
   this._masterItemsLength = null;
   this._masterCallback = null;
   this._masterItemTemplate = {
      url : null,
      content : null,
      before : null,
      ready : null,
      extension : null,
      xhr : null,
      order: null
   };
   this.config = {
      debug : false,
      timeout : 10000
   };
   this.config = !this._is.undefined(Stash.config) ? this._clone(this.config, Stash.config) : this.config;

   this._init();
};

Stash.prototype._is = {
   string : function(obj){ return typeof obj === 'string' ? true : false; },
   undefined : function(obj){ return typeof obj === 'undefined' ? true : false; },
   function : function(obj){ return typeof obj === 'function' ? true : false; },
   array : function(obj){ return Object.prototype.toString.call(obj) === '[object Array]' ? true : false; },
   object : function(obj){ return Object.prototype.toString.call(obj) === '[object Object]' ? true : false; },
};

Stash.prototype._init = function(){

   if(this._is.undefined(this._windowUrl)){
      Stash.prototype._windowUrl = this._getWindowUrl();
   }

   if(this._argsLength){
      for(var i = 0; i < this._argsLength; i++){
         this._prepareList(this._args[i]);
      }
   } else {
      return;
   }

   this._masterItemsLength = this._masterItems.length;
   this._processForAjax();
};

Stash.prototype._prepareList = function(item){
   if(this._is.string(item)){
      this._prepareItem(item, {url : item});
   } else if(this._is.object(item)){
      this._prepareItem(item, item);
   } else if (this._is.array(item)){
      for(var j = 0, len = item.length; j < len; j++){
         this._prepareList(item[j]);
      }
   } else if (this._masterCallback === null && this._is.function(item)){
      this._masterCallback = item;
   }
};

Stash.prototype._prepareItem = function(item, overide){
   item = this._clone(this._masterItemTemplate, overide);
   if(item.order) {
      this._orderedItems = true;
   }
   item.xhr = item.xhr || item.url.indexOf(this._windowUrl) > -1 ? true : false;
   this._masterItems.push(item);
};

Stash.prototype._processForAjax = function() {

   var items = this._masterItems;

   if(this._orderedItems) {
      var current = this._orderedIndex;
      items = items.filter(function(item) {
         return item.order == current
      });
      if(!items.length) {
         return;
      }
      this._orderedGroup = items;
   }

   for(var i = 0; i < items.length; i++){
      items[i].i = i;
      if(items[i].xhr){
         this._ajax(items[i]);
      } else {
         this._inject(items[i]);
      }
   }
};

Stash.prototype._ajax = function(item){
   var that = this,
      xhttp = new XMLHttpRequest(),
      sent = false,
      url = item.url;

   setTimeout(function(){
      if(!sent){
         sent = true;
         that._inject(item);
      }
   }, that.config.timeout);

   xhttp.onreadystatechange = function() {
      if(xhttp.readyState == 4){
         if(xhttp.status == 200){
            item.content = xhttp.responseText;
         }
         if(!sent){
            sent = true;
            that._inject(item);
         }
      }
   };
   if(this.config.debug) {
      url = url + '?r='+ Math.floor(Math.random() * 1000);
   }
   xhttp.open('GET', url, true);
   xhttp.send();
};

Stash.prototype._inject = function(item){
   this._holdingArray.push(item);
   this._holdingArrayLength = this._holdingArray.length;

   if(this._orderedItems) {
      if(this._holdingArrayLength === this._orderedGroup.length) {

         if(this.config.debug) {
            // console.log('Stash._inject:group =', this._orderedGroup);
         }

         this._orderedIndex++;

         this._holdingArray.sort(function(a, b){
            return parseFloat(a.i) - parseFloat(b.i);
         });

         for(var i = 0; i < this._holdingArrayLength; i++){
            this._performInjection(this._holdingArray[i]);
         }

         this._holdingArray = [];
         this._processForAjax();

      }
   }

   if(this._holdingArrayLength === this._masterItemsLength){

      if(this.config.debug) {
         // console.log('Stash._inject:default =', this._holdingArray);
      }

      this._holdingArray.sort(function(a, b){
         return parseFloat(a.i) - parseFloat(b.i);
      });

      for(var i = 0; i < this._holdingArrayLength; i++){
         this._performInjection(this._holdingArray[i]);
      }

      if(this._masterCallback){
         this._masterCallback();
      }
   }
};

Stash.prototype._performInjection = function(item){
   var file;

   item.extension = item.extension || this._getExtensionType(item.url);

   if(item.before){
      item.before();
   }

   if(!item.xhr){

      if(item.extension === 'js'){
         file = document.createElement('script');
         file.type = 'text/javascript';
         file.async = false;
         if(item.ready){
            file.onload = item.ready;
         }
         file.src = item.url;
      } else if(item.extension === 'css'){
         file = document.createElement('link');
         file.type = 'text/css';
         file.rel = 'stylesheet';
         file.href = item.url;
         // ready event? - use headjs method to check for presence in dom
      }

   } else if(item.xhr && item.content) {

      if(item.extension === 'js'){
         file = document.createElement('script');
         file.type = 'text/javascript';
      } else if(item.extension === 'css'){
         file = document.createElement('style');
      }
      try {
         file.appendChild(document.createTextNode(item.content));
      } catch (e) {
         file.text = item.content;
      }
   }

   document.getElementsByTagName('head')[0].appendChild(file);

   if(item.xhr && item.ready){
      item.ready();
   }

};

Stash.prototype._clone = function(obj, custom){
   var copy = obj.constructor(),
      attr;
   for(attr in obj) {
      if(custom && custom.hasOwnProperty(attr)){
         copy[attr] = custom[attr];
      } else {
         copy[attr] = obj[attr];
      }
   }
   return copy;
};

Stash.prototype._getExtensionType = function(str){
   return str.replace(str.substring(0, str.lastIndexOf('.') + 1), '');
};

Stash.prototype._getWindowUrl = function() {
   var _windowUrl = window.location.href,
      _windowUrlLength = _windowUrl.length;
   _windowUrl = _windowUrl.substring(0, 8) == 'https://' ? _windowUrl.substring(8, _windowUrlLength) : _windowUrl; // remove https:// from beginning of string if present
   _windowUrlLength = _windowUrl.length;
   _windowUrl = _windowUrl.substring(0, 7) == 'http://' ? _windowUrl.substring(7, _windowUrlLength) : _windowUrl; // remove http:// from beginning of string if present
   _windowUrlLength = _windowUrl.length;
   return _windowUrl.substring(0, _windowUrl.indexOf('/')); // remove the first slash and everything after it
};

    if ('serviceWorker' in navigator){
        window.addEventListener('load', function(){
            console.log('Ready to register SW');
            navigator.serviceWorker.register('/serviceworker.js')
            .then(function(registration){
                console.log('ServiceWorker registration successful with scope: ', registration.scope);
            }, function(err){
                console.log('ServiceWorker registration failed: ', err);
            });
        });
    }

    window.addEventListener('load', function() {
        var iframe = document.getElementsByTagName('iframe');
        for (var i = 0; i < iframe.length; i++) {
            if(iframe[i].getAttribute('data-src')) {
                iframe[i].setAttribute('src', iframe[i].getAttribute('data-src'));
            }
        }
    });

    document.addEventListener('lazybeforeunveil', function(e){
        var bg = e.target.getAttribute('data-bg');
        if(bg){
            e.target.style.backgroundImage = 'url(' + bg + ')';
        }
    });

