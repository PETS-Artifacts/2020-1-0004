function setEcity(uname,name,exptime,edition)
{
      var getedition = uname.split('@');
	  e = getedition[1]||uname;
	

    var d = new Date();
    d.setSeconds(d.getSeconds() + parseInt(exptime));
    var expires = "expires="+d.toUTCString();

	if(typeof edition!='undefined' && edition=='mainedition') {
        document.cookie ="subedition=" + uname + "; expires=Thu, 01-Jan-70 00:00:01 GMT; path=/";
		document.cookie ='mainedition=' + uname + "; " + expires+ "; path=/";
	}
	else
        document.cookie ="subedition=" + uname + "; " + expires+ "; path=/";
    

	if($('#edition_box').length>0)
	   document.querySelector("#edition_box").classList.add("dhide");
