<script type="text/javascript">
var socialShareElements = $('#new-social-share > .share-icons > li > a');
hideprint(socialShareElements);
socialShareElements.on('click', sendToSocialNetwork);
function sendToSocialNetwork() {
var link = $(this);
var linkType = link.attr('class').replace('icon-social ', '');
switch (linkType) {
case 'facebook': shareViaFacebook(); break;
case 'twitter': shareViaTwitter(link); break;
case 'linkedin': case 'in': shareViaLinkedin(link); break;
case 'gplus': shareViaGooglePlus(); break;
case 'mail': shareViaEmail(link); break;
case 'print': printPage(); break;
case 'weibo': shareViaWeibo(); break;
default:
alert('Social icon with linkType=' + linkType + ' is not supported');
break;
}
}
function hideprint(items) {
if (!window.print) {
items.filter('.print').parent().hide();
}
}
function shareViaFacebook() {
var url = 'https://www.facebook.com/sharer/sharer.php?u=' + encodeURIComponent(window.location.href);
window.open(url);
}
function shareViaWeibo() {
var title = document.title.trim();
var url = 'http://service.weibo.com/share/share.php?url=' + encodeURIComponent(window.location.href)+'&title='+title ;
window.open(url);
}
function shareViaTwitter(link) {
var remainingCharachters = 280 - 24;
var maxShareTextLength = 100;
var tags = '';
if (!nullOrWhiteSpace(link.attr('sharetext'))) {
var shareText = link.attr('sharetext').trim();
if (shareText.length > maxShareTextLength) {
shareText = shareText.substring(0, maxShareTextLength);
}
remainingCharachters -= shareText.length + 1;
tags = ' ' + shareText;
}
var title = document.title.trim();
if (title.length > remainingCharachters) {
title = title.substring(0, remainingCharachters - 3) + "...";
}
var tweet = title + tags;
var url = 'https://twitter.com/share?text=' + encodeURIComponent(tweet) + '&url=' + encodeURIComponent(window.location.href);
window.open(url);
}
function shareViaLinkedin(link) {
var post = "";
if (!nullOrWhiteSpace(link.attr('additionaltext'))) {
post += '&title=' + encodeURIComponent(link.attr('additionaltext'));
}
if (!nullOrWhiteSpace(link.attr('sharetext'))) {
post += '&summary=' + encodeURIComponent(link.attr('sharetext'));
}
post += '&source=' + encodeURIComponent(document.title) + '&url=' + encodeURIComponent(window.location.href);
var url = 'https://www.linkedin.com/shareArticle?mini=true' + post;
window.open(url);
}
function shareViaGooglePlus() {
var url = 'https://plus.google.com/share?url=' + encodeURIComponent(window.location.href);
window.open(url);
}
function shareViaEmail(link) {
var subject = nullOrWhiteSpace(link.attr('additionaltext')) ? document.title : link.attr('additionaltext');
var body = nullOrWhiteSpace(link.attr('sharetext')) ? window.location.href : link.attr('sharetext') + ' ' + window.location.href;
var url = 'mailto:?subject=' + encodeURIComponent(subject) + '&body=' + encodeURIComponent(body);
window.open(url, "_self");
}
function printPage() {
if (window.print) {
window.print();
}
}
function nullOrWhiteSpace(string) {
if (string != null && string.trim().length > 0) {
return false;
}
return true;
}
</script>
