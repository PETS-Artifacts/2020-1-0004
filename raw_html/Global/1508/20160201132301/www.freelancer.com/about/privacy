<script type="text/javascript">


  (function(d) {
    function get_session_id() {
      function rand_4_char_str() {
        return Math.floor((1 + Math.random()) * 0x10000)
          .toString(16)
          .substring(1);
      }
      var session_id = '';
      // 80 character long session id
      for (var i = 0; i < 20; i++) {
        session_id += rand_4_char_str();
      }
      return session_id;
    }

    window.tags_session_id = localStorage.getItem('tags_session_id');
    if (window.tags_session_id === null) {
      window.tags_session_id = get_session_id();
      localStorage.setItem('tags_session_id', window.tags_session_id);
    }
    var url = 'https://cdn1.f-cdn.com/fp/tags.js?org_id=lar9e74p&session_id=' +
      window.tags_session_id;
    var po = d.createElement('script');
    po.type = 'text/javascript'; po.async = true; po.src = url;
    var s = d.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(po, s);

    // set thm_session in cookie
    var date = new Date();
    date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000));
    var expires = "path=/;expires=" + date.toUTCString();
    d.cookie = "session2=" + window.tags_session_id + "; " + expires;
  })(document);

</script>




