<!-- login/signup modals logic -->
<script type="text/javascript">
    var backToSignupSourcePage = false;
    //set page as sign up source page for future redirection back
    //If no local storage supported, we will ignore this redirection
    if (typeof sessionStorage != 'undefined' && backToSignupSourcePage) {
        sessionStorage.setItem('signupSourcePage', window.location.href);
    }
    require([
        'jquery-wrapper',
        'loginsignup/loginsignup'],
      function(jq, LoginSignup) {
        var login_signup = false;
        
          var loginOptions = {
            isLoginSignup: login_signup
          };
          var signupOptions = {
            isLoginSignup: login_signup
          };
          
          

          var loginCallback = false;
          var employerSignupClbk = false;
          var workerSignupClbk = false;
        
        

        

        

        

        
        
          var target = jq('#default-modal-placeholder');
        

        var loginSignup = new LoginSignup({
          el: target,
          loginClbk: loginCallback,
          employerSignupClbk: employerSignupClbk,
          workerSignupClbk: workerSignupClbk
        });
        jq('#login-normal').click(function(e) {
          e.preventDefault();
          loginSignup.showLogin(loginOptions);
          loginSignup.setBackdrop();
        });
        jq('#sign-up').click(function(e) {
          e.preventDefault();
          loginSignup.showSignup(signupOptions);
          loginSignup.setBackdrop();
        });

        

        
      });
</script>
