<script>
// Facebook Connect and Login are now loaded asynchronously and uses the
// Facebook SDK
window.fbAsyncInit = function () {
    if (!window.fbinit_run) {
        window.fbinit_run = true;
        (typeof FB != 'undefined') && FB.init({
            appId: '120131118061981',
            version: 'v2.2',
            status: true,
            cookie: true,
            xfbml: true,
            channelUrl: 'https://www.freelancer.com/facebook/channel.html',
            oauth: true
        });
    }

    if (typeof fbAsyncInit_func == 'function') {
        fbAsyncInit_func();
    }

    if (typeof fbAsyncInit_pinky == 'function') { //if not defined, do it later
        fbAsyncInit_pinky();
    }
}
</script>


