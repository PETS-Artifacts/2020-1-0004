    // Gets cookie for `event` and sends it if exist
    const gaCookies = document.cookie.split(';');
    for (let cookie of gaCookies) {
        cookie = cookie.trim();
        const [gaKey, gaRawData] = cookie.split('=');
        if (gaKey === 'gaevent' && gaRawData) {
            const gaData = decodeURIComponent(gaRawData);
            const gaEventData = JSON.parse(gaData.replace(/\+/g, ' '));
            gaEventData.forEach(event => {
                event.hitType = 'event';
                ga('send', event);
            });
            document.cookie = 'gaevent=; expires=Thu, 01 Jan 1970 00:00:00 UTC; path=/;'
            break;
        }
    };

    // Send Pageview after everything is set
    ga('send', 'pageview');


</script>
