    <script type="text/javascript">
  (function(d) {
    function get_session_id() {
      function rand_4_char_str() {
        return Math.floor((1 + Math.random()) * 0x10000)
          .toString(16)
          .substring(1);
      }
      var session_id = '';
      // 80 character long session id
      for (var i = 0; i < 20; i++) {
        session_id += rand_4_char_str();
      }
      return session_id;
    }
    // Wrapped setItem() in a function due to empty block statement lint error
    function setSessionIdToLocalStorage(tagsSessionId) {
      try {
        localStorage.setItem('tags_session_id', tagsSessionId);
      } catch (err) { // Safari private browsing mode fix.
        return false;
      }
    }

    window.tags_session_id = localStorage.getItem('tags_session_id');
    if (window.tags_session_id === null) {
      window.tags_session_id = get_session_id();
      setSessionIdToLocalStorage(window.tags_session_id);
    }

    // Do not initialise again if it has.
    var existingElement = document.getElementById('external-threatmetrix');
    if (!existingElement) {
        var arr = window.document.getElementsByName('thm_session');
        for (var i = 0; i < arr.length; i++) {
            arr[i].setAttribute('value', window.tags_session_id);
        }

        var url = 'https://cdn1.f-cdn.com/fp/tags.js?org_id=lar9e74p&session_id=' +
        window.tags_session_id;
        var po = d.createElement('script');
        po.id = 'external-threatmetrix';
        po.type = 'text/javascript'; po.async = true; po.src = url;
        var s = d.getElementsByTagName('script')[0];
        s.parentNode.insertBefore(po, s);

        // set thm_session in cookie
        var date = new Date();
        date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000));
        var expires = "path=/;expires=" + date.toUTCString();
        d.cookie = "session2=" + window.tags_session_id + "; " + expires;
    }

  })(document);
</script>

    
    


