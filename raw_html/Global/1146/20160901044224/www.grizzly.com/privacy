    <script type="text/javascript">
        var dragging = false;
        $(document).ready(function(e) {
            $('img[usemap]').rwdImageMaps();
           
            $("body").on("touchmove", function () {
                dragging = true;
            });
            $("body").on("touchstart", function () {
                dragging = false;
            });

        });
        $(function () {
            $("#searchform1").submit(function () {
                submitSearch($("#searchbox1").val());
                return false;
            });

            $("#searchform2").submit(function () {
                submitSearch($("#searchbox2").val());
                return false;
            });

            $("a#searchform1button").click(function () {
                submitSearch($("#searchbox1").val());
            });

            $("a#searchform2-submit").click(function () {
                submitSearch($("#searchbox2").val());
            });
        });

        function submitSearch(searchTerms) {
            if (encodeURIComponent(searchTerms.trim()).split("%20").length > 1) {
                window.location.href = "/search?q=(" + encodeURIComponent(searchTerms.trim()).split("%20").join("+AND+") + ")+OR+(" + encodeURIComponent(searchTerms.trim()).split("%20").join("+OR+") + ")";
            }
            else {
                window.location.href = "/search?q=(" + searchTerms + ")";
            }
        }
    </script>
