    setTimeout(function(){var clientId = '';    
	try {
      var tracker = ga.getAll()[0];
      clientId = tracker.get('clientId');    
    } catch(e) {
      console.log("Error fetching clientId");
    }
    
	var bqArray = [];  
  	var winningBids = pbjs.getAllWinningBids();
  	var winnerIds = {};    
  	for(var i=0; i < winningBids.length; i++){
      winnerIds[winningBids[i]['adUnitCode']] = winningBids[i]['adId'];
    }  
  	var bidRequestsArray = [];
  	if(Array.isArray(pbjs._bidsRequested) && (pbjs._bidsRequested.length>0)){
	  for(var i=0;i<pbjs._bidsRequested.length;i++){
		if(Array.isArray(pbjs._bidsRequested[i].bids) && (pbjs._bidsRequested[i].bids.length>0)){
		  for(var k=0;k<pbjs._bidsRequested[i].bids.length;k++){
			bidRequestsArray[pbjs._bidsRequested[i].bids[k].bidder+'-'+pbjs._bidsRequested[i].bids[k].placementCode]=[pbjs._bidsRequested[i].bids[k].placementCode, pbjs._bidsRequested[i].bids[k].bidder, pbjs._bidsRequested[i].bidderCode,pbjs._bidsRequested[i].requestId,pbjs._bidsRequested[i].start];
		  }
		}
	  }
	} 	
  	var allResponses = pbjs.getBidResponses();    
    for(var adUnitCode in allResponses){
       var winner = "";
       if(winnerIds.hasOwnProperty(adUnitCode)){
       		winner = winnerIds[adUnitCode];
       }      
      for(var i=0; i<allResponses[adUnitCode]['bids'].length; i++){
        var bid = allResponses[adUnitCode]['bids'][i];
        var adWinner = false;        
        if(bid.adId === winner){
        	adWinner = true;
        }
        bidRequestsArray[bid['bidder']+'-'+bid['adUnitCode']].push(1);   	        
        var bqRow = {
          "bidWinner": adWinner,
          "adId":bid['adId'],
          "adUnitCode":bid['adUnitCode'],
