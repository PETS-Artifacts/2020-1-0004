    function get_proper_slot_name(tmp_slo){
  var proper_slot_name=tmp_slo;
  if(proper_slot_name.indexOf("ad-inarticle-long-btf8") > -1){
    proper_slot_name="ad-inarticle-long-btf8";
  }else if(proper_slot_name.indexOf("ad-inarticle-long-btf7") > -1){
    proper_slot_name="ad-inarticle-long-btf7";
  }else if(proper_slot_name.indexOf("ad-inarticle-long-btf6") > -1){
    proper_slot_name="ad-inarticle-long-btf6";
  }else if(proper_slot_name.indexOf("ad-inarticle-long-btf5") > -1){
    proper_slot_name="ad-inarticle-long-btf5";
  }else if(proper_slot_name.indexOf("ad-inarticle-long-btf4") > -1){
    proper_slot_name="ad-inarticle-long-btf4";
  }else if(proper_slot_name.indexOf("ad-inarticle-long-btf3") > -1){
    proper_slot_name="ad-inarticle-long-btf3";
  }else if(proper_slot_name.indexOf("ad-inarticle-long-btf2") > -1){
    proper_slot_name="ad-inarticle-long-btf2";
  }else if(proper_slot_name.indexOf("ad-inarticle-long-btf1") > -1){
    proper_slot_name="ad-inarticle-long-btf1";
  }else if(proper_slot_name.indexOf("ad-inarticle-350top") > -1){
    proper_slot_name="ad-inarticle-350top";
  }else if(proper_slot_name.indexOf("ad-inarticle-350") > -1){
    proper_slot_name="ad-inarticle-350";
  }else if(proper_slot_name.indexOf("article-box-350top") > -1){
    proper_slot_name="article-box-350top";
  }else if(proper_slot_name.indexOf("article-box-350") > -1){
    proper_slot_name="article-box-350";
  }else if(proper_slot_name.indexOf("ad-leaderboard-mobile") > -1){
    proper_slot_name="ad-leaderboard-mobile";
  }else if(proper_slot_name.indexOf("ad-leaderboard") > -1){
    proper_slot_name="ad-leaderboard";
  }else if(proper_slot_name.indexOf("ad-waterfall6") > -1){
    proper_slot_name="ad-waterfall6";
  }else if(proper_slot_name.indexOf("ad-waterfall5") > -1){
    proper_slot_name="ad-waterfall5";
  }else if(proper_slot_name.indexOf("ad-waterfall4") > -1){
