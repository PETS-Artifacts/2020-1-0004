	}//console.log(apstagSlots);
	if (tmp_ads_app.indexOf('adhesive_oop') > -1) {		
  		apstagSlots.push({		
  			slotID: tmp_ads_app, // example: div-gpt-ad-1505491670016-0		
  			slotName: '/1020897/adhesive-oop', // example:/116082170/A9AdUnit		
  			sizes: [[320, 50]]		
  		});		
  	}
	return apstagSlots;
}

var headerBidderBack = [];
function headerBidderBackInitialize(tmp_ads_app) {
	headerBidderBack[tmp_ads_app] = [];
	headerBidderBack[tmp_ads_app]['called_to_render'] = false;
	headerBidderBack[tmp_ads_app]['prebid'] = false;
	headerBidderBack[tmp_ads_app]['a9'] = false;
}
function headerBidderBackMethod(bidder, tmp_ads_app) {
        headerBidderBack[tmp_ads_app][bidder] = true;
        if((headerBidderBack[tmp_ads_app]['prebid'] === true) && (headerBidderBack[tmp_ads_app]['a9'] === true)){
		  eval(('showSlot_' + (tmp_ads_app.replace(/-/g, '_') + '_prebid')))(tmp_ads_app);
		}
    }
function allowAmazonHeaderBidder(tmp_ads_app){
	var amazon_allowed_slots = ['ad-inarticle-350', 'ad-inarticle-350top', 'article-box-350top', 'article-box-350', 'ad-inarticle-long-btf1', 'ad-inarticle-long-btf2', 'ad-inarticle-long-btf3', 'ad-inarticle-long-btf4', 'ad-inarticle-long-btf5', 'ad-inarticle-long-btf6', 'ad-inarticle-long-btf7', 'ad-inarticle-long-btf8', 'ad-list-inimage1', 'ad-list-inimage2', 'ad-list-inimage3', 'ad-list-inimage4', 'ad-list-inimage5', 'ad-list-inimage6', 'ad-waterfall1', 'ad-waterfall2', 'ad-waterfall3', 'ad-waterfall4', 'ad-oop'];
	if(amazon_allowed_slots.indexOf(get_proper_slot_name(tmp_ads_app)) > -1){
	  var tmp_ads_app_split = tmp_ads_app.split('-');
	  var curr_nid='a0';
	  if((tmp_ads_app.indexOf('ad-inarticle-long-btf1') > -1) || (tmp_ads_app.indexOf('ad-inarticle-long-btf2') > -1) || (tmp_ads_app.indexOf('ad-inarticle-long-btf3') > -1) || (tmp_ads_app.indexOf('ad-inarticle-long-btf4') > -1) || (tmp_ads_app.indexOf('ad-inarticle-long-btf5') > -1) || (tmp_ads_app.indexOf('ad-inarticle-long-btf6') > -1) || (tmp_ads_app.indexOf('ad-inarticle-long-btf7') > -1) || (tmp_ads_app.indexOf('ad-inarticle-long-btf8') > -1)){
		  if(tmp_ads_app_split.length > 4){
			curr_nid='a'+tmp_ads_app_split[4];
		  }
	  }else if((tmp_ads_app.indexOf('ad-waterfall') > -1)){
		  if(tmp_ads_app_split.length > 2){
			curr_nid='a'+tmp_ads_app_split[2];
		  }
	  }else if((tmp_ads_app.indexOf('adhesive_oop') > -1)){		
  		  if(tmp_ads_app_split.length > 1){		
  			curr_nid='a'+tmp_ads_app_split[1];		
  		  }
	  }else{
		  if(tmp_ads_app_split.length > 3){
			curr_nid='a'+tmp_ads_app_split[3];
		  }
      }
	  
	  if((typeof amazon_allowed_nid[curr_nid] !== 'undefined')) {
	    return true;
      }else{
        return false;
	  }	  
	}else{
	  return false;
	}
