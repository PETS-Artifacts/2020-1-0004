    var isInViewport = function (elem,tmp_ads_app) {
		if((tmp_ads_app.indexOf("adhesive_oop") > -1) && (adAdhesive_1.indexOf(tmp_ads_app) > -1)){return true;}
		var bounding = elem.getBoundingClientRect();
		var boundary_top_operator = "bounding.top >= 0";
		if((tmp_ads_app == "ad-list-inimage1") || (tmp_ads_app == "ad-list-inimage2") || (tmp_ads_app == "ad-list-inimage3") || (tmp_ads_app == "ad-list-inimage4") || (tmp_ads_app == "ad-list-inimage5") || (tmp_ads_app == "ad-list-inimage6") || (tmp_ads_app == "ad-waterfall1")  || (tmp_ads_app == "ad-waterfall2")  || (tmp_ads_app == "ad-waterfall3")  || (tmp_ads_app == "ad-waterfall4")  || (tmp_ads_app == "ad-waterfall5")  || (tmp_ads_app == "ad-waterfall6")){
		  boundary_top_operator = "bounding.top > 0";
		}
		if(adSlotsRenderedArray.indexOf(tmp_ads_app) > -1){
			return (
				isYTAdActive == 1 &&
				eval(boundary_top_operator) &&
				//bounding.left >= 0 &&
				bounding.bottom <= ((window.innerHeight || document.documentElement.clientHeight)) //&&
				//bounding.right <= (window.innerWidth || document.documentElement.clientWidth)
			);
		}else{
			return (
				isYTAdActive == 1 &&
				eval(boundary_top_operator) &&
				//bounding.left >= 0 &&
				bounding.bottom <= ((window.innerHeight || document.documentElement.clientHeight)+525) //&&
				//bounding.right <= (window.innerWidth || document.documentElement.clientWidth)
			);
		}		
