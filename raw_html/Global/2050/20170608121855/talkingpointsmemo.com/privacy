  googletag.cmd.push(function () {
    cosmjs.cmd.push(function () {
      var asyncAdUnits = preloadStore.get('adUnits');
      var targeting = preloadStore.get('adTargeting');
  
      function setUpAdUnit(adUnit) {
        var dimensions = adUnit.dimensions;
        if (1 === dimensions.length)
          dimensions = dimensions[0];
        var slot = googletag.defineSlot(adUnit.path, dimensions, 'ad-unit-' + adUnit.name);
  
        slot.addService(googletag.pubads());
        
        for (var key in targeting) {
          slot.setTargeting(key, targeting[key]);
        }
      }
  
      asyncAdUnits.forEach(setUpAdUnit);
  
      preloadStore.get('specialAdUnits').forEach(setUpAdUnit);
  
      googletag.pubads().disableInitialLoad();
      googletag.enableServices();
  
      cosmjs.init({setupSlots: false});
    });
  });
