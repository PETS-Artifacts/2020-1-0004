        var n = Math.floor(Math.random() * (10000 - 1 + 1)) + 1;
        var bccbId = "live-chat-btn-"+n; document.write(unescape('%3Cdiv id=' + bccbId + '%3E%3C/div%3E'));
        window._bcvma = window._bcvma || [];
        _bcvma.push(["setAccountID", "3382901478003124227"]);
        _bcvma.push(["setParameter", "WebsiteID", "1747722814601271287"]);
        _bcvma.push(["addStatic", {type: "chat", bdid: "4456396031336838878", id: bccbId}]);
        var bcLoad = function(){
			
            if(window.bcLoaded) return; window.bcLoaded = true;
            var vms = document.createElement("script"); vms.type = "text/javascript"; vms.async = true;
            vms.src = ('https:'==document.location.protocol?'https://':'http://') + "vmss.boldchat.com/aid/3382901478003124227/bc.vms4/vms.js";
            var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(vms, s);
			
			$(document).on('click', '.bc-headbtn-close-icon, .bc-close-chatwindow', function(){
				$('#live_chat_container').show();
			})
			
			$(document).on('click', '.bc-headbtn-minimize, .bc-headbtn-minimize-icon', function(){
				$('#live_chat_container').hide();
			})
			
            var loaded_chat = false;
            
            $('body').on('DOMSubtreeModified', '#bc-chat-container', function() {
                if($('#bc-chat-container').length && loaded_chat != true){
                    loaded_chat = true;
                    $('#live_chat_container').hide();
                }
            })
            
            
			
        };
        if(window.pageViewer && pageViewer.load) pageViewer.load();
        else if(document.readyState=="complete") bcLoad();
        else if(window.addEventListener) window.addEventListener('load', bcLoad, false);
        else window.attachEvent('onload', bcLoad);
		
		
		$("#live_chat_container").click(function(){
				$('#live_chat_container').hide();
        });
        
    </script>
