}

var products = new Array();
	var microsoftDate = new Date();
	var microsoftstartdate = new Date();
	
	if(microsoftDate >= microsoftstartdate.setFullYear(2017,10,07) && microsoftDate <= microsoftstartdate.setFullYear(2017,10,09)){
		products = ['116111', '116114', '116113', '116112', '116121', '116118', '116117'];
	}
	
    if($('.product_page').length){
		var product_id = $('#product_id').val();
	    if($.inArray(product_id, products) > -1){
		    $('a img[src*="authorized_btn.png"]').unwrap();
	    }
	}
	
	if($('.category_page').length || $('.search_page').length){
		$('.category_list li').each(function(){
			var list_product_id = $('.productPageLink', this).attr('data-productid');
			if($.inArray(list_product_id, products) > -1){
		        $('a img[src*="authorized_btn.png"]', this).unwrap();
	        }
		})
	}

