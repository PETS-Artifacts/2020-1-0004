<script type="text/javascript">
  var pathname = window.location.pathname;
  $('.topsubtabs a').each(function(){
    if(pathname.startsWith($(this).attr('href')) && pathname!='/'){
      $(this).parents('ul').show();
    }
    if($(this).attr('href') == pathname){
      $(this).parents('li').addClass('current');
    }
  });
</script>

<script type="text/javascript">
  $('.topdropdown select').bind('change', function(){
    var opt = $("select option:selected");
    if(opt.attr('value').length > 0) {
      document.location.href = opt.attr('value');
    }
  });
</script>
