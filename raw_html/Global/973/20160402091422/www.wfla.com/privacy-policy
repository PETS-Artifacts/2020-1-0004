		<script type="text/javascript">
		var s_account="lintvwfla,dpsglobal";
		var s=s_gi(s_account);

		/************************** CONFIG SECTION **************************/
		/* You may add or alter any code config here. */
		// s.debugTracking=true;
		s.visitorNamespace="lintv";

		s.trackDownloadLinks=true;
		s.trackExternalLinks=true;
		s.trackInlineStats=true;
		s.linkDownloadFileTypes="exe,zip,wav,mp3,mov,mpg,avi,wmv,pdf,doc,docx,xls,xlsx,ppt,pptx";
		s.linkInternalFilters="javascript:wfla.com,interactives.wfla.com";
		s.linkLeaveQueryString=true;
		s.linkTrackVars="None";
		s.linkTrackEvents="None";
		s.dstStart="3/13/2016";
		s.dstEnd="11/6/2016";
		s.currentYear="2016";
		s.linGetScreenSize = new Function( "return linScreens.getScreenSize()" );
		s.linGetRenderedExperience = new Function( "return linScreens.getExperience( window.innerWidth );" );
		s.linGetLayoutName = new Function( "return linScreens.getLayout( window.innerWidth ); ");

		s.usePlugins=true;
		function s_doPlugins(s) {

			// use implementation plug-ins that are defined below
			// in this section. For example, if you copied the append
			// list plug-in code below, you could call:
			// s.events=s.apl(s.events,"event1",",",1);
			
			s.pageName='/privacy-policy/';
			s.eVar1='';
			s.eVar2='';
			s.eVar3='';
			s.eVar4='';
			s.eVar12=s.getNewRepeat(30,'s_getNewRepeat');
			s.eVar17=s.getTimeParting('h','-5');
			s.eVar18=s.getTimeParting('d','-5');
			s.eVar19=s.getTimeParting('w','-5');
			s.eVar20=s.getVisitNum();
			s.eVar21=s.getDaysSinceLastVisit('s_dayslastvisit');
			s.eVar41='/privacy-policy/';
			s.eVar42='';
			s.eVar55='http://wfla.com/privacy-policy/';
			s.eVar56=s.linGetScreenSize();
			s.eVar57=s.linGetRenderedExperience();
			s.eVar58=s.linGetLayoutName();
			s.prop1='';
			s.prop2='';
			s.prop3='';
			s.prop4='';
			s.prop12=s.getNewRepeat(30,'s_getNewRepeat');
			s.prop17=s.getTimeParting('h','-5');
			s.prop18=s.getTimeParting('d','-5');
			s.prop19=s.getTimeParting('w','-5');
			s.prop20=s.getVisitNum();
			s.prop21=s.getDaysSinceLastVisit('s_dayslastvisit');
			s.prop41='/privacy-policy/';
			s.prop42='';
			s.prop55='http://wfla.com/privacy-policy/';
			s.prop56='http://wfla.com/privacy-policy/';
			s.prop57=s.linGetRenderedExperience();
			s.prop58=s.linGetLayoutName();
			s.hier1='/privacy-policy/';		}
		s.doPlugins=s_doPlugins;

		s.trackingServer="lintv.122.2o7.net";

		/************************** PLUGINS SECTION *************************/
		s = lin_aa_plugins(s);

		//Instantiating the code
		var lintvDil = DIL.create({
		    partner: 'lintv',
		    uuidCookie:{
		      name:'aam_uuid',
		      days:30
		    }
		  });

		//SC Data collection
		var _scDilObj = s_gi(s_account);
		DIL.modules.siteCatalyst.init(_scDilObj, lintvDil, {
		names: ['pageName', 'channel', 'campaign', 'products', 'events', 'pe', 'referrer', 'server', 'purchaseID', 'zip', 'state'],
		        	iteratedNames: [{
		               name: 'eVar',
		               maxIndex: 75
		        	}, {
		               name: 'prop',
		               maxIndex: 75
		        	}, {
		               name: 'pev',
		               maxIndex: 3
		        	}, {
		               name: 'hier',
		               maxIndex: 4
		        	}]
		});
		//Decompose URL information and pass it to AAM
		function objIsEmpty(obj) {
		    for(var prop in obj) {
		        if(obj.hasOwnProperty(prop) && prop !== "")
		                return false;
		    }
		        return true;
		 }
		var uriData = DIL.tools.decomposeURI(document.URL);
		delete uriData.search;
		delete uriData.href;
		if(! objIsEmpty(uriData.uriParams)){
		      lintvDil.api.signals(uriData.uriParams, 'c_');
		};
		delete uriData.uriParams;
		lintvDil.api.signals(uriData, 'c_').submit();

		/****************************** MODULES *****************************/
		s = lin_aa_modules(s);
		s.loadModule("Media");
		s.Media.autoTrack=false;
		s.Media.trackWhilePlaying = true;
		s.trackMilestones = "25,50,75";
		s.Media.trackVars="prop29,prop30,prop31,prop32,eVar17,eVar18,eVar19";
		s.Media.trackEvents="None";
		s.loadModule("Integrate");

		var s_code=s.t();if(s_code)document.write(s_code);
		</script>
		


