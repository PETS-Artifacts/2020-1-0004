<script>
function siteSearchClick()
{
	var f = document.forms["site_search_form"];
	if ( (isEmpty(f.searchKeyTB.value) || f.searchKeyTB.value.toLowerCase()=="search") && (f.searchOptionDD.value == "All" || f.searchOptionDD.value == "health_topics"))
	{
		alert("Search keyword required.");
		return false;
	}
	else
	{
		if (f.searchKeyTB.value == "Search")
		{
			alert("Please provide search keyword");
			return;
		}
		var search_url = "/site-search?key="+f.searchKeyTB.value+"&type=";
		
		var pn_cnt=0;
		if (f.searchOptionDD.value == "All")
		{
		  search_url += "physician,spe,news,event,location,gen";
		  pn_cnt=5;
		}
		else if (f.searchOptionDD.value == "doctor")
		{
		  search_url += "physician";
		  pn_cnt++;
		}
		else if (f.searchOptionDD.value == "medical_specialty")
		{
		  search_url += "spe";
		  pn_cnt++;
		}
		else if (f.searchOptionDD.value == "news")
		{
		  search_url += "news";
		  pn_cnt++;
		}
		else if (f.searchOptionDD.value == "event")
		{
		  search_url += "event";
		  pn_cnt++;
		}
		else if (f.searchOptionDD.value == "location")
		{
		  search_url += "location";
		  pn_cnt++;
		}																																																																																																																																				
		
		if (pn_cnt>1)
			window.location=search_url+"&cata=1";
		else if (pn_cnt==1)
			window.location=search_url+"&cata=1&pn=1";
			
		
	}
}

function isEmpty(str)
{
    for(var intLoop=0; intLoop<str.length; intLoop++)
      if(str.charAt(intLoop)!=" ")
        return false;
    return true;
}
</script>

