<!--//--><![CDATA[//><!--

	function mycarousel_itemVisibleInCallback(carousel, listitem, list_pos, c_action) {
		var img_total = jQuery('a.slide-nav').length;
		var img_current = (list_pos - 1) % img_total;
		jQuery('a.slide-nav').removeClass('on');
		jQuery('a.slide-nav:eq(' + img_current + ')').addClass('on');
	}
	
	function mycarousel_initCallback(carousel) {
		jQuery('.jcarousel-item').mouseover( function() {
			jQuery('#mycarousel').jcarousel('pauseAuto');
		});
		
		jQuery('.jcarousel-item').mouseout( function() {
			jQuery('#mycarousel').jcarousel('startAuto');
		});
		
		jQuery('.slide-nav').eq(0).addClass('on');
		
		jQuery('.slide-nav').bind('click', function(e) {
			e.preventDefault();
			
			var context = jQuery(this);
			jQuery('.slide-nav.on').removeClass('on');
			context.addClass('on');
			
			carousel.scroll(jQuery.jcarousel.intval(context.text()));
			return false;
			});
		
		jQuery('.slide-next').bind('click', function(e) {
			e.preventDefault();
			
			var img_total = jQuery('a.slide-nav').length;
			var img_current = parseInt(jQuery('#slide-nav a.on').text()) - 1;
			var next = (img_current + 1) % img_total;
				
			jQuery('.slide-nav').eq(next).trigger('click');
			
			//carousel.scroll(jQuery.jcarousel.intval(next));
			return true;
			});
			
		jQuery('.slide-back').bind('click', function(e) {
			e.preventDefault();
			
			var img_total = jQuery('a.slide-nav').length;
			var img_current = parseInt(jQuery('#slide-nav a.on').text()) - 1;
			var back = (img_current + img_total - 1) % img_total;

			jQuery('.slide-nav').eq(back).trigger('click');
			
			//carousel.scroll(jQuery.jcarousel.intval(back));
			return false;
			}); 
		};
		
	jQuery(window).load(function(){
		var img_total = jQuery('a.slide-nav').length;
		if (img_total == 1) {
			var c = jQuery('#mycarousel').jcarousel({
				scroll: 1
			});
		} else {
			var c = jQuery('#mycarousel').jcarousel({
				scroll: 1,
				wrap:'circular',
				initCallback: mycarousel_initCallback,
				itemVisibleInCallback: mycarousel_itemVisibleInCallback
			});
		}
		});
	
