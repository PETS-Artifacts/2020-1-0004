<script type="text/javascript">
  $( document ).ready(function() {
    updateSidebarHeight();
  });
  $( window ).resize(function() {
    updateSidebarHeight();
  });
  function updateSidebarHeight() {
    if ($(window).width() > 768) {
      $("#sidebar").addClass("placeholder-side-banner");
      $("#dose").addClass("placeholder-side-banner");
      $(".sidebar.col-sm-4").height($("#primary-section.col-sm-8").height());
    } else {
      $("#sidebar").removeClass("placeholder-side-banner");
      $("#dose").removeClass("placeholder-side-banner");
      $(".sidebar.col-sm-4").css({"height":"auto"});
    };
  }
</script>
