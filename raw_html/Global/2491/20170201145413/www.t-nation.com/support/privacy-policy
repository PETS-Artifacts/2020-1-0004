<script>
$(document).ready(function(){
  //ol_container will hold all the instances of ol tags and their children
  var ol_container = document.getElementsByTagName('OL');
  //loop goes through the ol_container array one at a time
  for(var k=0; k < ol_container.length; k++){
    //some ordered lists start at one, in others the start is specified
    var start_num = ol_container[k].start;
    //li_item holds all the li tags
    var li_item = ol_container[k].getElementsByTagName('LI');
    //oop through array of li_item
    for(var i=0; i < li_item.length; i++) {
      //a class is added if the ordered list has 10 or higher to adjust spacing
      if (i + start_num > 9) {
        li_item[i].className = "ol-offset";
      }
    }
  }
});
</script>


