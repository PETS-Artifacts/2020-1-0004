                          &#8220;<strong data-bind="text: $parent.query"></strong>&#8221;
                        </a>
                      </li>
                      <!-- ko foreach: {data: space.suggestions, as: 'suggestionType'} -->
                        <!-- ko foreach: {data: $parents[1].suggestionData()[suggestionType], as: 'suggestion'} -->
                          <li>
                            <a ref="#" class="js-suggestion"
                               data-bind="attr: {'data-suggestion-destination': suggestion.path, 'data-suggestion-search-space-or-path': 'path'},
                                          click: function(){$parents[2].searchInterfaceSubmit({destination: suggestion.path, searchSpaceOrPath: 'path'})}">
                              <span data-bind="html: $parents[2].highlight(suggestion.name.trim())"></span>
                              <span class="search-context" data-bind="text: 'in ' + suggestion.bucket"></span>
                            </a>
                          </li>
                        <!-- /ko -->
                      <!-- /ko -->
                    </ul>
                  </div>
                <!-- /ko -->
