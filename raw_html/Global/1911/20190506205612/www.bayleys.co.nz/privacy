    <script type="text/javascript">
        $(window).on('load', function () {
            if (window.location.href.endsWith("waterfront")) {
                if (document.cookie.indexOf("ModalShown=true") < 0) {
                    var delayInMilliseconds = 3000;

                    setTimeout(function () {
                        document.getElementById("subscribeButton").click();
                    }, delayInMilliseconds);

                };

                $("#modalClose").click(function () {
                    var today = new Date();
                    var expire = new Date();
                    expire.setTime(today.getTime() + 3600000 * 24 * 14);
                    document.cookie = "ModalShown=true; expires=" + expire.toGMTString();
                });
            } 
        });
    </script>

        <script type="text/javascript">
            (function () {
                var didInit = false;
                function initMunchkin() {
                    if (didInit === false) {
                        didInit = true;
                        Munchkin.init('530-IIW-969');
                    }
                }
                var s = document.createElement('script');
                s.type = 'text/javascript';
                s.async = true;
                s.src = '//munchkin.marketo.net/munchkin.js';
                s.onreadystatechange = function () {
                    if (this.readyState == 'complete' || this.readyState == 'loaded') {
                        initMunchkin();
                    }
                };
                s.onload = initMunchkin;
                document.getElementsByTagName('head')[0].appendChild(s);
            })();
        </script>
        <script type="text/javascript">
            var enquiryCallback = function (response) {
                $('#EnquiryRecaptchaResponse').val(response);
                $('#enquiryCaptchaError').addClass('hide');
            };

            var shareCallback = function (response) {
                $('#ShareRecaptchaResponse').val(response);
                $('#shareCaptchaError').addClass('hide');
            };

            var onloadCallback = function () {
                $.fn.modal.Constructor.prototype.enforceFocus = function () { };
                siteEnquiryCaptcha = grecaptcha.render('siteEnquiryCaptcha', {
                    'sitekey': '6LfLgRkTAAAAAJc4h0SqNIcxmzHJiFpgWUIS8Mpn',
                    'callback': enquiryCallback,
                });
                if ($('#shareCaptcha').length > 0) {
                    shareCaptcha = grecaptcha.render('shareCaptcha', {
                        'sitekey': '6LfLgRkTAAAAAJc4h0SqNIcxmzHJiFpgWUIS8Mpn',
                        'callback': shareCallback,
                    });
                }
            }
        </script>
