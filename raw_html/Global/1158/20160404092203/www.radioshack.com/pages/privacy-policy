  <script src="//cdn.shopify.com/s/files/1/0953/5270/t/7/assets/stores.js?4732669296768222337" type="text/javascript"></script>
  <script src="//cdn.shopify.com/s/files/1/0953/5270/t/7/assets/vendor.js?4732669296768222337" type="text/javascript"></script>
  <script src="//cdn.shopify.com/s/files/1/0953/5270/t/7/assets/district.js?4732669296768222337" type="text/javascript"></script>
  
  <script>$(function() {
  // display original SKU
  $('*[data-sku]').each(function(i, element) {
    var $this = $(element);
    var $skuValueContainer = $this.find('.sku-value');

    $skuValueContainer.text($this.attr('data-sku').split('@').shift());
  });

  // show variant title in the cart
  $('.cart-item-title .variant').each(function(i, element) {
    var $this = $(element);
    var data = $this.data();
    var variantTitle = getVariantTitle(data);

    $this.html(variantTitle);
  });

  // show variant title in checkout
  $('.product-table .product').each(function(i, element) {
    var $this = $(element);
    var variantId = parseInt($this.data('variantId') || '0') || 0;
    var $variantContainer = $this.find('.product__description__variant');
    var data = {
      variantTitle: $variantContainer.text()
    };

    for(var i = 0; i < checkoutItemsSKUs.length; i++) {
      if(checkoutItemsSKUs[i].variant_id === variantId) {
        data.option1Value = checkoutItemsSKUs[i].option1;
        data.option2Value = checkoutItemsSKUs[i].option2;
        data.option3Value = checkoutItemsSKUs[i].option3;

        data.compare_at_price = checkoutItemsSKUs[i].compare_at_price;

        for(var j = 0; j < checkoutItemsSKUs[i].options.length; j++) {
          data['option' + (j + 1)] = checkoutItemsSKUs[i].options[j];
        }

        break;
      }
    }
    
    if(data.option1Value === 'Ship' || data.option1Value === 'Default Title') {
      return;
    }

    var variantTitle = getVariantTitle(data);
    var quantity = parseInt($this.find('.product__quantity').text()) || 1;

    $this.find('.product__price span').text('$'
      + (data.compare_at_price * quantity / 100).toFixed(2));
    $this.find('.product__price').append('<span class="small-price-note">to pay in store</span>');

    $variantContainer.html(variantTitle).show();
  });

  // 1) in the cart we need to compile inventory available
  //  per SKU, per Destination - done
  // 2) check if any of the products in the cart now is over limit
  //  considering that stores destinations pick up windows share the inventory
  // 3) change quantity of products in cart to fit in the limit
  var needCartSubmit = false;
  for(var i = 0; i < checkoutItemsSKUs.length; i++) {
    if(checkoutItemsSKUs[i].options[0] !== 'Pick Up Store') {
      continue;
    }

    var originalSku = checkoutItemsSKUs[i].sku.split('@').shift();
    var destination = checkoutItemsSKUs[i].option1;

    if(skuDestinationQuantity[originalSku][destination] <= skuDestinationInventory[originalSku][destination]) { // 1 <= 11
      continue;
    }

    var newQuantity = checkoutItemsSKUs[i].quantity // 1
      - (skuDestinationQuantity[originalSku][destination] - skuDestinationInventory[originalSku][destination]); // - (1 - 11) = 11

    if(newQuantity < 0) {
      newQuantity = 0;
    }

    skuDestinationQuantity[originalSku][destination] -= checkoutItemsSKUs[i].quantity - newQuantity;

    checkoutItemsSKUs[i].newQuantity = newQuantity;
    $('.cart-form #updates_' + checkoutItemsSKUs[i].variant_id).val(newQuantity);
    needCartSubmit = true;
  }
  // make request to update the cart
  if(needCartSubmit) {
    if(document.location.pathname === '/cart') {
      $('input.button[name="update"]').click();
    } else if($('body.checkout-body').length) {
      document.location = $('.breadcrumb__link[href*="/cart"]').attr('href');
    }
  }
});</script>
