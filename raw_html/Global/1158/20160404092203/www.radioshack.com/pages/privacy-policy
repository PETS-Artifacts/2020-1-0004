<script>
function getBackendHost() {
  var backend_host = 'radioshack-ecom.prod.bbhosted.com';
  if(window.Shopify) {
    if(Shopify.theme && Shopify.theme.name.indexOf('Dev') > -1) {
      backend_host = 'localhost:4001';
    } else if(Shopify.theme && Shopify.theme.name.indexOf('UAT') > -1) {
      backend_host = 'radioshack-ecom.uat.bbhosted.com';
    } else if(window.location.host.indexOf('-uat') > -1) {
      backend_host = 'radioshack-ecom.uat.bbhosted.com';
    }
  } else {
    if(window.location.host.indexOf('-uat') > -1) {
      backend_host = 'radioshack-ecom.uat.bbhosted.com';
    }
  }

  return backend_host;
}
</script>

