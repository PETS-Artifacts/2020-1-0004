    $('#compare .typeahead').typeahead({ minLength: 2 }, {
        display: 'name',
        source: iecUniverse
    })
    $('#compare .typeahead').bind('typeahead:select', function (ev, data) {
        console.log('Typeahead: ', JSON.stringify(data));
        SetComparison(data.url);
    });
</script>
    
