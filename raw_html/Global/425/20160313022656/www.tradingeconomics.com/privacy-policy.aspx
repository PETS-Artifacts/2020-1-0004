<script>
    var iecUniverse = new Bloodhound({
        datumTokenizer: Bloodhound.tokenizers.obj.whitespace('name'),
        queryTokenizer: Bloodhound.tokenizers.whitespace,
        remote: {
            url: 'http://autocomplete.ieconomics.com/src/iecac/%QUERY?tab=te-charts&t=te',
            wildcard: '%QUERY'
        }
