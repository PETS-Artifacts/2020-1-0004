<script type="text/javascript">
    $(document).ready(function() {
        if (window.bvProdArr) {
            $BV.ui( 'rr', 'inline_ratings', window.bvProdArr);
        }

        //create list of BV elements
        var bvElements = $("[id^=BVRRInlineRating-]");
        var bvIdSet = {};
        var ctr = 0;
        var dupCtr = 0;
        $.each(bvElements, function() {
            var divId = $(this).attr("id");
            if (!(divId in bvIdSet)) {
                bvIdSet[divId] = divId;
                ctr++;
            } else {
                dupCtr++
            }
        });
        //only do this whole thing if there are duplicates on the page
        if (bvIdSet && ctr > 0 && dupCtr > 0) {
            //timeout is here to ensure that copying of the bazaar voice stars only happen after everything has loaded
            setTimeout(function() {
                $.each(bvIdSet, function(i, v) {
                    var divId = v;
                    var divArr = $("[id=" + divId + "]");
                    if (divArr && divArr.length > 1) {
                        var src = divArr[0];
                        var cloneSrc = null;
                        for (i = 1; i < divArr.length; i++) {
                            cloneSrc = $(src).clone();
                            var target = $(divArr[i]);
                            target.replaceWith(cloneSrc);
                        }
                    }
                });
            }, 1000);
        }
    });
</script><!--  end BV API call -->

	</body>

</html>
