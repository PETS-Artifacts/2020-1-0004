	<script type="text/javascript">
window.dataLayer=window.dataLayer||[];

var entityMap = {
   "&": "&amp;",
   "<": "&lt;",
   ">": "&gt;",
   '"': '&quot;',
   "'": '&#39;'
 };

 function escapeHtml(string) {
   return String(string).replace(/[&<>"']/g, function (s) {
     return entityMap[s];
   });
 }

var initArr={
		'event':'dataLayer-initialized',
		'page':{
			'category':'',//page category
			'type':'Content Page 1 Template'//page type
		},
		'region':'AAC',//region
		'country':'US',//country
		'brand':'ASICS', //brand
		'language':'en-us',//language	
		'user':{
			'id':'not logged in',//id
			
			'loggedin':'false',//not loggedin
				
            'hashId' : ''
		}
};

		window.dataLayer.push(initArr);
		var fullUrl = window.location.href;
		var pattern = new RegExp("(https?\://.+)(/../..\-..)(/.+)");
		var urlPrefix = "";
		var res = pattern.exec(fullUrl);
		if (res && res.length > 0) {
			urlPrefix = res[1];
			urlPrefix = escapeHtml(urlPrefix);
		}

		

function trackAddToCart_google(productCode, quantityAdded) {
	var prodArr = parseProductId(productCode);
	if (prodArr == null) return;
	dataLayer.push({
		'event':'add-to-cart',
		'productId':prodArr.productCodeColor,
		'full-id':productCode,
		'quantity':Number(quantityAdded)
	});
}

function trackUpdateCart_google(productCode, initialQuantity, newQuantity) {
	if (initialQuantity != newQuantity) {
		if (initialQuantity > newQuantity) {
			trackRemoveFromCart_google(productCode, initialQuantity - newQuantity);
		} else {
			trackAddToCart_google(productCode, newQuantity - initialQuantity);
		}
	}
}

function trackRemoveFromCart_google(productCode, initialQuantity) {
	var prodArr = parseProductId(productCode);
	if (prodArr == null) return;

	dataLayer.push({
		'event':'remove-from-cart',
		'productId':prodArr.productCodeColor,
		'full-id':productCode,
		'quantity':Number(initialQuantity)
	});
}

function parseProductId(fullId) {
	//use regex to split color with size
	var sizeVarRegExp = /(\d+\.[^\.]{1,4})\.([A-Z])\.(\w+)/g;
	var match = sizeVarRegExp.exec(fullId);
	if (match == null || match.length < 4) {
		return [];
	}
	var prodArr = [];
	prodArr.productCodeColor = match[1];
	prodArr.productWidth = match[2];
	prodArr.productSize = match[3];
	
	return prodArr;
}
</script>
