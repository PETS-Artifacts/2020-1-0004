                    <script type="text/javascript">
                      $(document).ready(function() {

                        $('#mobileSiteSearchbox').keypress(function(event) {

                          if (event.keyCode == 13) {
                            var searchtxt = $("#mobileSiteSearchbox").val();
                            var searchUrl = $("#mobilesitesearchurl").val();
                            //alert ('button clicked'+searchtxt);    
                            window.location.href = searchUrl + '?keyword=' + encodeURIComponent(searchtxt);
                            return false;
                          }
                        });

                      });
                    </script>




