<script>
function checkEnter(e, resultPage, isModal, sticky) {
			var characterCode;
			 isModal = typeof(isModal) !== 'undefined' ? isModal : false;
			sticky = typeof(sticky) !== 'undefined' ? sticky : false;
			resultPage = typeof(resultPage) !== 'undefined' ? resultPage : false;
			if (e && e.which) {
				e = e;
				characterCode = e.which;
			} else {
				e = event;
				characterCode = e.keyCode;
			}
			var searchTxt = "";
			if (resultPage.id === "searchForm1") {
				searchTxt = document.getElementById("mainSearch").value;
			}
			else if(resultPage.id === "searchOne") {
				 searchTxt = document.getElementById("searchModaltxt").value;
			}
			else {
				searchTxt = document.getElementById("searchtxt").value;
			}
			if (characterCode == 13) {
			   return validateChangeSearchURL(resultPage, isModal, sticky);
			} else {
				return true;
			}
		};
function validateForm(form){
								var searchVar = jQuery.trim(document.forms["searchForm"]["keyword"].value);
				if(searchVar == undefined || searchVar == ""){
					alert("Please enter a keyword to search");
					return false;
				}
								document.getElementById("keyword").style.borderColor="#c1c1c1";
								document.getElementById("keyword").value = searchVar;
						return true;
}
function validateSearchForm(form){
								var searchVar = jQuery.trim(document.forms["searchForm1"]["mainSearch"].value);
				if(searchVar == undefined || searchVar == ""){
					alert("Please enter a keyword to search");
					return false;
				}
								document.getElementById("mainSearch").style.borderColor="#c1c1c1";
								document.getElementById("mainSearch").value = searchVar;
						return true;
}
function srchValidate(resultPage, isModal, sticky){
	var searchInput = "";
	var divError = "";
	 isModal = typeof(isModal) !== 'undefined' ? isModal : false;
			sticky = typeof(sticky) !== 'undefined' ? sticky : false;
			resultPage = typeof(resultPage) !== 'undefined' ? resultPage : false;
			 if (isModal && !sticky) {

				var txt = document.getElementById("searchMobiletxt").value;
				//document.getElementById("searchModaltxt").value = txt.replace(/[^a-zA-Z0-9\@\#\%\/\$\&\-\"\.\,\'\s]/g, ''); //Remove any special characters
				var searchTxt = document.getElementById("searchMobiletxt");
				var divError = document.getElementById("dvModalError");
				}
		else if(isModal.id === "searchForm1")
		{
			var txt = document.getElementById("mainSearch").value;
			var searchTxt = document.getElementById("mainSearch");
			 var divError = document.getElementById("dvErrorSearch");

		}
		else if(isModal.id === "searchOne")
		{
			 var txt = document.getElementById("searchModaltxt").value;
			 var searchTxt = document.getElementById("searchModaltxt");
			 var divError = document.getElementById("dvModalError");

		}
		else {
			var txt = document.getElementById("searchtxt").value;
			 var searchTxt = document.getElementById("searchtxt");
			 var divError = document.getElementById("dvError");
		}

		if (txt.trim() == "" || txt.trim().length < 2) {
			 var errorHTML = "<div class='alert alert-warning'>Please enter search terms.</div>";
			  if (resultPage && resultPage !== "all-main") {
					dvError[0].innerHTML = errorHTML;
				} else {
					dvError.innerHTML = errorHTML;
				}
				 document.getElementById("searchModaltxt").value = "";
					if(isModal.id === "searchForm1"){
					document.getElementById("mainSearch").value = "";
					}
					else if(isModal.id === "searchOne"){
					 document.getElementById("searchModaltxt").value = "";
					}
					else {
					document.getElementById("searchtxt").value = "";
					}
				 searchTxt.focus();
				return false;
		}
		else {
				if (txt.length > 255) {
						var errorHTML = "<div class='alert alert-warning'>Search terms cannot exceed 255 characters.</div>"
					dvError.innerHTML = errorHTML;
					searchTxt.focus();
					return false;
				}
				else{
					return true;
				}
	}
};
  CloseError = function() {
			document.getElementById("dvError").style.display = "none";
		};

function validateChangeSearchURL(resultPage, isModal, sticky) {
		searchOmnitureTrack();
			isModal = typeof(isModal) !== 'undefined' ? isModal : false;
			sticky = typeof(sticky) !== 'undefined' ? sticky : false;
			resultPage = typeof(resultPage) !== 'undefined' ? resultPage : false;
				// Do validation, if validation fails then return immediately
			var validation = srchValidate(resultPage, isModal, sticky);
						if (!validation) {
								return false;
						}
						var searchForm = document.forms.searchForm;
			var lang = '';

						var redirectionUrl = 'http://www.newswire.ca/search-results/';
			if(lang.indexOf('fr') > -1){
			redirectionUrl = '/fr/search-results/';
			}
						var searchTxt = "";
						if (isModal && !sticky) {
				searchTxt = document.getElementById("searchMobiletxt").value;
				}
					else if (isModal.id === "searchForm1") {
								searchTxt = document.getElementById("mainSearch").value;
						}
			else if(isModal.id === "searchOne") {
				 searchTxt = document.getElementById("searchModaltxt").value;
			}
						else {
								searchTxt = document.getElementById("searchtxt").value;
						}
						//searchTxt = searchTxt.replace(/\s+/g, '+');
						searchTxt = encodeURIComponent(searchTxt);
						if (location.href.indexOf("/investors/") == -1 && (location.href.indexOf("profnet/support") == -1)) {
								if (document.getElementById("rdnewsreleases").checked) {
										if (document.getElementById("rdnewsreleases").value == "bloggers") {
												redirectionUrl += 'bloggers/' + encodeURI(searchTxt) + '-7-days-page-1';
										} else {
												redirectionUrl += 'news/' + encodeURI(searchTxt) + '-30-days-page-1-pagesize-20';
										}
								} else if (document.getElementById("rdproducts").checked) {

										if (document.getElementById("rdproducts").value == "Blogposts") {
												redirectionUrl = '/bloggers/blogger-search/?keywords=' + escape(searchTxt) + '&page=1';
										} else {
												redirectionUrl += 'products-knowledge/' + escape(searchTxt) + '-page-1';
										}
								}
						}
						if (location.href.indexOf("/investors/") > -1) {
								redirectionUrl += 'investors/' + escape(searchTxt) + '-7-days-page-1';
						}
						if (location.href.indexOf("mediawebsite.net") > -1) {
								redirectionUrl = "http://www.prnewswire.com" + redirectionUrl;
						}
						if (location.href.indexOf("profnet/support") > -1) {
								redirectionUrl = "/profnet/support" + redirectionUrl;
								redirectionUrl += searchTxt + '-page-1';
						}
						//alert("redirectionUrl >>"+redirectionUrl);
						location.href = redirectionUrl;
						return false;

				}
function logOut()
{
	jQuery.ajax({
		type: 'POST',
		url: '/mycnw/logout/',
					success: function (json) {
			var responseCode = json.trim();
			if(responseCode === "202"){
				jQuery('#subtoblob').show();
				window.location.href = "//stage.www.newswire.ca/";
			}

		}
	});


}

function getCookieData(c_name) {
	if (document.cookie.length > 0) {
		var c_start = document.cookie.indexOf(c_name + "=");
		if (c_start != -1) {
			c_start = c_start + c_name.length + 1;
			c_end = document.cookie.indexOf(";", c_start);
			if (c_end == -1) c_end = document.cookie.length;
			return unescape(document.cookie.substring(c_start, c_end));
		}
	}
	return "";
}

var sessionVal = getCookieData("session_cnw");
var displayNameVal = getCookieData("displayName_cnw");
displayNameVal = displayNameVal.split('+').join(' ');
	 if(sessionVal && sessionVal != "")
	 {
		$("#logRegister").html('<span class="loggedIn"> Logged in as ' + displayNameVal + '<a href="javascript:logOut();" id="sign_out_link"> (Log out)</a>');
		$('#logRegister').css("text-decoration", "none");
		$("#logRegister").prop("href", "/mycnw/subscriptions/");
		$("#registerLog").html('<p><a href="javascript:logOut();" id="sign_out_link">Log out('+ displayNameVal +')</a></p>');
		$("#registerLog").prop("href", "/mycnw/subscriptions/");
		$("#subtoblob").removeAttr("data-toggle");
		$("#subtoblob").removeAttr("data-target");
		$("#subtoblob").prop("href", "/mycnw/subscriptions/");
		$("#subtoblob1").removeAttr("data-toggle");
		$("#subtoblob1").removeAttr("data-target");
		$("#subtoblob1").prop("href", "/mycnw/subscriptions/");
		}
$(document).ready(function(){
	jQuery('.dataClear').click(function(){
		jQuery('#dvModalError').hide();
		jQuery('#searchOne').find('#searchModaltxt').val('');
	});
})
</script>



