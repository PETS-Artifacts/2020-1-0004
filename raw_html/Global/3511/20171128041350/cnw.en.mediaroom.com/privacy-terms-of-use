<script src="js/jquery.html"></script>
<script>
function validateForm(form){
	var searchVar = jQuery.trim(document.forms["searchForm"]["keyword"].value);
	if(searchVar == undefined || searchVar == ""){
		alert("Please enter a keyword to search");
		return false;
	}
	document.getElementById("keyword").style.borderColor="#c1c1c1";
	document.getElementById("keyword").value = searchVar;
	return true;
}
function validateSearchForm(form){
	var searchVar = jQuery.trim(document.forms["searchForm1"]["mainSearch"].value);
	if(searchVar == undefined || searchVar == ""){
		alert("Please enter a keyword to search");
		return false;
	}
	document.getElementById("mainSearch").style.borderColor="#c1c1c1";
	document.getElementById("mainSearch").value = searchVar;
	return true;
}
function srchValidate(isModal){
	var searchInput = "";
	var divError = "";
	if(isModal.id === "searchForm1")
	{
		var txt = document.getElementById("mainSearch").value;
		searchInput = "mainSearch";
		divError = "dvErrorSearch";

	}
	else if(isModal.id === "searchOne")
	{
		var txt = document.getElementById("searchModaltxt").value;
		searchInput = "searchModaltxt";
		divError = "dvModalError";
	}
	else {
		var txt = document.getElementById("searchtxt").value;
		searchInput = "searchtxt";
		divError = "dvError";
	}

	if (txt == "") {
		document.getElementById(divError).style.display = "inline";
		document.getElementById(divError).innerHTML = "Please enter search key words";
		document.getElementById(searchInput).focus();
		return false;
	}
	else {
		if (txt.length > 255) {
			document.getElementById(divError).style.display = "inline";
			document.getElementById(divError).innerHTML = "Search terms cannot exceed 255 characters.";
			document.getElementById(searchInput).focus();
			return false;
		}
		else{
			document.getElementById(divError).style.display = "none";
			return true;
		}
	}
}

function validateChangeSearchURL(isModal) {
	searchOmnitureTrack();
	isModal = typeof(isModal) !== 'undefined' ? isModal : false;
		// Do validation, if validation fails then return immediately
		var validation = srchValidate(isModal);
		if (!validation) {
			return false;
		}
		var searchForm = document.forms.searchForm;
		var lang = '';

		var redirectionUrl = '/search-results/';
		if(lang.indexOf('fr') > -1){
			redirectionUrl = '/fr/search-results/';
		}
		var searchTxt = "";
		if (isModal.id === "searchForm1") {
			searchTxt = document.getElementById("mainSearch").value;
		}
		else if(isModal.id === "searchOne") {
			searchTxt = document.getElementById("searchModaltxt").value;
		}
		else {
			searchTxt = document.getElementById("searchtxt").value;
		}
			//searchTxt = searchTxt.replace(/\s+/g, '+');
			searchTxt = encodeURIComponent(searchTxt);
			if (location.href.indexOf("/investors/") == -1 && (location.href.indexOf("profnet/support") == -1)) {
				if (document.getElementById("rdnewsreleases").checked) {
					if (document.getElementById("rdnewsreleases").value == "bloggers") {
						redirectionUrl += 'bloggers/' + encodeURI(searchTxt) + '-7-days-page-1';
					} else {
						redirectionUrl += 'news/' + encodeURI(searchTxt) + '-30-days-page-1-pagesize-20';
					}
				} else if (document.getElementById("rdproducts").checked) {

					if (document.getElementById("rdproducts").value == "Blogposts") {
						redirectionUrl = '/bloggers/blogger-search/?keywords=' + escape(searchTxt) + '&page=1';
					} else {
						redirectionUrl += 'products-knowledge/' + escape(searchTxt) + '-page-1';
					}
				}
			}
			if (location.href.indexOf("/investors/") > -1) {
				redirectionUrl += 'investors/' + escape(searchTxt) + '-7-days-page-1';
			}
			if (location.href.indexOf("mediawebsite.net") > -1) {
				redirectionUrl = "http://www.prnewswire.com" + redirectionUrl;
			}
			if (location.href.indexOf("profnet/support") > -1) {
				redirectionUrl = "/profnet/support" + redirectionUrl;
				redirectionUrl += searchTxt + '-page-1';
			}
			//alert("redirectionUrl >>"+redirectionUrl);
			location.href = redirectionUrl;
			return false;

		}
		function logOut() {
			jQuery.ajax({
				type: 'POST',
				url: '/mycnw/logout/',
				success: function (json) {
					var responseCode = json.trim();
					if(responseCode === "202"){
						jQuery('#subtoblob').show();
						window.location.href = "//www.newswire.ca/";
					}

				}
			});
		}
		$(document).ready(function(){
			jQuery('.dataClear').click(function(){
				jQuery('#dvModalError').hide();
				jQuery('#searchOne').find('#searchModaltxt').val('');
			});
		})
