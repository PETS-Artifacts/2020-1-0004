				<div class="modal fade" id="cnwSignIn" tabindex="-1" role="dialog" aria-labelledby="cnwSignInLabel" aria-hidden="true" data-keyboard="false" data-backdrop="static">
					<div class="modal-dialog">
						<div class="modal-content">
							<div class="modal-header">
								<button type="button" class="close formDataClear" data-dismiss="modal" aria-label="Close"><span aria-hidden="true">Ã</span></button>
								<h4 class="modal-title" id="myModalLabel"> Login to my CNW</h4>
							</div>
							<div class="modal-body">
								<form id="loginForm" name="loginForm">
									<span class="emailError loginError" style="display:none;color:#ff0000"> Invalid username/password</span>
									<div class="form-group">
										<label for="exampleInputEmail1"> Email address</label>
										<input class="form-control" id="InputEmail1" name="InputEmail1" placeholder="Enter email" type="email">
									</div>
									<div class="form-group">
										<label for="exampleInputPassword1"> Password</label>
										<input class="form-control" id="InputPassword1" name="InputPassword1" placeholder="Password" type="password">
										<span class="PwdError loginError" style="display:none;color:#ff0000"> Invalid password</span>
									</div>
									<div class="form-group">
										<p class="help-block"><a href="#" data-toggle="modal" data-target="#cnwResetPassword"> Forgot your password?</a></p>
									</div>
									<div class="checkbox">
										<label>
											<input name="remember" id="remember" type="checkbox">  Remember me            </label>
										</div>
										<button id="myLogin" class="btn btn-default"> Submit</button>
									</form>
									<script type="text/javascript">
									$("#loginForm").submit(function(){
										var email = document.getElementById("InputEmail1").value;
										var pawd = document.getElementById("InputPassword1").value;
										jQuery.ajax({
											method:"POST",
											url:"/mycnw/login/",
											data: {
												'InputEmail1': email,
												'InputPassword1': pawd
											},
											success: function (json) {
												var response = jQuery.parseJSON(json);
												var responseVal = response['val'];
												var responseCode = response['responseCode'];
						//console.log('responseVal' + responseVal);
						//console.log('responseCode' + responseCode);
						if(responseCode === "201" && responseVal == '0'){
							if ($('#remember').is(':checked'))
							{
								var userID = email; // check userid
								// set cookies to expire in 14 days
								setCookieData('userId', userID);
								setCookieData('remember', true);
							} else {
								setCookieData('remember', false);
							}
							location.href="/mycnw/subscriptions/";
						}
						else if(responseCode >= "400" )
						{
							setCookieData('remember', false);
							if(responseVal = -1){
								$('.emailError').show();
								return false;
							//alert('Invalid Username');
						}
						else if(responseVal = -2){
							$('.PwdError').show();
							return false;
						   //alert('Invalid password');
						}
						else if(responseVal = -3){
							$('.resposeError').show();
							return false;
						   //alert('Something Went wrong. please try again');
						}
					}
				}
			});
return false;
});
</script>
<script type="text/javascript">
$(document).ready(function(){
	$('.formDataClear').click(function(){
		var remember = readCookie('remember');
		if (remember != 'true')
		{
			$('#loginForm').find('#InputEmail1, #InputPassword1').val('');
		}
		$('#loginForm').find('.resposeError, .loginError').hide();
		$('#emailError').hide();
		$('.forgetPwd').show();
		$('#myButton').show();
		$('#thankYou').hide();
	});
})
</script>
