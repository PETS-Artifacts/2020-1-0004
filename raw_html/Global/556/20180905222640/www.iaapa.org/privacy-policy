<script type="text/javascript">    function gTranslate(lang) {
        var translationWindow = window.open('', '_blank');
        translationWindow.location = 'http://www.microsofttranslator.com/bv.aspx?from=&to=' + lang + '&a=' + window.location.href;
    }</script><script type="text/javascript" src="/js/jquery.hoverIntent.minified.js"></script><script type="text/javascript" src="http://www.iaapa.org/js/scripts.js"></script><script type="text/javascript">    $(document).ready(function () {
    
      //Madi, New - adding a width to start the JS
      var triggerWidth = 961;

      function megaHoverOver() {

        //Madi, New if statement to check width, all the rest is the same
        if( $(window).width() >= triggerWidth ){

          $(this).find(".sub").stop().fadeTo('fast', 1).show();

          //Calculate width of all ul's
          (function ($) {
              jQuery.fn.calcSubWidth = function () {
                  rowWidth = 0;
                  //Calculate row
                  $(this).find("ul").each(function () {
                      rowWidth += $(this).width();
                  });
              };
          })(jQuery);

          if ($(this).find(".row").length > 0) { //If row exists...
              var biggestRow = 0;
              //Calculate each row
              $(this).find(".row").each(function () {
                  $(this).calcSubWidth();
                  //Find biggest row
                  if (rowWidth > biggestRow) {
                      biggestRow = rowWidth;
                  }
              });
              //Set width
              $(this).find(".sub").css({ 'width': biggestRow });
              $(this).find(".row:last").css({ 'margin': '0' });

          } else { //If row does not exist...

              $(this).calcSubWidth();
              //Set Width
              $(this).find(".sub").css({ 'width': rowWidth });

          }
        }
      }

      function megaHoverOut() {
        if( $(window).width() >= triggerWidth ){
          $(this).find(".sub").stop().fadeTo('fast', 0, function () {
              $(this).hide();
          });
        }
      }

      //Madi, New
      // - adding the config and inline styling added by JS to a function
      // - adding the if statement w/ the width
      // - adding an else statement to remove inline styling added by JS at small screen sizes
      function runMM(){

        if( $(window).width() >= triggerWidth ){
          var config = {
            sensitivity: 1, // number = sensitivity threshold (must be 1 or higher)    
            interval: 0, // number = milliseconds for onMouseOver polling interval    
            over: megaHoverOver, // function = onMouseOver callback (REQUIRED)    
            timeout: 0, // number = milliseconds delay before onMouseOut    
            out: megaHoverOut // function = onMouseOut callback (REQUIRED)    
          };
          $("ul#topnav li .sub").css({ 'opacity': '0' });
          $("ul#topnav li").hoverIntent(config);
        } else {
          $("ul#topnav li .sub").removeAttr('style');
        }
      }
      
      //Madi, New to run the function above
      runMM();

      // Madi, New - Execute code when the window is resized.  It runs the function above
      $(window).resize(function () {
        runMM();
      });
      
      
      // Madi, New code for the mobile menu
      //------------------------------------------------------
      
      //Button to open menu
      $('.rwdBtn').click(function(e){
        e.preventDefault();
        $('ul#topnav').toggleClass('show-mobilemenu');
      })
      
      //Top level links
      // $('ul#topnav > li > a').click(function(e){
      //   if( $(window).width() <= triggerWidth ){
      //     e.preventDefault();
      //     $(this).parent().toggleClass('open');
      //   }
      // });
      //Top level links
      $('ul#topnav > li > a').click(function(e){
        if( ($(window).width() <= triggerWidth) && !$(this).parent().hasClass('open') ){
          e.preventDefault();
          $(this).parent().toggleClass('open');
        }
      });
      //2nd level links
      //This is the code that exposes the submenus
      $('.h4-submenu > h4').click(function(){
        if( ($(window).width() <= triggerWidth) ){
          $(this).parent().toggleClass('open');
        }
      });
      
      //This code prevents a link within the h4 from opening unless the sublinks are exposed
      $('.h4-submenu h4.with-link a').click(function(e){
        if( ($(window).width() <= triggerWidth) && !$(this).parents('.h4-submenu').hasClass('open')){
          e.preventDefault();
        }
      });

    });</script>


