<script type="text/javascript">
    var searchText;
    $(document).ready(function () {
        searchText = $('.sfsearchTxt').first().val();
        $('.sfsearchTxt').focus(function () {
            if ($(this).val() == searchText) {
                $(this).val('');
            }
        }).blur(function () {
            if ($(this).val() == '') {
                $(this).val(searchText);
            }
        });
        $('.sfsearchSubmit').click(function () {
            if ($('.sfsearchTxt').val() == searchText) {
                alert('Please enter a keyword');
                $('.sfsearchTxt').focus();
            }
            return false;
        });
    });
</script>
