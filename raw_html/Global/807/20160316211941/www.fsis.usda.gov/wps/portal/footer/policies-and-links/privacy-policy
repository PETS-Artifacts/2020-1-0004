	<script type="text/javascript" src="/foresee/foresee-trigger.js"></script>
	<script>
	//external site in a new window
	$(document).ready(function(){
	hostname = new RegExp(location.host);
        // Act on each link
        $('a',$('#layoutContainers')).each(function(){

            // Store current link's url
            var url = $(this).attr("href");
			if(url != undefined){

			//Test if current link is a document, add target '_blank'
			if(url.slice(url.length -3, url.length) == "pdf" || url.slice(url.length -3, url.length) == "xlsx" ||
			url.slice(url.length -3, url.length) == "doc" || url.slice(url.length -3, url.length) == "ppt" ||
			url.slice(url.length -4, url.length) == "xlsx" || url.slice(url.length -4, url.length) == "docx" || url.toLowerCase().indexOf(".pdf") >=0 )
			{
			   $(this).attr('target','_blank');
			}

            // Test if current host (domain) is in it
            if(hostname.test(url)){
               //local do nothing
            }
			else if(url.slice(0, 1) == "?" || url.slice(0, 1) == "/" || url.slice(0, 1) == "#" || url.slice(0, 11) == "http://fsis" || url.slice(0, 15) == "http://www.fsis"){
                //portal local
            }
            else if(url.slice(0, 4) == "http"){
               // a link that does not contain the current host

			  if($(this).find('img').length == 0){
			    if($(this).attr("class") !="footerLink") {
               $(this).addClass('external');
                 }
			   }


               $(this).attr('target','_blank');
            }
			}
        });
	  });
	</script>
	<!--Added script to track PFF's automatically-->
	<script>
		$("a[href*='.pdf']").each(function(index) {
		  pdfLabel = $(this).attr('href');
		  pdfOnClick = "_gaq.push(['_trackEvent', 'PDF', 'PDFDownload', '" + pdfLabel + "']);";
		  $(this).attr("onClick", pdfOnClick);
		});

		$("a[href*='.xls']").each(function(index) {
		  xlsLabel = $(this).attr('href');
		  xlsOnClick = "_gaq.push(['_trackEvent', 'XLS', 'XLSDownload', '" + xlsLabel + "']);";
		  $(this).attr("onClick", xlsOnClick);
		});

		$("a[href*='.zip']").each(function(index) {
		  zipLabel = $(this).attr('href');
		  zipOnClick = "_gaq.push(['_trackEvent', 'ZIP', 'ZIPDownload', '" + zipLabel + "']);";
		  $(this).attr("onClick", zipOnClick);
		});
	</script>

