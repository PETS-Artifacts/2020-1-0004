<script type="text/javascript">

function getUrlVars() {
    var vars = {};
    var parts = window.location.href.replace(/[?&]+([^=&]+)=([^&]*)/gi, function(m,key,value) {
        vars[key] = value;
    });
    return vars;
}

$(document).ready( function() {

var printerFriendly = getUrlVars()['PrinterFriendly'];

$('#share-container').prepend('<a id="print-button" class="at300b" target="_blank"><span class="at16nc at300bs at15nc"><span class="at_a11y"></span></span></a>');

if(printerFriendly)
{

$('#share-container').append('<link rel="stylesheet" href="http://www.fsis.usda.gov/css/interior/interior-print.css" type="text/css" />');

//append print-specific css
$('#share-container').append('<link rel="stylesheet" href="http://www.fsis.usda.gov/css/interior/interior-print-only.css" type="text/css" media="print"/>');

$('#print-button').bind('click', function() { window.print(); } );

/*Fix for IE8*/
if($('html').hasClass('wptheme_ie8'))
{

    var pageBreadcrumbs = '<div class="breadcrumb">' + $('.breadcrumb').html() + '</div>';
    var pageBody = '<div id="landing-page-container">' + $('#landing-page-container').html() + '</div>';

    var afterStyling = '<style>body{background-image: none; background-color: white; } #mainContent { min-width: 100% !important; max-width: 100% !important; width: 100% !important;}</style>';
    $(document.body).html(pageBody + afterStyling);
    window.print();
}

}
else
{

//append print-specific css
$('#share-container').append('<link rel="stylesheet" href="http://www.fsis.usda.gov/css/interior/interior-print-only.css" type="text/css" media="print"/>');

$('#print-button').unbind('click');
$('#print-button').attr('href', '?PrinterFriendly=true');

}

});

</script>
				        
				    		  
				
				
			
		
	

    
    	





    	






    	
    
