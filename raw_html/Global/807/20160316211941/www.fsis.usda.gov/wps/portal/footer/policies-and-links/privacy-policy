	<script>
	  $(document).foundation();
	  $("#navButtonMobile").bind('click', function () { $('#mobile-SideContent').removeClass('navHiddenInitially'); $('#mobile-SideContent').toggleClass('navHidden').toggleClass('navVisible'); $('#mainDisabledOverlay').toggleClass('overlayIsHidden').toggleClass('overlayIsVisible'); $('#mainContent').toggleClass('mainShrunk').toggleClass('mainExpanded'); $('#mobile-HeaderSearchBarContainer').toggleClass('hidden') });
