      <span property="schema:name" content="Privacy and Data Protection" class="rdf-meta hidden"></span>


  
  <div class="node__content">
    
            <div class="field field--name-field-page-hero-slice field--type-entity-reference field--label-hidden field__item">
                    
        
            <section id="privacy-and-data-protection" class="banner banner--hero ">
            <picture class="banner_image">
                
            <div class="field field--name-field-slice-image field--type-entity-reference field--label-hidden field__item"><article class="media media-image view-mode-hero">
      
