<script>
	$(function () {
		function messageController() {
			var currentTime = moment().tz('America/New_York');
			var officeHourOpen;
			var officeHourClosed;
			var dayOfWeek = currentTime.format('E');
			var supportwidget_visited_value;
			var defaultMessageClosed = '<p>Our normal business hours are Mon-Fri 8AM-11PM EST and Sat-Sun 9AM-8PM EST.</p><p>Please schedule a time to speak with a Princeton Review Enrollment Advisor.</p><p><a href="https://the-princeton-review.appointlet.com/s/25346?utm_source=TPR_Website&utm_medium=Website&utm_campaign=Chat_Widget" target="_blank">Schedule an Appointment</a></p>';
			var defaultMessageOpen = '<p>Talk to a Princeton Review<br /> Enrollment Advisor. Call Now!</p><p class="support-widget-phone">1-888-580-8378</p>';

			if ($(location.pathname == '/' && '.support-widget-contentItem-message').children().length != 0) {
				$('.support-widget-message').html($('.support-widget-contentItem-message').text());
			}

			else if (window.location.href.indexOf('/corporate/careers') > -1 || window.location.href.indexOf('/test/tps') > -1) {
				$('.support-widget-message').html('Please contact <a href="mailto:TPRTeacherApplicantSupport@review.com?subject=New Teacher Applicant Support Request">Teacher Support</a>.');
			}

			else if (dayOfWeek <= 5) {
				officeHourOpen = moment('8', 'H');
				officeHourClosed = moment('23', 'H');
				if (currentTime.isAfter(officeHourOpen) && currentTime.isBefore(officeHourClosed)) {
					$('.support-widget-message').html(defaultMessageOpen);
				}
				else {
					$('.support-widget-message').html(defaultMessageClosed);
				}
			}

			else if (dayOfWeek > 5) {
				officeHourOpen = moment('9', 'H');
				officeHourClosed = moment('20', 'H');
				if (currentTime.isAfter(officeHourOpen) && currentTime.isBefore(officeHourClosed)) {
					$('.support-widget-message').html(defaultMessageOpen);
				}
				else {
					$('.support-widget-message').html(defaultMessageClosed);
				}
			}
		}

		function cookieController() {
			if ($.cookie('supportwidget_visited')) {
				supportwidget_visited_value = parseInt($.cookie('supportwidget_visited')) + 1;
				$.removeCookie("supportwidget_visited");
				$.cookie('supportwidget_visited', supportwidget_visited_value, { path: '/' });
			} else {
				supportwidget_visited_value = 1;
				$.cookie('supportwidget_visited', supportwidget_visited_value, { path: '/' });
			}

			if (parseInt($.cookie('supportwidget_visited')) <= 3) {
				var timeout = setTimeout(function () {
					$('.support-widget-title').trigger('click');
				}, 5000);
			}

			$('.support-widget-title').click(function () {
				$('.support-widget-message').slideToggle(function () {
					$('.fa-chevron-down').toggle();
					clearTimeout(timeout);
				});
			});
		}

		cookieController();
		messageController();
	});
</script>

