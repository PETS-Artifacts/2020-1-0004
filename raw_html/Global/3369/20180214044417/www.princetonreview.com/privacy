<script>
	$(function () {
		if (window.location.href.indexOf('college') > -1) {
			//console.log('User visited page contains college in url');
			var date = new Date();
			var days = 30;
			date.setTime(date.getTime() + (days * 24 * 60 * 60 * 1000));
			$.cookie("college_visited", "college_visited_value", { expires: date });
		}
	});
</script>

