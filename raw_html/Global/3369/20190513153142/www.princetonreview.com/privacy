<script>
	$('.mobilenav').mobilenav({
		speed:100,
		position:'left'
	});

	
	$(document).ready(function () {
		$('ul.nav > li').on('keydown', function (e) {
			if (e.which == 13) {
				$('ul.nav li div').hide();
				$('div', this).show();
			}
		});

		
		var isCanada = $.tpr.geo.geoInfo()["Country"];
		if (isCanada && isCanada == "CA") {
			$('.dat-fundamentals-link').show();
		}

		
		if ($('.desktop-ribbon-content').length > 0) {

			if ($(window).width() >= 768) {
				var cssClasses = '.navbar-fixed-top, #mobilenav, .tp-search, #failureResult, #successResult';

				if ($('#successResultMsg').html() != "" || $('#failureResultMsg').html() != "") {
					cssClasses = '.navbar-fixed-top, #mobilenav, #failureResult, #successResult';
				}

				$(cssClasses).animate({ 'margin-top': '40px' }, 500);
			}

			var clicked = false;
			$('.desktop-ribbon-content .fa-times').click(function () {
				$('#homepageTopBanner .desktop-ribbon-content').slideUp();
				$('.navbar-fixed-top, .tp-search, #mobilenav, #failureResult, #successResult').animate({ 'margin-top': 0 }, 400);
				clicked = true;
			});

			//mobilenav
			$(window).on('resize', function () {
				if ($(window).width() < 768) {
					$('#mobilenav, #failureResult, #successResult').css('margin-top', '0');
				}
				else {
					if (!clicked)
						$('#mobilenav, .navbar-fixed-top, #failureResult, #successResult').css('margin-top', '40px');
				}
			});

		}
	});
</script>


