    (function() {
    if (window.newrelic) {
        var tokens = location.hostname.split('.'),
        siteType = tokens.shift(),
        hostname = tokens.join('.');

        if (siteType === 'www') {
            siteType = 'wired';
        } else if (siteType === 'm') {
            siteType = 'mobile';
        } else {
            siteType = 'other';
        }

        newrelic.setCustomAttribute('site_type', siteType);
        newrelic.setCustomAttribute('hostname', hostname);
        newrelic.setCustomAttribute('page_type', cmg.ads.config.targeting.type_name);
        newrelic.setCustomAttribute('media_type', cmg.ads.config.targeting.mediatype);
        newrelic.setCustomAttribute('environment', cmg.ads.config.targeting.environ);

        function parse_query_params() {
            var query = location.search.substring(1),
                params = query.split('&'),
                vals = {};
            for (var i = 0; i < params.length; i++) {
                var param = params[i],
                    eq = param.indexOf('=');
                if (eq != -1) {
                    var key = decodeURIComponent(param.substring(0, eq)),
                        val = decodeURIComponent(param.substring(eq+1)).replace(/\+/g, ' ');
                    vals[key] = val;
                }
            }
            return vals;
        }

        var params = parse_query_params();
        if ('disable_template' in params) {
            newrelic.setCustomAttribute('disable_template', params.disable_template);
        }
    }
})();

