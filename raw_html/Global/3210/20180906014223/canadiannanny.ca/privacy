<!-- Why is this inline? -->
<!-- We want the `viewport_size` field on the `cg:page:view` event. -->
<!-- We want `cg:page:view` to fire before all the JS on the site is loaded, -->
<!-- parsed, and executed, since that process takes some time. -->
<script>
  (function () {
    function setViewportSize (viewportSize, match) {
      if (match.matches && window.k6n.setDefaults) {
        window.k6n.setDefaults({ viewport_size: viewportSize })
      }
    }
  
    function watchViewportSize (viewportSize, query) {
      if (typeof window.matchMedia !== 'function') return
  
      var match = window.matchMedia(query)
      setViewportSize(viewportSize, match)
      match.addListener(setViewportSize.bind(null, viewportSize))
    }
  
    if (window.k6n) {
      watchViewportSize('xs', '(max-width: 767px)')
      watchViewportSize('sm', '(min-width: 768px) and (max-width: 991px)')
      watchViewportSize('md', '(min-width: 992px) and (max-width: 1199px)')
      watchViewportSize('lg', '(min-width: 1200px)')
    }
  })()
</script>
<!-- Uses a second script block in case the matchMedia code fails -->
