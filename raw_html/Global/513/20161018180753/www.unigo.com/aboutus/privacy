    <script type="text/javascript" src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-502410b700b9a544" async="async"></script>
    
        

        <script type="text/javascript">
            var openModal = $('a.ctaReveal');
            $(openModal).on('click', function() {
                var packageURL = $(this).data("url");
                var packageURLElse = packageURL.replace("payment", "paymentforsomeoneelse");
                $('#choosePay.reveal-modal .choosePayMe').attr('href', packageURL);
                $('#choosePay.reveal-modal .choosePayElse').attr('href', packageURLElse);
            });
        </script>
    
