    function scriptsOnLoad()
    {
        Url.base = 'https://www.unigo.com/cms/';
        Url.unigoBase = 'https://www.unigo.com/';
        ;
    }

    scriptsOnLoad();

    //Any additional eventLogger key/value pairs must be set PRIOR TO the (window).load event. See Eric for questions.
    $(window).load(function() {
        eventLogger.log();
        eventLogger.clearLog();
        Setup.Dfp();
        Setup.Sticky();
        Setup.RedirectModals();
    });

    //fix large blank space on ios and safari
    $(document).foundation({
        reveal : {
            animation : 'fade',
            root_element : ''
        }
    });
</script>



