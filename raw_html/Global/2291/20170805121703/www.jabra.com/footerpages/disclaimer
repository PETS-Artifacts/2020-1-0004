<!-- Campaigns-->
<script>

(function(){
  var url = location.pathname;
  
  if ( !(url.indexOf('/c/') > -1) || (url.indexOf('/c/us/smb-2017') > -1) ){
    return;
  }

  else{
    $('ul.top-navigation__top-nav, ul.top-navigation__main-menu ').css('display','none');
  
  }
})();
</script>
