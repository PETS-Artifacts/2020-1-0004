<!-- SMB Campaign -->
<script language="javascript" type="text/javascript">

(function($){

// cookies
function createCookie(name,value,days) {
    var expires = "";
    if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days*24*60*60*1000));
        expires = "; expires=" + date.toUTCString();
    }
    document.cookie = name + "=" + value + expires + "; path=/";
}

function readCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}

//show smb msg
var allowedPages = ["/business/office-headsets/jabra-evolve/jabra-evolve-30", "/business/office-headsets/jabra-evolve/jabra-evolve-40", "/business/office-headsets/jabra-evolve/jabra-evolve-80", "/business/contact-center-headsets/jabra-biz-2300", "/business/office-headsets/jabra-evolve/jabra-evolve-65", "/business/office-headsets/jabra-pro-900", "/business/speakerphones/jabra-speak-series/jabra-speak-510", "/business/speakerphones/jabra-speak-series/jabra-speak-810", "/bluetooth-headsets/jabra-steel", "/business/mobile-worker-headsets/jabra-motion", "/bluetooth-headsets/jabra-stealth", "/in-car/jabra-freeway"];
var thisPage = location.pathname.toLowerCase();
var isAllowed = $.inArray(thisPage, allowedPages) > -1;
var isCookie = readCookie('smbpromo');

if (!isCookie){
  if (isAllowed) {

    $('body').append('<div class="smb-promo-coupon"><h4>Deals for Small Businesses</h4> <p>Try this product out in your company with a 50% discount.<br/> Limited time only. See Jabra.com/SMB for details</p>  <span class="smb-close-msg">&#10005;</span><a class="jbr-btn jbr-btn-large jbr-btn--dark-outline" href="/c/us/smb">Get your promo code</a></div>');
  
    $('.smb-promo-coupon .smb-close-msg').on('click', function() {
      $(this).parent().hide();
      createCookie('smbpromo','yes',7);

    });
    
    setTimeout(function() {
      $('.smb-promo-coupon').fadeIn('slow');
    }, 2000);
  }
}

else{
  return;
}
})($);










</script>

