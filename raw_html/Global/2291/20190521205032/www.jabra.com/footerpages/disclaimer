<!-- Dynamic Yield - Ajax Cart Test -->
<script language="javascript" type="text/javascript">
    //function updateCartAjax(sku = "", qty = 1, method = "add") {
    function updateCartAjax(sku, qty, method) {
        if(typeof sku === "undefined"){sku = ""}
        if(typeof qty === "undefined"){qty = 1}
        if(typeof method === "undefined"){method = "add"}
        $('.ajax-cart').closest('div[data-dy-embedded-object]').addClass('loading');
        console.log('SKU0: ', sku);
        if (sku != "" || $('span[ng-bind="selection.sku"]').text() != "") {
            if (sku == "") {
                sku = $('span[ng-bind="selection.sku"]').text();
            }
            console.log('SKU1: ', sku);
            if (method == "remove") {
                qty = (qty * -1);
            }
            $('.hero-customizer-spot product-action .jbr-btn--yellow, .family-customizer-spot product-action .jbr-btn--yellow').addClass('jbr-btn--loading');
            var token = JSON.parse(sessionStorage.getItem('dr-token'));
            if (token != null) {
                token = token.access_token;
                processCartAjax(sku, qty, token);
            } else {
                $.ajax({
                    type: "GET",
                    url: "https://checkout.jabra.com/store/jabra/SessionToken",
                    data: 'format=jsonp&apiKey=WZWxsKIlLOuwUxyn43usGMl2mBFJuczV&locale=en_US&callback=JSONPcallback',
                    dataType: "jsonp",
                    jsonp: 'callback',
                    jsonpCallback: 'JSONPcallback',
                    cache: false,
                    success: function(data) {
                        var token = data['access_token'];
                        console.log('SKU2: ', sku);
                        processCartAjax(sku, qty, token);
                        console.log('Process Cart');
                    }
                });
            }
        } else {
            return false;
        }
    }
    
    function processCartAjax(sku, qty, token) {
        console.log('Processing Cart:');
        console.log(sku, qty, token);
        $.ajax({
            type: "GET",
            url: "https://api.digitalriver.com/v1/shoppers/me/carts/active/line-items",
            data: 'action=add&quantity=' + qty + '&externalReferenceId=' + sku + '&token=' + token + '&method=post&format=json&callback=JSONPCallback',
            dataType: "jsonp",
            jsonp: 'callback',
            jsonpCallback: 'JSONPcallback',
            cache: false,
            success: function(data) {
                console.log('Load Cart');
                $('.hero-customizer-spot product-action .jbr-btn--yellow, .family-customizer-spot product-action .jbr-btn--yellow').removeClass('jbr-btn--loading');
                loadCart();
            }
        });
    }
    
    function loadCart() {
        var token = JSON.parse(sessionStorage.getItem('dr-token'));
        if (token != null) {
            token = token.access_token;
            showCart(token);
        } else {
            $.ajax({
                type: "GET",
                url: "https://checkout.jabra.com/store/jabra/SessionToken",
                data: 'format=jsonp&apiKey=WZWxsKIlLOuwUxyn43usGMl2mBFJuczV&locale=en_US&callback=JSONPcallback',
                dataType: "jsonp",
                jsonp: 'callback',
                jsonpCallback: 'JSONPcallback',
                cache: false,
                success: function(data) {
                    var token = data['access_token'];
                    showCart(token);
                }
            });
        }
    }
    
    function showCart(token) {
        $.ajax({
            type: "GET",
            url: "https://api.digitalriver.com/v1/shoppers/me/carts/active",
            data: 'expand=lineitems.lineItem.product.externalReferenceId,%20lineitems.lineItem.product.sku&format=json&method=get&token=' + token + '&callback=JSONPcallback',
            dataType: "jsonp",
            jsonp: 'callback',
            jsonpCallback: 'JSONPcallback',
            cache: false,
            success: function(data) {
                var html = '';
                var qtyTotal = data['cart']['totalItemsInCart'];
                var prices = data['cart']['pricing'];
                var isItemsInCart = false;
                var skusToCheckStock = "";
                if ("lineItem" in data['cart']['lineItems']) {
                    var isItemsInCart = true;
                    var lines = data['cart']['lineItems']['lineItem'];
                    //lines.forEach(element => {
                    /*lines.forEach( function(element) {
                        var thumb = element.product.thumbnailImage;
                        var name = element.product.displayName;
                        var sku = element.product.sku;
                        var qty = element.quantity;
                        var unitPrice = element.pricing.listPrice;
                        var unitPriceText = element.pricing.formattedListPrice;
                        var qtyPrice = element.pricing.salePriceWithQuantity;
                        var qtyPriceText = element.pricing.formattedSalePriceWithQuantity;
                        if(skusToCheckStock != ""){
                            skusToCheckStock = skusToCheckStock + "&skus=" + sku;
                        }else{
                            skusToCheckStock = sku;
                        }
                        var line = '<div class="cart-row row"><div class="col-img"><img src="' + thumb + '?w=100" alt=""></div><div class="col-name"><p>' + name + '<span class="col-stock" data-sku="' + sku + '"></span></p><small class="col-price-mobile">' + qtyPriceText + '</small></div><div class="col-amount"><span class="minus-line" data-sku="' + sku + '" data-qty="' + qty + '">&minus;</span><input class="amount-line" type="text" value="' + qty + '" data-sku="' + sku + '" data-qty="' + qty + '"><div class="amount-line-overlay"></div><span class="plus-line" data-sku="' + sku + '" data-qty="' + qty + '">&plus;</span></div><div class="col-price">' + qtyPriceText + '</div><div class="col-remove"><span class="remove-line" data-sku="' + sku + '" data-qty="' + qty + '">&times;</span></div><div class="col-amount-mobile" data-sku="' + sku + '" data-qty="' + qty + '"><div class="col-controls-mobile"><div class="col-minus-amount-mobile"><span>-</span></div><div class="col-edit-amount-mobile"><span>' + qty + '</span></div><div class="col-plus-amount-mobile"><span>+</span></div><div class="col-confirm-mobile"><span class="icon icon__easy_use"></span></div></div></div></div>';
                        html += line;
                    });*/
                    for (var i = 0; i < lines.length; i++) {
                        var thumb = lines[i].product.thumbnailImage;
                        var name = lines[i].product.displayName;
                        var sku = lines[i].product.sku;
                        var qty = lines[i].quantity;
                        var unitPrice = lines[i].pricing.listPrice;
                        var unitPriceText = lines[i].pricing.formattedListPrice;
                        var qtyPrice = lines[i].pricing.salePriceWithQuantity;
                        var qtyPriceText = lines[i].pricing.formattedSalePriceWithQuantity;
                        if(skusToCheckStock != ""){
                            skusToCheckStock = skusToCheckStock + "&skus=" + sku;
                        }else{
                            skusToCheckStock = sku;
                        }
                        var line = '<div class="cart-row row"><div class="col-img"><img src="' + thumb + '?w=100" alt=""></div><div class="col-name"><p>' + name + '<span class="col-stock" data-sku="' + sku + '"></span></p><small class="col-price-mobile">' + qtyPriceText + '</small></div><div class="col-amount"><span class="minus-line" data-sku="' + sku + '" data-qty="' + qty + '">&minus;</span><input class="amount-line" type="text" value="' + qty + '" data-sku="' + sku + '" data-qty="' + qty + '"><div class="amount-line-overlay"></div><span class="plus-line" data-sku="' + sku + '" data-qty="' + qty + '">&plus;</span></div><div class="col-price">' + qtyPriceText + '</div><div class="col-remove"><span class="remove-line" data-sku="' + sku + '" data-qty="' + qty + '">&times;</span></div><div class="col-amount-mobile" data-sku="' + sku + '" data-qty="' + qty + '"><div class="col-controls-mobile"><div class="col-minus-amount-mobile"><span>-</span></div><div class="col-edit-amount-mobile"><span>' + qty + '</span></div><div class="col-plus-amount-mobile"><span>+</span></div><div class="col-confirm-mobile"><span class="icon icon__easy_use"></span></div></div></div></div>';
                        html += line;
                    }
                }
                var subtotal = prices.subtotal;
                var subtotalText = prices.formattedSubtotal;
                if (isItemsInCart) {
                    $('.cart-rows').html(html);
                    $('.ajax-cart .cart-cta .go-to-checkout').show();
                    if(skusToCheckStock != ""){
                        $.ajax({
                            type: "GET",
                            url: "https://pricingapi.jabra.com/v1/products/filter",
                            data: 'skus='+skusToCheckStock+'&marketLocale=en-us',
                            dataType: "json",
                            json: 'callback',
                            jsonCallback: 'JSONcallback',
                            cache: false,
                            success: function(data) {
                                console.log(data)
                                //data.forEach(element => {
                                /*data.forEach( function(element) {
                                    console.log(element.sku);
                                    switch(element.availabilityState) {
                                      case 0:
                                        // Add to cart
                                        $('.cart-row .col-name .col-stock[data-sku="'+element.sku+'"]').html('In stock: Ships in 1 business day').attr('data-stock','in-stock');
                                        break;
                                      case 1:
                                        // Pre-order
                                        $('.cart-row .col-name .col-stock[data-sku="'+element.sku+'"]').html('In pre-order: Usually ships within 2 - 3 weeks').attr('data-stock','pre-order');
                                        break;
                                      case 2:
                                        // Backorder
                                        $('.cart-row .col-name .col-stock[data-sku="'+element.sku+'"]').html('In backorder: Usually ships within 2 - 3 weeks').attr('data-stock','backorder');
                                        break;
                                    }
                                });*/
                                for (var i = 0; i < data.length; i++) {
                                    console.log(data[i].sku);
                                    switch(data[i].availabilityState) {
                                      case 0:
                                        // Add to cart
                                        $('.cart-row .col-name .col-stock[data-sku="'+data[i].sku+'"]').html('In stock: Ships in 1 business day').attr('data-stock','in-stock');
                                        break;
                                      case 1:
                                        // Pre-order
                                        $('.cart-row .col-name .col-stock[data-sku="'+data[i].sku+'"]').html('In pre-order: Usually ships within 2 - 3 weeks').attr('data-stock','pre-order');
                                        break;
                                      case 2:
                                        // Backorder
                                        $('.cart-row .col-name .col-stock[data-sku="'+data[i].sku+'"]').html('In backorder: Usually ships within 2 - 3 weeks').attr('data-stock','backorder');
                                        break;
                                    }
                                    
                                }
                            }
                        });
                    }
                } else {
                    $('.cart-rows').html('<div class="cart-row row"><div class="cart-empty"><span>Your cart is empty</span></div></div>');
                    $('.ajax-cart .cart-cta .go-to-checkout').hide();
                }
                $('.cart-total .total-price').html(subtotalText);
                $('.top-navigation__main-menu__label--cart').attr('data-cartitems', qtyTotal);
                $('.top-navigation__main-menu__input').prop('checked', false);
                $('html, body').animate({
                    scrollTop: 0
                }, 'slow', function() {
                    $('.ajax-cart').closest('div[data-dy-embedded-object]').removeClass('loading').addClass('open');
                    $('#page-main-content').addClass('cart-open');
                    $('.ajax-cart').slideDown('slow');
                    $('.top-navigation__main-menu__item--ajaxcart').removeClass('loading');
                });
            }
        });
    }
    
    function goToCheckout() {
        var token = JSON.parse(sessionStorage.getItem('dr-token'));
        if (token != null) {
            token = token.access_token;
            checkoutRedirect(token);
        } else {
            $.ajax({
                type: "GET",
                url: "https://checkout.jabra.com/store/jabra/SessionToken",
                data: 'format=jsonp&apiKey=WZWxsKIlLOuwUxyn43usGMl2mBFJuczV&locale=en_US&callback=JSONPcallback',
                dataType: "jsonp",
                jsonp: 'callback',
                jsonpCallback: 'JSONPcallback',
                cache: false,
                success: function(data) {
                    var token = data['access_token'];
                    checkoutRedirect(token);
                }
            });
        }
    }
    
    function checkoutRedirect(token) {
        window.location.href = 'https://api.digitalriver.com/v1/shoppers/me/carts/active/web-checkout?token=' + token;
    }
    
    var ajaxCartIconInterval;
    
    function addAjaxCartIcon() {
    if($('.top-navigation__main-menu').length >= 1){
        console.log('setup interval');
        ajaxCartIconInterval = setInterval(function() {
            console.log('interval running');
            if (!$('.ajax-cart-icon').length) {
                var itemsInCart = $('.top-navigation__main-menu__label--cart:not(.ajax-cart-icon)').attr('data-cartitems');
                var cartLabel = $('.top-navigation__main-menu__label--cart:not(.ajax-cart-icon)').html();
                console.log('data', itemsInCart, cartLabel);
                $('<li class="top-navigation__main-menu__item top-navigation__main-menu__item--ajaxcart top-navigation__main-menu__item--cart"><label for="top-navigation__ajaxcart" class="top-navigation__main-menu__label top-navigation__main-menu__label--cart ajax-cart-icon" data-cartitems="' + itemsInCart + '">' + cartLabel + '</label></li>').insertBefore('.top-navigation__main-menu__item.top-navigation__main-menu__item--cart');
            } else {
                console.log('clear interval');
                clearInterval(ajaxCartIconInterval);
                var itemsInCart = $('.top-navigation__main-menu__label--cart:not(.ajax-cart-icon)').attr('data-cartitems');
                $('.top-navigation__main-menu__label--cart.ajax-cart-icon').attr('data-cartitems', itemsInCart);
            }
        }, 500);
    }else{
    console.log('hidden nav');
    }
    }
    
    function closeCart() {
        $('.ajax-cart').closest('div[data-dy-embedded-object]').removeClass('open');
        $('#page-main-content').removeClass('cart-open');
        $('.ajax-cart').slideUp('slow');
    }
    
    $('.top-navigation').on('click', '.ajax-cart-icon', function() {
        if($('#page-main-content.cart-open').length == 0){
            $('.top-navigation__main-menu__item--ajaxcart').addClass('loading');
            loadCart();console.log('loadCart');
        }else{
            closeCart();console.log('closeCart');
        }
    });
    </script>
