var target = document.querySelector('body');
var observer = new MutationObserver(function(mutations) {
    for(var mutation in mutations) {
        if (mutations[mutation].target.matches('.open-cookie-info')){
        dataLayer.push({'event':'cookieBannerOpened','eventLabel':'BannerOpened','eventCategory':'cookieBannerOpened','eventAction':'BannerOpened','eventValue':'BannerOpened'});
