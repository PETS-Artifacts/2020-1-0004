<script>
    (function () {
        if (window.location.pathname === '/search') {
            var searchItem = document.querySelector('.top-navigation__main-menu__item--search');

            if (searchItem === null) {
                return;
            }
            if (!('remove' in Element.prototype)) {
                searchItem.parentNode.removeChild(searchItem);
            } else {
                searchItem.remove();
            }
        }
    })();
</script>

