<script  type="text/javascript">
(function () {
    window.DY = window.DY || {};
    var headLanguage = (document.documentElement.className.split(' ')[1] || '').toLowerCase();
    if (headLanguage === 'en') {
        headLanguage = 'en-us';
    }

    var headLanguageParts = headLanguage.split('-');
    headLanguageParts[1] = headLanguageParts[1].toUpperCase();
    window.dyLanguage = headLanguageParts.join('_');
})();
</script>
