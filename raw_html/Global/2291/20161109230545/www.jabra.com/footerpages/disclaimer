<!-- SMB Campaign -->
<script language="javascript" type="text/javascript">

(function($){
//show smb msg
var allowedPages = ["/business/office-headsets/jabra-evolve/jabra-evolve-30", "/business/office-headsets/jabra-evolve/jabra-evolve-40", "/business/office-headsets/jabra-evolve/jabra-evolve-80", "/business/contact-center-headsets/jabra-biz-2300", "/business/office-headsets/jabra-evolve/jabra-evolve-65", "/business/office-headsets/jabra-pro-900", "/business/speakerphones/jabra-speak-series/jabra-speak-510", "/business/speakerphones/jabra-speak-series/jabra-speak-810", "/bluetooth-headsets/jabra-steel", "/business/mobile-worker-headsets/jabra-motion", "/bluetooth-headsets/jabra-stealth", "/in-car/jabra-freeway"];
var thisPage = location.pathname.toLowerCase();
var isAllowed = $.inArray(thisPage,allowedPages) > -1;
var isSmbCookie = document.cookie.indexOf("smb-cookie=smb-campaign") > -1;
if (isAllowed && isSmbCookie) {
  $('body').append('<div class="smb-promo-coupon"><h4>Deals for Small Businesses</h4> <p>Try this product out in your company with a 50% discount.<br/> Limited time only. See Jabra.com/SMB for details</p>  <span class="smb-close-msg">&#10005;</span> <i class="icon icon__people_3"></i><a href="/c/us/smb">Get your promo code  &#10095; </a></div>');
    
    $('.smb-promo-coupon .smb-close-msg').on('click', function(){
      $(this).parent().hide();
    });


  setTimeout(function(){
    $('.smb-promo-coupon').fadeIn('slow');
  }, 2000);
  }
  else{
    return;
  }

})($);

</script>

