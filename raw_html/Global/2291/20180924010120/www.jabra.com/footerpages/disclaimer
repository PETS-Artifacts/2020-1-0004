    var searchSettings = {};

    searchSettings.url = '/search';
    searchSettings.argument = 'q';    

    if (window.location.pathname === searchSettings.url) {
        document.getElementsByClassName('top-navigation__main-menu__item--search')[0].remove();        
    }
</script>



