<!-- outlet - US -->
<script type="text/javascript">
(function(){
  var country = $('meta[name="user.country"]').attr('content');
  
  if (!(country == 'US')){      
      $('a[href="http://outlet.jabra.com"]').remove();
  }
  else{
    return;
  }
})();
</script>

