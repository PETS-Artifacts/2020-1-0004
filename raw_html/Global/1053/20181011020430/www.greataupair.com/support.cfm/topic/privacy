	<script>
		$(function() { 

			// Main nav for desktop - JTR
	if( $(window).width() > 640 ) {
		
		
		
	};

						
			if( $(window).width() < 640 ) {

				$('.top-level-category > a').attr('href','#');
				
				$('.top-level-category a').removeClass('active');

				$('.top-level-category a').on('click', function() {
					
					if ( $(this).hasClass('nav-item-open') ) {
						
						$(this).parent().find('.sub_menu').hide();
						$(this).find('.icon-expand').text('+');
						$(this).removeClass('nav-item-open');
						
					} else {
						
						$('.sub_menu').hide();
						$(this).parent().find('.sub_menu').show();
						
						$('.icon-expand').text('+');
						$(this).find('.icon-expand').text('-');

						$('.top-level-category a').removeClass('nav-item-open');
						$(this).addClass('nav-item-open');
						
					}

				});
				
				var mobileNavOpen = false;
				
				$('#mobile-nav-toggle').on('click', function() {

					if (mobileNavOpen === false) {
						openMobileNav();
					} else {
						closeMobileNav();
					}
					
				});
				
				$('.content_body').on('click', function() {
					closeMobileNav();
				});
				
				function openMobileNav() {
					$('#menu').css('display', 'block');
					$('#btn-icon').css('display', 'none');
					$('#btn-icon-close').css('display', 'block');
					mobileNavOpen = true;
				}
				
				function closeMobileNav() {
					$('#menu').css('display', 'none');
					$('#btn-icon').css('display', 'block');
					$('#btn-icon-close').css('display', 'none');
					mobileNavOpen = false;
				}
				
			} else {

				var activeMenu = false;
		
				$('body').click( function() {
					closeAllSubMenus(); 
				});
				
				$("#menu .top-level-nav").click(
					function (event) {

						var subMenu = $(this).find('.sub_menu');
						var activeNavItem = $(this).find('a');
						
						event.stopPropagation();
						
						subMenu.toggle();
						$('#menu .top-level-nav .sub_menu').not(subMenu).hide();
						
						activeNavItem.toggleClass('active-menu');
						$("#menu .top-level-nav a").not(activeNavItem).removeClass('active-menu');

					}
				);
				
				function closeAllSubMenus() {
					$('#menu .top-level-nav .sub_menu').hide();
					$("#menu .top-level-nav a").removeClass('active-menu');
				}

			}

			var j1EligibleFlag = 0;
			var gacnEligibleFlag = 0;

			$(".J1OptOut").on("click", function(e) { 
				$('.J1OptOut').text('Processing...');
				e.preventDefault();
				$.ajax({  
					type: "GET",
					url: '/components/aupair-usa/main.cfc?method=recordInterest&isInterested=0',
					success: function(){
						window.location = "/myaccount.cfm";
					}
				});    
			});

			$('.J1OptIn').on("click", function(e) { 
				$('.J1OptIn').text('Processing...');
				e.preventDefault();
				$.ajax({  
					type: "GET",
					url: '/components/aupair-usa/main.cfc?method=recordInterest&isInterested=1',
					success: function(){

						if ( gacnEligibleFlag != 0 ) {
							$.ajax({  
								type: "GET",
								url: '/components/cn/main.cfc?method=isInterested&isInterested=0',
								success: function(){
									window.location = "/myaccount.cfm";
								}
							});
						} else {
							window.location = "/myaccount.cfm";
						}
					}
				});    
			});

			$('.gacnOptOut').on('click', function(e) { 
				$('.gacnOptOut').text('Processing...');
				e.preventDefault();
				$.ajax({  
					type: "GET",
					url: '/components/cn/main.cfc?method=isInterested&isInterested=0',
					success: function(){
						window.location = "/myaccount.cfm";
					}
				});  
			});

			$('.gacnOptIn').on('click', function(e) {
				$('.gacnOptIn').text('Processing...');
				e.preventDefault();
				$.ajax({  
					type: "GET",
					url: '/components/cn/main.cfc?method=isInterested&isInterested=1',
					success: function(){
						if ( j1EligibleFlag != 0 ) {
							$.ajax({  
								type: "GET",
								url: '/components/aupair-usa/main.cfc?method=recordInterest&isInterested=0',
								success: function(){
									window.location = "/myaccount.cfm";
								}
							});
						} else {
							window.location = "/myaccount.cfm";
						}
					}
				});  
			});

			/*
