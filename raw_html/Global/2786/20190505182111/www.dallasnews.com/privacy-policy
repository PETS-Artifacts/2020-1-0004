<script type="text/javascript">
    function getCookie(cname) {
        var name = cname + "="
        var ca = document.cookie.split(';')

        for (var i = 0; i < ca.length; i++) {
            var c = ca[i]

            while (c.charAt(0) == ' ') {
                c = c.substring(1)
            }

            if (c.indexOf(name) == 0) {
                return c.substring(name.length, c.length)
            }
        }

        return ""
    }

    s_account2 = "ahdallasnews";
    om_linkInternalFilters="javascript:,dallasnews.com";
    om_fpcdomain="dallasnews.com";
    var omPageName = "";
    var omSiteSection = "basic";
    var omSubSection1 = "";
    var omSubSection2 = "";
    var omSubSection3 = "";
    var omSubSection4 = "";
    var omSectionContentType = "";
    var omArticleTitle = "";
    var omDirectoryPath = "";
    var omArticleId = "";
    var omPageType = "";
    var omPageTypeForSite = "DMN/exclude";
    var omAuthor = "";
    var omSubscriber = "";

    var dmnp = getCookie("DMN-P")
    var DMNPuid = ""
    if (!!dmnp) {
        var dmnSplit = dmnp.split("^")
        DMNPuid = dmnSplit[dmnSplit.length - 1]
    }
    
    //Load dnLoginId
    var delim = "^";
    var cIndex = "3";
    var lc = dmnp,
        result;
   if (lc !==null){
       var parts;
       parts = lc.split(delim);
       if (parts.length > 0){
          result = parts[cIndex];
       }
       else
       {
          result = null;
       }
   }
   else
   {
       result = null;
   }
   window.dnLoginId = result;
   //Set dnLoggedIn based on dnLoginId
   if(window.dnLoginId && (typeof window.dnLoginId !="undefined"))
     { 
       window.dnLoggedIn=1;
     } 
     else { 
       window.dnLoggedIn=0;
     }

    var omLoginId = DMNPuid
</script>
