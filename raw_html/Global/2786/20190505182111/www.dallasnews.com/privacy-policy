                    // Load Montserrat
            var Montserrat = new FontFaceObserver('Montserrat')
            
            Montserrat.load(null, 8000)
                .then(function () {
                console.timeEnd("Load Montserrat")
                docEl.className += " wf-active wf-montserrat-ssm-a-active"
            }).catch(function () {
                console.warn('Google font Montserrat failed to load.')
                console.timeEnd("Load Montserrat")
            })
        
                
            // Load PT Serif
            var PTSerif = new FontFaceObserver( "PT Serif", {
                weight: 400
            });
            var PTSerifBold = new FontFaceObserver( "PT Serif", {
                weight: 700
            });
            var PTSerifItalic = new FontFaceObserver( "PT Serif", {
                weight: 400,
                style: "italic"
            });
            var PTSerifBoldItalic = new FontFaceObserver( "PT Serif", {
                weight: 700,
                style: "italic"
            });
            PTSerif.load(null, 8000)
                .then(function(){
                    console.timeEnd("Load PT Serif");
                    docEl.className += " wf-pt-serif-active";
                    console.time("Load PT Serif Variants")
                    Promise
                        .all([PTSerifBold.load(null, 8000), PTSerifBoldItalic.load(null, 8000), PTSerifItalic.load(null, 8000)])
                        .then(function(){
                            console.timeEnd("Load PT Serif Variants");
                        }).catch(function() {
                            console.warn("PT Sans Variants did not load");
                            console.timeEnd("Load PT Serif Variants");
                        });
                }).catch(function() {
                    console.warn("PT Sans did not load");
                    console.timeEnd("Load PT Serif");
                });
            })( document )

