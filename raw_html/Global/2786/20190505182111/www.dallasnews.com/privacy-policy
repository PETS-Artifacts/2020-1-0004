<script>

    window.DEBUG_ADS = (window.location.href.indexOf('pbjs_debug=true') >= 0 || window.location.href.indexOf('ads_debug') >= 0);
    function debug(msg) {
        if (!window.DEBUG_ADS) {
            return;
        }
        if (typeof msg != 'string') {
            console.log("XJ", msg)
        } else {
            console.log ("XJ - " + msg);
        }
    }

    // OVERVIEW OF HOW ADS WORK:
    // Ads have gone through a few iterations on dmn, the most recent work attempting to bring sanity back to the situation.
    // There are three primary facets of ads: Normal On-The-Fly Ads, Roadblock-Ready Ads, and Prebidding Ads
    //
    // Normal On-The-Fly Ads, is the ability to support building and rendering google dfp ads at any point in a page's lifecycle.
    // Things like ajax based rendering, feed loads, or building ads with JS though directly get an ad from Google for that slot.
    //
    // Roadblock-Ready Ads, is the ability to target all ads on first page load with a specific set of ads for a campaign takeover.
    // An example would be the Superbowl, if the NFL wanted to render a Superbowl ad in every slot on the page we need to support that
    // with what is called "roadblock" ads. Google requires all of these ads to be built in the <head> and to be requested in their
    // single Page Load request they make.
    //
    // Prebidding Ads, is the ability to allow bidders at the Prebid.js auction to bid on rendering in a slot on the page. This results
    // in more ad revenue than a standard google ad. For this to work all ads must be defined and then sent to Prebid.js to auction
    // them, then returned winner information appended to the ad before Google renders it.
    //
    // Given this, the general order of operations for ads is:
    //   1. Define all ads that would appear on first page load for the page
    //   2. Define bidders for the ads that will appear on first page load too
    //   3. Make sure Google is setup to render all first page load ads in a single request
    //   4. Send the ad info to auction with an async callback when completed
    //   5. Additionally set a max timeout before we quit waiting for action to complete
    //   6. Apply prebid callback data to ads and call "display" before the page finishes loaded
    //   7. Google takes all ad data and returns ads, supporting potentially both Roadblocks and Prebid
    //   8. Disable single page load request for Google ads
    //   9. All remaining and dynamic ads created at any point then individually make calls to render without Auction
    debug("START");

    window.DFP_networkId = "11222444";
    window.DFP_sitepage = "DMN";
    window.DFP_adTargets = {};
    window.DFP_adTargetsParam = " ";
    window.adSlots = {};
    window.bidSlots = []; // Prebid.js bid slots
    window.amzSlots = []; // Amazon prebid bid slots
    window.PREBID_TIMEOUT = 2000;
    window.useMobileAds = window.innerWidth / (window.devicePixelRatio ? window.devicePixelRatio : 1)  < 992 ? true : false;
    window.AMAZON_PUB_ID = "592c72a8-a9c3-44d2-ae9e-42bc09831712";
    window.ROXOT_PUBLISHER_ID = "f4300502-9fc2-416b-8fec-1cd6894679a9";

                window.DFP_adTargets.beta = "true";
                        window.DFP_adTargets.dnLoggedIn = "false";
                        window.DFP_adTargets.isPremiumSubscriber = "false";
                    window.DFP_adTargetsParam = "beta%3Dtrue%26dnLoggedIn%3Dfalse%26isPremiumSubscriber%3Dfalse";
        
    // GOOGLE DFP SETUP
    // Create Google Tag queue to process pushed functions asynchronously
    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];

    var prebidCompleted = false;
    var amazonCompleted = false;

    function prebidComplete() {
        debug("PREBID COMPLETE");
        prebidCompleted = true;
        attemptInitAdServer();
    }

    function amazonComplete() {
        debug("AMAZON COMPLETE");
        amazonCompleted = true;
        attemptInitAdServer();
    }

    function attemptInitAdServer() {
        debug("ATTEMPT INIT AD SERVER");
        if (prebidCompleted && amazonCompleted) {
            initAdserver();
        }
    }

    // Define the function that will load the Google Publisher Tag library (used by DFP) asynchronously
    function initAdserver() {
        // Skip reloading ad server if we already did with prebid (since this is called in a timeout)
        if(window.DallasNews.settings.ADS_HEADER_BIDDING){ if (pbjs.initAdserverSet) return; }
        debug("INIT AD SERVER");

        // Build the GPT library <script> to include in the page
        (function() {
            var gads = document.createElement('script');
            gads.async = true;
            gads.type = 'text/javascript';
            var useSSL = 'https:' == document.location.protocol;
            gads.src = (useSSL ? 'https:' : 'http:') +
            '//www.googletagservices.com/tag/js/gpt.js';
            var node = document.getElementsByTagName('script')[0];
            node.parentNode.insertBefore(gads, node);
        })();

        // Set our variable indicating we have already built the ad server (to prevent duplicate runs)
        if(window.DallasNews.settings.ADS_HEADER_BIDDING){ pbjs.initAdserverSet = true };
    };

    function getPrebuildSizeForWindow(prebuild, windowWidth) {
        // Build a non-serialized version
        var list = [];
        for (var k = 0; k < prebuild.sizes.length; k++) {
            var size = JSON.parse(prebuild.sizes[k]);
            list.push(size);
        }
        // Sort the list by their width
        list.sort(function(a, b) {
            var aBreakpointWidth = a[0][0];
            var bBreakpointWidth = b[0][0];
            if (aBreakpointWidth < bBreakpointWidth)
                return -1;
            if (aBreakpointWidth > bBreakpointWidth)
                return 1;
            return 0;
        })
        // Iterate the list until a size is larger than specified
        var acceptable = null;
        for (var l = 0; l < list.length; l++) {
            var lsize = list[l];
            if (lsize[0][0] < windowWidth) {
                acceptable = lsize;
            }
        }
        return acceptable;
    }

    function debugPrebidResponse() {
        // this logs the winning bids for each ad unit
        if (window.DEBUG_ADS) {
            var responses = pbjs.getBidResponses()
            var output = []
            for (var adunit in responses) {
                if (responses.hasOwnProperty(adunit)) {
                    var bids = responses[adunit].bids
                    for (var i = 0; i < bids.length; i++) {
                        var b = bids[i]
                        output.push({
                            'adunit': adunit,
                            'adId': b.adId,
                            'bidder': b.bidder,
                            'time': b.timeToRespond,
                            'cpm': b.cpm,
                            'msg': b.statusMessage
                        })
                    }
                    debug(bids);
                }
            }
            if (output.length) {
                if (console.table) {
                    console.table(output)
                } else {
                    for (var j = 0; j < output.length; j++) {
                        console.log(output[j])
                    }
                }
            } else {
                console.warn('NO prebid responses')
            }
        } // end debug
    }

    function runPrebid() {
        if (pbjs.hasRunAds) return;
        if (window.bidSlots.length == 0) return;

        // Once all ads are initialized on the page (or feed load), add those ad slots to the prebid auction
        pbjs.que.push(function () {
            // Add the all created ads to the prebid auction (most are put here in prebid.view)
            // and clear the bidSlots since they have been added
            pbjs.addAdUnits(window.bidSlots)
            window.bidSlots = [];

            // Make the prebid request to fetch the bidders for the ads
            debug("PREBID.JS BIDS REQUESTED");
            pbjs.requestBids({
                // When bidders are complete, initialize the ad server which should render the ads according to bids
                // Note, a timeout exists that could potentially init the ad server before this (bottom of file)
                bidsBackHandler: function (bidResponses) {
                    debug("PREBID.JS BIDS BACK");
                    debugPrebidResponse();
                    prebidComplete();
                }
            });
        });

        pbjs.hasRunAds = true;
    }

    function runAmazon() {
        if (apstag.hasRunAds) return;
        if (window.amzSlots.length == 0) return;

        debug("AMAZON BIDS REQUESTED");
        apstag.fetchBids({
            slots: window.amzSlots,
            timeout: 2e3
        }, function(bids) {
            debug("AMAZON BIDS BACK");
            debug(bids);
            // bids are applied in the queued googletag.cmd that runs after the ad server is initialized
            // tell peer code im done and initAdServer can run (when other services are complete)
            amazonComplete();
        });

        apstag.hasRunAds = true;
    }

    // AMAZON PUBLISHING SERVICES
    // This was added after the Prebid project
    // It works similar to Prebid in that Amazon will add Targeting attributes to DFP Slots
    // It needs to run in parallel to Prebid and the initAdServer call should not fire until after both have completed (or the TIMEOUT has hit)
    // load the apstag.js library
    if(window.DallasNews.settings.ADS_HEADER_BIDDING){
        debug("AMAZON SETUP");
        !function(a9,a,p,s,t,A,g){if(a[a9])return;function q(c,r){a[a9]._Q.push([c,r])}a[a9]={init:function(){q("i",arguments)},fetchBids:function(){q("f",arguments)},setDisplayBids:function(){},targetingKeys:function(){return[]},_Q:[]};A=p.createElement(s);A.async=!0;A.src=t;g=p.getElementsByTagName(s)[0];g.parentNode.insertBefore(A,g)}("apstag",window,document,"script","//c.amazon-adsystem.com/aax2/apstag.js");

        //initialize the apstag.js library on the page to allow bidding
        apstag.init({
            pubID: window.AMAZON_PUB_ID, //enter your pub ID here as shown above, it must within quotes
            adServer: 'googletag'
        });
    }

    // PREBID.JS SETUP
    // Setup Prebid Bidder Ad Units

    // Bidders are configured above in other partial
    // Create our prebid command queue, similar to the google command queue
    var pbjs = pbjs || {};
    pbjs.que = pbjs.que || [];

    if(window.DallasNews.settings.ADS_HEADER_BIDDING){
        debug("PREBID.JS SETUP");

        // If Prebid is enabled, append the prebid.js script to the page to load it asynchronously
        (function() {
            var pbjsEl = document.createElement("script");
            pbjsEl.type = "text/javascript";
            pbjsEl.async = true;
            pbjsEl.src = "/resources/motif/dist/js/prebid.js";
            var pbjsTargetEl = document.getElementsByTagName("head")[0];
            pbjsTargetEl.insertBefore(pbjsEl, pbjsTargetEl.firstChild);
        })();

        // Queue in a command to immediately configure prebid settings
        pbjs.que.push(function() {
            // Tell Prebid to send its analytics to Roxot (why?)
            pbjs.enableAnalytics([{
                provider: 'roxot',
                options: {
                    publisherIds: [window.ROXOT_PUBLISHER_ID]
                }
            },{
                provider: 'rubicon',
                options: {
                    accountId: rubicon.accountId,
                    endpoint: 'https://prebid-a.rubiconproject.com/event'
                }
            }]);
            // Change the bid amount granularity sent to Google so we can match to simpler line item prices (I think)
            // TODO: WILL BE DEPRECATED IN v1.0 and need to be passed in setConfig
            const customGranularity = {
              "buckets" : [{
                  "precision": 2,
                  "min" : 0,
                  "max" : 20,
                  "increment" : 0.01
                },
                {
                  "precision": 2,
                  "min" : 20,
                  "max" : 50,
                  "increment" : 0.05
                }]
            };
            // Setup Prebid to send all bids to Google, instead of just the highest bidder
            pbjs.setConfig({
                debug: window.DEBUG_ADS,
                priceGranularity: customGranularity,
                enableSendAllBids: true
            });
        });
    } // eo prebid installation


    // Lets configure our ad slot paramaters for page load ads, to support roadblock ads
    // Back in the day, ads were all loaded on the fly, these days first page load ads are configured in the head
    // then all requested in a single google ad request so that ad dudes can target all ads for one campaign (called roadblocks)




    // Prebid Slots, defined first so we can setup some targeting data for the ad
    if(window.DallasNews.settings.ADS_HEADER_BIDDING){
        debug("PREBID.JS BID SLOT DEFINITIONS");
        for(var slot in window.adSlotDefinitions){
            var prebuild = window.adSlotDefinitions[slot];

            // Skip ads that are inactive
            if (!prebuild.active) {
                continue;
            }

            // Skip ads we are not bidding for
            if (!prebuild.bid) {
                continue;
            }

            // Skip ads without bids
            var pos = prebuild["position"];
            if (!(window.useMobileAds ? window.mobileBids[pos] : window.desktopBids[pos])) {
                continue;
            }

            var size = getPrebuildSizeForWindow(prebuild, window.innerWidth);
            if (!size) {
                continue;
            }
            size = size[1];

            // We can have various sizes of ads depending on window width
            // So we need to determine the appropriate size Google will be rendering
            var bidSlot = {
                code: slot,
                sizes: size,
                bids: window.useMobileAds ? window.mobileBids[pos] : window.desktopBids[pos]
            }

            // If the ad definition for this page included additional bidder stick them also in the list of normal bidders
            if ('additionalBids' in prebuild  && 'mobile' in prebuild.additionalBids) {
                for (var mb in prebuild.additionalBids.mobile) {
                    bidSlot.bids.unshift(prebuild.additionalBids.mobile[mb]);
                }
            }
            if ('additionalBids' in prebuild  && 'desktop' in prebuild.additionalBids) {
                for (var db in prebuild.additionalBids.desktop) {
                    bidSlot.bids.unshift(prebuild.additionalBids.desktop[db]);
                }
            }
            debug(bidSlot);

            // Add this ad unit to the prebid auction list (which will be requested after the ads are prepped in integration)
            window.bidSlots.push(bidSlot);
        }
    } // eo prebid slot building for ads


    // Amazon Prebid Slots, defined first so we can setup some targeting data for the ads
    if(window.DallasNews.settings.ADS_HEADER_BIDDING){
        debug("AMAZON BID SLOT DEFINITIONS");
        for(var slot in window.adSlotDefinitions){
            var prebuild = window.adSlotDefinitions[slot];

            // Skip ads that are inactive
            if (!prebuild.active) {
                continue;
            }

            // Skip ads we are not bidding for
            if (!prebuild.bid) {
                continue;
            }

            // Skip ads without bids
            var pos = prebuild["position"];
            if (!(window.useMobileAds ? window.mobileBids[pos] : window.desktopBids[pos])) {
                continue;
            }

            var size = getPrebuildSizeForWindow(prebuild, window.innerWidth);
            if (!size) {
                continue;
            }
            size = size[1];

            // We can have various sizes of ads depending on window width
            // So we need to determine the appropriate size Google will be rendering
            var amzSlot = {
                slotID: slot, //example: 'div-gpt-ad-1475102693815-0'
                slotName: '/' + DFP_networkId + '/' + DFP_sitepage, //example: '12345/box-1'
                sizes: size, //example: [[300,250], [300,600]]
            }
            debug(amzSlot);

            // Add this ad unit to the prebid auction list (which will be requested after the ads are prepped in integration)
            window.amzSlots.push(amzSlot);
        }
    } // eo prebid slot building for ads

    // Google DFP Ad, setup second, so if we are using prebid we can extend with prebid targeting data (once the cmd queue is processed)
    googletag.cmd.push(function () {
        debug("DFP SLOT BUILDING");

        // Build our slots
        for(var slot in window.adSlotDefinitions){
            debug(slot);

            var prebuild = window.adSlotDefinitions[slot];
            var dfpSlot = null;

            if (!prebuild.active) {
                continue;
            }

            if (prebuild.outOfPage) {
                // Special ad type that is out of page
                dfpSlot = googletag.defineOutOfPageSlot('/' + DFP_networkId + '/' + DFP_sitepage, slot);
                dfpSlot.addService(googletag.pubads());
            } else {
                // Normal ads that appear in page with sizes
                var defaultSize = JSON.parse(prebuild.sizes[0]);

                // Iterate our sizes and add them
                var dfpSizeMappingBuilder = googletag.sizeMapping();
                for (var k = 0; k < prebuild.sizes.length; k++) {
                    var size = JSON.parse(prebuild.sizes[k]);
                    dfpSizeMappingBuilder.addSize(size[0], size[1]);
                }
                var dfpSizeMappingArray = dfpSizeMappingBuilder.build();

                // Build the slot
                // Support custom adUnitPaths if needed
                var adUnitPath = ("adUnitPath" in prebuild) ? prebuild.adUnitPath : '/' + DFP_networkId + '/' + DFP_sitepage;
                debug(adUnitPath)
                dfpSlot = googletag.defineSlot(adUnitPath, defaultSize[1], slot);
                dfpSlot.defineSizeMapping(dfpSizeMappingArray);
                dfpSlot.addService(googletag.pubads());
            }

            // Slot targetting
            dfpSlot.setTargeting("position", prebuild.position).
                                setCollapseEmptyDiv(true);
                if (window.DFP_adTargets) {
                    for (let key in window.DFP_adTargets) {
                        dfpSlot.setTargeting(key, window.DFP_adTargets[key])
                    }
                }
            dfpSlot.addService(googletag.pubads());

            // Window variables for our slot
            window[slot] = dfpSlot;
        }

        // Now that slots are built, if prebid was completed we have bid data to attach to the slots, before its sent off
        if (window.DallasNews.settings.ADS_HEADER_BIDDING) {
            debug("PREBID.JS SETTING BID TARGETS");
            pbjs.setTargetingForGPTAsync();
            debug("AMAZON SETTING BID TARGETS");
            apstag.setDisplayBids()
        }

        // Finalize Google Services
        // Make sure we load ads asynchronously so we dont block the page
        googletag.pubads().enableAsyncRendering();
        // Enable single request mode so all ads are fetched in one call, to support roadblock ads
        // (roadblock ads is how they can render a specific set of ads in each slot, usually because someone wanted to buy all ad space on a page)
        googletag.pubads().enableSingleRequest();
        // This would normally prevent google from loading ads on page load (until refresh() was called), but it is commented out so ads will load on page load
        // It is re-enabled after the first load of ads for dynamic ads in the Ad.js class
        // googletag.pubads().disableInitialLoad();
        // Enable google services for all defined ad slots
        googletag.enableServices();
    }); // eo google cmd for building the ads


    // If we are loading ads with Prebid, set a fallback timeout to load them if prebid is slow
    // Otherwise always load the ad server asap
    if(window.DallasNews.settings.ADS_HEADER_BIDDING){
        setTimeout(function () { 
            debug("PREBID TIMEOUT REACHED");
            initAdserver();
        }, window.PREBID_TIMEOUT);

        runPrebid();
        runAmazon();
    }
    else{
        debug("HEADER BIDDING DISABLED");
        initAdserver();
    }
</script>
<!-- // End Ads Setup -->

