<script type="text/javascript">
function getDevice() {
var view = "full";
var mobile = (/iphone|ipad|ipod|android|blackberry|mini|windows\sce|windows phone|palm/i.test(navigator.userAgent.toLowerCase()));
if (mobile) {
view = "mobile";
var userAgent = navigator.userAgent.toLowerCase();
if ((userAgent.search("android") > -1) && !(userAgent.search("mobile") > -1))
{
view = "tablet";
}
else if (userAgent.search("ipad") > -1)
{
view = "tablet";
}
}
return(view);
}
function mobileSetDevice(c_name,value,exdays) {
mobileDelCookie(c_name);
var c_value;
var exdate=new Date();
exdate.setDate(exdate.getDate() + exdays);
c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString()) + "; path=/;domain=dallasnews.com";
document.cookie=c_name + "=" + c_value;
//c_value=escape(value) + ((exdays==null) ? "" : "; expires="+exdate.toUTCString()) + "; path=/";
//document.cookie=c_name + "=" + c_value;
//document.location.href=document.location.pathname + document.location.search;
window.location.reload();
}
function mobileDelCookie(name) {
document.cookie = name +'=; expires=Thu, 01-Jan-70 00:00:01 GMT;path=/;domain=www.dallasnews.com';
document.cookie = name +'=; expires=Thu, 01-Jan-70 00:00:01 GMT;path=/;domain=dallasnews.com';
}
jqLoad.push(function(){
if (getDevice() == 'mobile') $("#mobileViewTop").css("display","block");
});
</script>
