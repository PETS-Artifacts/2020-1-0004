<script type="text/javascript">
	(function($) {
		$(document).ready(function(){
			_fontSize=parseInt( $('body').css('font-size') );
		})
		//Resizer
		$('.resize').click(function(e){
			e.preventDefault();
			task=$(this).attr('data-task');
			font_size=parseInt( $('body').css('font-size') );
			if(task=='minus'){
				if(font_size>9){
					$('body').css('font-size', font_size-1+"px" )
				}
			}
			if(task=='plus'){
				if(font_size<20){
					$('body').css('font-size', font_size+1+"px" )
				}
			}
		})
	})(jQuery);
</script>
