<script>
var slider1 =
	jQuery('.bxslider').bxSlider({
	  slideWidth: 624,
	  slideHeight: 415,
	  slideMargin: 0,
	  pager:false
	});

var slider2 =	
	jQuery('.bxslider_extra').bxSlider({
	  slideWidth: 624,
	  slideHeight: 415,
	  slideMargin: 0,
	  pager:false
	});
