	jQuery(window).scroll(function() {
		var height = jQuery(window).scrollTop();
		if(height  > 130) {
			jQuery('.main_nav_wrap').addClass('static-menu');
		}
	  else{
		jQuery('.main_nav_wrap').removeClass('static-menu');
	  }
	   
	});
	
	jQuery('#searchArchive').on('click', function() {
