<script>
jQuery(document).ready(function(){
   jQuery("#login_popup").click(function(event){
      if(event.target.className=="popup_bg"){
            close_login_popup();
          }
   });
    
});

jQuery(document).keyup(function(e) {
     if (e.keyCode == 27) { 
         close_login_popup();
    }
});
</script>
