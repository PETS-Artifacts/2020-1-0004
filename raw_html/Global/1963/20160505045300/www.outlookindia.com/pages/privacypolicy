function windowPopup(url, width, height) {
  var left = (screen.width / 2) - (width / 2),
      top = (screen.height / 2) - (height / 2);

  window.open(
    url,
    "",
    "menubar=no,toolbar=no,resizable=yes,scrollbars=yes,width=" + width + ",height=" + height + ",top=" + top + ",left=" + left
  );
}
jQuery(document).ready(function(){
	jQuery(".social-links").on("click", function(e) {
	  e.preventDefault();

	  windowPopup(jQuery(this).attr("href"), 500, 300);
	});
	
	jQuery(function() {
		if(screen.width > 960){
			var $sidebar   = jQuery(".stry_dtl_rgt"), 
			$window    = jQuery(window),
			offset     = $sidebar.offset(),
			topPadding = 60,
			sidebarHeight = $sidebar.height();

			var $leftCont = jQuery(".stry_dtl_lft"),
			leftHeight = offset.top + $leftCont.height() - sidebarHeight - topPadding;
		
			$window.scroll(function() {
				if ($window.scrollTop() > offset.top && $window.scrollTop() < leftHeight ) {
					$sidebar.stop().animate({
						marginTop: $window.scrollTop() - offset.top + topPadding
					});
				} else if ($window.scrollTop() < leftHeight){
					$sidebar.stop().animate({
						marginTop: 0
					});
				} else {
					console.log("OUT: scroll-top : "+$window.scrollTop()+" offset.top : "+offset.top+'  leftHeight : '+leftHeight);
					$sidebar.stop().animate({
						marginTop: leftHeight-offset.top
					});
				}
			});
		}	
	});
});

</script>

	
