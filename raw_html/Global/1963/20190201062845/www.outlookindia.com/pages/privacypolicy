<script>
var lastScrollTop = 0;
jQuery(window).scroll(function(event){
   var scroll = jQuery(window).scrollTop();
  if(scroll>=750){ 
	jQuery(".floater_outlook").show();  
  }
   var st = jQuery(this).scrollTop();
   if (st > lastScrollTop){
	   if(scroll>=750){ 
	 	jQuery(".floater_outlook").show();
	   }
	 } else {
	   jQuery(".floater_outlook").hide(); 
   }
   lastScrollTop = st;
});
</script>
