<script type="text/javascript">
    $(document).ready(function () {
        $('.logOut > a').text('log out');
    });

    (function ($) {
        $(function () {
            $.ajax({
                url: '/rest-api/login-status',
                cache: false,
                success: function (statusViewModel) {
                    if (statusViewModel && statusViewModel.IsLoggedIn) {
                        $('.loggedInPnl').show();

                        var blueVoicesUrl = $('#HeaderPlaceholder_T22B85045062_hlBlueVoices').attr('href');
                        if (statusViewModel.HasBlueVoicesRole && blueVoicesUrl !== undefined) {
                            $('#bvLinkContainer').show();
                        }
                        else //bmain-23 nalin
                        {
                            $('#bvLinkContainer').hide();
                        }
                    }
                    else {
                        $('.loggedOutPnl').show();
                    }
                }
            });            
        });
    }(jQuery));
</script>
            
