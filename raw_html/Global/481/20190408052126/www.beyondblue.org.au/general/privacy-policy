    <script type="text/javascript" src="/App_Themes/reskin/scripts/main-hub.js?v=20181206"></script>


    
    <script type="text/javascript">
        var ytTID = null;
        // BB - deferred activation of iframes, but only if they set data-src attribute to target url
        $(function () {
            processDeferredIframes();
        });

        function processDeferredIframes() {
            var deferIframes = $('iframe[data-src]');

            // set the src attribute to activate the video
            deferIframes.each(function () {
                $(this).attr('src', $(this).attr('data-src'));
            });

            if ((deferIframes.length > 0) && (ytTID == null)) {
                // kick off interval check for YT API
                ytTID = setInterval(handleYouTubeApiLinking, 250);
            }
        }

        function handleYouTubeApiLinking() {
            if (typeof onYouTubeIframeAPIReady === 'function') {
                // stop interval
                clearInterval(ytTID);
                ytTID = null;

                // attach API to video iframes
                onYouTubeIframeAPIReady();
            }
        }
    </script>
</body>
