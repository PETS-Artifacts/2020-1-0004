<script type="text/javascript">
    $(function () {
        $(".searchForm--header input").keypress(function (e) {
            if (e.keyCode === 13) {
                return GlobalSearch();
            }
        });

        $(".searchForm--header .button").click(function (e) {
            return GlobalSearch();
        });
    });

    function GlobalSearch() {
        var keyword = $("#" + 'HeaderPlaceholder_T22B85045077_txt_Keyword').val();

        location.href = '/general/search-results?keywords={{keyword}}'.replace('{{keyword}}', keyword);

        return false;
    }
</script>
