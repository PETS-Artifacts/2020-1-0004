    <script type="text/javascript">
        $(document).ready(function(){
            var submitted = false;
            $("#captcha_code").click(function(){
                $('.incorrect-code').remove();
            });
            $("#suggestion-box").submit(function(e){
                e.preventDefault();
                if (!submitted) {  // first click of the button
                    if ($("#suggestion").val() == '') {
                        alert('Please enter a suggestion');
                        return;
                    }
                    $("#captcha-li").fadeIn(100);
                    $(this).find('button').text('Send');
                    submitted = true;
                } else { // make sure captcha is verified
                    var parent = $(this);
                    $.post('/submit_suggestion.php',
                        {
                            token : '275c577a19bd968c26797aa2d7425416',
                            suggestion : $("#suggestion").val(),
                            captcha_code : $("#captcha_code").val()
                        },
                    function(response){
                        if (response.status == 200) {
                            parent.find('.c_white').text('Thanks for your suggestion!').css({
                                'font-size' : '16px !important',
                                'font-weight' : '600'
                            });
                            parent.find('li').filter(':not(".c_white")').fadeOut(100).remove();
                            submitted = false;
                        } else {
                            $('#captcha').get(0).src = '/securimage_show.php?' + Math.random();
                            $("#captcha_code").val('');
                            $("#captcha-li").append('<span class="incorrect-code" style="color: #ff0003; font-weight: 500;"><br>Incorrect Code!</span>');
                        }
                    });
                }
            });
        });
    </script>
