<script type="text/javascript">
    
    var gblStrSearchPageUrl = 'https://www.visitbarbados.org/search';
    function DoSidebarSearch() {
        KeyWord = document.getElementById("txtSidebarSearchBox").value;
        if (KeyWord.trim() != "") {
            window.location.href = 'https://www.visitbarbados.org/search' + "?q=" + $('#txtSidebarSearchBox').val();
            //document.forms[0].action = 'https://www.visitbarbados.org/search';
            //document.forms[0].submit();
        }
        else {
            return false;
        }
    }
    function checkSidebarSubmit(e) {
        if (e && e.keyCode == 13) {
            return DoSidebarSearch();
        }
    }
    </script>
 
