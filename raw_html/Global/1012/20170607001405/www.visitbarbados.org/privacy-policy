<script type="text/javascript">
         // Flowplayer add common classes
         // $(".flowplayer").addClass("play-button no-time fixed-controls no-fullscreen no-volume");
         // FlowPlayer Api for Experiences control

         $(window).load(function () {
             setTimeout(function () {
                 $(".midbar-bg .mid-flwplayer").each(function () {
                     var midbarPlayer = $(this);
                     if (midbarPlayer != null && midbarPlayer != undefined) {
                         midbarPlayer.flowplayer();
                         var api = $(this).data('flowplayer');
                         apiArray.push(api);
                         api.bind('ready', function () {
                             api.pause();
                             api.mute(true);
                         });
                         api.bind('finish', function () {
                             api.play();
                         });
                     }
                 });
             }, 2000);
         });
     </script>
