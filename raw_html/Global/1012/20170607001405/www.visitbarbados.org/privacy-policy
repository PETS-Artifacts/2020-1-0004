    function NewsLetterFooter() {    
        var blnStatus = true;
        if (document.getElementById("txtNewsLetterEmail").value == "") {
            $('#txtMsgfooter').addClass("text-danger")
            $('#txtNewsLetterEmail').addClass("has-error")
            $('#txtMsgfooter').removeClass("text-success")            
            document.getElementById("txtMsgfooter").innerHTML = 'Please enter email address.';
            document.getElementById("txtMsgfooter").style.display = "";            
            return false;
        }
        else {
            if (!validateEmail(document.getElementById("txtNewsLetterEmail").value)) {
                $('#txtMsgfooter').addClass("text-danger")
                $('#txtNewsLetterEmail').addClass("has-error")
                $('#txtMsgfooter').removeClass("text-success")
                document.getElementById("txtMsgfooter").innerHTML = 'Please enter valid email address.';
                document.getElementById("txtMsgfooter").style.display = "";
                return false;
            }
        }
        if (blnStatus == true) {
            
            post('https://www.visitbarbados.org/newsletter', { email: document.getElementById("txtNewsLetterEmail").value });
            //InsertNewSLatterFooter()
        }
    }
    $('#txtNewsLetterEmail').keypress(function (event) {
        if (event.which == 13) {
            NewsLetterFooter();
            return false;
        }
    });

    /*
