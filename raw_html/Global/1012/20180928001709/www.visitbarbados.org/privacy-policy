<script type="text/javascript">
    // Disable Back button issue script : Start
    $(document).ready(function ($) {
        if (window.history && window.history.pushState) {
            window.history.pushState('forward', null, null);
            $(window).on('popstate', function () {
                //alert('Back button was pressed.');
                return false;
            });
        }
        setTimeout(function () {
            if (getCookie(objCookieName) != "accept") {
                $('.cookieBox').css('display', '');
            }
        }, 10);
    });
    // Disable Back button issue script : End
    function NewsLetterFooter() {    
        var blnStatus = true;
        if (document.getElementById("txtNewsLetterEmail").value == "") {
            $('#txtMsgfooter').addClass("text-danger")
            $('#txtNewsLetterEmail').addClass("has-error")
            $('#txtMsgfooter').removeClass("text-success")            
            document.getElementById("txtMsgfooter").innerHTML = 'Please enter email address.';
            document.getElementById("txtMsgfooter").style.display = "";            
            return false;
        }
        else {
            if (!validateEmail(document.getElementById("txtNewsLetterEmail").value)) {
                $('#txtMsgfooter').addClass("text-danger")
                $('#txtNewsLetterEmail').addClass("has-error")
                $('#txtMsgfooter').removeClass("text-success")
                document.getElementById("txtMsgfooter").innerHTML = 'Please enter valid email address.';
                document.getElementById("txtMsgfooter").style.display = "";
                return false;
            }
        }
        if (blnStatus == true) {
            
            post('https://www.visitbarbados.org/newsletter', { email: document.getElementById("txtNewsLetterEmail").value });
        }
    }
    $('#txtNewsLetterEmail').keypress(function (event) {
        if (event.which == 13) {
            NewsLetterFooter();
            return false;
        }
    });

    function post(path, params) {
        var method = "post"; // Set method to post by default if not specified.
        var form = document.createElement("form");
        form.setAttribute("method", "post");
        form.setAttribute("action", path);

        for (var key in params) {
            if (params.hasOwnProperty(key)) {
                var hiddenField = document.createElement("input");
                hiddenField.setAttribute("type", "hidden");
                hiddenField.setAttribute("name", key);
                hiddenField.setAttribute("value", params[key]);

                form.appendChild(hiddenField);
            }
        }

        document.body.appendChild(form);
        form.submit();
    }

		$(window).load(function () {
    	setTimeout(function () {
    		if (blnInstagram == true) {
    			GetInstagramPhotos();
    			GetTwitterFeed();
    		}
    	}, 6000);

    	
    	/* Check Availability Gedget : Start */
    	setTimeout(function () {
    		$(document).on('click', '#trigger_gadget', function () {
    			$('#hih_gadget_main_wrap').addClass('open');
    			$(this).html('X');
    		});
    		$(document).on('click', '#hih_gadget_main_wrap.open #trigger_gadget', function () {
    			$('#hih_gadget_main_wrap').removeClass('open');
    			$(this).html('Check availability');
    		});
    	}, 6000);
    	/* Check Availability Gedget : End */
    	

			$('#checkavail_btn').click(function () {
				if ($('#hih_gadget').attr('id') == undefined) {
					var linkerParam = '';
					var strURL = 'http://gadgets.hotelinternethelp.com/render?gid=323&referrer=visitbarbados.org';
					if (window[window.GoogleAnalyticsObject] != undefined) {
						linkerParam = window[window.GoogleAnalyticsObject].getAll()[0].get('linkerParam');
						strURL = strURL + '&_ga=' + linkerParam;
					}
					$('<iframe>', {
						src: strURL,
						id: 'hih_gadget',
						allowtransparency: 'true',
						scrolling: 'no',
						frameborder: '0'
					}).appendTo('#iframe_content_1514782135_323');
					$('#hih_gadget').css("height", "235px");

					setTimeout(function () {
						$('#hih_gadget_main_wrap').css("display", "");
						$('#hih_gadget_main_wrap').addClass("open");
					}, 1000);
				}
				else {
					$('#hih_gadget_main_wrap').css("display", "");
					$('#hih_gadget_main_wrap').addClass("open");
				}
			});

    	
    	setTimeout(function () {
    		(function () {
    			var po = document.createElement('script'); po.type = 'text/javascript'; po.async = true;
    			po.src = 'https://apis.google.com/js/plusone.js';
    			var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(po, s);
    		})();
    		;
    	}, 5000);
    	setTimeout(function () {
    		$('iframe[name=google_conversion_frame]', parent.document).css('display', 'none');
    	}, 200);
    	

    });

    var intCount = 0;
    var offsetDiff = 100;
    if (offsetForAutoscroll != undefined && offsetForAutoscroll != null) {
        offsetDiff = parseInt(offsetForAutoscroll);
    }
    offsetDiff = offsetDiff + 70;
    intCount = parseInt($('.page-number.selected').html());
    if (intCount > 1) {
        setTimeout(function () {
            $("html,body").animate({
                scrollTop: $(".pagination").eq(0).offset().top - offsetDiff
            }, 100);
        }, 2000);
    }

    //Get photos
    function GetInstagramPhotos() {     
  
        $("#PhotosUL").html("");
        $("#PhotoLarge").html("");
        $.ajax({
            type: "GET",
            contentType: "application/json; charset=utf-8",
            url: 'https://www.visitbarbados.org/templates/Defaults/Handlers/hdlInstagram.ashx',
            cache: true,
            error: function () {
            },
            success: function (data) {
                if (data == "") {
                    $("#PhotosDiv").hide();
                } else {
                    $("#PhotosDiv").show();
                    var intI;
                    if (data["data"].length >= 5) {
                        intI = 5;
                    }
                    else {
                        intI = data["data"].length;
                    }
                    for (var i = 0; i < intI; i++) {
                        var clsName ='';
                        if(i == 4){
                            clsName ='last';
                        }
                        if (i == 0) {
                            $("#imgLargeInstagramImage").attr("src", data.data[i].images.standard_resolution.url);
                        }
                        else {
                            $("#PhotoLarge").append("<li class='"+clsName+"'><div class='tabl-middel'><a href=\"http://instagram.com/visitbarbados\" target=\"_blank\" title=\"@VisitBarbados\"><img style='width:209px;' class='lazy' src='"+data.data[i].images.thumbnail.url+"' style='display: none;'></a></div></li>");
                        }
                    }
                }
            }
        });
    }

    function GetTwitterFeed() {
        $.ajax({
            type: "GET",
            contentType: "application/json; charset=utf-8",
            url: 'https://www.visitbarbados.org/templates/Defaults/Handlers/hdlTwitter.ashx',
             cache: true,
            error: function () {
            },
            success: function (data) {
                for (var i = 0; i < 2; i++) {
                    $("#dvTwitter").append("<div class='barbados-twitter'><div class='twitter-head'><span class='twitter-icon'></span>@Barbados</div><div class='twitter-content'>" + data[i].text + "</div>");
                }
            }
        });
    }

	$(window).load(function () {
		setTimeout(function () {
			$('.language-dropdown').find('li').each(function () {
				var originalImg = $(this).find('img').attr("data-original");
				$(this).find('img').attr("src", originalImg);
			});
		},1000);
	});
		
</script>



