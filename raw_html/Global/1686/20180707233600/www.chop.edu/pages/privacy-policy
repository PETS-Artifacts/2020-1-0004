    var platform_expose = jQuery.ajax({
      type: "POST",
      url: '/sites/all/libraries/chop_platform_detection/detect.php',
      dataType: 'json',
      async: false,
      data: {function_name: 'detect_device_type_os_browser'},
      success: function (obj, textstatus) {
        if( ('error' in obj) ) {
          console.log(obj.error);
        }
      }
    });
      var classes_to_add  = JSON.parse(platform_expose.responseText);
      document.getElementsByTagName('body')[0].className+=classes_to_add["result"];
  </script>
<script type="text/javascript">
  var ip_detection = jQuery.ajax({
    type: "POST",
    url: '/sites/all/libraries/chop_ip_detection/detect.php',
    dataType: 'json',
    async: false,
    data: {function_name: 'detect_internal_ip'},
    success: function (obj, textstatus) {
      if( ('error' in obj) ) {
        console.log(obj.error);
      }
    }
  });
  var user_location  = JSON.parse(ip_detection.responseText);
  document.getElementsByTagName('body')[0].className+=" "+user_location["result"];
</script>
