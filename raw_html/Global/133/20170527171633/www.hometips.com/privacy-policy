<script>
jQuery(document).ready(function() {
        jQuery("img").each(function() {
          var link = jQuery(this).attr("src");
          var newLink = link.replace("http://", "https://");
          jQuery(this).attr("src", function() {
            return newLink
        });
     });
  });
</script>

