    jQuery(document).ready(function ($) {

        $('#headerShortlist').on('click', function (e) {
            e.preventDefault();
            var request = $.ajax({
                type: "GET",
                url: '/public/shortlist/headershortlist',
                datatype: "html",
                contentType: "text",
                success: function (response) {
                    $("#fillHeaderShortlist").html(response);
                    //document.getElementById("compareProdAssets").innerHTML = response;
                    console.log("shortlist header loaded successfully");
                },
                error: function (e) {
                    console.log("Error occurred while shortlist header load");
                }
            });

        });
    });
