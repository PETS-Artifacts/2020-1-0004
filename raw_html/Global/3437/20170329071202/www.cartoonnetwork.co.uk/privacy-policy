<script type="text/javascript">

    (function() {

        var placementId     = '31735',
            size            = '728x90',
            slotName        = '728x90 bottom',
            ad_id           = 'ad_area_' + placementId + '_' + slotName.replace(/\s/g,'_'),
            ad_className    = '',
            ad              = new AwesomeDisplay(placementId, null, "privacy-policy".split("/"));

        if (slotName !== '728x90 bottom') {
            switch (size) {
                case '300x250':
                case '728x90':
                case '970x250':
                    ad_className = 'refreshable-ad';
                    break;
                default:
                    break;
            }
        }

        document.write('<div id="' + ad_id + '" class="' + ad_className + '" data-placement-id="' + placementId + '"><\/div>');
        document.getElementById(ad_id).appendChild(ad.element);

    })();

</script>
