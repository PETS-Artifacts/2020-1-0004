<script language="javascript" type="text/javascript">

  function validateInputs() {
        var checked = document.getElementById("phleftrail_3_CheckBox1").checked;
        var streetText = document.getElementById("phleftrail_3_txtStreet").value;
        var zipText = document.getElementById("phleftrail_3_txtZip").value;
        var textNotOk = (streetText == 'Enter your Address' || streetText.length == 0) && (zipText == 'Enter your Zip Code' || zipText.length == 0)
        if (!checked) {
            var message = "Please accept terms of use.";
            if (textNotOk) {
                message = message + "\nAlso please enter a city or zip code.";
            }
            alert(message);
        } else {
            if (textNotOk) {
                alert("Please enter a city or zip code");
            }
        }
    }

</script>

