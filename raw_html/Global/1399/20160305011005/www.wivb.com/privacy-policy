		});
		//Decompose URL information and pass it to AAM
		function objIsEmpty(obj) {
		    for(var prop in obj) {
		        if(obj.hasOwnProperty(prop) && prop !== "")
		                return false;
		    }
		        return true;
		 }
		var uriData = DIL.tools.decomposeURI(document.URL);
		delete uriData.search;
		delete uriData.href;
		if(! objIsEmpty(uriData.uriParams)){
		      lintvDil.api.signals(uriData.uriParams, 'c_');
		};
		delete uriData.uriParams;
		lintvDil.api.signals(uriData, 'c_').submit();

