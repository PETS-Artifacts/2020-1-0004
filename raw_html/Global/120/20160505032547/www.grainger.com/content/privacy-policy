        var pageUID = digitalData.page.pageInfo.pageUID;
        var pageType = digitalData.page.pageInfo.pageType;
        if (jQuery("h1").text()!==''){       
            digitalData.page.pageInfo.contentTitle= jQuery.trim(jQuery("h1").text());
        }
        else if(isNotEmpty(pageUID) && pageUID == 'authrecovery'){
             digitalData.page.pageInfo.contentTitle= jQuery.trim(jQuery("h2").text());
        }
        else if(isNotEmpty(pageUID) && pageUID == 'e_homepage'){
         digitalData.page.pageInfo.contentTitle= 'homepage';
