<script>

jQuery(document).ready(function() {
	getDocumentreadyForAdobeAnalytics();
	updateFilter() ;
	updateSignUpEmailURL();
});


function updateSignUpEmailURL(){
	jQuery('#e_footercol7 p a').attr('href',jQuery('#e_footercol7 p a').attr('href')+"&communityType="+digitalData.user.profile.attributes.communityType);
	jQuery('#signUpForEmailHrefId').attr('href',jQuery('#signUpForEmailHrefId').attr('href')+"&communityType="+digitalData.user.profile.attributes.communityType);
}

function updateFilter() {
    try {
        var filterType = '';
        var filterValue = '', filterNavType = '';
        var attributeTemplate = "Text";
        var filterName = filterType;
        if (filterNavType) {
            switch (filterNavType) {
            case "KEYWORD":
                filterType = "keyword";
                attributeTemplate = "keyword";
                break;
            case "REFINE_KEYWORD":
                filterType = "keyword";
                attributeTemplate = "keyword";
                break;
            case "TECHNICAL_ATTRIBUTE_DIMENSION":
                filterType = "Technical";
                attributeTemplate = "Text";
                break;
            case "VISUAL_ATTRIBUTE_DIMENSION":
                filterType = "Visual";
                attributeTemplate = "Visual";
                break;
            case "DIMENSION":
                filterType = "Navigation";
                break;
            }
        }
        if (filterName) {
            switch (filterName) {
            case "Product Categories":
                filterName = "Browse";
                break;
            case "SG_Brand":
                filterName = "Brand";
                break;
            case "Shop Items By":
                filterName = "Shop By";
                break;
            }
        }

        if (typeof window.digitalData.internalSearch !== 'undefined') {
           var  tempModalItem = {
                filter : {}
            };
            jQuery.extend(true, digitalData.internalSearch, tempModalItem);
            var resultViewCookie = jQuery.cookie('resultViewFilterApplied');
            if (resultViewCookie == "true") {
                filterType = "Results Displayed";
                filterName = "Results Displayed";
                attributeTemplate = "Results Displayed";
                filterValue = jQuery('.itemsPerPage span').html();
            }
            var sortFilterCookie = jQuery.cookie('isSortFilterApplied');
            if (sortFilterCookie == "true") {
                filterType = "Sort";
                filterName = "Sort";
                attributeTemplate = "Sort";
                filterValue = jQuery(
                        '.dropdownSelect select.sortBySelect option:selected')
                        .val();
            }
            var previuoslyPushasedFilterCookie = jQuery
                    .cookie('previouslyPurchased');
            if (previuoslyPushasedFilterCookie == "true") {
                filterType = "Previously Purchased";
                filterName = "Previously Purchased";
                attributeTemplate = "Previously Purchased";
                filterValue = "Previously Purchased";
            }
            var paginationFilterCookie = jQuery
                    .cookie('paginationFilterApplied');
            if (paginationFilterCookie == "true") {
                filterType = "Pagination";
                filterName = "Pagination";
                attributeTemplate = "Pagination";
                filterValue = "Page "
                        + jQuery('.searchPagination ul li.current').html();
            }

            jQuery.cookie('isSortFilterApplied', null, {
                expires : 7,
                path : '/'
            });
            jQuery.cookie('previouslyPurchased', null, {
                expires : 7,
                path : '/'
            });
            jQuery.cookie('paginationFilterApplied', null, {
                expires : 7,
                path : '/'
            });
            jQuery.cookie('resultViewFilterApplied', null, {
                expires : 7,
                path : '/'
            });

            digitalData.internalSearch.filter.attributeType = filterType;
            digitalData.internalSearch.filter.category = filterName;
            digitalData.internalSearch.filter.filterValue = filterValue;
            digitalData.internalSearch.filter.attributeTemplate = attributeTemplate;
        }
    } catch (err) {
        console.log("Error publishing the event for filter Tag");
    }
}

