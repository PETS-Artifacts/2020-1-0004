<script>
var app = {};
var $cache = {};
var callbacks = {};
app.register = function(event, callback) {
if (callback === undefined) {
callback = event;
event = 'document.ready';
}
if (typeof event === 'string') {
event = [event];
}
for (var item in event) {
if (callbacks[event[item]] === undefined) {
callbacks[event[item]] = [];
}
callbacks[event[item]].push(callback);
}
return callback;
};
</script>

