<script>
  var miniCartSelector = '#mini-cart';
  var miniCartAnchorSelector = 'a.mini-cart-link';
  app.register(function() {
    $(miniCartAnchorSelector).addClass('navigation__icon');

    var onMobileBreakPointHandler = function() {
      if (window.innerWidth < 768) {
        $(miniCartSelector).addClass('navigation__icon');
      } else {
        $(miniCartSelector).removeClass('navigation__icon');
      }
    };

    onMobileBreakPointHandler();

    app.register('minicart.update', function() {
      $(miniCartAnchorSelector).addClass('navigation__icon');
      onMobileBreakPointHandler();
    });

    $(window).on('resize', onMobileBreakPointHandler);
  });
</script>

<style>
  @media (max-width: 767px) {
    #navigation .mini-cart-container {
      display: flex !important;
      order: 2;
    }

    .mini-cart-item-total, .mini-cart-content {
      display: none;
    }
  }
</style>




    
  

  

  



  
  

  
  

  
  

    
    
      
        



      
    
  

  

  















