<!-- This is a hack created to display mush bundle messaging when it applies to items that qualify -->
<script>
app.register(function() {
  // Copy insert for Olowahu and Mush Bundle
    var olowahuBundleMessage = 'Get 2 pairs of these flips for $45! Snag more for less of this sun-ready style.';
    var mushBundleMessage = 'Get 2 pairs of these flips for $45! Snag more for less of this sun-ready style.';
    var wedgeBundleMessage = 'Get 2 pairs for $55! Snag more for less of this sun-ready style.';

    var checkModelNumberOnElement = function(checkOnElementSelector, againstModelNumberArray, callbackFunction) {
      var modelNumber = $(checkOnElementSelector).text().split('Model: ');

      if (modelNumber.length > 1) {
        modelNumber = modelNumber[1].replace(/\s+/, '');

        for (var i = 0; i < againstModelNumberArray.length; i++) {
          var againstModelNumber = againstModelNumberArray[i];
          if (modelNumber === againstModelNumber) {
            callbackFunction();
            break;
          }
        }
      }
    }

    // Function for checking model numbers
    var checkModelNumbers = function() {
      checkModelNumberOnElement('.product-page__description__model-number', ["6840"], function() {
        $('.product-add-to-cart').before( '<div class="bundle-message">' + olowahuBundleMessage + '</div>');
      });

      checkModelNumberOnElement('.product-page__description__model-number', ["4198", "1001824", "1001825", "4168", "1004890", "1015198"], function() {
        $('.product-add-to-cart').before( '<div class="bundle-message">' + mushBundleMessage + '</div>');
      });
      checkModelNumberOnElement('.product-page__description__model-number', ["1000099", "1000093", "1015869"], function() {
        $('.product-add-to-cart').before( '<div class="bundle-message">' + wedgeBundleMessage + '</div>');
      });
    }

    // Check model numbers now
    checkModelNumbers();

    // Check after variation update
    app.register('product.variant.update', function() {
      checkModelNumbers();
    });

  });
</script>
