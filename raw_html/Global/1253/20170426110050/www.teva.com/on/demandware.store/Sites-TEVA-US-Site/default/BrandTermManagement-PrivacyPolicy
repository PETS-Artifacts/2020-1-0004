<script>
app.register(function() {
  var isSafari = /^((?!chrome|android).)*safari/i.test(navigator.userAgent);

  if (isSafari) {
    $('.apple-pay').css('display', 'block');
  } else {
    $('.shipping-only').css('display', 'block');
  }
});
