<script>
  app.register(function() {

    // If there is no cookie set by the user to dismiss the messaging
    if (typeof $.cookie('header-message') === 'undefined') {

      // Show the messaging
      $('#header-messaging').css('display', 'block');
    }

    // When the user clicks close to dismiss this messaging
    $('#header-messaging .close-btn').click(function(e) {
      e.preventDefault();
      $('#header-messaging').addClass('is-dismissed');
      $.cookie('header-message', 'dismissed', { path: '/' });
    });
  });
</script>

