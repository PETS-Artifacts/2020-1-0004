<script>
var app = {};
var $cache = {};
var callbacks = {};
app.register = function(event, callback) {
if (callback === undefined) {
callback = event;
event = 'document.ready';
}
if (typeof event === 'string') {
event = [event];
}
for (var item in event) {
if (callbacks[event[item]] === undefined) {
callbacks[event[item]] = [];
}
callbacks[event[item]].push(callback);
}
};
app.unregister = function(event, callback) {
if (typeof event === 'string') {
event = [event];
}
if (!callback) {
for (var item in event) {
if (typeof callbacks[event[item]] !== undefined
&& callbacks[event[item]].length > 0) {
callbacks[event[item]] = [];
}
}
} else {
for (var item in event) {
if (typeof callbacks[event[item]] !== undefined
&& callbacks[event[item]].length > 0
&& callbacks[event[item]].indexOf(callback) > -1) {
callbacks[event[item]]
.splice(callbacks[event[item]].indexOf(callback), 1);
}
}
}
}
</script>

