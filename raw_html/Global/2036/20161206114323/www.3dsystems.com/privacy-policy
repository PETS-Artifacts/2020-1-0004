<source media="screen and (min-width: 720px) and (max-width: 959px)" srcset="http://www.3dsystems.com/sites/www.3dsystems.com/files/styles/mega-menu-three-l/public/2016-printers-lineup_0_0.png?itok=Ic6rYV49 1x" />
<source media="screen and (min-width: 481px) and (max-width: 719px)" srcset="http://www.3dsystems.com/sites/www.3dsystems.com/files/styles/mega-menu-three-m/public/2016-printers-lineup_0_0.png?itok=9WPz1O90 1x" />
