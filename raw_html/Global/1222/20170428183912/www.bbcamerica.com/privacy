					//close container on click off
					$(document).mouseup(function (e){
					    var share_expand_div = $( ".share-expand" );

					    if (!share_expand_div.is(e.target) // if not share_expand_div
					        && share_expand_div.has(e.target).length === 0) // ... nor a descendant of the share_expand_div
					    {
					        $( ".social-share .facebook" ).parents("li").removeClass("active");
					    }
					});
				
					

				}) ( jQuery );

