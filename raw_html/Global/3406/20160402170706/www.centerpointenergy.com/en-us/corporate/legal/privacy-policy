<script type="text/javascript">
    //<![CDATA[    
    var _tag = new WebTrends();

    function trackTrends() {
        _tag.dcsGetId();

        _tag.dcsCustom = function () {
            // Add custom parameters here.
            //_tag.DCSext.param_name=param_value;
        }

        _tag.WT.mc_id = _tag.dcsQP("WT.mc_id");
        _tag.WT.ac = _tag.dcsQP("WT.ac");
        _tag.dcsCollect();
    };

    if (ExecuteOrDelayUntilScriptLoaded && _spBodyOnLoadFunctionNames) {
        _spBodyOnLoadFunctionNames.push('CheckMetaTags');
        _spBodyOnLoadFunctionNames.push('trackTrends');
    }

    function CheckMetaTags()
    {
        var metaDesc;
        var metaKeywords;
        var Desc = $("meta[name='Description']").attr('content');
        var CNPKeywords = $("meta[name='Keywords']").attr('content');

        if (document.querySelector("meta[name='keywords']") != null) {
            metaKeywords = $("meta[name='keywords']").attr('content');
            $("meta[name='keywords']").remove();
            if (CNPKeywords)
                $("meta[name='Keywords']").attr('content', CNPKeywords + ' ' + metaKeywords);
            else
                $("meta[name='Keywords']").attr('content', metaKeywords);
           
        }
        if (document.querySelector("meta[name='description']") != null) {
            metaDesc = $("meta[name='description']").attr('content');
            $("meta[name='description']").remove();
            if(Desc)
                $("meta[name='Description']").attr('content', Desc + ' ' + metaDesc);
            else
                $("meta[name='Description']").attr('content', metaDesc);
            
        }
    }
    //]]>
</script>
