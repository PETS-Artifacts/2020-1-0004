    <script>
        function footer() {
            var footerH = $('toggleshow');
            var fH = footerH.height();
            var i = 1;

            $('.fTab').on('click', function () {

                $(this).toggleClass('current');
                if (i === 1) {
                    jQuery("#messageSpan").removeClass('footer-up');
                    jQuery("#messageSpan").addClass('footer-down');
                    $('toggleshow').slideToggle(500);

                    i = 0;
                }
                else {

                    jQuery("#messageSpan").addClass('footer-up');
                    jQuery("#messageSpan").removeClass('footer-down');
                    $('toggleshow').slideToggle(500);
                    i = 1;
                }


            });





        }

        footer();

    </script>
    <script>

        $(document).ready(function () {
            var pixel = 500;// $('footer.onscroll-show').offset().top - 300;

            function isMyStuffScrolling() {
                var docHeight = $(document).height();
                var scroll = $(window).height() + $(window).scrollTop();
                return (docHeight == scroll);
            }
            if (isMyStuffScrolling()) {
                $('footer.onscroll-show').addClass('footer-visible');
            }

            $(window).scroll(function () {
                $('footer.onscroll-show').each(function (i) {
                    var y = $(document).scrollTop();
                    var t = $(this).offset().top;
                    if (y > pixel) {
                        $(this).removeClass('footer-hidden').addClass('footer-visible');
                    } else {
                        $(this).removeClass('footer-visible').addClass('footer-hidden');

                    }

                });
            });

            $('.footer_menu').on('click', function () {
                $('footer.onscroll-show').addClass('footer-active');
            });
            $('.footer-toggle-icons').on('click', function () {

                if ($('footer.onscroll-show').hasClass('footer-active')) {

                    $('footer.onscroll-show').removeClass('footer-active');
                }
                else {
                    $('footer.onscroll-show').addClass('footer-active');
                }
            });
        });

    </script>

