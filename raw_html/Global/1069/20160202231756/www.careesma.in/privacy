            <script type="text/javascript">
              





var validate_settings = {
    errorElement: "span",
    errorClass: "error",
    invalidHandler: function(event, validator) {
        var errors = validator.numberOfInvalids();
        if (errors) {
            $('#ErrorsFound').show();
        }
        validator.focusInvalid();
    },
    errorPlacement: function(error, element) {
        element.parents('.form-inputs').append(error);
    },
    highlight: function(element, errorClass, validClass) {
        var $element = $(element);
        if($(element).hasClass('multiselect-option')) {
            $element = $(element).parents('div.multiselect-widget');
        }
        $element.addClass(errorClass).removeClass(validClass);
    },
    unhighlight: function(element, errorClass, validClass) {
        var $element = $(element);
        if($(element).hasClass('multiselect-option')) {
            $element = $(element).parents('div.multiselect-widget');
        }
        $element.removeClass(errorClass).addClass(validClass);
    },
    rules: {
    
        'email': {
            
                required: true,
            
                email: true,
            
                maxlength: 75
            
        }
    
    },
    messages: {
    
        'email': {
            
                required: 'This field is required.',
            
                email: 'Enter a valid e-mail address.',
            
                maxlength: 'Ensure this value has at most 75 characters'
            
        }
    
    }
};
if(typeof(pop_recovery_pass_validator_settings) != "undefined") {
    $.extend(validate_settings, pop_recovery_pass_validator_settings)
}
var pop_recovery_pass_validator =  $("#pop_recovery_pass").validate(validate_settings);

            </script>
