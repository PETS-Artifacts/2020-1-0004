<script language="Javascript">

    (function($) {

        $('#top_news_check').on('change', function() {
            var feedForm = $(this).parents('form').first();

            var topNewsFeedId = 924363;
            var latestNewsFeedId = 924364;

            $(this).prop('checked', $(this).is(':checked'));

            if($(this).prop('checked')) {
                feedForm.find('input[name = "FEEDID"]').val(topNewsFeedId);
                feedForm.find('.btn').bind('click', function(){
                    FeedBlitz_b4674d51363b11e3851e0019998b9c3fs($(this).parents('form').first());
                });
            } else {
                feedForm.find('input[name = "FEEDID"]').val(latestNewsFeedId);
                feedForm.find('.btn').bind('click', function(){
                    FeedBlitz_0174909b363f11e3851e0019998b9c3fs($(this).parents('form').first());
                });
            }
        });

    })(jQuery);

</script>
