<script type="text/javascript">
(function($) {
    $(document).ready(function() {
        var policyCookie = $.cookie('bl_cookie_policy');

        if(typeof policyCookie === "undefined") {
            $('.cookie_warn').show();
            // cookies
            $('#cookie_bar').on('click', function(e){
                e.preventDefault();
                $.cookie('bl_cookie_policy', 1, { path: '/', expires: 90 });
                $('.cookie_warn').slideUp();
            });
        }

    });
})(jQuery);
