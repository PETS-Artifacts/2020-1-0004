  <script type="text/javascript">
window.onload = function () {
        $("#stxt").keyup(function (event) {
            event.stopPropagation();
            event.preventDefault();
            if (event.keyCode == 13) {
                $("#stxtbtn").click();
                return false;
            }
        });

        $("#stxtbtn").click(function () {
            var sSearch = $("#stxt").val().trim();
            if (sSearch.length > 0) {
                window.location.href = "/the-screen-guide/search/" + encodeURI(sSearch);
                return false;
            }
        });
};

</script>
              
