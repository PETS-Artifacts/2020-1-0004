  <div class="panel-pane pane-views-panes pane-node-elements-panel-pane-2 banner-image"  id="header-banner" >
    
            
    
    <div class="view view-node-elements view-id-node_elements view-display-id-panel_pane_2 view-dom-id-b834d05b2d1d03c5732c26921fb0a522">
        
  
  
