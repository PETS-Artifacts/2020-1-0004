  <div class="panel-pane pane-views-panes pane-node-elements-panel-pane-2 banner-image"  id="header-banner" >
    
            
    
    <div class="view view-node-elements view-id-node_elements view-display-id-panel_pane_2 view-dom-id-1a778632a6c13dd47ca245c65981920e">
        
  
  
