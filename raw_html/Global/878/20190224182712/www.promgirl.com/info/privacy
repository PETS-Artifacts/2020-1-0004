                                (function () {
                                        var interval;
                                
                                        function checkJQuery(){
                                                if ( typeof jQuery !== 'undefined' ) {
                                
                                                        $navChange = $('nav');
                                                        $navsecond = $('.nav-secondary');
                                                        trueWidth = screen.width,
                                                        $topReturnsContent = $(".js-top-banner-returns-content");
                
