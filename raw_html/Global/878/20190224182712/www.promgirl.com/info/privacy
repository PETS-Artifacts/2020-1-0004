                                                     console.log('this is working!')
                                                     $topReturnsContent.slideDown('fast');
                                                });
                
                                    
                                    
                                                    $('.close-top-banner-return-content').on('click', function() {
                                                            $topReturnsContent.slideUp();
                                                    });
                                                        
                                                        clearInterval( interval );			
                                                }
                                        }
                                
                                        interval = setInterval( checkJQuery, 10 );
                                }(document));
