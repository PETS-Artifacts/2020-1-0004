		$(document).ready(function(){

			//try {
				var dirCat = $.cookie('dirCat'), 
					menu = $('#ajax-menu'),
					pageId = $.cookie('pageId'),
					referer = ($.cookie('referer') && $.cookie('referer').length > 0 ? $.cookie('referer') : document.referrer ), 
					isReferred =  (referer.length > 0 ? true : false), 
					isSpecial =  (referer.indexOf("/info") >= 0 || referer.indexOf("/extras") >= 0 || referer.indexOf("/prom-guide") >= 0 ? true : false), 
					isShop = (referer.indexOf("/shop") >= 0 ? true : false), 
					thisLoc = window.location.pathname;

				if (
					(dirCat != null)
					
					&& (referer != null)
					&& (isReferred === true || isSpecial === true || isShop === true)
				){
					referer = referer.replace('#!', '');
					thisLoc = thisLoc.replace('#!', '');

					$.ajax({
						url: '/catalog/directory',
						dataType: "html",
						cache: true, /* we want the nav to be cached by the CDN */
						data: {
							js: true,
							dirCat: dirCat,
							referer: referer,
							this_page: thisLoc,
							mtype: 'left'
						},
						success: function(data){
							var temp = data;
							if (temp.length > 0){
								/* only overwrite the default menu if data is returned */
								menu.html(temp);
							}
						}
					});
				}
			//} catch(err){
			//	console.error(err);
			//}
		});
	</script>
	
