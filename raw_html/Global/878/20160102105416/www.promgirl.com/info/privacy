<script type="text/javascript">
	$(document).ready(function(){
		try {
			var dirCat_top = "", //$.cookie('dirCat'),
				menu_top = $("ul.navbar-nav"), 
				referer_top = "", //$.cookie('referer'),
				menu_type_top = 'top';

				$.ajax({
					url: '/catalog/directory',
					dataType: "html",
					cache: true, /* cache nav at CDN */
					data: {
						js: true,
						dirCat: dirCat_top,
						referer: referer_top,
						this_page: "/info/privacy",
						mtype: menu_type_top
					},
					success: function(data){
						var temp = data;


						if (temp.length > 0){
							stemp = '<li class="search">' + $("li.search").html(); + '</li>'; /* save a copy of the search box */
							menu_top.html(temp);
							menu_top.append(stemp);
						}
					}
				});
		} catch(err){
			console.error(err);
		}
	});
</script>
