<script type="text/javascript">
function ajaxsearchsubmit(form){
 var search = encodeURIComponent(form.w.value);
 if(search==''){
 return false;
 }
 var cat = encodeURIComponent(form.GenreCode2.value.toLowerCase());
 var url = form.action;
 url = url+'?w='+search;
 if(cat!='all'){
 url = url+'&af=category%3a'+cat;
 }
 document.activeElement.blur(); 
 window.location.href=url;
 return false;
}
</script>
