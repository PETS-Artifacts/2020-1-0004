<script type="text/javascript">
function fxmSkin(resp) {
    if (window.innerWidth > 1000) {
        var columnWidth = Math.ceil((window.innerWidth-1000)/2);
        document.body.style.background = resp.background + " url(" + resp.imageURL + ") fixed center 50px no-repeat";
        var wrapper = document.createElement('div');
        wrapper.innerHTML = '<a href="' + resp.clickURL + '"><div id="rightColumn" style="width:100%;height:100%;position:fixed;top:0;left:0;right:0;display:block;z-index:0;"></div></a>';
        document.body.appendChild(wrapper);
    }
    if (resp.impressionURLs.length > 0) {
        for (var i = 0; i < resp.impressionURLs.length; i++) {
            var impressionPixel = document.createElement('img');
            impressionPixel.src = resp.impressionURLs[i];
            document.body.appendChild(impressionPixel);
        }
    }
}
</script>
