		<a href="javascript:nextPic('prev');" class="banner-btn-prev">prev</a>
		<a href="javascript:nextPic('next');" class="banner-btn-next">next</a>
		
		<script type="text/javascript">
		function nextPic(direction){
			var cid = 22;
			var timer = 7;
			//var t;
			var q = timer * 1000;
			var n = $('#showNumber'+cid).text();
			var a = $('#totalNumber'+cid).text();
			
			$('#ad_'+cid+'_'+n).fadeOut("slow", function(){
					if(direction == 'next'){	
						n++;
						if(n > a) {
							n = 1;
						}
					} else {
						n--;
						if(n == 0) {
							n = a;
						}
					}				
					$('#ad_'+cid+'_'+n).fadeIn("slow");
				}
			);
			
			$('#showNumber'+cid).text(n);
			//alert($('#showNumber'+cid).text());
			clearTimeout(t);
			t=setTimeout("looper()",q);
		}
		function looper() {
			//alert(cid+' '+timer);
			var cid = 22;
			var timer = 7;
			//var t;
			var q = timer * 1000;
			var n = $('#showNumber'+cid).text();
			var a = $('#totalNumber'+cid).text();
			
			$('#ad_'+cid+'_'+n).fadeOut("slow", function(){
					n++;
					if(n > a) {
						n = 1;
					}
					$('#ad_'+cid+'_'+n).fadeIn("slow");
					}
			);
			
			$('#showNumber'+cid).text(n);
			//alert($('#showNumber'+cid).text());
			t=setTimeout("looper()",q);
		}
		$(document).ready(function() {
			var t;
			looper();
		});
		</script>
		
