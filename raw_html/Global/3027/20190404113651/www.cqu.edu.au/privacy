    document.addEventListener("DOMContentLoaded", function(event) {

        //other cookie saving functions are in boilerplate, this is hear so we dont have a hardcoded URL
        sq_js.fn.ajaxGetListedSavedCourses = function(){
            var savedCoursesCookie = sq_js.fn.read_cookie('saved_courses');
            var url = 'https://www.cqu.edu.au/_designs/2019-assets-requiring-urls/bread-crumb-saved-courses?SQ_ASSET_CONTENTS_RAW&saved_courses='+savedCoursesCookie;
            var myRequest = new XMLHttpRequest();
            myRequest.open('GET', url);
            myRequest.onreadystatechange = function () { 
                if (myRequest.readyState === 4) {
                    var savedCourseContainer = document.getElementsByClassName('saved-courses__content')[0];
                    savedCourseContainer.innerHTML = myRequest.responseText;
                    sq_js.fn.initBreadCrumbSavedCourseClicks();
                }
            };
            myRequest.send();
        }
        
     
        sq_js.fn.ajaxGetListedSavedCourses();
       
    });

    
</script>
    

