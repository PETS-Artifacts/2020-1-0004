</nav><script>
    var initMobileMenuActivePages = function(){
        var allNavItems = document.querySelectorAll('.header-mobile__wrapper .header-menu .header-menu__item');
        var currentURL = 'https://www.cqu.edu.au/privacy';
        // console.log(currentURL);
        for (var i = 0 ; i < allNavItems.length ; i++){
            var navItem = allNavItems[i];
            var navItemLink = navItem.querySelector('.header-menu__link').href;
            if(navItemLink === currentURL){
                navItem.classList.add('menu-item--active');
            }
        }
        
        var allNavInputs = document.querySelectorAll('.header-mobile__wrapper .header-menu .header-menu__input');
        for (var i = 0 ; i < allNavInputs.length ; i++){
            var navInput = allNavInputs[i];
            var parentNavItem = navInput.parentElement;
            var activeParent = parentNavItem.classList.contains('menu-item--active');
            var activeChild = parentNavItem.querySelector('.menu-item--active');
            if (activeChild !== null || activeParent) {
                navInput.checked = true;
            }
        }
    }

    document.addEventListener("DOMContentLoaded", function(event) {
        initMobileMenuActivePages();
    });
</script>
            
            
