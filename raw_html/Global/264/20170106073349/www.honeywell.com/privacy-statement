<script type="text/javascript">        
        $(document).ready(function () {
                var url = 'https://youtube.com/favicon.ico';
                var img = $("<img />").attr('src', url)
                       .on('error', function () {
                          $(".score-youtube-video.embed-responsive.score-component.score-content.score-video.embed-responsive-16by9").html("<div  id='youtubeblocked' style='text-align: center; padding-top: 50px;  padding-bottom: 50px;   border: solid 1px gray;   background-color: black;   color: white;  font-size: x-large;    margin: 20px;'> The YouTube video requested is blocked by company firewall restrictions.</div>");               
                     $("#youtubeblocked").unwrap();
                       $("iframe").each(function() { 
                              var src= $(this).attr('src');
                              if($(this).attr('src')!=undefined) {
                             if(src.indexOf("youtube.com") != -1) {  $(this).css('display','none');$(this).after("<div  id='youtubeblocked' style='text-align: center; padding-top: 50px;  padding-bottom: 50px;   border: solid 1px gray;   background-color: black;   color: white;  font-size: x-large;    margin: 20px;'> The YouTube video requested is blocked by company firewall restrictions.</div>");}
                              }
							 });                     
                       })            
        });
</script>

