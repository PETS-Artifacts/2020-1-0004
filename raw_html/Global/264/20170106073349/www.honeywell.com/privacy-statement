                var counter = 1;

                function waitForElement() {

                    var currentPrivacyUrl = jQuery('#HoneywellEUCookie-PrivacyLink').attr('href');
                    if (typeof currentPrivacyUrl != 'undefined') {
                        jQuery('#HoneywellEUCookie-PrivacyLink').attr('href', 'http://www51.honeywell.com/privacy/en.html');
                        jQuery('#HoneywellEUCookie-PrivacyLink').attr('url', 'http://www51.honeywell.com/privacy/en.html');
                        jQuery('#HoneywellEUCookie-PrivacyText').attr('url', 'http://www51.honeywell.com/privacy/en.html');
                        jQuery('#HoneywellEUCookie-Banner').css("display","block");
                    }
                    else {
                        if (counter < 10) {
                            setTimeout(function () {
                                waitForElement();
                            }, 250)
                        }
                        counter = counter + 1;
                    }
                }

                jQuery(document).ready(function () {
                    euCookieImplementationRun('en', 'honeywell.com');
                    waitForElement();
                });
            </script>


