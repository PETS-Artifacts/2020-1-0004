            //Get the list of keywords as a string from field
            var listOfKeywordsAsString = "careers,aircraft,detector,app,connected,thermostats";

            if (listOfKeywordsAsString != '') {

                //Remove unnecessary commas from end of string if any
                while (listOfKeywordsAsString[listOfKeywordsAsString.length - 1] === ',') {
                    listOfKeywordsAsString = listOfKeywordsAsString.slice(0, -1);
                }

                //Remove spaces in string if any
                listOfKeywordsAsString.replace(/\s/g, '');


                //Get element where keywords should be displayed
                var doc = document.getElementById('keywords');

                //Create a list element
                var output = '<ul>';

                //Append each keyword as an li element to ul created above
                listOfKeywordsAsString.split(',').forEach(function (keyword) {
                    if (keyword.trim() && keyword.trim() != '') {
                        //search page name is hardcoded for now
                        output = output + '<li>' +
                                        '<a class="keyword" href="/search#q=' + keyword + '">' + keyword +'</a>' +
                                        '</li>';
                    }
                });

                //Close the ul element
                output = output + '</ul>';

                //Append output as HTML to document element
                doc.innerHTML = output;

            }
        </script>
