<script type="text/javascript">
    function prepareModal(){ // Used by Add To Cart Modal.
        $('.modal .product-grid').each(function(){
            var price_old = $(this).find('.price-old');
            var price_new = $(this).find('.price-new');

            if (price_old.length && price_new.length) {
                var o = price_old.text().replace(/\D/g,'');
                var n = price_new.text().replace(/\D/g,'');
                var disc = Math.round((o - n) / o * 100);

                $(this).find('.sale').html('-' + disc + '%');
            }
        });

        $(".modal .overlay-icon").each(function(){
            if($(this).text().length > 4){
                $(this).css("font-size", "10px");
            }
        });
    }
</script>
<script async type='text/javascript'>
    $('.lazy').Lazy({
        threshold: 500
    });
    
    $('.lazy-dont-style').Lazy({
        threshold: 500,
        afterLoad: function(element) {
            element.removeAttr("style");
        }
    });
    
    $('#rentals-date-start').datepicker({
        dateFormat: 'yy-mm-dd',
        minDate: new Date(),
        maxDate: "+4m"
    });
    
    $('#rentals-date-end').datepicker({
        dateFormat: 'yy-mm-dd',
        minDate: new Date(),
        maxDate: "+4m"
    });
    
    $('#rentals-date-start').on("change", function(){
        $("#rentals-date-end").datepicker("option", "minDate", new Date($("#rentals-date-start").datepicker("getDate")));
    });
</script>
