        }
        for (i; i >= 0; i--) {
            // Get previous iframe used as popup
            var hiddenFrame = window.allModals[i];
            if (hiddenFrame != null) {
                // Set it as currently visible
                visiblePopup = hiddenFrame;
                break;
            } 
            else {
                visiblePopup = null;
            }
        }
    }

