(function($){
	var ua = navigator.userAgent;
	// Find new Kindle Fire models using this page for reference: https://developer.amazon.com/docs/fire-tablets/ft-device-and-feature-specifications.html
	var isKindle = /Kindle|Silk|KFAPW|KFARW|KFASW|KFFOW|KFJW|KFMEW|KFOT|KFSAW|KFSOW|KFTBW|KFTHW|KFTT|KFKAW|KFSUW|KFAUW|KFDOW|KFGIW/i.test(ua);
	var disqusShortname  = "brbusinessreport";
	var disqusIdentifier, disqusTitle, disqusUrl, scrollOffset, disqusHeight, disqusOffset;
	(function() {
		var dsq = document.createElement("script"); dsq.type = "text/javascript"; dsq.async = true;
		dsq.src = "//" + disqusShortname + ".disqus.com/embed.js";
		(document.getElementsByTagName("head")[0] || document.getElementsByTagName("body")[0]).appendChild(dsq);
	})();
	var reset = function (newIdentifier, newUrl, newTitle) {
		//Kindle Fire seems to have issues with loading DISQUS, try the AJAX method
		if (typeof DISQUS != "undefined" && !isKindle) {
			DISQUS.reset({
				reload: true,
				config: function () {
					this.page.identifier = newIdentifier;
					this.page.url = newUrl;
					this.page.title = newTitle;
				}
			});
		} else {
			$.ajax({
				type: "GET",
				url: "//" + disqusShortname + ".disqus.com/embed.js",
				dataType: "script",
				cache: false
			});
		}
		if (disqusHeight !== null && disqusOffset < scrollOffset) {
			$(window).scrollTop(scrollOffset-disqusHeight);
		} else {
			$(window).scrollTop(scrollOffset);
		}
	};
	$(document.body).on("click", ".comment-link" , function(e) {
		scrollOffset=$(window).scrollTop(),disqusHeight=$("#disqus_thread").height(),disqusOffset=$("#disqus_thread").offset().top;
		disqusIdentifier = $(this).data("disqusIdentifier"), disqusTitle = $(this).data("disqusTitle"), disqusUrl = $(this).data("disqusUrl");
		var disqus_config = function () {this.language = "en";};
		e.preventDefault();
		
		//var elem = document.querySelector("script[src$=\"//brbusinessreport.disqus.com/embed.js\"]");
		//if (elem !== null) {elem.parentNode.removeChild(elem);}
		$("div[id=disqus_thread]").remove();
		$(".comment-text").each(function(){$(this).show()});
		$(this).parents(".comment-text").fadeOut();
		$(this).parents(".comment-container").append("<div id=\"disqus_thread\"></div>");

		reset(disqusIdentifier, disqusUrl, disqusTitle);
	});
})(jQuery);
</script>
