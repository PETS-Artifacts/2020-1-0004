<script>
 var ua = $.browser;
 hide = readHide("hide");
 //alert(hide);
 if ( ua.msie && ua.version.slice(0,1) <= "8"  && hide != "1") {
   $("#oldbrowser").show("slow");
 } else {
   //alert('something else');
 }

function dismiss()
{
//alert('something else');
document.cookie = "hide=1;";
$("#oldbrowser").hide("slow");
}

function readHide(name) {
	var nameEQ = name + "=";
	var ca = document.cookie.split(';');
	for(var i=0;i < ca.length;i++) {
		var c = ca[i];
		while (c.charAt(0)==' ') c = c.substring(1,c.length);
		if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
	}
	return null;
}
</script>

