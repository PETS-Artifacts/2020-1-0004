        function submitslisearch(form){
            if (form.w.value == ""){
                return false;
            }
            else{
                var search = encodeURIComponent(form.w.value);
                var asug = encodeURIComponent(form.asug.value);
                window.location="https://aquarium.bulkreefsupply.com/search?w=" + search + "&asug=" + asug;                  return false;
            }
        }
    </script>

