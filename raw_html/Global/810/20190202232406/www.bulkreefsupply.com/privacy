<script>
	// Ensure the Liveperson object is loaded
	var waitForLpTag_Tracking_Iterations = 0;
	function waitForLpTag_Tracking() {
		if( waitForLpTag_Tracking_Iterations < 48 ) {
			waitForLpTag_Tracking_Iterations += 1;
			if( typeof( lpTag ) == 'object' ) {
				trackLpTag();
			} else {
				window.setTimeout(function() {
					waitForLpTag_Tracking();
				},250);
			}
		}
	}
	waitForLpTag_Tracking();

	function trackLpTag() {
				lpTag.events.bind("LP_OFFERS", "OFFER_CLICK", function(eventData,eventInfo){
			// Fire when engagement is shown on page
						if( Number.isInteger( eventData.engagementType ) && eventData.engagementType > -1 && eventData.engagementType < 7 ) {
				var state = "";
				switch( eventData.state ) {
					case 1:
						state = "Online";
						break;
					case 2:
						state = "Offline";
						break;
					case 4:
						state = "Back In 5";
						break;
				}

				ga('send', 'event', 'Chat Conversation', 'Conversation Started' + ( state.length > 0 ? ' - ' + state : '' ), window.location.href);
			}
		});
	}
</script>	<script type="text/javascript">
		// Define feedback ReCaptcha widgets
						var recaptchaWidget_FeedbackFooter;  		var recaptchaWidget_FeedbackFooterFormElement = jQuery( ".footer-container .footer-feedback #contactForm" );  
		// ReCaptcha Validation
				function validateReCaptchaFeedback() {
			jQuery( recaptchaWidget_FeedbackFooterFormElement ).children( ".dynamicReCaptchaForm .dynamicReCaptchaFormError" ).remove();
			if( grecaptcha.getResponse( recaptchaWidget_FeedbackFooter ).length > 0 ) {
				return true;
			} else {
				event.preventDefault();
				jQuery( recaptchaWidget_FeedbackFooterFormElement ).children( ".dynamicReCaptchaForm" ).append('<div class="dynamicReCaptchaFormError error">Please verify you are not a robot</div>');
				return false;
			}
		}

		// Add ReCaptcha to feedback form via AJAX
				function footerFeedbackReCaptchaShow( element ) {
			if( !jQuery( "#g-recaptcha-feedback-footer" ).length ) {
				var ajaxUrl = "https://www.bulkreefsupply.com/brsajax/ajax/recaptchadynamic/location/feedback/display/footer";
				jQuery.get( ajaxUrl, function( ajaxResponse ) {
					// Append ReCaptcha to form
					jQuery( recaptchaWidget_FeedbackFooterFormElement ).find( "div.actionbutton.actionbutton-submit" ).before( ajaxResponse );

					// Render ReCaptcha
					recaptchaWidget_FeedbackFooter = grecaptcha.render(
						document.getElementById( "g-recaptcha-feedback-footer" ), {
							"sitekey" : "6LecbygTAAAAAH8XDhSp85OGUSoTqTn5kvDXJisC",
							"size" : "compact"
						}
					);

					// Add validation for ReCaptcha via event listener
					jQuery( recaptchaWidget_FeedbackFooterFormElement ).on( "submit", function() {
						validateReCaptchaFeedback();
					});

				});
			}
		}
	</script>
