	<script type="text/javascript">

        function submitslisearch(form){
            if (form.w.value == ""){
                return false;
            }
            else{
                var search = encodeURIComponent(form.w.value);
                var asug = encodeURIComponent(form.asug.value);
                window.location="http://aquarium.bulkreefsupply.com/search?w=" +search+"&asug="+asug;
                return false;
            }
        }
	</script>

<form id="search_mini_form" name="searchform" action="//aquarium.bulkreefsupply.com/search" method="get" onsubmit="return submitslisearch(this)">
  <div class="input-box">
   <input type="hidden" name="asug" />
   <input type="hidden" name="view" value="grid" />
   <input type="hidden" name="cnt" value="16" />
   <input type="text" name="w" placeholder="Search entire store here..." maxlength="128" class="input-text required-entry" value="" onfocus="this.value=''" id="sli_search_1" autocomplete="off" />

   <!--<button type="submit" title="Go" class="button"><span></span></button>-->
   <button type="submit" title="Search" class="button search-button"><span><span>Search</span></span></button>
  </div>
</form>
    </div>
    </div>
</header>


<script type="text/javascript">
var wi = jQuery(window).width();
resizeMobile(wi);

MenuIpad(wi);

modifyMobileFromDesktop(wi);

jQuery(window).resize(function () {
    jQuery( "li.item-ii" ).find( "li" )
    var wi = jQuery(window).width();
    //alert('Screen width is currently: ' + wi + 'px.');
    resizeMobile(wi);

    MenuIpad(wi);
});

function modifyMobileFromDesktop(wi){
    if(wi >= 320 && wi <= 640){
        jQuery('.desktop-header').find('#header-search').remove();
        /*var headerMenuDesktop = jQuery('.desktop-header').find('#header-nav');
        var headerMenuMobile = jQuery('.mobile-header').find('#header-nav');
        headerMenuMobile.html(headerMenuDesktop.html());
        headerMenuDesktop.remove();*/
    }
}

function MenuIpad(wi){
    if(wi >= 960 && wi <= 1024){
        jQuery('.nav-container').addClass('menu-ipad');
        ShowMenuIpad();
    }else{
        jQuery('.nav-container').removeClass('menu-ipad');
        jQuery('.nav-container #nav > li > a.level-top').off('click');
    }
}

function ShowMenuIpad(){
    jQuery('.nav-container.menu-ipad #nav > li > a.level-top').on('click', function() {
        if(jQuery(this).hasClass('click-on')){
            return true;
        }else{
            jQuery('.nav-container #nav > li > a.level-top').removeClass('click-on');
            jQuery(this).addClass('click-on');
            
            jQuery('.nav-container #nav > li > .sub-menu').hide();
            
            jQuery(this).next('.sub-menu').show();
            
            jQuery(this).next('.sub-menu').children('ul').css("display", "inline-block");
            width_ul_parent = height_ul_parent = 0;
            jQuery(this).next('.sub-menu').children('ul').children('li').each(function () {
                width_ul_parent = width_ul_parent + jQuery(this).width() + 20;
                if (jQuery(this).height() > height_ul_parent) {
                    height_ul_parent = jQuery(this).height();
                }
                //console.log(jQuery(this).height());
            });
            
            jQuery(this).next('.sub-menu').width(width_ul_parent + 15);
        
            jQuery(this).next('.sub-menu').children('ul').width(width_ul_parent);
            jQuery(this).next('.sub-menu').children('ul').children('li').each(function () {
                jQuery(this).parent('li').height(height_ul_parent);
            });
            
            return false;    
        }
    });
}

function resizeMobile(wi) {
//    if (wi <= 960) {
//        mostrar('');
//        var event_first_click = false;
//        var event_secon_click = false;
//        jQuery('#nav-links li a').unbind("click");
