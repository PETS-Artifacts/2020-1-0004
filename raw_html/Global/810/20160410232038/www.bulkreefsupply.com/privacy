<script>
	function ready_lr_prod(prod_id) {
		var quickview = $('grid-item_'+prod_id).down('.quickview-link');
		if (quickview && quickview.getAttribute('href')) {
			quickview.observe('click', function (evt) {
				evt.stop();
				Lightview.show({
		            url: this.getAttribute('href'),
		            type: 'ajax',
		            options: {
						width: 695,
						height: 500,
						overflow: 'hidden',
						ajax: {
							evalScripts: true
						}
					}
		        });
			});
		}

		$('product-form-'+prod_id).observe('submit', function(e){
			e.stop();
			var form = Event.element(e);
			var button = form.down('button');
			var loader = form.down('.loading');
			topCart.addToCart(button, null, loader, form);
		});
	}
	
