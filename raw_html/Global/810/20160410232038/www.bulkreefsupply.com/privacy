<script type="text/javascript">
    
     jQuery(function($) {
        var click = $('button.resp-nav');
        var allct = $('div.wrapper');
        click.click(function() {
            if (click.hasClass('clicked')) {
                $('div#pronav-selection-container').css('position', '');
                click.removeClass('clicked');
                $('.page').animate({left: '0px'}, 600, function() {
                    $('div#pronav-selection').css('height', '100%');
                    $('.page').css('position', '');
                });
            } else {
                $('.page').animate({left: $('div#pronav-selection').width() + 'px'}, 600, function() {
                    $('div#pronav-selection').css('height', 'auto');
                    $('.page').css('position', '');
                    //$('div#pronav-selection-container').css('position', 'absolute');
                });
                click.addClass('clicked');
            }
            return false;
        });
    });
</script>
