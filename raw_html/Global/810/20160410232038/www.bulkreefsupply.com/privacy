//            if (jQuery(this).attr('id') == 'primer-link') {
//                if (!event_first_click) {
//                    mostrar('');
//                    mostrar('shop', this);
//                    event_first_click = true;
//                    if (event_secon_click) {
//                        event_secon_click = false;
//                    }
//                } else {
//                    mostrar('');
//                    event_first_click = false;
//                }
//            } else {
//                if (!event_secon_click) {
//                    mostrar('');
//                    mostrar('reefSources', this);
//                    event_secon_click = true;
//                    if (event_first_click) {
//                        event_first_click = false;
//                    }
//                } else {
//                    mostrar('');
//                    event_secon_click = false;
//                }
//            }
//
//        });
//    } else {
    jQuery('#nav-links li a').unbind("click");
    mostrar('shop', '#primer-link');
    jQuery('#nav-links li a').click(function () {
        if (jQuery(this).attr('id') == 'primer-link') {
            mostrar('shop', this);
        } else {
            mostrar('reefSources', this);
        }
    });
            jQuery('.nav-container').hide();
    //    }
}

function mostrar(mostrar, el) {
    jQuery('#nav-links > li').removeClass('active');
    jQuery(el).parent('li').addClass('active');

    if (mostrar == 'shop') {
        jQuery('#nav').show();
        jQuery('#reefSources').hide();
    } else {
        if (mostrar == 'reefSources') {
            jQuery('#nav').hide();
            jQuery('#reefSources').show();
        } else {
            jQuery('#nav').hide();
            jQuery('#reefSources').hide();
        }
    }
}
function activarShopp() {
    /*document.getElementById('menu1shop').style.display='block';
     document.getElementById('menu2shop').style.display='block';
     */
    document.getElementById('blog').style.display = 'block';
    document.getElementById('brstv').style.display = 'block';
    document.getElementById('how-to').style.display = 'block';
    document.getElementById('calculators').style.display = 'block';
    document.getElementById('reefing-manual').style.display = 'block';
    document.getElementById('inblock').style.display = 'block';
    document.getElementById('account-box').style.display = 'block';

    document.getElementById('ro-di').style.display = 'block';
    document.getElementById('rock-and-sand').style.display = 'block';
    document.getElementById('auto-top-off').style.display = 'block';
    document.getElementById('calcium-and-alkalinity').style.display = 'block';
    document.getElementById('fish-and-coral-foods').style.display = 'block';
    document.getElementById('fragging').style.display = 'block';
    document.getElementById('heaters-and-chillers').style.display = 'block';
    document.getElementById('controllers-and-testing').style.display = 'block';
    document.getElementById('skimmers-and-filtration').style.display = 'block';
    document.getElementById('salt-and-maintenance').style.display = 'block';
    document.getElementById('pumps-and-plumbing').style.display = 'block';
    document.getElementById('sumps-and-tanks').style.display = 'block';
    document.getElementById('specials-and-new').style.display = 'block';
}

function mostrarBloque(bloque) {
    var menu1 = document.getElementById("menu1shop");
    var menu2 = document.getElementById("menu2shop");

    var submenu1 = document.getElementById('auxMenu1Shop"');
    var submenu2 = document.getElementById('auxMenu2Shop"');

    if (bloque == 'bloque1') {
        menu1.style.background = 'none repeat scroll 0 0 #dee6f5';
        menu1.style.color = '#1a4581';
        //submenu1.style.color = '#1a4581';

        menu2.style.background = 'none repeat scroll 0 0 #1a8dcc';
        menu2.style.color = '#ffffff';
        //submenu2.style.color = '#ffffff';

        document.getElementById('ro-di').style.display = 'inline-block';
        document.getElementById('rock-and-sand').style.display = 'inline-block';
        document.getElementById('auto-top-off').style.display = 'inline-block';
        document.getElementById('calcium-and-alkalinity').style.display = 'inline-block';
        document.getElementById('fish-and-coral-foods').style.display = 'inline-block';
        document.getElementById('fragging').style.display = 'inline-block';
        document.getElementById('heaters-and-chillers').style.display = 'inline-block';
        document.getElementById('controllers-and-testing').style.display = 'inline-block';
        document.getElementById('skimmers-and-filtration').style.display = 'inline-block';
        document.getElementById('salt-and-maintenance').style.display = 'inline-block';
        document.getElementById('pumps-and-plumbing').style.display = 'inline-block';
        document.getElementById('sumps-and-tanks').style.display = 'inline-block';
        document.getElementById('specials-and-new').style.display = 'inline-block';

        document.getElementById('blog').style.display = 'none';
        document.getElementById('brstv').style.display = 'none';
        document.getElementById('how-to').style.display = 'none';
        document.getElementById('calculators').style.display = 'none';
        document.getElementById('reefing-manual').style.display = 'none';
        document.getElementById('inblock').style.display = 'none';
        document.getElementById('account-box').style.display = 'none';


    } else {
        if (bloque == 'bloque2') {
            menu2.style.background = 'none repeat scroll 0 0 #dee6f5';
            menu2.style.color = '#1a4581';

            menu1.style.background = 'none repeat scroll 0 0 #1a8dcc';
            menu1.style.color = '#ffffff';

            document.getElementById('blog').style.display = 'inline-block';
            document.getElementById('brstv').style.display = 'inline-block';
            document.getElementById('how-to').style.display = 'inline-block';
            document.getElementById('calculators').style.display = 'inline-block';
            document.getElementById('reefing-manual').style.display = 'inline-block';
            document.getElementById('inblock').style.display = 'inline-block';
            document.getElementById('account-box').style.display = 'inline-block';

            document.getElementById('ro-di').style.display = 'none';
            document.getElementById('rock-and-sand').style.display = 'none';
            document.getElementById('auto-top-off').style.display = 'none';
            document.getElementById('calcium-and-alkalinity').style.display = 'none';
            document.getElementById('fish-and-coral-foods').style.display = 'none';
            document.getElementById('fragging').style.display = 'none';
            document.getElementById('heaters-and-chillers').style.display = 'none';
            document.getElementById('controllers-and-testing').style.display = 'none';
            document.getElementById('skimmers-and-filtration').style.display = 'none';
            document.getElementById('salt-and-maintenance').style.display = 'none';
            document.getElementById('pumps-and-plumbing').style.display = 'none';
            document.getElementById('sumps-and-tanks').style.display = 'none';
            document.getElementById('specials-and-new').style.display = 'none';


        }
    }
}

function mostrarBuscador() {

    var miBuscador = document.getElementById('otroBuscador');
    miBuscador.style.display = 'block';
    miBuscador.style.zindex = '2';

}

var width_ul_parent = 0;
var height_ul_parent = 0;
jQuery('#nav').children('li').hover(function () {
    width_ul_parent = 0;
    height_ul_parent = 0;

    jQuery(this).stop().animate(200, function() {
        jQuery(this).children('.sub-menu').show();
        
        jQuery(this).children('.sub-menu').children('ul').css("display", "inline-block");
        jQuery(this).children('.sub-menu').children('ul').children('li').each(function () {
            width_ul_parent = width_ul_parent + jQuery(this).width() + 20;
            if (jQuery(this).height() > height_ul_parent) {
                height_ul_parent = jQuery(this).height();
            }
            //console.log(jQuery(this).height());
        });
        
        jQuery(this).children('.sub-menu').width(width_ul_parent + 15);

        jQuery(this).children('.sub-menu').children('ul').width(width_ul_parent);
        jQuery(this).children('.sub-menu').children('ul').children('li').each(function () {
            jQuery(this).height(height_ul_parent);
        });
    });


}, function () {
    width_ul_parent = 0;
    jQuery(this).stop().animate(200, function() {
        jQuery(this).children('.sub-menu').hide();
        
        jQuery(this).children('.sub-menu').children('ul').css("display", "none");
    });
});

function mostrarMenuResponsivo(){


    if (jQuery('.nav-container').is(':visible')){
        jQuery('.nav-container').hide();
    }else{

        jQuery('.nav-container').show();
       /* var headerNav=document.getElementById('#nav');
        headerNav.style.display='inline-block';*/
    }


}

</script>

