        <script>
        jQuery("#nav > li").each( function(){
            /* mouse enabled devices */
            jQuery(this).mouseover( function(){
                if( window.innerWidth > 960 ) {
                    jQuery(this).off("mouseover");
                    loadDeferedImages( jQuery(this).find("img.defer") );
                }
            });
            /* touch enabled devices */
            jQuery(this).on("touchstart", function(e){
                if( window.innerWidth > 960 ) {
                    e.preventDefault;
                    jQuery(this).off("touchstart")
                    loadDeferedImages( jQuery(this).find("img.defer") );
                }
            });
        });

        /* load defered images */
        function loadDeferedImages( element ) {
            for (var i=0; i<element.length; i++) {
                if(!element[i].getAttribute("src") && element[i].getAttribute("data-src")) {
                    element[i].setAttribute("src",element[i].getAttribute("data-src"));
                    jQuery(element[i]).removeClass("defer").removeAttr("data-src");
                }
            }
        }
    </script>
</div>
			</div>

					</div>

			</div>
</header>


