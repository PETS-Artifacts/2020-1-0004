    if( window.innerWidth <= viewport_breakpoint ) {
        jQuery('#nav .sub-menu').width('auto');
        jQuery('#nav .sub-menu ul').width('auto');
        jQuery('#nav .sub-menu ul li').height('auto');
    } else {
        jQuery("#nav li.level-top").show();
        jQuery('#nav div.sub-menu, #nav div.sub-menu ul.level0').hide();
    }
});

