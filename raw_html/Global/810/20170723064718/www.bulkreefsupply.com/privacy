<script>
	// Ensure the Liveperson object is loaded
	var waitForLpTag_Tracking_Iterations = 0;
	function waitForLpTag_Tracking() {
		if( waitForLpTag_Tracking_Iterations < 48 ) {
			waitForLpTag_Tracking_Iterations += 1;
			if( typeof( lpTag ) == 'object' ) {
				trackLpTag();
			} else {
				window.setTimeout(function() {
					waitForLpTag_Tracking();
				},250);
			}
		}
	}
	waitForLpTag_Tracking();

	function trackLpTag() {
				lpTag.events.bind("LP_OFFERS", "OFFER_CLICK", function(eventData,eventInfo){
			// Fire when engagement is shown on page
						if( Number.isInteger( eventData.engagementType ) && eventData.engagementType > -1 && eventData.engagementType < 7 ) {
				var state = "";
				switch( eventData.state ) {
					case 1:
						state = "Online";
						break;
					case 2:
						state = "Offline";
						break;
					case 4:
						state = "Back In 5";
						break;
				}

				ga('send', 'event', 'Chat Conversation', 'Conversation Started' + ( state.length > 0 ? ' - ' + state : '' ), window.location.href);
			}
		});
	}
</script>	<script type="text/javascript">
		// Define feedback ReCaptcha widgets
		var recaptchaWidget_FeedbackDesktop;
		var recaptchaWidget_FeedbackMobile;

		// ReCaptcha Validation
		function validateReCaptchaFeedback(display) {
			if( display == 'desktop' && grecaptcha.getResponse(recaptchaWidget_FeedbackDesktop).length > 0 ) {
				jQuery(".footer-container .footer."+display+" #contactForm .dynamicReCaptchaForm .dynamicReCaptchaFormError").remove();
				return true;
			} else if( display == 'mobile' && grecaptcha.getResponse(recaptchaWidget_FeedbackMobile).length > 0 ) {
				jQuery(".footer-container .footer."+display+" #contactForm .dynamicReCaptchaForm .dynamicReCaptchaFormError").remove();
				return true;
			} else {
				event.preventDefault();
				jQuery(".footer-container .footer."+display+" #contactForm .dynamicReCaptchaForm .dynamicReCaptchaFormError").remove();
				jQuery(".footer-container .footer."+display+" #contactForm .dynamicReCaptchaForm").append('<div class="dynamicReCaptchaFormError error">Please verify you are not a robot</div>');
				return false;
			}
		}

		// Add ReCaptcha to feedback form via AJAX
		function footerFeedbackReCaptchaShow(element) {
			var display = ( jQuery(element).parent().parent().parent().hasClass('desktop') ? "desktop" : "mobile" );
			if( !jQuery("#g-recaptcha-"+display).length ) {
				var ajaxUrl = "http://www.bulkreefsupply.com/brsajax/ajax/recaptchadynamic/display/" + display;
				jQuery.get( ajaxUrl, function( ajaxResponse ) {
					// Append ReCaptcha to form
					jQuery(".footer-container .footer."+display+" #contactForm .actions").before(ajaxResponse);

					// Render ReCaptcha
					if( display == 'desktop' ) {
						recaptchaWidget_FeedbackDesktop = grecaptcha.render(
							document.getElementById('g-recaptcha-' + display), {
								'sitekey' : '6LecbygTAAAAAH8XDhSp85OGUSoTqTn5kvDXJisC',
								'size' : 'compact'
							}
						);
					} else if( display == 'mobile' ) {
						recaptchaWidget_FeedbackMobile = grecaptcha.render(
							document.getElementById('g-recaptcha-' + display), {
								'sitekey' : '6LecbygTAAAAAH8XDhSp85OGUSoTqTn5kvDXJisC',
								'size' : 'compact'
							}
						);
					}

					// Add validation for ReCaptcha via event listener
					jQuery(".footer-container .footer." + display + " #contactForm").on('submit', function(){
						validateReCaptchaFeedback(display);
					});

				});
			}
		}
	</script>
