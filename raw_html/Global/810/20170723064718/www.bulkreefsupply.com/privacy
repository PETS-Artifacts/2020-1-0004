}

/* 2016-07-08 : bprom : BRS-254 added START */
// Toggle - Main menu
jQuery("#nav div#nav_label").on("click tap",function(){ mmToggle_Top() });

// Toggle - Level 0
jQuery("#nav li.level0.parent > a").on("click tap",function(e){ mmToggle_Level0(e) });

function mmToggle_Top() {
    jQuery("#nav li.level-top").toggle();
}

function mmToggle_Level0(e) {
    if( window.innerWidth < viewport_breakpoint ) {  /* 2016-07-15 : bprom : BRS-417 added */
    e.preventDefault();  // prevent link navigation
    var sameMenu = jQuery(e.target).closest('li.level0.level-top').find('div.sub-menu ul.level0').is(':visible');  // Detect if user trying to close this menu
    jQuery("#nav div.sub-menu, #nav div.sub-menu ul.level0").hide();  // Close all menus
    if( !sameMenu ) {
        jQuery(e.target).closest('li.level0.level-top').find('div.sub-menu, div.sub-menu ul.level0').toggle();  // Open selected menu
    }
    }  /* 2016-07-15 : bprom : BRS-417 added */
}
/* 2016-07-08 : bprom : BRS-254 added END */
</script>

