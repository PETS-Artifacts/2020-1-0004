	$('.site-notifications-list').find('a').click(function() {
		var label = $(this).data('label');
		if (label) {
			_gaq.push(['_trackEvent', 'Internal Campaigns', 'Global Alert', label]);
			_uaTracker('send', 'event', 'Internal Campaigns', 'Global Alert', label);
		}
	});
</script>

