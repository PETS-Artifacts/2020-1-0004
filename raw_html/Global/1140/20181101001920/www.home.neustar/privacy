        $('.modal').hide();

        $('.contact-rfi-trigger').click(function () {
            $('#contact-rfi').fadeIn(250);
            $('.modalbg.contact-modal').fadeIn(250);
            return false;
        });

        // for DDoS Alert
        $('.ddos-rfi-trigger').click(function () {
            $('#ddos-rfi').fadeIn(250);
            $('#ddosformContainer').fadeIn(250);
            $('.modalbg.contact-modal').fadeIn(250);
            $('#ddosconfirmform').hide();
            return false;
        });
        $('.close').click(function () {
            $(this).parent('.modal').fadeOut(500);
            $('.modalbg.contact-modal').fadeOut(500);
        });
        $('.modalbg.contact-modal').click(function () {
            $(this).fadeOut(500);
            $('.modal').fadeOut(500);
        });
    });
    $(document).keyup(function (e) {
        if (e.keyCode == 27) {
            $("#contact-rfi").fadeOut(500);
            $("#ddos-rfi").fadeOut(500);
            $('.modalbg.contact-modal').fadeOut(500);
        }
    });
