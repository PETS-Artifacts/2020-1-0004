              <script type="text/javascript">
                  $(document).ready(function() {
                      if(($.cookie('_countrycode')) && ($.cookie('_countrycode')!=="null")) {

                      } else {
                          host = location.hostname.replace('www','');
                          cookieDomain = host;
                          if(window.location.hostname.match(/neuweb/g)){
                              cookieDomain = '.neuweb.biz';
                          }
                          if(window.location.hostname.match(/localhost/g)){
                              cookieDomain = '.neustar.localhost';
                          }
                          $.getJSON('/ajax/ipintel', function(data){
                              countryCode = 'us';
                              continent='north america';
                              if(!data['error'] && !data['info']){
                                  countryCode = data['ipinfo']['Location']['CountryData']['country_code'];
                                  continent= data['ipinfo']['Location']['continent'];
                                  $.cookie('_ipintel', JSON.stringify(data),{domain: cookieDomain});
                                  $.cookie('_countrycode', countryCode, {domain: cookieDomain});
                                  $.cookie('_continent', continent, {domain: cookieDomain});
                                  //locationBasedDdosPhone(countryCode);
                              }
                          });
                      }

                      function GetParameterValues(param) {
                          var url = window.location.href.slice(window.location.href.indexOf('?') + 1).split('&');
                          for (var i = 0; i < url.length; i++) {
                              var urlparam = url[i].split('=');
                              if (urlparam[0] == param) {
                                  return urlparam[1];
                              }
                          }
                      }
                      if(GetParameterValues('sem-cname')) {
                          $.cookie('leadsource','Paid Search');
                          $.cookie('leadsourcedetails','google');
                      }

                  });
              </script>
    </body>
</html>