		jQuery('#edit-go').css("display", "none");
		var vSeleText = jQuery.trim(jQuery('#views-exposed-form-map-by-program-page #edit-title').val());
		if (vSeleText == '') {
			vSeleText = 'All programs';
		}
		jQuery("#edit-jump option:contains('" + vSeleText + "')").attr('selected', true);
		jQuery('#ctools-jump-menu button[data-id=edit-jump]').attr('title', vSeleText);
		jQuery('#ctools-jump-menu button[data-id=edit-jump] span:first').text(vSeleText);
		
					//Placing the tick to selected text after removing from first index
					jQuery('#ctools-jump-menu button[data-id=edit-jump]').next().find('ul li').each(function( index ) {
						if (index == 0) {
							jQuery(this).removeAttr('class');
						}
						//jQuery(this).find("span:contains('" + vSeleText + "')").attr('class', 'selected');
						jQuery(this).find("span:contains('" + vSeleText + "')").parent().parent().attr('class', 'selected');
						//jQuery(this).is(":contains('" + vSeleText + "')").attr('class', 'selected');
					});

					
	});
}
