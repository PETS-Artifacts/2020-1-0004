		if (vJumpText == "All programs") {
			vJumpText = '';
			jQuery('#views-exposed-form-map-by-program-page-1 #edit-title').val(vJumpText);
			jQuery('#views-exposed-form-map-by-program-page #edit-title').val(vJumpText);
		} else {
			jQuery('#views-exposed-form-map-by-program-page-1 #edit-title').val(vJumpText);
			jQuery('#views-exposed-form-map-by-program-page #edit-title').val(vJumpText);
		}
