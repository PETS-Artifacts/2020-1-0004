Drupal.behaviors.investorMailerStopInterrupt = {
	attach: function(context, settings) {

//Draft Mode not enabled for link page component (Investor)
if (jQuery('div.entity-page-component.page-component-links').size() > 0) {
	jQuery('div.entity-page-component.page-component-links div.field-name-field-pc-links-links div').each(function( index ) {
		(typeof jQuery(this).find('a').attr('title') !== 'undefined' && (jQuery(this).find('a').attr('title').toLowerCase().indexOf('draft mode') != -1)) ? jQuery(this).remove() : false;
	});
}

function vzc_remove_dup_rows_in_table(customTableClass) {
	var seen = {};
	jQuery('table.'+customTableClass+' tr').each(function() {
			var txt = jQuery(this).text();
			if (seen[txt])
					jQuery(this).remove();
			else
					seen[txt] = true;
	});
	//Removing caption Region
	jQuery('table.'+customTableClass+' caption').each(function() {
		jQuery(this).text(jQuery(this).text().replace('Region:',''));
	});
	//dropdown to select picker
	jQuery('#edit-jump').addClass('selectpicker');
	jQuery('#edit-jump').addClass('interactive_dropdown');
	jQuery('.selectpicker').selectpicker({ });
	//Region static logic
	var vRegTabSize = jQuery('div.view-footer div.view-id-map_by_program table.views-table').size();
	var vRegArr = ['Pacific', 'Mountain', 'Central', 'East'];
	var vRegArrPresent = ['N', 'N', 'N', 'N'];
	var vRegCloneArr = [];
	if (vRegTabSize >= 1 && vRegTabSize <= 3) {
		for( var vRegIndex = 0; vRegIndex < vRegArr.length; vRegIndex++ ) {
			jQuery('table.views-table caption').each(function( index ) {
				//console.log(jQuery(this).text());
				if (jQuery.trim(jQuery(this).text()) == vRegArr[vRegIndex]) {
					//jQuery(vRegionTable).insertBefore(jQuery(this).parent());
					vRegArrPresent[vRegIndex] = 'Y';
					//cloning
					vRegCloneArr[vRegIndex] = jQuery(this).parent().clone(true);
					jQuery(this).parent().remove();
				}
			});
		}

		for( var vRegIndex = 0; vRegIndex < vRegArr.length; vRegIndex++ ) {
			if (vRegArrPresent[vRegIndex] == 'N') {
				//$("#holder>div:nth-child(2)").after("<div>inserted div");
				//$("#holder>div:eq(1)").after("<div>inserted div");
				//jQuery('div.view-footer div.view-id-map_by_program div.view-content table:eq('+(vRegIndex)+')').after('<table class="views-table cols-0 table"><caption>'+vRegArr[vRegIndex]+'<tbody><tr class="odd views-row-first views-row-last"><td class="views-field views-field-nothing">');
				jQuery('div.view-footer div.view-id-map_by_program div.view-content').append('<table class="views-table cols-0 table"><caption>'+vRegArr[vRegIndex]+'<tbody><tr class="odd views-row-first views-row-last"><td class="views-field views-field-nothing">');
				vRegArrPresent[vRegIndex] = 'Y';
			} else {
				//for Y
				//jQuery('div.view-footer div.view-id-map_by_program div.view-content table:eq('+(vRegIndex)+')').after(vRegCloneArr[vRegIndex]);
				jQuery('div.view-footer div.view-id-map_by_program div.view-content').append(vRegCloneArr[vRegIndex]);
			}
		}
	}
}
