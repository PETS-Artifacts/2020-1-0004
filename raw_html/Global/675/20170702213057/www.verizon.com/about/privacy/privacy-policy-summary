		//if (index == 0) {

			varTempIndex = index;

		}

		if (varTempIndex == index && index != 0) {

			//jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image a.prev').removeClass('vzc_pg_comp_flexslider_colorbox_disabled').addClass('vzc_pg_comp_flexslider_colorbox_enabled');

			jQuery(this).parent().prev().find('a.prev').removeClass('vzc_pg_comp_flexslider_colorbox_disabled').addClass('vzc_pg_comp_flexslider_colorbox_enabled');

			return false;

		}

	});	

}));




