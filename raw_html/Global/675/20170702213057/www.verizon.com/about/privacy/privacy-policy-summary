//EOOV-5976 - EOOV-5981,leadership bio contact link enable disable
if (jQuery("#block-views-vzc-leadership-bios-block").size() > 0) {
	jQuery( "#block-views-vzc-leadership-bios-block div.field-name-vzc-leadership-contact" ).each(function( index ) {
		//if (jQuery(this).is(':hidden')) {
		if (jQuery(this).is(':visible')) {
			//console.log("index "+ index + " is visible");
			//console.log(jQuery(this).next('div.field-name-field-enable-contact-link').size());
			//console.log(jQuery(this).next('div.field-name-field-enable-contact-link').text());
			if (jQuery.trim(jQuery(this).next('div.field-name-field-enable-contact-link').text()) == "0") {
				jQuery(this).css("display", "none");
			}
			jQuery(this).next('div.field-name-field-enable-contact-link').remove();
		}
		else if (jQuery(this).is(':hidden')) {
			//console.log(jQuery.trim(jQuery(this).next('div.field-name-field-enable-contact-link').text())=="1");
			if (jQuery.trim(jQuery(this).next('div.field-name-field-enable-contact-link').text()) == "1") {
				jQuery(this).css("display", "block");
			}
			jQuery(this).next('div.field-name-field-enable-contact-link').remove();
		}
	});
}
