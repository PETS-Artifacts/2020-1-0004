if (jQuery('.node-news .ds-media .field-name-field-colorbox-image').size() > 0) {

	console.log("vGlobalSourceReference::"+vGlobalSourceReference);

	jQuery(document).unbind('cbox_complete').bind('cbox_complete', function() {

		if (vColorboxRefStore == "socialshareemail") {

			jQuery('#colorbox').removeClass('flex_image_colorbox');

			jQuery('#colorbox').addClass('social_icon_email');

		} else {

			jQuery('#colorbox').addClass('flex_image_colorbox');

		}		

		//if (jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').size() > 1) {

		if (jQuery('.node-news div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').size() > 1) {

			jQuery('#cboxPrevious').css('display', 'block').addClass('vzc_colorbox_disabled');

			jQuery('#cboxNext').css('display', 'block').addClass('vzc_colorbox_disabled');

		}

		

		//vGlobalColorboxNavIndex = vGlobalSliderRetainIndex;

		vGlobalColorboxNavIndex = vPgCompGlobalSliderRetainIndex2;

		

		jQuery('#cboxNext').unbind('click').bind('click', ( function (event) {

			//jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').each(function( index ) {

			jQuery('.node-news div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').each(function( index ) {

				if ((vGlobalColorboxNavIndex + 1) == index) {

					console.log("cbox_open vGlobalColorboxNavIndex::"+vGlobalColorboxNavIndex + " src::" +jQuery(this).find('span.colorbox').attr('href'));

					jQuery('#cboxLoadedContent img').attr('src', jQuery(this).find('span.colorbox').attr('href'));

					//updating caption

					jQuery('#cboxTitle').html(jQuery(this).find('div.field-slideshow-caption span').html());

					//updating caption

					vGlobalColorboxNavIndex+=1;

					

					if (jQuery(this).next().length == 0) {

						jQuery('#cboxNext').removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

					}

					return false;

				}

			});

		}));

		

		jQuery('#cboxPrevious').unbind('click').bind('click', ( function (event) {

			//jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').each(function( index ) {

			jQuery('.node-news div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').each(function( index ) {

				if ((vGlobalColorboxNavIndex - 1) == index) {

					jQuery('#cboxLoadedContent img').attr('src', jQuery(this).find('span.colorbox').attr('href'));

					//updating caption

					jQuery('#cboxTitle').html(jQuery(this).find('div.field-slideshow-caption span').html());

					//updating caption

					vGlobalColorboxNavIndex-=1;

					

					if (jQuery(this).prev().length == 0) {

						jQuery('#cboxPrevious').removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

					}

					return false;

				}

			});

		}));

		

		

		

jQuery("#cboxPrevious").unbind('mouseenter').bind('mouseenter', (function (event) {

	if (vGlobalColorboxNavIndex != 0) {

		jQuery('#cboxPrevious').removeClass('vzc_colorbox_disabled').addClass('vzc_colorbox_enabled');

	}

}));




jQuery("#cboxNext").unbind('mouseenter').bind('mouseenter', (function (event) {

	//if (vGlobalColorboxNavIndex != jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').size()-1) {

	if (vGlobalColorboxNavIndex != jQuery('.node-news div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').size()-1) {

		jQuery('#cboxNext').removeClass('vzc_colorbox_disabled').addClass('vzc_colorbox_enabled');

	}	

}));







jQuery("#cboxPrevious").unbind('mouseleave').bind('mouseleave', (function (event) {

	//console.log('cboxPrevious anchor mouse leave!!!');

	jQuery(this).removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

	jQuery(this).removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

}));




jQuery("#cboxNext").unbind('mouseleave').bind('mouseleave', (function (event) {

	//console.log('cboxNext anchor mouse leave!!!');

	jQuery(this).removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

	jQuery(this).removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

}));




		

		if (vGlobalIsFieldColorbox == "yes") {

			fnFlexSliderColorboxVZCInit();

			fnRetainSliderAfterColorboxClose();

		} else {

			fnPgCompFlexSliderColorboxVZCInit();

			fnPgCompRetainSliderAfterColorboxClose();

		}

	});

	

	jQuery(document).unbind('cbox_open').bind('cbox_open', function() {

		console.log("vColorboxRefStore:::"+vColorboxRefStore);

		//jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').each(function( index ) {

		jQuery('.node-news div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').each(function( index ) {

			if (jQuery(this).css('display') !== 'none') {

				vPgCompGlobalSliderRetainIndex2 = index;

				console.log("cbox_open vPgCompGlobalSliderRetainIndex2::"+vPgCompGlobalSliderRetainIndex2);

			}

		});

	});

}




function fnRetainSliderAfterColorboxClose() {

	jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').each(function( index ) {

		if (vPgCompGlobalSliderRetainIndex2 == index) {

			jQuery(this).css('display', 'block');

			jQuery(this).css('opacity', '1');

		} else {

			jQuery(this).css('display', 'none');

			jQuery(this).css('opacity', '0');		

		}

	});

}




function fnFlexSliderColorboxVZCInit() {

	//hiding next prev if one slider present

	if (jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').size() == 1) {

		jQuery('.node-news .ds-media .field-name-field-colorbox-image a.next').css('display', 'none');

		jQuery('.node-news .ds-media .field-name-field-colorbox-image a.prev').css('display', 'none');

	}

	jQuery('.node-news .ds-media .field-name-field-colorbox-image a.next').addClass('vzc_flexslider_colorbox_disabled');

	jQuery('.node-news .ds-media .field-name-field-colorbox-image a.prev').addClass('vzc_flexslider_colorbox_disabled');

	jQuery('#field-slideshow-1-wrapper div.field-slideshow').cycle( {




				timeout: 0, 

        fx:     'fade', 

        //speed:  'fast', 

        prevNextEvent: 'click', 

        prev:   '#field-slideshow-1-controls .prev', 

        next:   '#field-slideshow-1-controls .next',

				nowrap:  1,

autostop: 0,

autostopCount: 0,

manualTrump: false,				

	}).cycle('stop');

}




if (jQuery('.node-news .ds-media .field-name-field-colorbox-image').size() > 0) {

	jQuery(document).ready(function() {

		jQuery(".field-name-social-media-share .social-media-share-icons a#email").unbind('click').bind('click', ( function (event) {

			vColorboxRefStore = "socialshareemail";

		}));

		fnFlexSliderColorboxVZCInit();




		if (jQuery(window).width() < 767) {

			jQuery('.node-news .ds-media .field-name-field-colorbox-image span.colorbox').addClass('init-colorbox-processed cboxElement');

		}

	});

}




jQuery('.ds-media .field-name-field-colorbox-image .field-slideshow-wrapper .field-slideshow-controls .next').unbind('click').bind('click', ( function (event) {

	var varTempIndex = -1;

	jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').each(function( index ) {

		if (jQuery(this).css('display') !== 'none') {

		//if (index == 0) {

			varTempIndex = index;

			vGlobalSliderRetainIndex = index + 1;

		}

		if (varTempIndex == index && index != jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').size()-1) {

			jQuery(this).css('display', 'none');

			jQuery(this).css('opacity', '0');		

			jQuery(this).next().css('display', 'block');

			jQuery(this).next().css('opacity', '1');

			

			if (jQuery(this).next().next().length == 0) {

				jQuery('.node-news .ds-media .field-name-field-colorbox-image a.next').removeClass('vzc_flexslider_colorbox_enabled').addClass('vzc_flexslider_colorbox_disabled');

			}

			return false;

		}

	});

}));

jQuery('.ds-media .field-name-field-colorbox-image .field-slideshow-wrapper .field-slideshow-controls .prev').unbind('click').bind('click', ( function (event) {

	//jQuery('#field-slideshow-1-wrapper div.field-slideshow').cycle('prev');

	var varTempIndex = -1;

	jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').each(function( index ) {

		if (jQuery(this).css('display') == 'block') {

		//if (index == 0) {

			varTempIndex = index;

			vGlobalSliderRetainIndex = index - 1;

		}

		if (varTempIndex == index && index != 0) {

			jQuery(this).css('display', 'none');

			jQuery(this).css('opacity', '0');			

			jQuery(this).prev().css('display', 'block');

			jQuery(this).prev().css('opacity', '1');

			

			if (jQuery(this).prev().prev().length == 0) {

				jQuery('.node-news .ds-media .field-name-field-colorbox-image a.prev').removeClass('vzc_flexslider_colorbox_enabled').addClass('vzc_flexslider_colorbox_disabled');

			}

			return false;

		}

	});

}));




jQuery(".ds-media .field-name-field-colorbox-image .field-slideshow-wrapper .field-slideshow-controls .prev").unbind('mouseenter').bind('mouseenter', (function (event) {

	var varTempIndex = -1;

	jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').each(function( index ) {

		if (jQuery(this).css('display') == 'block') {

		//if (index == 0) {

			varTempIndex = index;

		}

		if (varTempIndex == index && index != 0) {

			jQuery('.node-news .ds-media .field-name-field-colorbox-image a.prev').removeClass('vzc_flexslider_colorbox_disabled').addClass('vzc_flexslider_colorbox_enabled');

			return false;

		}

	});	

}));




