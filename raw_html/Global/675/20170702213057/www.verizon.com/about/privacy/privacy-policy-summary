					});

					
	});
}

	function fnDisplayToolTipDescriptionForProgram(vSeleText) {
		if (vSeleText != '') {
			jQuery('td.views-field.views-field-field-program-tooltip').each(function( index ) {
				//console.log(jQuery('<p>' + jQuery(this).text() + '').text());
				//console.log("td value::"+jQuery(this).text());
				//v_program_description = jQuery('<p>' + jQuery(this).text() + '').text();
				v_program_description = jQuery.trim(jQuery(this).text());
				//console.log(v_program_description + '    progname:'+ vSeleText);
				if (v_program_description.indexOf('_prg_title_'+vSeleText) != -1) {
					//v_program_description = jQuery('<p>' + jQuery(this).text() + '').text();
					//stripping html tags
					v_program_description = v_program_description.replace(/(<([^>]+)>)/ig,"");
					//console.log('inside!!!!'+v_program_description);
					v_program_description = v_program_description.substring(0, v_program_description.indexOf('_prg_title_'));
					if (v_program_description != '') {
						//jQuery( '<input type="text" id="txtProgramToolTip" value="' + v_program_description + '">' ).insertAfter( "#ctools-jump-menu div.container-inline" );
						jQuery('#ctools-jump-menu div.container-inline').parent().append('<div id="divProgramToolTip">' + v_program_description + '');
						return false;
					}
				}
				/*
				else {
					jQuery('#divProgramToolTip').remove();
				}
				*/
			});
		}
	}
