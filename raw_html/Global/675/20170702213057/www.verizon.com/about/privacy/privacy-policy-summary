//start of PG Component flexslider/colorbox




var vPgCompGlobalSliderRetainIndex = 0;

var vPgCompGlobalColorboxNavIndex = 0;

var vPgCompGlobalSliderRetainIndex2 = 0;




var vColorboxRefStore = "socialshareemail";




jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div.field-slideshow-slide').each(function( index ) {

	jQuery(this).prepend(jQuery(this).find('a:first-child').html());

});




jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div.field-slideshow-slide a').each(function( index ) {

	var vToChange = jQuery('<div>').append(jQuery(this).clone(true)).html();

	vToChange = vToChange.replace("<a", "<span");

	vToChange = vToChange.replace("", "");

	jQuery(this).parent().append(vToChange);

});




jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div.field-slideshow-slide').each(function( index ) {

	jQuery(this).find('a').remove();

	jQuery(this).find('span img').remove();

	

	jQuery(this).find('span').hover(function() {

		jQuery(this).css('cursor','pointer');

	});

	

	jQuery(this).find('span').attr('referenceID', jQuery(this).parent().parent().attr("id"));

	jQuery(this).find('span').attr('serialID', (index));

	

	jQuery(this).find('span').unbind('click').bind('click', ( function (event) {

		vColorboxRefStore = jQuery(this).parent().parent().parent().attr("id");

	}));

});










//removing Prev Next text

jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div.field-slideshow-controls a').each(function( index ) {

	jQuery(this).text('');

});




if (jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image').size() > 0) {

	jQuery(document).unbind('cbox_complete').bind('cbox_complete', function() {

		console.log("vColorboxRefStore:::"+vColorboxRefStore);

		if (vColorboxRefStore == "socialshareemail") {

			jQuery('#colorbox').removeClass('flex_image_colorbox');

			console.log("class removed !!!");

			jQuery('#colorbox').addClass('social_icon_email');

		} else {

			jQuery('#colorbox').addClass('flex_image_colorbox');

		}

		

		

	//if (jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').size() > 1) {

	if (jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').size() > 1) {

		jQuery('#cboxPrevious').css('display', 'block').addClass('vzc_colorbox_disabled');

		jQuery('#cboxNext').css('display', 'block').addClass('vzc_colorbox_disabled');

	}

		

		vPgCompGlobalColorboxNavIndex = vPgCompGlobalSliderRetainIndex2;

		

		jQuery('#cboxNext').unbind('click').bind('click', ( function (event) {

			jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').each(function( index ) {

				if ((vPgCompGlobalColorboxNavIndex + 1) == index) {

					jQuery('#cboxLoadedContent img').attr('src', jQuery(this).find('span.colorbox').attr('href'));

					//updating caption

					jQuery('#cboxTitle').html(jQuery(this).find('div.field-slideshow-caption span').html());

					//updating caption

					vPgCompGlobalColorboxNavIndex+=1;

					

					if (jQuery(this).next().length == 0) {

						jQuery('#cboxNext').removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

					}

					return false;

				}

			});			

		}));

		

		jQuery('#cboxPrevious').unbind('click').bind('click', ( function (event) {

			jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').each(function( index ) {

				if ((vPgCompGlobalColorboxNavIndex - 1) == index) {

					jQuery('#cboxLoadedContent img').attr('src', jQuery(this).find('span.colorbox').attr('href'));

					//updating caption

					jQuery('#cboxTitle').html(jQuery(this).find('div.field-slideshow-caption span').html());

					//updating caption

					vPgCompGlobalColorboxNavIndex-=1;

					

					if (jQuery(this).prev().length == 0) {

						jQuery('#cboxPrevious').removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

					}

					return false;

				}

			});

		}));

		

		

		

jQuery("#cboxPrevious").unbind('mouseenter').bind('mouseenter', (function (event) {

	if (vPgCompGlobalColorboxNavIndex != 0) {

		jQuery('#cboxPrevious').removeClass('vzc_colorbox_disabled').addClass('vzc_colorbox_enabled');

	}

}));




jQuery("#cboxNext").unbind('mouseenter').bind('mouseenter', (function (event) {

	if (vPgCompGlobalColorboxNavIndex != jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').size()-1) {

		jQuery('#cboxNext').removeClass('vzc_colorbox_disabled').addClass('vzc_colorbox_enabled');

	}	

}));







jQuery("#cboxPrevious").unbind('mouseleave').bind('mouseleave', (function (event) {

	//console.log('cboxPrevious anchor mouse leave!!!');

	jQuery(this).removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

	jQuery(this).removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

}));




jQuery("#cboxNext").unbind('mouseleave').bind('mouseleave', (function (event) {

	//console.log('cboxNext anchor mouse leave!!!');

	jQuery(this).removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

	jQuery(this).removeClass('vzc_colorbox_enabled').addClass('vzc_colorbox_disabled');

}));




		

		

		fnPgCompFlexSliderColorboxVZCInit();

		fnPgCompRetainSliderAfterColorboxClose();

	});

	

	jQuery(document).unbind('cbox_open').bind('cbox_open', function() {

		jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').each(function( index ) {

			if (jQuery(this).css('display') !== 'none') {

				vPgCompGlobalSliderRetainIndex2 = index;

			}

		});

	});

}




function fnPgCompRetainSliderAfterColorboxClose() {

	jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div#' + vColorboxRefStore + ' div.field-slideshow div.field-slideshow-slide').each(function( index ) {

		if (vPgCompGlobalSliderRetainIndex2 == index) {

			jQuery(this).css('display', 'block');

			jQuery(this).css('opacity', '1');

		} else {

			jQuery(this).css('display', 'none');

			jQuery(this).css('opacity', '0');		

		}

	});

}




function fnPgCompFlexSliderColorboxVZCInit() {

	//hiding pg comp admin title

	jQuery('div.field-name-field-fp-colorbox div.field-label').remove();

	jQuery('div.page-component-colorbox h2').remove();

	jQuery('div.field-name-field-pc-full-width-colorbox').css("display", "none");




	jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div.field-slideshow-wrapper').each(function( index ) {

		if (jQuery(this).find('div.field-slideshow div.field-slideshow-slide').size() == 1) {

			jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image a.next').css('display', 'none');

			jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image a.prev').css('display', 'none');

		}

	});

	jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image a.next').addClass('vzc_pg_comp_flexslider_colorbox_disabled');

	jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image a.prev').addClass('vzc_pg_comp_flexslider_colorbox_disabled');

	console.log('after');




	jQuery('.field-slideshow-wrapper div.field-slideshow').cycle( {




				timeout: 0, 

        fx:     'fade', 

        //speed:  'fast', 

        prevNextEvent: 'click', 

        prev:   '.field-slideshow-controls .prev', 

        next:   '.field-slideshow-controls .next',

				nowrap:  1,

autostop: 0,

autostopCount: 0,

manualTrump: false,				

	}).cycle('stop');




}




if (jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image').size() > 0) {

	jQuery(document).ready(function() {

		jQuery(".field-name-social-media-share .social-media-share-icons a#email").unbind('click').bind('click', ( function (event) {

			vColorboxRefStore = "socialshareemail";

		}));

		fnPgCompFlexSliderColorboxVZCInit();




		if (jQuery(window).width() < 767) {

			jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image span.colorbox').addClass('init-colorbox-processed cboxElement');

		}

	});

}




jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image .field-slideshow-wrapper .field-slideshow-controls .next').unbind('click').bind('click', ( function (event) {

	var varTempIndex = -1;

	jQuery(this).parent().next().find('div.field-slideshow-slide').each(function( index ) {




		if (jQuery(this).css('display') !== 'none') {

		//if (index == 0) {

			varTempIndex = index;

			vPgCompGlobalSliderRetainIndex = index + 1;

		}

		//if (varTempIndex == index && index != jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').size()-1) {

		if (varTempIndex == index && index != jQuery(this).parent().find('div.field-slideshow-slide').size()-1) {

			jQuery(this).css('display', 'none');

			jQuery(this).css('opacity', '0');		

			jQuery(this).next().css('display', 'block');

			jQuery(this).next().css('opacity', '1');

			

			if (jQuery(this).next().next().length == 0) {

				//jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image a.next').removeClass('vzc_pg_comp_flexslider_colorbox_enabled').addClass('vzc_pg_comp_flexslider_colorbox_disabled');

				jQuery(this).parent().prev().find('a.next').removeClass('vzc_pg_comp_flexslider_colorbox_enabled').addClass('vzc_pg_comp_flexslider_colorbox_disabled');

			}

			return false;

		}

	});

}));

jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image .field-slideshow-wrapper .field-slideshow-controls .prev').unbind('click').bind('click', ( function (event) {

	//jQuery('#field-slideshow-1-wrapper div.field-slideshow').cycle('prev');

	var varTempIndex = -1;

	jQuery(this).parent().next().find('div.field-slideshow-slide').each(function( index ) {

	//jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').each(function( index ) {

		if (jQuery(this).css('display') == 'block') {

		//if (index == 0) {

			varTempIndex = index;

			vPgCompGlobalSliderRetainIndex = index - 1;

		}

		if (varTempIndex == index && index != 0) {

			//console.log('inside 1234');

			jQuery(this).css('display', 'none');

			jQuery(this).css('opacity', '0');			

			jQuery(this).prev().css('display', 'block');

			jQuery(this).prev().css('opacity', '1');

			

			if (jQuery(this).prev().prev().length == 0) {

				//jQuery('.node-news .page-component-colorbox .field-name-field-pc-colorbox-image a.prev').removeClass('vzc_pg_comp_flexslider_colorbox_enabled').addClass('vzc_pg_comp_flexslider_colorbox_disabled');

				jQuery(this).parent().prev().find('a.prev').removeClass('vzc_pg_comp_flexslider_colorbox_enabled').addClass('vzc_pg_comp_flexslider_colorbox_disabled');

			}

			return false;

		}

	});

}));




jQuery(".node-news .page-component-colorbox .field-name-field-pc-colorbox-image .field-slideshow-wrapper .field-slideshow-controls .prev").unbind('mouseenter').bind('mouseenter', (function (event) {

