	  jQuery(".popup .view-grouping:eq(0) .view-grouping-header > p").insertAfter(".popup .view-grouping:eq(0) .view-grouping-content h3");
	  jQuery(".popup .view-grouping:eq(1) .view-grouping-header > p").insertAfter(".popup .view-grouping:eq(1) .view-grouping-content h3");
