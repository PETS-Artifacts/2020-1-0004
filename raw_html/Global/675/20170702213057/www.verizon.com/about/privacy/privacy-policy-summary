//bitly issue fix
jQuery(document).ready(function() {
	if(jQuery('.node-news .social-media-share-icons').length !=0) {
		var varText=encodeURIComponent(jQuery('.page-header').html());
		jQuery('.social-media-share-icons .facebook').attr('href', "https://www.facebook.com/sharer/sharer.php?u=" + window.location.href);
		jQuery('.social-media-share-icons .googleplus').attr('href', "https://plus.google.com/share?url=" + window.location.href);
		jQuery('.social-media-share-icons .linkedin').attr('href', "http://www.linkedin.com/shareArticle?mini=true&url="+window.location.href+"&title="+varText+"&summary=&source=");
		jQuery('.social-media-share-icons .twitter').attr('href', "http://www.twitter.com/share?url="+window.location.href+"&via=verizon&text="+varText);
		jQuery("#vzc-news-emailshare-form input[name='url']").val(window.location.href);
	}
});
