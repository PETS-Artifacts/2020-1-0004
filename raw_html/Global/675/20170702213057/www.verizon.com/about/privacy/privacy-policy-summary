function fnSliderAfterCallback() {

	jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-wrapper div.field-slideshow div.field-slideshow-slide').each(function( index ) {

		if (jQuery(this).css('display') !== 'none') {




			//has next check

			if (jQuery(this).next().length == 0) {

				//jQuery('div.field-slideshow-controls a.next').css('display', 'none');

				jQuery('div.field-slideshow-controls a.next').remove();

				jQuery('div.field-slideshow-controls div.next').css('display', 'block');

			} else {

				//jQuery('div.field-slideshow-controls a.next').css('display', 'block');

				jQuery('div.field-slideshow-controls div.next').css('display', 'none');

			}

			//has prev check

			if (jQuery(this).prev().length == 0) {

				////console.log("inside prev!! First child");

			}

		}

	});	

}

