var vGlobalColorboxNavIndex = 0;

var vGlobalSliderRetainIndex2 = 0;

var vGlobalSourceReference = "";

var vGlobalIsFieldColorbox = "";




jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-slide').each(function( index ) {

	jQuery(this).prepend(jQuery(this).find('a:first-child').html());

