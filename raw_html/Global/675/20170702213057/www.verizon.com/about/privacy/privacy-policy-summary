jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-slide a').each(function( index ) {

	var vToChange = jQuery('<div>').append(jQuery(this).clone(true)).html();

	vToChange = vToChange.replace("<a", "<span");

	vToChange = vToChange.replace("", "");

	jQuery(this).parent().append(vToChange);

});




jQuery('.node-news .ds-media .field-name-field-colorbox-image div.field-slideshow-slide').each(function( index ) {

	jQuery(this).find('a').remove();

	jQuery(this).find('span img').remove();

	

	jQuery(this).find('span').hover(function() {

		jQuery(this).css('cursor','pointer');

	});

	

	jQuery(this).find('span').attr('referenceid', jQuery(this).parent().parent().attr("id"));

	jQuery(this).find('span').attr('isfieldcolorbox', "yes");

	

	jQuery(this).find('span').unbind('click').bind('click', ( function (event) {

		vColorboxRefStore = jQuery(this).attr("referenceid");

		vGlobalIsFieldColorbox = jQuery(this).attr("isfieldcolorbox");

		console.log("vColorboxRefStore:"+vColorboxRefStore);

	}));

});




