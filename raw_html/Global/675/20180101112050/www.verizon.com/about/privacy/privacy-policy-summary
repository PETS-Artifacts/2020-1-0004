    var pathArray = window.location.pathname.toLowerCase();
    pathArray = pathArray.split('/');


    if (((pathArray.indexOf('careers') != -1) || (pathArray.indexOf('work') != -1) || (pathArray.indexOf('subscribe') != -1)) && !("")
    ) {

        function indicative_call(event, properties, count) {
            count = count || 0;
            if (typeof Indicative != 'undefined') {
                Indicative.buildEvent(event, properties);
                //console.log('event built: ', event);
                //console.log('event content: ', properties);
            } else if (count < 10) {
                count++;
                setTimeout(function () {
                    indicative_call(event, properties, count)
                }, 1000);
            } else {
                console.log('cannot find Indicative');
            }
        }

        function indicativePageView(){
            if(pathArray.indexOf('careers') + 1 === pathArray.length){
                indicative_call('Career Page View');
            }
            else{
                var trail = jQuery('ol[class = "breadcrumb"]');
                trail = trail[0];
                trail = trail.children;
                var careerIndex = 'none' ;
                for (var i  = 0; i < trail.length ; i++){
                    if(jQuery(trail[i]).text() === "Careers"){
                        careerIndex = i;
                        break;
                    }
                }
                var currentTrail = jQuery(trail[trail.length - 1]).text();

                if (careerIndex !== 'none') {
                    switch (jQuery(trail[careerIndex + 1]).text()) {
                        case 'Working Here' :
                            indicative_call('Working Here Page View', {'Working Here Page Viewed': currentTrail});
                            break;
                        case 'Career Areas' :
                            indicative_call('Career Areas Page View', {'Career Area Page View': currentTrail});
                            break;
                        case 'College Students' :
                            indicative_call('College Students Page View', {'College Students Page Viewed': currentTrail});
                            break;
                        case 'Military' :
                            indicative_call('Military Page View', {'Military Page Viewed': currentTrail});
                            break;
                        case 'Locations' :
                            indicative_call('Locations Page View', {'Locations Page Viewed': currentTrail});
                            break;
                    }
                }else{
                    indicative_call('Page View', {'Page View': currentTrail});
                }
            }
        }

        (function (apiKey) {
            var ind = document.createElement('script');
            ind.src = '//cdn.indicative.com/js/Indicative.min.js';
            ind.type = 'text/javascript';
            ind.async = 'true';
            var ind_init = false;
            ind.onload = ind.onreadystatechange = function () {
                var rs = this.readyState;
                if (ind_init || (rs && rs != 'complete' && rs != 'loaded')) return;

                ind_init = true;
                Indicative.initialize(apiKey, { cookiesOnMainDomain : true});
            };

            var s = document.getElementsByTagName('script')[0];
            s.parentNode.insertBefore(ind, s);
        })("8a58d35b-c07e-4f38-b0ec-01199a834990");


        jQuery(document).ready(function ($) {
            var anchorsUp = $('a').not('.flex-prev').not('.flex-next').not('.colorbox').not('.flex-prev').not(function(index, element){
                return((element.href.indexOf(window.location.href + '?') > -1)|| (element.href.indexOf(window.location.href + '#') > -1) || (element.href.indexOf('#') === element.href[0] || element.href === '' || element.href.indexOf('javascript') !== -1) );
            });


            var tabs = $('a[id^=quicktabs-tab-field_collection_quicktabs-');
            anchorsUp = anchorsUp.not(tabs);


            anchorsUp.one("click" ,function(event){
                event.stopImmediatePropagation();
                indicative_call("Link Click", {'link text': this.href, 'link url': $(this).text()});
                //clicker(this.href, 'link url', this.text, event);
            });

            tabs.one("click" ,function(event){
                event.stopImmediatePropagation();
                event.preventDefault();
                indicative_call('Career Page Tab Click', {'Tab Name': $(this).text()});
            });

            indicativePageView();

        });



