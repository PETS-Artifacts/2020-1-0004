jQuery('a[href*="iot-signup"]').each(function() {
	//if (window.location.href.indexOf("about/iot-signup?") == -1) {
if (typeof jQuery(this).attr("id") == "undefined") {
		jQuery(this).attr("href", "#");
		jQuery(this).attr("target", "");
		jQuery(this).attr("onclick", "vzc_iot_popup_click('check')");		
	}
});

function vzc_iot_popup_click(report_url) {
console.log('inside vzc_iot_popup_click!!!');
	var popup;
	var template_path;

var v_loc_path = window.location.href;
console.log("v_loc_path:::"+v_loc_path);
var v_base_url = "";

if (v_loc_path.toLowerCase().indexOf("https://") != -1) {
	v_base_url = "https://";
} else {
	v_base_url = "http://";
}

if (v_loc_path.toLowerCase().indexOf("www98.verizon.com") != -1) {
	v_base_url += "www98.verizon.com/";
} else if (v_loc_path.toLowerCase().indexOf("vzcorp.ebiz.verizon.com") != -1) {
	v_base_url += "vzcorp.ebiz.verizon.com/";
} else if (v_loc_path.toLowerCase().indexOf("www.verizon.com") != -1) {
	v_base_url += "www.verizon.com/";
} else if (v_loc_path.toLowerCase().indexOf("vzcorp.verizon.com") != -1) {
	v_base_url += "vzcorp.verizon.com/";
}
console.log("v_base_url::"+v_base_url);
//v_base_url = "https://vzcorp.ebiz.verizon.com";
	template_path = v_base_url + "about/iot-signup?width=800&amp;height=600&amp;iframe=true";

//jQuery("a[onclick^=vzc_iot_popup_click]").attr("href", template_path);
//jQuery("a[onclick^=vzc_iot_popup_click]").addClass("colorbox-load");
  var vleft = (screen.width/2)-(800/2);
  var vtop = (screen.height/2)-(600/2);
	window.popup = window.open(template_path, "", "resizable=no,status=no,location=no,toolbar=no,menubar=no,fullscreen=no,scrollbars=yes,dependent=no,width=800,height=600,left="+vleft+",top="+vtop);
/*
var win=jQuery(window.popup);
jQuery(window.popup).ready(function() {
console.log('pop ready!!!');
jQuery(this).find('h1').text("CHANGED");
window.popup.document.getElementsByTagName('h1')[0].innerHTML = "HELLO";
console.log(jQuery(win).find("#main-page-content.col-sm-12 h1").text());   
jQuery(this).find("#main-page-content.col-sm-12 h1").text("test 2017");
console.log(jQuery(this).find("#main-page-content.col-sm-12 h1").text());   
});
*/


