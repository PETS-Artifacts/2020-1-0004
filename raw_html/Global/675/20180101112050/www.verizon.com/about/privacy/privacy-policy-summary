//Leader contact ellipsis functionality, ie limiting to 90 characters
if (jQuery("#block-views-vzc-leadership-bios-block").size() > 0) {
	jQuery( "#block-views-vzc-leadership-bios-block div.field-name-field-leadership-bio" ).each(function( index ) {
		var divLeaBioChar = jQuery(this).html();
		var varAllWords = divLeaBioChar.split(" ");
		//console.log("Total Words : " + varAllWords.length);
		var varStoreChar = 0;
		var varRequired = "";
		if (varAllWords.length >=1) {
			for(var k=0;k<varAllWords.length;k++) {
				varStoreChar += varAllWords[k].length;
				varRequired += varAllWords[k] + " ";
				//console.log("Word : "+varAllWords[k] + " Length : " +varAllWords[k].length + "Tot Count : " +varStoreChar);
				//limiting total characters to 100 (excluding spaces)
				if (varStoreChar>=90) {
					//console.log("Character count 100 reached word formed::"+varRequired);
					//adding ellipsis i.e., ...
					if ((k+1) < varAllWords.length) {
						//console.log("k value:"+k+ " length: "+varAllWords.length);
						varRequired = jQuery.trim(varRequired);
						varRequired+="...";
					}
					break;
				}
			}
			//console.log("Final word formed::"+varRequired);
			jQuery(this).html(varRequired);
		}
	});
}

//Intro Copy component's headline in red
jQuery('div.page-component-cta-intro-copy').each(function( index ) {
	//console.log(jQuery(this).find('h2 a').text().toLowerCase().endsWith("_red"));
	var vHeadLineElement = jQuery(this).find("div.field-name-field-fp-headline-color > :nth-child(2)").text();
	//console.log("vHeadLineElement="+vHeadLineElement);
	//if (jQuery(this).find('h2 a').text().toLowerCase().endsWith("_red")) {
	if (jQuery.trim(vHeadLineElement.toLowerCase()) == "brand red") {
		//console.log(jQuery(this).find( "div.field-name-field-pc-cta-headline > :nth-child(2)").html());
		//previous headline text field
		//jQuery(this).find( "div.field-name-field-pc-cta-headline > :nth-child(2)").attr("style", "");
		//jQuery(this).find( "div.field-name-field-pc-cta-headline > :nth-child(2)").css("color", "#D52B1E");
		//new headline textarea field
		jQuery(this).find( "div.field-name-field-pc-cta-text-headline > :nth-child(2)").css("color", "#D52B1E");
		//previous headline text field
		jQuery(this).find( "div.field-name-field-pc-cta-headline div.field-items").attr("style", "");
		jQuery(this).find( "div.field-name-field-pc-cta-headline div.field-items").css({"color":"#D52B1E"});
		jQuery(this).find( "div.field-name-field-pc-cta-headline div.field-items div.field-item").css({"color":"#D52B1E"});
	}
	jQuery(this).find("div.field-name-field-fp-headline-color").hide();
});
