jQuery(document).ready(function(){
  var x = jQuery("div > div.paragraphs-container-outer.ds-2col-stacked.entity.entity-paragraphs-item.paragraphs-item-fp-two-columns.view-mode-full > div.two-column__row > div > div> div > div.field.field-name-field-pc-cta-description > iframe");
  for(var i = 0; i < x.length; i++){
    var parentx = jQuery(x[i]).parent().parent().parent();
    var detachedx =   jQuery(x[i]).detach();
    parentx.empty();
    parentx.parent().parent().css('margin-bottom','-10px');
    parentx.append(detachedx);
    parentx.css('padding','0');
    detachedx.css({'border':'0','height':'390px'});

  }
});
