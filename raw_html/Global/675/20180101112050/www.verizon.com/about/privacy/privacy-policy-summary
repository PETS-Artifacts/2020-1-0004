//Media contact page fix
jQuery('form[id="views-exposed-form-vzc-news-media-contacts-block"]').each(function( index ) {
	//jQuery(this).find('div[id^="edit-field-media-contact-type-tid-"][class^="form-item-edit-field-media-contact-type-tid-"]').each(function( index ) {
	jQuery(this).find('div[class*="form-item-edit-field-media-contact-type-tid-"], div[class*="selected-edit-field-media-contact-type-tid-"]').each(function( index ) {
		//console.log(jQuery(this).find("a").text());
		var a_text = jQuery(this).find("a").text();
		//console.log(a_text);
		if (a_text.toLowerCase() === "author - news stories") {
			jQuery(this).remove();
		}
		if (a_text.toLowerCase().indexOf("media - ") != -1) {
			//Removing "Media - "
			jQuery(this).find("a").text(a_text.substring(7));
		}
	});
});
