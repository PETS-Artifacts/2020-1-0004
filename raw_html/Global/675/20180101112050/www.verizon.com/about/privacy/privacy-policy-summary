	  jQuery(".popup .view-grouping:eq(2) .view-grouping-header > p").insertAfter(".popup .view-grouping:eq(2) .view-grouping-content h3");
	  jQuery(".popup .view-grouping:eq(3) .view-grouping-header > p").insertAfter(".popup .view-grouping:eq(3) .view-grouping-content h3");
	  jQuery(".popup .view-grouping:eq(4) .view-grouping-header > p").insertAfter(".popup .view-grouping:eq(4) .view-grouping-content h3");
	}
	                //template for popup
                var popup = '<div class="popup-container"><div class="popup-holder"><p class="popup">sample';
                
                //gets block items
                var $blocks = jQuery('.popup .view-grouping');

                for(var i=0; i < $blocks.length; i++){
                  //resets variable
                  pText = '';
                  //gets value
                  pText = $blocks.eq(i).find('.view-grouping-content h3 > p').text();
                  //appends to bottom   
                  if(pText != ''){
                                $blocks.eq(i).append(popup);
                                $blocks.eq(i).find('p.popup').text(pText);
                                $blocks.eq(i).find('.view-grouping-content h3 > p').remove();
                                
                                //creates event listener for h3 - hover state
                                $blocks.eq(i).find('.view-grouping-content h3').hover(function() {
                                                //gets sidebar width
                                                //$sidebar_width = jQuery('.view-grouping-content.popup').width();
                                                //h3 width and 20px
                                                $sidebar_width = jQuery(this).width() + 20;
                                                
                                                $offset =jQuery(this).offset();

                                                 jQuery(this).parent().parent().find('.popup-container .popup-holder').offset({top:$offset.top ,left: $offset.left  + $sidebar_width});
                                                
                                               jQuery(this).parent().parent().find('.popup-container').css({'visibility':'visible'});
                                                
                                }, function() {
                                                jQuery(this).parent().parent().find('.popup-container').css({'visibility':'hidden'});
                                });
                                
                  }
                }

strfilter=jQuery('form#views-exposed-form-map-by-state-page-1 #edit-title').val();
if (strfilter=="")
{
jQuery("form#views-exposed-form-map-by-state-page-1 #edit-submit-map-by-state").hide();
}
if (jQuery("#views-exposed-form-map-by-state-page-1 .views-submit-button button").css("display") == "none") {
		jQuery(".view-id-map_by_state.view-display-id-page_2 .view-content .view-grouping-content.popup").css({'height':'350px'});
