var v_label = "";
jQuery("#vzc-leadership-contact-form select").each(function( index ) {
	v_label = jQuery(this).next().find("button").children().first().text();
	v_label = jQuery.trim(v_label);
	//Get last character
	if (v_label.substring(v_label.length - 1, v_label.length) == "*") {
		v_label = v_label.substring(0, v_label.length - 1);
	}
	jQuery(this).next().find("button").children().first().text(v_label);
	//jQuery(this).next().find("button").children().first().text("");
});

jQuery("#vzc-leadership-contact-form ul.dropdown-menu.selectpicker").each(function( index ) {
	v_label = jQuery(this).children().first().find("span").text();
	v_label = jQuery.trim(v_label);
	v_label = v_label.substring(0, v_label.length - 1);
	jQuery(this).children().first().find("span").text(v_label);
});

