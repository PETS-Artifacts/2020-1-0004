jQuery(document).ajaxComplete(function( event, xhr, settings ) {
	if (jQuery("#views-exposed-form-vzc-event-events-block-browse-events").size() > 0) {
		if (jQuery("div.view-display-id-block_browse_events div.view-content table.views-table tr td div.field-name-field-event-address a span.country").size() > 1) {
			vzcCareerEventAddressMerge("div.view-display-id-block_browse_events div.view-content table.views-table tr td div.field-name-field-event-address a");
			jQuery("#views-exposed-form-vzc-event-events-block-browse-events #edit-submit-vzc-event-events").attr("title", "Apply");
			jQuery("#views-exposed-form-vzc-event-events-block-browse-events #edit-reset").attr("title", "Reset");
			jQuery("div.view-display-id-block_browse_events div.view-content table.views-table tr td a.event-remind-me").attr("title","Remind me");
		}
	}
});
function vzcCareerEventAddressMerge(element) {
jQuery(element).each(function( index ) {
	var v_txt = "";
	if (jQuery(this).find("div.thoroughfare").text() != "") {
		v_txt += jQuery(this).find("div.thoroughfare").text() + "<br>";
	}
	if (jQuery(this).find("div.premise").text() != "") {
		v_txt += jQuery(this).find("div.premise").text() + "<br>";
	}
	
	if (jQuery(this).find("span.locality").text() != "") {
		v_txt += jQuery(this).find("span.locality").text() + ", ";
	}
	if (jQuery(this).find("span.state").text() != "") {
		v_txt += jQuery(this).find("span.state").text() + " ";
	}
	if (jQuery(this).find("span.postal-code").text() != "") {
		v_txt += jQuery(this).find("span.postal-code").text();
	}
	v_txt += "<br>";
	if (jQuery(this).find("span.country").text() != "") {
		v_txt += jQuery(this).find("span.country").text();
	}
	jQuery(this).html(v_txt);
});
}

