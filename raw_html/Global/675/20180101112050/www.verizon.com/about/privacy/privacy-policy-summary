jQuery("#searchsubmit").click(function() {

if (jQuery("#edit-query").val()!="")
{

if(window.location.href.toLowerCase().indexOf("www.verizon.com") != -1)
var url = 'https://www.verizon.com/search/SearchResults?Dy=1&Nty=1&Ntt='+jQuery("#edit-query").val()+'&ES=Corporate&Ns=NA_Dt|1';
else
var url = 'https://www98.verizon.com/search/SearchResults?Dy=1&Nty=1&Ntt='+jQuery("#edit-query").val()+'&smu=Y&ES=Corporate&Ns=NA_Dt|1';
window.location=url;
return false;
}
});
