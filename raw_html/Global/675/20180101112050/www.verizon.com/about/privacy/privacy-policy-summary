	jQuery(this).html(jQuery(this).html().replace(/&lt;/g, '<').replace(/&gt;/g, '>'));
});
//end of investor LP for headline html fix

//fix on all form to remove text node containing Form ID: 
jQuery('form div').each(function( index ) {
	jQuery(this).contents().filter(function () {
		if (this.nodeType === 3) {
			if (jQuery.trim(this.nodeValue).indexOf("Form ID: ") != -1) {
				return this;
			}
		}
	}).remove();
});
//end of fix on all form to remove text node containing Form ID: 


//investor LP
jQuery("#block-vzc-news-blocks-vzc-news-investor-news a.email-cta").attr("title", "Subscribe to investor email");
//Costbasis calculator
//jQuery("[#vzc-taxcost-calculator, #views-exposed-form-vzc-event-events-block-browse-events] button, input, select, #edit-calc-date-container").each(function() {
jQuery("form button, input, select, #edit-calc-date-container").each(function() {
	var vTitle = jQuery(this).attr("data-original-title");
	vTitle = (typeof vTitle != "undefined" && vTitle != "") ? ((vTitle.charAt(vTitle.length - 1) == ".") ? (vTitle.slice(0,-1)) : vTitle) : vTitle;
	//jQuery(this).attr("title", jQuery(this).attr("data-original-title"));
	if (typeof vTitle != "undefined" && vTitle != "") {
		jQuery(this).attr("title", vTitle);
		jQuery(this).attr("data-original-title", "");
	}
});
jQuery("div[id^=" + "cal_companies_" + "]").click(function (e) {
	//jQuery("#edit-cost-company").attr("data-original-title" ,jQuery("#edit-cost-company").val());
	vzcGenericTitleFunction("#edit-cost-company");
});

function vzcGenericTitleFunction(element) {
	jQuery(element).each(function() {
		var vTitle = jQuery(this).attr("data-original-title");
		vTitle = (typeof vTitle != "undefined" && vTitle != "") ? ((vTitle.charAt(vTitle.length - 1) == ".") ? (vTitle.slice(0,-1)) : vTitle) : vTitle;
		jQuery(this).attr("title", vTitle);
		jQuery(this).attr("data-original-title", "");
	});
}

//careers/events-calendar
//for select multiple
