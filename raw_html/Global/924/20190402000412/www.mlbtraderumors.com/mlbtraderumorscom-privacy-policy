      <script>
      document.addEventListener("DOMContentLoaded", function(event) {
        jQuery(document).on('click', '.login-box-close', function(e) {
          jQuery('.login-box').slideUp();
          return false;
        });
        jQuery(document).on('click', '.login-button', function(e) {
          var login_box = jQuery('.login-box');
          var redirect_field = login_box.find('input[name=redirect_to]');
          var redirect_to = redirect_field.val();
          if( !redirect_to.match(/sdf/) ) redirect_to += '#comments';
          redirect_to = redirect_to.replace(/#\S+/,'#comments');
          if( jQuery(this).hasClass('header-login-buton') ) {
            redirect_to = redirect_to.replace(/#\S+/,'');
            login_box.insertAfter(this);
          } else if( jQuery(this).hasClass('comment-reply-login') ) {
            var parent_id = jQuery(this).parents('[id]').eq(0).attr('id');
            redirect_to = redirect_to.replace(/#\S+/,'#'+parent_id);
            login_box.insertAfter(this);
          } else{
            login_box.insertAfter(jQuery(this).parent());
          }
          redirect_field.val(redirect_to);
          
          login_box.slideDown();
          return false;
        });
      });
      </script>
