      data-google-tag-manager="{&quot;productSet&quot;:&quot;BN&quot;,&quot;currencyCode&quot;:&quot;USD&quot;,&quot;languageCode&quot;:&quot;en-us&quot;,&quot;shipToCountryCode&quot;:&quot;USA&quot;,&quot;eventType&quot;:&quot;visit&quot;,&quot;trackPageView&quot;:&quot;&quot;,&quot;selectedSegment&quot;:&quot;none&quot;,&quot;trackPageLoadTime&quot;:false,&quot;userEmail&quot;:&quot;&quot;,&quot;hashedEmail&quot;:&quot;&quot;,&quot;pageType&quot;:&quot;content&quot;,&quot;pageId&quot;:&quot;Privacy Policy&quot;,&quot;noSearchResults&quot;:false,&quot;loginStatus&quot;:false,&quot;memberType&quot;:false,&quot;guid&quot;:&quot;70015E1B_6299_4156_AB2C_ACD960E6DB8A&quot;,&quot;gdprAgreed&quot;:false,&quot;isMarketingEmail&quot;:false,&quot;isHomePage&quot;:false}"
      class="desktop  v360-active">
  <script type="text/javascript">
    (function () {
      var errorPixelUrl         = "https://www.bluenile.com/api/public/log-client-error-pixel",
          pageState             = JSON.parse(document.getElementsByTagName("body")[0].getAttribute("data-pagestate") || "{}"),
          tagManagerState       = JSON.parse(document.getElementsByTagName("body")[0].getAttribute("data-google-tag-manager") || "{}"),
          pageStateCaptureInfo  = ["isFirstPageVisitOfSession", "isHomePage", "pageId"],
          tagManagerCaptureInfo = ["currencyCode", "languageCode", "loginStatus", "pageType", "productSet", "shipToCountryCode"];

      window.onerror = function (message, file, line, column, error) {
        var i, errorObj = {
              note: "An error occurred that wasn't handled by log-client-error.js",
              message: message,
              file: file,
              userAgent: navigator.userAgent,
              line: line,
              column: column,
              error: error
            },
            image       = document.createElement('img');

        if (pageState) {
          for (i = 0; i < pageStateCaptureInfo.length; i++) {
            if (pageState.hasOwnProperty(pageStateCaptureInfo[i])) {
              errorObj["pageState_" + pageStateCaptureInfo[i]] = pageState[pageStateCaptureInfo[i]];
            }
          }
        }
        if (tagManagerState) {
          for (i = 0; i < tagManagerCaptureInfo.length; i++) {
            if (tagManagerState.hasOwnProperty(tagManagerCaptureInfo[i])) {
              errorObj["tagManager_" + tagManagerCaptureInfo[i]] = tagManagerState[tagManagerCaptureInfo[i]];
            }
          }
        }

        image.src = errorPixelUrl + '?error=' + encodeURIComponent(JSON.stringify(errorObj));
        image.style.position = 'absolute';
        document.body.appendChild(image);
      }
    }());
  </script>



