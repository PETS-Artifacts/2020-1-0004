	    		dataLayer.push({'eventCategory': 'Pop-Up - Exit Intent', 'eventAction': 'Show', 'eventLabel': 'Camping Directory', 'event': 'GAevent'}); 
            }
            else {
                jQuery('body').css('overflow','visible');
            }
}
    });
    function validateExitIntentModal() {

        errorMsg = "";

        if (jQuery("#exit-directory-popup-form #control_EMAIL").val() == "") {

            errorMsg += "Email is required. <br/>";

        } else {

            if (!validateExitModalEmail(jQuery("#exit-directory-popup-form #control_EMAIL").val())) { errorMsg += "Email is not valid. <br/>" }
        }

        if (errorMsg != "") {
            jQuery("#exitModalError").show().html(errorMsg);
            return false;
        } else {
            dataLayer.push({'eventCategory': 'Pop-Up - Exit Intent', 'eventAction': 'Submit', 'eventLabel': 'Receive a FREE Online Camping Directory!', 'event': 'GAevent'});
            return true;

        }

    }

    function validateExitModalEmail($email) {
        var emailReg = /^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/;
        return emailReg.test($email);
    }
</script>

