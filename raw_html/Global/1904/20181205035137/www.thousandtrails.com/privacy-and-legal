jQuery(document).ready(function ($) {
    var megaSearch = $('#autocomplete');

    if (megaSearch.length) {

        megaSearch.bind('typeahead:selected', function (obj, datum, name) {
            megaSearch.val(datum.value);

            $('#campground-search').submit()
        });

        megaSearch.typeahead({
            name: 'resorts',
            limit: 15,
            remote: {
                url: '/index.php?option=com_rv&task=autocomplete&q=%QUERY',
                filter: function (data) {
                    return $.map(data, function (item) {

                        var parts = item.title.split(' ');

                        return {
                            tokens: parts,
                            value: item.title
                        }
                    });
                }
            }
        });
    }
});

