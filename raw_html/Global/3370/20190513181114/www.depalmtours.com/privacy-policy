<script id="slideshow_js">var jq$ = jQuery.noConflict();
// Global variables
const slidecontainerWrapper = jq$('#rotatingArticles');
const slidecontainer = jq$('#rotatingArticles .slides');
const slidesNumber = slidecontainer.children().length;
const slidedata = jq$('.slides .slide_item').attr('data-slide');
const slideNavdata = jq$('#slide-nav .slide-button').attr('data-slide');
const slidewidth = 1600;
const slideheight = 480;
const fadespeed = 2000;
const fastfadespeed = 1000;
const slidespeed = 7000;
var startslide;
var content;
jq$.fn.slideShow_calc = function () {
// Scoped variables
const bottom_pad_val = (slideheight / slidewidth) * 100;
const bottom_pad_round = (bottom_pad_val).toFixed(2);
slidecontainer.css({
'padding-bottom': bottom_pad_round + '%',
'width': '100%'
});
}
jq$.fn.slideShow = function () {
if (slidesNumber <= 1) {
return;
} else {
slidecontainer.children(':gt(0)').hide();
slidecontainer.removeClass('stopped');
const sDimensionsW = slidecontainer.outerWidth(false);
const sDimensionsH = slidecontainer.outerHeight(false);
slidecontainer.children().each(function () {
const target = jq$(this).find('img');
const targetDataSrc = target.attr('data-src') + `&cropMode=cropscale&cropX=center&cropY=center&width=${sDimensionsW}&height=${sDimensionsH}`;
// const targetDataSrc = target.attr('data-src');
// console.log(targetDataSrc);
target.attr('data-src', targetDataSrc);
});
slidecontainer.children(':first-child').loadSlideImage();
startslide = setInterval(function () {
slidecontainer.children(':first-child').removeClass('selected').fadeOut(fadespeed)
.next().loadSlideImage()
.fadeIn(fadespeed).toggleClass('selected').end()
.appendTo(slidecontainer);
}, slidespeed);
}
}
jq$.fn.loadSlideImage = function () {
const thisImage = jq$(this).find('img');
const dataSrc = thisImage.attr('data-src');
thisImage.attr('src', dataSrc).css({
'visibility': 'visible'
});
// console.log(this);
return this;
}
jq$.fn.slideShowNavPrev = function () {
slidecontainer.children(':last-child').find('img').attr('src', function () {
const imgsrc = jq$(this).attr('data-src');
return imgsrc;
}).css('visibility', 'visible').end().addClass('selected').fadeIn(fastfadespeed).prependTo(slidecontainer)
.next().removeClass('selected').fadeOut(fastfadespeed);
}
jq$.fn.slideShowNavNext = function () {
slidecontainer.children(':first-child').removeClass('selected').fadeOut(fastfadespeed)
.next().find('img').attr('src', function () {
const imgsrc = jq$(this).attr('data-src');
return imgsrc;
}).css('visibility', 'visible').end().fadeIn(fastfadespeed).toggleClass('selected').end().appendTo(slidecontainer);
}
jq$.fn.add_link_slide = function () {
const slide_item_link = slidecontainer.find('.slide_item').has('.description a');
slide_item_link.each(function () {
const slide_link_url = jq$(this).find('a').attr('href');
const slide_link = '<a href="' + slide_link_url + '"></a>';
jq$(this).children('img').wrapAll(slide_link);
})
}
jq$('#slide-nav .pause').click(function () {
const this_show = slidecontainer;
if (this_show.hasClass('stopped')) {
this_show.slideShow();
jq$(this).removeClass('play').find('i').removeClass('fa-play').addClass('fa-pause');
} else if (this_show.not('stopped')) {
clearInterval(startslide);
this_show.addClass('stopped');
jq$(this).addClass('play').find('i').addClass('fa-play').removeClass('fa-pause');
}
});
jq$('#slide-nav .next').click(function () {
slidecontainer.slideShowNavNext();
});
jq$('#slide-nav .prev').click(function () {
slidecontainer.slideShowNavPrev();
});
jq$(document).ready(function () {
slidecontainer.slideShow_calc();
slidecontainer.add_link_slide();
slidecontainer.children(':gt(0)').each(function () {
jq$(this).find('img').attr('src', '').css('visibility', 'hidden');
});
if (slidesNumber <= 1) {
jq$('#slide-nav').remove();
}
slidecontainer.find('.description').each(function () {
const content = jq$(this).text();
const contentLength = content.length;
if (contentLength <= 3) {
jq$(this).remove();
}
})
});
jq$(document).ready(function () {
slidecontainer.slideShow_calc();
slidecontainer.slideShow();
});
jq$(window).load(function () {
// slidecontainer.slideShow_calc();
// slidecontainer.slideShow();
jq$('#rotatingArticles .slides.initial-load').removeClass('initial-load');
});
jq$(window).resize(function () {
slidecontainer.slideShow_calc();
});</script>
