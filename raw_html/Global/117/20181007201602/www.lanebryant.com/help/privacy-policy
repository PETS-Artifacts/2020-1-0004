/* Borderfree Welcome Mat code(Global) */
var wlcmMatCookieText = readCookie('wlcme'),
defaultCountry = "US",
contextCookieValue;
if (contextCookieValue = readCookie("GEOLOCATION_INFO")) {
contextCookieValue = contextCookieValue.split("|");
if (contextCookieValue[0].match(new RegExp(defaultCountry, "i")) === null
&& wlcmMatCookieText === null) {
wlcme51func("https://embassy.borderfree.com/welcome/welcome.srv?merchId="+4397+"&countryId="
+ contextCookieValue[0] + "&setCookie=Y");
window.onload = function(){
document.body.className += " no-scroll";
}
}
}
function wlcme51func(url) {
var wlcme51 = document.createElement("script");
wlcme51.src = url;
wlcme51.type = "text/javascript";
wlcme51.async = "async";
document.getElementsByTagName("head")[0].appendChild(wlcme51);
}
function readCookie(name) {
if (name.length === 0) {
return null;
}
var cName = name + "=", cArray = document.cookie.split(';'), cACount = cArray.length;
while (cACount) {
cACount--;
var c = cArray[cACount];
while (c.charAt(0) == ' ')
c = c.substring(1, c.length);
if (c.indexOf(cName) === 0)
return c.substring(cName.length, c.length);
}
return null;
};
</script>
