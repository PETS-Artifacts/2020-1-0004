						    <script type="text/javascript">
   function _populateTwitterFeed() {
        function processTweets(tweets) {
            var $target = $('.TwitterFeed');
            for (var i = 0; i < tweets.length; i++) {
                var $post = $('<div>' + tweets[i] + '</div>');

                var time = $post.find('.timePosted').html();
                time = time.replace(/posted on|posted/i, '');
                $post.find('.timePosted').html(time);
                $post.find('a').attr("target", "_blank");

                //console.info($post);
                $target.append($post);
            }
        }

        twitterFetcher.fetch(bestjs.settings.twitterWidgetId, '', 3, true, false, true, '', false, processTweets, false);
    }
 $(document).ready(function () { _populateTwitterFeed(); })
</script>

