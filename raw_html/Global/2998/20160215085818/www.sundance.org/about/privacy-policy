  <script>
  $(document)
    .ready(function() {
    $('.desktop-header-container, #top-links').addClass("scrolled");

      // This prepends content to the signup modal for ticketing purposes.
      //$('.user-layout-container .button.modal-link')
      //  .before(
      //    '<h3>Attention Ticket Package Purchasers!</h3><p>Need to fulfill your previously purchased ticket package? If you have an assigned timeslot, <a href="http://ewaitlist.sundance.org" target="_blank">click here</a> to select your tickets.</p>'
      //  );

      // Toggles search
      $('#searchtoggle')
        .click(function() {
          $("#searchinput")
            .toggleClass("searchshow", "searchhide")
            .focus();
        });
    });

  var hashEl;
  var isSafari = !!navigator.userAgent.match(/Version\/[\d\.]+.*Safari/);

  function resizeCarouselVideos() {
    var newIframeHeight = $('#jumbotron .slick-slide img:nth-child(1), #carousel-home .slick-slide img:nth-child(1)')
      .outerHeight();
    $('#jumbotron .slick-slide iframe, #carousel-home .slick-slide iframe')
      .css("height", newIframeHeight + "px");
  }

  function resizeCarouselVideosOnLoad() {
    var newIframeHeight = $('#jumbotron .slick-slide img:nth-child(1), #carousel-home .slick-slide img:nth-child(1)')
      .outerHeight();
    $('#jumbotron .slick-slide iframe, #carousel-home .slick-slide iframe')
      .css("height", newIframeHeight + "px");
    $('.carousel-items')
      .css("visibility", "visible");
  }

  function changeDescFontSize() {
    // This will resize a carousel item's overlay description if it is too long for the overlay to fit.
    $('div.slick-active')
      .each(function(i, el) {
        var $ovrlayWrappr = $(this)
          .find('div.carousel-wrapper');
        var ovrlayTopPd = parseInt($ovrlayWrappr.css('padding-top'), 10);
        var ovrlayBottPd = parseInt($ovrlayWrappr.css('padding-bottom'), 10);
        var ovrlayVertPd = ovrlayTopPd + ovrlayBottPd;
        var $maxh = parseInt($(this)
          .find('.carousel-wrapper')
          .innerHeight() - ovrlayVertPd, 10);
        var descFontSize = 28;
        if($(this)
          .find('div')
          .hasClass("carousel-wrapper")) {
          $(this)
            .find('div.carousel-description')
            .css('font-size', descFontSize + "px");
          // }
          // if($maxh > 50) {
          var $headline = $(this)
            .find('div.carousel-headline');
          var $desc = $(this)
            .find('div.carousel-description');
          var $cta = $(this)
            .find('div.carousel-cta');
          var $attribution = $(this)
            .find('div.carousel-attribution');

          $ovrlayWrappr.css('visibility', "hidden");
          var actualh = ($headline.outerHeight() + $desc.outerHeight() + $cta.outerHeight() + $attribution.outerHeight());

          if(actualh > $maxh || isSafari) {
            $desc.css('font-size', "15px");
            /*
            while ( actualh > $maxh ) {
                descFontSize = descFontSize - 1;
                $desc.css('font-size', descFontSize + "px");
                actualh = ( $headline.outerHeight() + $desc.outerHeight() + $cta.outerHeight() + $attribution.outerHeight() );
                console.log(actualh + " vs " + $maxh);
            }
            */
          }

          //$desc.css('font-size', "15px");
          $ovrlayWrappr.css('visibility', "visible");
        }
      });
  }

  function changeInstagramLink() {
    $('#top-links .social a[href="' + 'http://instagram.com/sundanceinstitute' + '"]')
      .attr('href', '/instagram');
    $('#top-links .social a[href="' + 'https://twitter.com/sundancefest' + '"]')
      .attr('href', '/twitter');
  }

  function firstDetermineScrollClass() {
    if ( $("#jumbotron div.slick-track").children().length ) {
      resizeCarouselVideosOnLoad();
    }
    if(window.location.hash) {
      hashEl = window.location.hash;
      console.log(hashEl);
      if($(hashEl).parents('div.rib').length > 0 ) {
        $('html, body').animate({
            scrollTop: ($(hashEl)
              .position().top - 107)
          }, 800);
        $(hashEl).closest('div.rib').addClass('open');
      } else {
        $('html, body').animate({
            scrollTop: ($(hashEl)
              .position().top - 147)
          }, 800);
      }
    } /*else if((window.location.pathname === "/" || window.location.pathname === "/someotherpage") && document.body.scrollTop === 0) {
      $('.desktop-header-container, #top-links')
        .removeClass("scrolled");
      $(".header-pusher")
        .css("height", "160px");
    } else if(document.body.scrollTop === 0) {
      $('.desktop-header-container, #top-links')
        .addClass("scrolled");
      $(".header-pusher")
        .css("height", "59px");
    }  else {
      $('.desktop-header-container, #top-links')
        .addClass("scrolled");
      $(".header-pusher").css("height", "59px");
    } */
    if ( $("#jumbotron div.slick-track").children().length ) {
      changeDescFontSize();
    }
    changeInstagramLink();
  }

  function determineScrollClass() {
    /*if((window.location.pathname === "/" || window.location.pathname === "/anotherpage") && document.body.scrollTop === 0) {
      $('.desktop-header-container, #top-links')
        .removeClass("scrolled");
      $(".header-pusher")
        .css("height", "160px");
    } else if(document.body.scrollTop === 0) {
      $('.desktop-header-container, #top-links')
        .addClass("scrolled");
      $(".header-pusher")
        .css("height", "59px");
    } else {
      $('.desktop-header-container, #top-links')
        .addClass("scrolled");
      $(".header-pusher").css("height", "59px");
    }*/
  }

  $(window)
    .resize(function() {
      if ( $("#jumbotron div.slick-track").children().length ) {
        resizeCarouselVideos();
        changeDescFontSize();
      }
    });
  window.onscroll = function(ev) {
    //determineScrollClass();
  };
  if ( $("#jumbotron div.slick-track").children().length ) {
    window.setInterval(changeDescFontSize, 200);
  }
  window.onload = function() {
    firstDetermineScrollClass();
  };
  </script>
