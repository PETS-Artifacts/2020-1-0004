  #content.body-support section p,
  .content.body-support section p {width:initial;margin-right:initial;margin-bottom:1em;float:initial;}
    */
    
    .body-blog section li, .body_event section li, .body-event section li {
       font-size: .875em;
    }
    
    @media screen and (min-width: 512px) {
      .body-blog section li, .body_event section li, .body-event section li {
         font-size: 1.125em;
      }
      .body-blog section p, .body_event section p, .body-event section p {
         font-size: 1.125em;
      }
    }
    
