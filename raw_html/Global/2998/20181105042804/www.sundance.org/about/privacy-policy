<label class='cart-dropdown' for='cart-dropdown-950cb2eb-1537-4b3b-a338-d83927ceea91' ng-class='{loading: cart.isLoading}'>
Cart
<span animate-on-changed='cart.size' change-animation='cart-change-highlight' class='size ng-cloak' ng-bind-html='cart.totalItems' ng-if='cart.hasItems'></span>
</label>
<div class='expiration ng-cloak' ng-bind-html='cart.expiresIn | date: &quot;m:ss&quot;: &quot;UTC&quot;' ng-if='cart.expiresIn'></div>
<div class='cart-dropdown dropdown' ng-class='{empty: ! cart.hasItems}'>
<div class='wrapper cart-wrapper'>
<i class='arrow'></i>
<p class='cart-is-empty' ng-if='! cart.hasItems'>
Your cart is empty.
</p>
<ul class='items' ng-if='cart.hasItems'>
<li class='cart-row' ng-repeat='item in cart.items'>
<div class='title'>[[ (item.title || item.description) | stripTags ]]</div>
<div class='details'>
<div class='quantity' count='item.quantity' ng-if='item.type != &quot;merchandiseproduct&quot;' ng-pluralize when='{1: &quot;{} [[item.paymentName == &#39;Pass Price&#39; ? &#39;pass&#39; : item.type ]]&quot;, &quot;other&quot;: &quot;{} [[item.paymentName == &#39;Pass Price&#39; ? &#39;passe&#39; : item.type]]s&quot;}'></div>
<div class='price'>[[ item.price != 0 ? "$" + item.price : '' ]]</div>
</div>
</li>
</ul>
<div class='summary cart-row' ng-if='cart.hasItems'>
<div class='total-items'>
<div class='left'>[[ cart.totalItems ]] total items</div>
<div class='right'><a href="/my/cart">View cart</a></div>
</div>
<div class='balance'>
<div class='left'>Subtotal</div>
<div class='right'>$[[ cart.balance ]]</div>
</div>
<div class='checkout'>
<a class="button" href="/my/cart">Checkout</a>
</div>
</div>
