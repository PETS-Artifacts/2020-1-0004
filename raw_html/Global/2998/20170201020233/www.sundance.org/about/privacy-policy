<div class='cart' ng-click='links.menuClicked($event)' ng-controller='CartController as cart'>
<input class='droptrigger' id='cart-dropdown-4e4277ce-3abb-437f-a94a-c7c1f7fd8bc8' type='checkbox'>
<label class='cart-dropdown' for='cart-dropdown-4e4277ce-3abb-437f-a94a-c7c1f7fd8bc8' ng-class='{loading: cart.isLoading}'>
Cart
<span animate-on-changed='cart.size' change-animation='cart-change-highlight' class='size ng-cloak' ng-bind-html='cart.size' ng-if='cart.hasItems'></span>
</label>
<div class='expiration ng-cloak' ng-bind-html='cart.expiresIn | date: "m:ss": "UTC"' ng-if='cart.expiresIn'></div>
<div class='cart-dropdown dropdown' ng-class='{empty: ! cart.hasItems}'>
<div class='wrapper cart-wrapper'>
<i class='arrow'></i>
<p class='cart-is-empty' ng-if='! cart.hasItems'>
Your cart is empty.
</p>
<ul class='items' ng-if='cart.hasItems'>
<li class='cart-row' ng-repeat='item in cart.items'>
<div class='title'>[[ item.title || item.description ]]</div>
<div class='details'>
<div class='quantity' count='item.quantity' ng-pluralize when='{1: "{} [[item.type]]", "other": "{} [[item.type]]s"}'></div>
<div class='price'>$[[ item.price ]]</div>
</div>
</li>
</ul>
<div class='summary cart-row' ng-if='cart.hasItems'>
<div class='total-items'>
<div class='left'>[[ cart.totalItems ]] total items</div>
<div class='right'><a href="/my/cart">View cart</a></div>
</div>
<div class='balance'>
<div class='left'>Subtotal</div>
<div class='right'>$[[ cart.balance ]]</div>
</div>
<div class='checkout'>
<a class="button" href="/my/cart">Checkout</a>
</div>
</div>
