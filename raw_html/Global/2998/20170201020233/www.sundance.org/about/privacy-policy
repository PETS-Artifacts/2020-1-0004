<!-- begin tracking outbound links (axllent.org/docs/view/track-outbound-links-with-analytics-js) -->
<script type="text/javascript">
    function _gaLt(event) {
        if (!ga.hasOwnProperty("loaded") || ga.loaded != true || (event.which != 1 && event.which != 2)) {
            return;}
        var el = event.srcElement || event.target;
        while (el && (typeof el.tagName == 'undefined' || el.tagName.toLowerCase() != 'a' || !el.href)) {
            el = el.parentNode;}
        if (el && el.href) {
            var link = el.href;
            if (link.indexOf(location.host) == -1 && !link.match(/^javascript\:/i)) {
                var target = (el.target && !el.target.match(/^_(self|parent|top)$/i)) ? el.target : false;
                if (event.ctrlKey || event.shiftKey || event.metaKey || event.which == 2) {
                    target = "_blank";}
                var hbrun = false;
                var hitBack = function() {
                    if (hbrun) return;
                    hbrun = true;
                    window.location.href = link;};
                if (target) {
                    ga(
                        "send", "event", "Outgoing Links", link,
                        document.location.pathname + document.location.search);
                } else {
                    event.preventDefault ? event.preventDefault() : event.returnValue = !1;
                    ga(
                        "send", "event", "Outgoing Links", link,
                        document.location.pathname + document.location.search, {
                            "hitCallback": hitBack});
                    setTimeout(hitBack, 1000);}}}}
    var _w = window;
    var _gaLtEvt = ("ontouchstart" in _w) ? "click" : "mousedown";
    _w.addEventListener ? _w.addEventListener("load", function() {document.body.addEventListener(_gaLtEvt, _gaLt, !1)}, !1)
        : _w.attachEvent && _w.attachEvent("onload", function() {document.body.attachEvent("on" + _gaLtEvt, _gaLt)});
</script>
<!-- end tracking outbound links -->

<script>
  $(document).ready(function() {
    
	$('#pdfdownload').on('click', function() {
	  ga('send', 'event', 'Download', 'Press Release', document.URL);
	});
	$('.pdfdownload').on('click', function() {
      var doc = $(this).attr("href");
	  ga('send', 'event', 'Download', 'PDF', doc);
	});
    
    var toggleViewText="",trackCat="",trackAct="",trackLab="";
    
    $('ul.toggle-view li h3').on('click', function() {
      if ( $(this).hasClass( "open" ) ) {
        toggleViewText = $.trim( $(this).text() );
        ga('send', 'event', 'Toggle View Open', document.URL, toggleViewText );
      } else if ( $(this).hasClass( "close" ) ) {
        toggleViewText = $.trim( $(this).text() );
        ga('send', 'event', 'Toggle View Close', document.URL, toggleViewText );
      }
    });

    $('div.desktop-header-container a').on('click', function() {
      trackCat="Desktop Header Navigation";
      trackLab = $(this).attr("href");
      if ( $(this).parents('div.sub_nav').length ){
        if ( $(this).parents('div.right').length ){trackAct="subNav-right";}
        if ( $(this).parents('div.left').length ){trackAct="subNav-left";}
      }
      else {
        trackAct="Fixed Area";
      }
      ga('send', 'event', trackCat, trackAct, trackLab);
    });

    $('div.mobile-header-container a').on('click', function() {
      trackCat="Mobile Header Navigation";
      trackLab = $(this).attr("href");
      if ( typeof $(this).attr("title") !== 'undefined' ){
        trackAct = $(this).attr("title");
      }
      else {
        trackAct = $.trim( $(this).text() );
      }
      ga('send', 'event', trackCat, trackAct, trackLab);
    });

    $('div.footer-container a').on('click', function() {
      trackCat="Desktop Footer Navigation";
      trackLab = $(this).attr("href");
      if ( typeof $(this).attr("title") !== 'undefined' ){
        trackAct = $(this).attr("title");
      }
      else {
        trackAct = $.trim( $(this).text() );
      }
      ga('send', 'event', trackCat, trackAct, trackLab);
    });
  });
</script>

