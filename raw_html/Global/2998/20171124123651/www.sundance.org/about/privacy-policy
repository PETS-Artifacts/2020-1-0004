  <script>
  var inArr = {
  responsive: false,
  sub_nav : false
  };
  $('header.responsive, div.sub_nav').mouseover(function () {
  inArr [$(this).attr('class')] = true;
  });
  $('header.responsive, div.sub_nav').mouseout(function () {
  inArr [$(this).attr('class')] = false;
  setTimeout(function () {
  if (!inArr.responsive && !inArr.sub_nav) {
  $('div.sub_nav').fadeOut(150)
  }
  }, 300);
  });
  $(document).ready(function(){
  $("a[target='_blank']").attr('rel', 'noopener'); // prevent window.opener problems
  });
  </script>
