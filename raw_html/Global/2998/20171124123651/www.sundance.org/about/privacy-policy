    $(document)
        .ready(function () {

            $('input.required.email')
                .focus(function () {
                    var p  = window.location.pathname;
                    var q  = location.search;
                    var pr = document.referrer;
                    $('input[name="SIGNUPURL"]')
                        .val(p);
                    $('input[name="QUERYPARAM"]')
                        .val(q);
                    $('input[name="PREVPAGE"]')
                        .val(pr);
                    /**/
                    console.log("SIGNUPURL : " + p);
                    console.log("QUERYPARAM : " + q);
                    console.log("PREVPAGE : " + pr);
                    console.log("-------------");
                    /**/
                });
            $('input[name = "EMAIL"]')
                .focus(function () {
                    var n = window.location.pathname;
                    $('input[name = "SIGNUPURL"]')
                        .val(n);
                });
        });
</script>
