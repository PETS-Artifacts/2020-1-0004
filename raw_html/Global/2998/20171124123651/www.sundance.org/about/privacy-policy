  <script>


  $(document).ready(function(){
    
    // ensure mobile menu is never larger than the viewport height
    // var clientHeight = $( window ).height();
 	// $('.mobile-menu').css('height', clientHeight);
    $(window).resize(function() {
    	$('.mobile-menu').height($(window).height() - 46);
	});
	$(window).trigger('resize');
    
    var tabItem = $(".c-tab__item");
    // adjusts font size of c-tabs based on character c-background__light-gray
    tabItem.each(function (index) {
		
      if (tabItem.has('a, label > input').length) {
      	var textLength = $(this).children("a, label > input").html().length;
        var text       = $(this).children("a, label > input").html();
      } else {
      	var textLength = $(this).html().length;
        var text       = $(this).html();
      }
        if (textLength < 18) {
            $(this).addClass("c-tab__item--short");
        } else if ((textLength >= 18) && (textLength < 24)) {
            $(this).addClass("c-tab__item--long");
        } else if (textLength >= 24) {
            $(this).addClass("c-tab__item--long");
            if (!$(this).hasClass("c-tab__item--active")) {
                var shortText = jQuery.trim(text).substring(0, 26)
                        .trim(this) + "...";
                $(this).text(shortText);
            }
        }
    });

    // add margin if jumbotron has content
      if($('#jumbotron .featured-content-item').length > 0) {
        $('#jumbotron .featured-content-carousel-items').addClass('o-spacer__top--large');
      }

    // toggles a class on current modal as well as body
    $('.o-modal__toggle').click(function() {
      labelID = $(this).attr('for');
      $('#' + labelID).toggleClass('active');
      $('body, html').toggleClass('modal-open');
    });

    // closes all modals when user hits esc key
    $(document).keydown(function(e){
      if(e.keyCode === 27) {
        $('body, html').removeClass('modal-open');
        $('.o-modal--input.active').removeClass('active')
      }
    });

    // Toggles search
    $('#searchtoggle').click(function() {
      $("#searchinput").toggleClass("searchshow", "searchhide").focus();
      $("#searchtoggle").toggleClass("searchclose", "searchopen");
    });

    // add current url to anchor links
    $('a[href^="#"]:not([href="#"])').each(function() {
      //if(!$(this).hasClass('doNotFixHashLink')) {
        var originalLink = $(this).attr("href");
        $(this).attr("href", document.location.pathname + originalLink);
      //};
    });


    // anchor link scrollTo animation
    $(document).on('click', 'a[href*="#"]:not([href="#"])', function(e) {
      var pagesNotToSmoothScroll = ['/i-dont-want-smoothscroll-on-this-page', '/gallery'];
      var checkAgainst = location.pathname;var thisPageShouldSmoothScroll = true;
      $.each(pagesNotToSmoothScroll, function(index, value) {
        checkAgainst = checkAgainst.toLowerCase();
        if (checkAgainst.indexOf(value) >= 0) {
          thisPageShouldSmoothScroll = false;
          console.log("#PageLinkScrolling Deactivated: " + value + " is matched in " + checkAgainst);
        }
      });
      if (thisPageShouldSmoothScroll) {
        e.preventDefault();
        if (location.pathname.replace(/^\//, '') == this.pathname.replace(/^\//, '') && location.hostname == this.hostname) {
          var target = $(this.hash);
          target = target.length ? target : $('[name=' + this.hash.slice(1) + ']');
          if (target.length) {
            var targetElOffs = target.offset();
            $('html,body').stop().animate({
              scrollTop: (targetElOffs.top - 100)
            }, 300);
            return false;
          }
        } else {
          window.location = $(this).attr('href');
        }
      }
    });

  });

  function describeTopNavAnchors(){
    // Add descriptive href to top_nav "#" items (this sends more helpful info to analytic tracking)
    // and prevent default browser action when clicked.
    $('.desktop-header .site_menu .top_nav ul li a').each(function() {
      if ( $(this).attr("href") === "#" ) {
        var aTxt = $.trim( $(this).text() );
        $(this).attr("href", "#" + aTxt).attr("onclick", "return false;");
      }
    });
  }


  function resizeCarouselVideos() {
    var newIframeHeight = $('#jumbotron .slick-slide img:nth-child(1), #carousel-home .slick-slide img:nth-child(1)')
      .outerHeight();
    $('#jumbotron .slick-slide iframe, #carousel-home .slick-slide iframe')
      .css("height", newIframeHeight + "px");
  }

  function resizeCarouselVideosOnLoad() {
    var newIframeHeight = $('#jumbotron .slick-slide img:nth-child(1), #carousel-home .slick-slide img:nth-child(1)')
      .outerHeight();
    $('#jumbotron .slick-slide iframe, #carousel-home .slick-slide iframe')
      .css("height", newIframeHeight + "px");
    $('.carousel-items')
      .css("visibility", "visible");
  }


  function changeDescFontSize() {
    // This will resize a carousel item's overlay description if it is too long for the overlay to fit.
    /*
    var isSafari = !!navigator.userAgent.match(/Version\/[\d\.]+.*Safari/);
    $('div.slick-active')
