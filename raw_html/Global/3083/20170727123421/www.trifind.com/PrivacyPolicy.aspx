	}
	function cancelProgress() {
		$get('mask').className = '';
		$get('spinner').style.display = 'none';
	}
	function ShowSpinner() {
		window.scrollTo(0, 0);
		$get('pagemask').style.display = 'block';
		$get('pagespinner').style.display = 'block';
	}
