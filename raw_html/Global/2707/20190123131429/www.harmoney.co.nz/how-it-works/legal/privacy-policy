(function DefaultSignInAndSignUp() {
  var signInLinks = document.querySelectorAll('[href*="accounts/sign_in"]');
  [].forEach.call(signInLinks, function(link) {
    link.setAttribute('href', '/sign-in');
  });

  var signUpLinks = document.querySelectorAll('[data-target="#signup-modal"]');
  [].forEach.call(signUpLinks, function(link) {
    link.removeAttribute('data-toggle');
    link.removeAttribute('data-target');
    link.setAttribute('sso-modal', 'GuidedSignUp');
  });
})();

(function blogSubscribeButtonTest() {
  var uri = window.location.pathname;
  var isBlogPost = uri.indexOf('/making-it-happen/category') < 0 && uri.indexOf('/making-it-happen/') >= 0;

  if (isBlogPost) {
    if (!window.Sixpack) {
      var sixpackScript = document.createElement('script');
      sixpackScript.src = window.location.origin + '/silverstripe-sixpack/javascript/sixpack.service.min.js';
      sixpackScript.onload = runTest;
      document.body.appendChild(sixpackScript);
    } else {
      runTest();
    }
    
    function runTest() {
      var testName = 'mih_subscribe_button';
      Sixpack.participate(testName, ['control', 'prominent']).then(function(response) {
        var subscribeForm = document.querySelector('#blog-subscribe-modal .marketo-custom');
        var subscribeCTAs = document.querySelectorAll('h5[data-target="#blog-subscribe-modal"]');
        
        if (response.alternative === 'prominent') {
          [].forEach.call(subscribeCTAs, function(cta) {
            cta.innerHTML = '<button class="subscribe-cta-button">' + cta.innerHTML + '</button>';
          });
        }

        subscribeForm.addEventListener('marketo-submitted', function() {
          Sixpack.convert(testName);
        });
      });
    }
  }
})();

(function MIHPromotionSubscribeDismissCookie() {
  var link = document.querySelector('.mih-subscribe');
  
  if (link) {
    link.addEventListener('click', function(e) {
      e.preventDefault();
      
      var expiryDate = new Date();
      expiryDate.setSeconds(3600 * 24);
  
      document.cookie = 'newsletterModalDismissed=true; expires=' + expiryDate.toUTCString() + '; path=/';
      window.location = link.href;
    });
  }
})();
