	<script type="text/javascript">
		function searchKeyPress(input, event, searchpage) {
			var keyCode = event.which || event.keyCode;
			if (keyCode == 13) {
				search(input, event, searchpage);
			}
		}
		function search(input, event, searchpage) {
			searchpage = searchpage || "/search";
			window.location.href = searchpage + "?searchtext=" + input.value + "&searchmode=anyword";
			event.preventDefault();
		}


		// multi level nav
		(function ($) {

			var $multiLevelNavContainer = $(".js-multi-level-nav");

			var $subNavItems = $multiLevelNavContainer.find(".multi-level-nav__item-icon");


			if ($multiLevelNavContainer.length) {

				$subNavItems.on("click.multiLevelNav", toggleSubNav);
			}


			function toggleSubNav(e) {

				e.preventDefault();

				var $this = $(this);

				var $subNavContainer = $this.closest(".multi-level-nav__item");

				$subNavContainer.toggleClass("active");
			}

		})(jQuery);
	</script>

	

	

