<script type="text/javascript">
    jQuery(document).ready(function () {
        var promoSessionValue = 'show';
        if (promoSessionValue == 'hide') {
            
            // set top menu bar to the top edge
           // $('.nav_wrapper').css('top', '0');
            $('.no-scroll').css('top', '0');
        } else {

            // set top menu bar under promo bar
            $('.nav_wrapper').css('top', $('div.top-bar').outerHeight(true));
            $('.no-scroll').css('top', $('div.top-bar').outerHeight(true));
        }

        $(".close").click(function (e) {
            ajaxSetPromoSession('hide');
            $(".top-bar").slideUp();

            // set top menu bar to the top edge
            $('.nav_wrapper').css('top', '0');
            $('.no-scroll').css('top', '0');
        });
        
    });
</script>

<script type="text/javascript">
        function isDark(color) {
            var match = /rgb\((\d+).*?(\d+).*?(\d+)\)/.exec(color);
            if (match != null) {
                return parseFloat(match[1]) + parseFloat(match[2]) + parseFloat(match[3]) < 3 * 256 / 2; // r+g+b should be less than half of max (3 * 256)
            }
        };
</script>

