			});
			jQuery(".item .bookmark").on("click", function(event) {
				event.stopPropagation();
				if (jQuery(this).hasClass("is-marked") == false) {
					ga("send", "event", "Content Bookmark", "bookmark", "Bookmarks");
				} else {
					ga("send", "event", "Content Unbookmark", "unbookmark", "Bookmarks");
				}
			});
