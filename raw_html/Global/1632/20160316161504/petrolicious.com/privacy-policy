		<script>
			var flag = "";
			// Button Actions
			jQuery(".mobile-menu-button").on("click", function() {
				if (jQuery(".mobile-menu.menu").hasClass("hidden")) {
					jQuery(".mobile-menu.menu").removeClass("hidden");
				} else {
					jQuery(".mobile-menu.menu").addClass("hidden");
				}
			});
			jQuery(".close.mobile-menu").on("click", function() {
				jQuery(".mobile-menu.menu").addClass("hidden");
				jQuery(".overlay").addClass("hidden");
			});
			jQuery(".close.user-panel").on("click", function() {
				jQuery(".user-panel").addClass("hidden");
				jQuery(".overlay").addClass("hidden");
			});
			jQuery(".close.email").on("click", function() {
				jQuery('#mc_embed_signup').addClass('hidden');
				jQuery('.overlay').addClass('hidden');
			});
			jQuery(".overlay").on("click", function() {
				var view = 'article';
				if (view == 'article') {
					jQuery("#section-kmt").addClass("close");
					jQuery("#section-kmt").removeClass("open");
					jQuery( '.page-lightbox' ).addClass( 'hidden' );
				}
				jQuery(".user-panel").addClass("hidden");
				jQuery(".user").addClass("hidden");
				jQuery(".overlay").addClass("hidden");
				jQuery("#mc_embed_signup").addClass("hidden");
			});
			jQuery(".account-links .avatar, .mobile-account-links .avatar").on("click", function() {
				if (jQuery(".user-panel").hasClass("hidden")) {
					jQuery(".user-panel").removeClass("hidden");
					jQuery(".overlay").removeClass("hidden");
				} else {
					jQuery(".user-panel").addClass("hidden");
				}
			});

			// fixed nav
			var visible = false;
			jQuery(window).scroll(function() {
				if (!visible && jQuery(document).scrollTop() > 140) {
					visible = true;
					jQuery(".banner").stop().animate({top: 0}, 250);
				} else if (visible && jQuery(document).scrollTop() < 140) {
					visible = false;
					jQuery(".banner").stop().animate({top: -84}, 250);
				}
			});
			jQuery(".facebook.share").on("click", function(event) {
				FB.ui({
					method: 'share',
					href: 'http://petrolicious.com/privacy-policy',
				}, function(response){});
			});
			jQuery(".bookmark").on("click", function(event) {
				event.preventDefault();
				var element = jQuery(this);
				var id = element.attr("data-id");
				function updateDOM (action, status, DOM) {
					if (action == "added") {
						if (status == "success") {
							jQuery(DOM).addClass("is-marked");
						} else {
							alert("Unable to add bookmark at this time. Please try again later.")
						}
					} else if (action == "deleted") {
						if (status == "success") {
							jQuery(DOM).removeClass("is-marked");
						} else {
							alert("Unable to remove bookmark. Please try again later.")
						}
					}
				}
				if (jQuery(this).hasClass("is-marked")) {
					petroliciousBookmark( id, 1, updateDOM, element);
				} else {
					petroliciousBookmark( id, 0, updateDOM, element);
				}
			});
			jQuery(".tab").on("click", function() {
				jQuery(".tab").removeClass("active");
				jQuery(this).addClass("active");
				var content = jQuery(this).attr('data-content');
				jQuery(".tab-content").addClass("hidden");
				jQuery(".tab-content."+content).removeClass("hidden");
			});
			jQuery(".item").hover(function() {
				jQuery(this).find(".bookmark").removeClass("hidden");
			}, function() {
				jQuery(this).find(".bookmark").addClass("hidden");
			});
			jQuery(".view-more a").on("click", function() {
				jQuery(this).addClass("selected").html("Loading");
			});
			jQuery(".banner img.logo").on("click", function() {
				jQuery("html, body").animate({ scrollTop: 0 }, 600);
			});
			// Adjust the width of image sliders to fit contents
			// var slideSpacing = 6;
			jQuery(window).load(function() {
				// var sliderWidth = -slideSpacing;
				// var imgLength;
				jQuery(".image-slider .images").each(function() {
					// imgLength = 0;
					// jQuery(this).children('img').each(function(i, img) {
						// imgLength ++;
						// Note: img.getBoundingClientRect().width is required to capture sub-pixel image widths in Firefox
						// sliderWidth = sliderWidth + img.getBoundingClientRect().width + slideSpacing;
					// });
					if (jQuery(this).children('img').length < 2) {
						jQuery(this).parent().parent().addClass('hide-controls');
						jQuery(this).css({width: '100%', height: 'auto'});
						jQuery(this).children('img').css({width: '100%', height: 'auto'});
					} // else {
						// Note: sliderWidth must be rounded up to prevent the last image from getting pushed to the next line in IE11
						// jQuery(this).css({width: Math.ceil(sliderWidth)});
						// sliderWidth = -slideSpacing;
					// }
				});
			});
			var moveSlider = function(slider, direction) {
				// Set the target position
				var target = slider.scrollLeft();
				target = direction == "next" ? target + slider.width() : target - slider.width();
				var newLeft = target;
				var minDiff = 1000000;
				// Find closest image to target position
				slider.find(".images img").each(function() {
					var left = jQuery(this).offset().left + slider.scrollLeft();
					var diff = Math.abs(target - left);
					if (diff < minDiff) {
						newLeft = left;
						minDiff = diff;
					}
				});
				slider.animate({scrollLeft: newLeft}, 500);
			}
			// Handle arrows on image sliders
			jQuery(".image-slider-container .controls .previous").on("click", function() {
				moveSlider(jQuery(this).parent().siblings(".image-slider"), "previous");
			});
			jQuery(".image-slider-container .controls .next").on("click", function() {
				moveSlider(jQuery(this).parent().siblings(".image-slider"), "next");
			});
			jQuery(".play").on("click", function() {
				jQuery(this).parent().addClass("hidden");
				jQuery(this).parent().siblings(".poster").addClass("hidden");
			});
			jQuery(".item-191").on("click", function(event) {
				event.preventDefault();
				jQuery('#mc_embed_signup').removeClass('hidden');
				jQuery('.overlay').removeClass('hidden');
			});
			//=========================================
			// Image Grid Autolayout
			//=========================================
			var autoLayout = function() {
				var padding = 10;
				var minHeight = 250;
				var gridWidth = jQuery(".grid").width();
				var images = jQuery(".grid img");
				var previousImage;
				// Set all images to the minimum height
				images.attr({width: "auto", height: minHeight});
				// Add margins to all images in the grid
				images.css({marginRight: padding, marginBottom: padding});
				// Clear all previous row assignments
				var i = 0;
				var oldRow;
				while ((oldRow = jQuery(".grid img.row-" + i)).length > 0) {
					oldRow.removeClass("row-" + i++);
				}
				// Assign rows based on natural layout
				var row = 0;
				var totalWidth = 0;
				images.each(function(i) {
					var image = jQuery(this);
					// Assign the current image to the current row
					image.addClass("row-" + row);
					// Update the total width of all images on this row
					totalWidth += Math.round(image.width());
					// If this is the end of a row
					var nextImage = images.eq(i + 1);
					if (nextImage.length == 0 || nextImage.position().left == 0) {
						// Remove the margin from the current image
						image.css({marginRight: 0});
						// Adjust the height of the current row to fill the grid horizontally
						var currentRow = jQuery(".grid img.row-" + row);
						var totalPadding = currentRow.length > 1 ? (currentRow.length - 1) * padding : 0;
						var newHeight = Math.round((minHeight / totalWidth) * (gridWidth - totalPadding));
						currentRow.attr({height: newHeight});
						// Set the width to an integer (for Firefox)
						currentRow.each(function() {
							jQuery(this).attr({width: Math.round(jQuery(this).width())});
						});
						// Calculate new total width
						var newTotalWidth = 0;
						currentRow.each(function() {
							newTotalWidth += jQuery(this).width();
						});
						// Calculate excess
						var excess = (newTotalWidth + totalPadding) - gridWidth;
						// Adjust the last image in the row to account for excess
						image.attr({width: image.width() - excess});
						// Display the current row
						currentRow.removeClass("hidden");
						// Initialize the next row
						row++;
						totalWidth = 0;
					}
				});
			}
			jQuery(".grid img").on("load", function() {
				autoLayout();
			});
			jQuery(window).on("load resize", function() {
				autoLayout();
			});
		</script>
		<script>
			(function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
			(i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
