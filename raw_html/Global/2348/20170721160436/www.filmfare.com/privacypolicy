		if($(this).hasClass("active") === false){
		       $(this).find("ul").css("visibility", "visible");
		}
		   }, function() {
		       $(this).find("ul").css("visibility", "hidden");
		   });
	$('.top_hdr .search').click(function(){
		//$('.searchui').fadeIn();
			if($(document).width() >=981){
					$('.searchui').css('width','400px');
			}else{
					$('.searchui').css('width','300px');
			}
		//$('.searchui .searchbtn').css('width','95px');
		//setTimeout(function(){
			$('.searchui .searchbtn').fadeIn();	
		//},500);
		


		//$('.search_overlay').show();
		$(this).hide();

		$('.top_hdr ul li a.instagram,.top_hdr ul li a.twitter,.top_hdr ul li a.facebook,.top_hdr ul li a.youtube').hide();
		$('.top_hdr .closesearch').show();
		$('#topq').focus();
	});
	$('.top_hdr .closesearch').click(function(){
		//$('.searchui').hide();
		$('.searchui').css('width','0');
		setTimeout(function(){
		$('.searchui .searchbtn').hide();	
		$('.top_hdr ul li a.instagram,.top_hdr ul li a.twitter,.top_hdr ul li a.facebook,.top_hdr ul li a.youtube').show();
		},200);
		//$('.search_overlay').hide();
		$(this).hide();
		$('.top_hdr .search').show();
		$('#topq').blur();
	});
