		  <script>
$(document).ready(function() {
    $(".right_nav ul li").hover(function() {
		if($(this).hasClass("active") === false){
		       $(this).find("ul").css("visibility", "visible");
		}
		   }, function() {
		       $(this).find("ul").css("visibility", "hidden");
		   });
	$('.top_hdr .search').click(function(){
		$('.searchui').fadeIn();
		$('.search_overlay').fadeIn();
		$(this).hide();
		$('.top_hdr .closesearch').fadeIn();
	});
	$('.top_hdr .closesearch').click(function(){
		$('.searchui').fadeOut();
		$('.search_overlay').fadeOut();
		$(this).hide();
		$('.top_hdr .search').fadeIn();
	});
	$('.search_overlay').click(function(){
		$('.searchui').fadeOut();
		$('.top_hdr .closesearch').hide();
		$(this).hide();
		$('.top_hdr .search').fadeIn();
	});
	$(window).scroll(function(){
		$('.searchui').fadeOut();
		$('.search_overlay').fadeOut();
		$('.top_hdr .closesearch').hide();
		$('.top_hdr .search').fadeIn();
	});
	$(document).keyup(function(e) {
		if (e.keyCode == 27) // for esc key
		{
			$('.searchui').fadeOut();
			$('.search_overlay').fadeOut();
			$('.top_hdr .closesearch').hide();
			$('.top_hdr .search').fadeIn(); 
		}
	});	

	
var checkPage = $("#filmfare_middle").find(".inner-wrapper");
	if(checkPage.length >= 1){
	  $("#filmfare_middle").css({"max-width": "100%"});
	  $("#filmfare_middle .inner-wrapper").css({"max-width": "980px", "margin": "0 auto"});
	}
	$(".addComm").click(function(e){
	e.preventDefault();
	   var commenter = "";
	   commenter = $(this).parents(".features");
	   commenter.children(".addCommmentSection").toggle();
	});
	$(".addCommmentSection .close-i").click(function(e){
	e.preventDefault();
	   var commenter = "";
	   commenter = $(this).parents(".addCommmentSection");
	   commenter.toggle();
	});
	
	var windowWidth = $(window).width();


if(windowWidth > 768)
{
  var detectMobile = detectmob();
  var windowHeight = $(window).height();
  var adsHeight = $(".rightS .ads").height();
  var footerHeight = $("footer").height() + 30;
  var widgetHeight = windowHeight - adsHeight - 166;
  $(".rightS .s_viewd_Widget .tab-content").css({"height":widgetHeight,"overflow-y":"auto","overflow-x":"hidden"});
     if(detectMobile === true){
        $.lockfixed("#filmfare_middle .rightS .widget_bar",{offset: {top: 10, bottom: footerHeight}});
     }
     else{
       var bodyHeight = $("#filmfare_middle").height();
       $(".rightS").css('height', bodyHeight);
      $.lockfixed("#filmfare_middle .rightS .widget_bar",{offset: {top: 10, bottom: footerHeight}});
     }
}


$(function () {
    $('.tree li').show();
    $('.tree li li li').hide();
    $('.tree li:first').show();
    $('.tree li').on('click', function (e) {
        var children = $(this).find('> ul > li');
        if (children.is(":visible")) children.hide('fast');
        else children.show('fast');
        e.stopPropagation();
    });
});
});
//For Sticky RHS
function detectmob()
{
        return !navigator.userAgent.match(/iPad|iPhone|Android|BlackBerry|Windows Phone|webOS/i);
}
