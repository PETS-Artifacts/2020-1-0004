<script>
function fireGAevent(eventCategory,eventAction,eventLabel,eventValue){	
	if ( ga != undefined ) {
		if (eventValue!=parseInt(eventValue)){
			ga('send', 'event', eventCategory, eventAction, eventLabel);		
		}else{
			ga('send', 'event', eventCategory, eventAction, eventLabel,eventValue);		
		}
	  }
}
$(document).ready(function() {
	fireGAevent('PageView','load','Antiadblock');
});
</script>
