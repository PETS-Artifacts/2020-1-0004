		//$('.search_overlay').show();
		$(this).hide();

		$('.top_hdr ul li a.instagram,.top_hdr ul li a.twitter,.top_hdr ul li a.facebook,.top_hdr ul li a.youtube').hide();
		$('.top_hdr .closesearch').show();
		$('#topq').focus();
	});
	$('.top_hdr .closesearch').click(function(){
		//$('.searchui').hide();
		$('.searchui').css('width','0');
		setTimeout(function(){
		$('.searchui .searchbtn').hide();	
		$('.top_hdr ul li a.instagram,.top_hdr ul li a.twitter,.top_hdr ul li a.facebook,.top_hdr ul li a.youtube').show();
		},200);
		//$('.search_overlay').hide();
		$(this).hide();
		$('.top_hdr .search').show();
		$('#topq').blur();
	});
	// $('.search_overlay').click(function(){
	// 	//$('.searchui').hide();
	// 	$('.searchui').css('width','0');
	// 	$('.searchui .searchbtn').hide();	
	// 	$('.top_hdr .closesearch').hide();
	// 	$(this).hide();
