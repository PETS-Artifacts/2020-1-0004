  <script>
      (function ($) {
          $('.dropdown-toggle').dropdown();
          var submitIcon = $(".searchbox-icon");
          var inputBox = $(".form-control");
          var searchBox = $(".search-form");
          var formSubmit = $("form-submit");
          var isOpen = false;
          submitIcon.click(function () {
              if (false == isOpen) {
                  searchBox.addClass("search-form-open");
                  formSubmit.remove();
                  inputBox.focus();
                  isOpen = true;
              } else {
                  searchBox.removeClass("search-form-open");
                  inputBox.focusout();
                  isOpen = false;
              }
          });
          var submitIconMobile = $(".searchbox-icon-mobile");
          var inputBoxMobile = $(".form-text");
          var searchBoxMobile = $(".navbar-form");
          var formSubmitMobile = $("form-submit");
          var isOpenMobile = false;
          submitIconMobile.click(function () {
              if (false == isOpenMobile) {
                  searchBoxMobile.addClass("navbar-form-open").show("slide", {
                      direction: "right"
                  }, 1e3);
                  formSubmitMobile.remove();
                  inputBoxMobile.focus();
                  isOpenMobile = true;
              } else {
                  searchBoxMobile.removeClass("navbar-form-open");
                  inputBoxMobile.focusout();
                  isOpenMobile = false;
              }
          });
          $(".views-field-field-testimony-image > .field-content > img").addClass("img-responsive");
      })(jQuery);
  </script>
