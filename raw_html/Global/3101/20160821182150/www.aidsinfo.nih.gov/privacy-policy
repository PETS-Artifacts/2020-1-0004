                <!-- Clinical Trials Search-->
<script src="/content/themes/assets/js/jquery?v=9qlOuiLMDkIB0NzJimz6lX5ldRxSxrgBXdv1n5tvEMo1"></script>


<script type="text/javascript">
   
    $(document).ready(function () {
        $('#ddlCTClass').change(function () {
            var value = $(this).val();
            var originalContent = $('#ddlOrgSubTopics').html();

            $('#ddlSubTopics').html(originalContent); //Restore Original Content

            $('#ddlSubTopics option').each(function () {

                if ($(this).val() != value && $(this).val() != "-1") {
                    $(this).remove(); // Filter subtopics options
                }

                  });

        });

        $('#ddlSubTopics').change(function () {
           var url = $(':selected', this).attr('href');        
           window.location.href = "/"+ url;
        });

    });

</script>       

