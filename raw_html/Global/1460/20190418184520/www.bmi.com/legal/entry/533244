$(document).ready(function(){

	// record modal interactions as GA events
	$("#loginModal a, #loginModal button, #loginModal input:submit").click(function() {
		// track this as a GA event
		var GA_event_label = $(this).attr('data-title');
		_gaq.push(['_trackEvent', 'Modal', 'Login Modal', GA_event_label ]);
	});

	// enable toggle on login modal help text
	$("#loginModal .collapse").collapse();

});
</script>



