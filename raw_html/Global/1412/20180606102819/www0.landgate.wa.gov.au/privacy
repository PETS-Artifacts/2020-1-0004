<!-- Funnelback Javascript Options -->
    <script>

        // Domain check to dynamically set correct collection and profile for FB address auto complete
        currentDomain = document.domain.slice(0, document.domain.indexOf('.'));
        var AddressProfile, AddressCollection, WebCollection;
        switch(currentDomain){
          case "uat0":
            AddressProfile = "qc_uat";
            AddressCollection = "landgate-uat-address"
            WebCollection = "landgate-uat-web";
            break;
          case "www0":
            AddressProfile = "qc";
            AddressCollection = "landgate-address";
            WebCollection = "landgate-corp-web";
            break;
          case "test01":
            AddressProfile = "qc_test";
            AddressCollection = "landgate-test01-address";
            WebCollection = "landgate-test01-web";
            break;
          case "test02":
            AddressProfile = "qc_test";
            AddressCollection = "landgate-test02-address";
            WebCollection = "landgate-test02-web";
            break;
          case "test03":
            AddressProfile = "qc_test";
            AddressCollection = "landgate-test03-address";
            WebCollection = "landgate-test03-web";
            break;
          case "test04":
            AddressProfile = "qc_test";
            AddressCollection = "landgate-test04-address";
            WebCollection = "landgate-test04-web";
            break;
          case "test05":
            AddressProfile = "qc_test";
            AddressCollection = "landgate-test05-address";
            WebCollection = "landgate-test05-web";
            break;
          case "test06":
            AddressProfile = "qc_test";
            AddressCollection = "landgate-test06-address";
            WebCollection = "landgate-test06-web";
            break;
          case "test07":
            AddressProfile = "qc_test";
            AddressCollection = "landgate-test07-address";
            WebCollection = "landgate-test07-web";
            break;
          default:
            AddressProfile = "qc";
            AddressCollection = "landgate-address";
            WebCollection = "landgate-corp-web";
        }
        // Use the correct profile and collection and append to any address search fields to pass to the REST JS
        $.cookie('collection', AddressCollection);
        $.cookie('webcollection', WebCollection);

        var FB =[];
 
        <!-- Query Completion Options -->
  
        FB.fbcompletionOptionsAddress = {
            'enabled'    : 'enabled',
            'standardCompletionEnabled': true,
            'collection' : AddressCollection,
            'program'    : 'https://funnelback.landgate.wa.gov.au/s/search.html',
            'format'     : 'extended',
            'alpha'      : '.5',
            'show'       : '10',
            'sort'       : 'metaZ',
            'length'     : '1',
            'delay'      : '0',
            'profile'    : AddressProfile,
            //Search based completion
            'searchBasedCompletionEnabled': false,
            'searchBasedCompletionProgram': 'https://funnelback.landgate.wa.gov.au/s/search.json'
        }
 
        FB.fbcompletionOptionsWeb = {
            'enabled'    : 'enabled',
            'standardCompletionEnabled': true,
            'collection' : 'landgate-corp-web',
            'program'    : 'https://funnelback.landgate.wa.gov.au/s/suggest.json',
            'format'     : 'extended',
            'alpha'      : '.5',
            'show'       : '10',
            'sort'       : '0',
            'length'     : '3',
            'delay'      : '0',
            'profile'    : '_default',
            //Search based completion
            'searchBasedCompletionEnabled': false,
            'searchBasedCompletionProgram': 'https://funnelback.landgate.wa.gov.au/s/search.json'
        }
       
        <!-- Faceted Navigation more/less links -->
        FB.displayedCategories = 8;
       
    </script>

