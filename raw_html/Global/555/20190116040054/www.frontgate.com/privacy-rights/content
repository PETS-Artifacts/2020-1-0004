<script>
	var timeoutId;
	var timerCount = 0;
	var unbxdEnabledParam = '';
	var unbxdEnabledApplication = 'true';
	var unbxdEnabled;
	
	if (parent.isMobify) {
		unbxdEnabledApplication = 'true';
		
		if (unbxdEnabledApplication == 'true'  ) {
			document.cookie = "unbxdEnabledFlag=" + unbxdEnabledApplication + ";path=/" ;
		}

		var unbxdMobileParamOverride = 'true';
		if(unbxdMobileParamOverride == 'true') {
			if (unbxdEnabledParam == 'true' || unbxdEnabledParam == 'false') {
				document.cookie = "unbxdEnabledFlag=" + unbxdEnabledParam  + ";path=/" ;
			}
		}
		
		unbxdEnabled = Get_Cookie("unbxdEnabledFlag");

		if (unbxdEnabled == null) {
			unbxdEnabled = "false";
			if (com_csb_abVars !== undefined) {
				timeoutId = setInterval(handleMonetateValue, 400);
			} else {
				document.cookie = "unbxdEnabledFlag=" + unbxdEnabled  + ";path=/" ;
			}
		}
		
	}else {
		if (unbxdEnabledApplication == 'true') {
			document.cookie = "unbxdEnabledFlag=" + unbxdEnabledApplication + ";path=/" ;
		}
		
		if (unbxdEnabledParam == 'true' || unbxdEnabledParam == 'false') {
			document.cookie = "unbxdEnabledFlag=" + unbxdEnabledParam  + ";path=/" ;
		}
		
		unbxdEnabled = Get_Cookie("unbxdEnabledFlag");
		
		if (unbxdEnabled == null) {
			unbxdEnabled = "false";
			if (com_csb_abVars !== undefined) {
				timeoutId = setInterval(handleMonetateValue, 400);
			} else {
				document.cookie = "unbxdEnabledFlag=" + unbxdEnabled  + ";path=/" ;
			}
		}
	}
												

    function handleMonetateValue() {
		var unbxdEnabledMonetateValue = com_csb_abVars.getABVariable("unbxdEnabledFlag");
	    if (unbxdEnabledMonetateValue !== undefined || timerCount >= 6) {
	    	clearTimeout(timeoutId);
		    if (unbxdEnabledMonetateValue !== undefined && (unbxdEnabledMonetateValue == true || unbxdEnabledMonetateValue === 1)) {
		    	unbxdEnabled = 'true';
				var autoSuggestionScript = document.createElement('script');
				autoSuggestionScript.type = 'text/javascript';
				autoSuggestionScript.src ='//libraries.unbxdapi.com/frontgate-com701601521464564.js';
				autoSuggestionScript.async = true;
				document.body.appendChild(autoSuggestionScript);
   				if(!disableSliAutocomplete() && window.addEventListener) {
					window.addEventListener("load", function(event) {
						disableSliAutocomplete();
				 });
				}
		    } 
		    fireMonetateAnalyticsEvent("unbxdEnabledFlag", unbxdEnabled);
			document.cookie = "unbxdEnabledFlag=" + unbxdEnabled + ";path=/" ;
		} 
        timerCount ++;
    }

    function disableSliAutocomplete() {
        var sliAutocompleteAreDisabled = false;
    	var element = document.getElementById("sli_autocomplete");
    	if (element !== undefined && element != null) {
    		element.className = 'nodisplay';
    		sliAutocompleteAreDisabled = true;
    	}
    	return sliAutocompleteAreDisabled;
    }
</script>



