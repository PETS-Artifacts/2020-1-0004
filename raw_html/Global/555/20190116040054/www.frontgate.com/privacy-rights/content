	<script type="text/javascript">	
			function getImageUrl(imageInfo) {
				var imageURL;
				if(imageInfo != null && imageInfo.mfpartnumber != null && imageInfo.defAtt != null) {
					var mfpartnumber = imageInfo.mfpartnumber.replace(" ","_");
					var imageSrcInfo = mfpartnumber + "_" + imageInfo.defAtt;
					var defaultImageInfo = mfpartnumber + "_main";
					imageURL = "https://akamai-scene7.frontgate.com/is/image/frontgate/" + imageSrcInfo + "?$SLI_GridView$" + "&defaultImage=" + defaultImageInfo;
				} 
				return imageURL
			
		}
	</script>


