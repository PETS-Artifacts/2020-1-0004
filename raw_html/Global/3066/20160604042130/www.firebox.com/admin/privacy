<script>
        (function($) {
            $(function() {
                FastClick.attach(document.body);

                var viewHeight = $(window).height();
                $(window).resize(function() {
                    viewHeight = $(window).height();
                });

                $(window).on("basketCountUpdate", function(e, count) {
                    if (count > 0) {
                        $(".basket-icon").addClass("active").find(".basket-count").html(count).show();
                    }
                });

                window.addEventListener('basketCountUpdate2', function(e) {
                    console.log("event received", e);
                    var count = e.detail.quantity;
                    if (count > 0) {
                        $(".basket-icon").addClass("active").find(".basket-count").html(count).show();
                    }
                });

                // Hover on mobile is crap, it leaves a stick hover state, so we'll do it with JS instead
                $("#top-bar .nav-icon").on("mouseenter", function() {
                    $(this).addClass("hover");
                }).on("mouseleave", function() {
                    $(this).removeClass("hover");
                });

                // Object representing the mobile/tablet dropdown menu
                var menuDropdown = (function() {
                    // Private state
                    var isOpen = false;
                    var iconElement = $("#menu-icon");
                    var panelElement = $("#menu-dropdown-panel");

                    // Handlers
                    iconElement.on("click", function(e) {
                        e.preventDefault();
                        e.stopPropagation();
                        var nowOpen = menuDropdown.toggle();
                        if (nowOpen) {
                            window.scrollTo(0,0);
                        }
                    });

                    panelElement.on("click", function(e) {
                        if(e.target != this) {
                            return;
                        }
                        menuDropdown.close();
                    });

                    // Public methods
                    return {
                        open: function() {
                            if (!isOpen) {
                                searchDropdown.close();
                                isOpen = true;
                                iconElement.addClass("active");
                                panelElement.addClass("active");
                            }
                        },
                        close: function() {
                            if (isOpen) {
                                isOpen = false;
                                iconElement.removeClass("active").blur();
                                setTimeout(function() {
                                    iconElement.blur();
                                }, 100);
                                panelElement.removeClass("active");
                            }
                        },
                        toggle: function() {
                            if (isOpen) {
                                this.close();
                                return false;
                            } else {
                                this.open();
                                return true;
                            }
                        }
                    }
                })();

                var searchDropdown = (function() {
                    // Private state
                    var isOpen = false;
                    var iconElement = $("#nav-search-icon");
                    var iconScrollElement = $("#nav-search-icon-scroll");
                    var inputElement = $(".search-area input, .search-bar__input");
                    var resultslElement = $(".search-results");
                    var cancelElement = $(".search-area .btr");
                    var cancelElement2 = $(".search-bar__cancel");
                    var willClose = false;

                    // State for react
                    var currentQuery = "";
                    var currentlySearching = false;
                    var currentTags = [];
                    var currentProducts = [];
                    var currentTotalProducts = 0;

                    // Handlers
                    $(window).scroll(function() {
                        if ($(this).scrollTop() > 100) {
                            if (!isOpen) {
                                iconScrollElement.addClass("show");
                                $(".search-bar").addClass("search-bar--hide");
                            }
                        } else {
                            iconScrollElement.removeClass("show");
                            $(".search-bar").removeClass("search-bar--hide");
                        }
                    });

                    iconScrollElement.on("click", function(e) {
                        e.preventDefault();
                        $('body,html').animate({
                            scrollTop : 0
                        }, 500);
                    });

                    iconElement.on("click", function(e) {
                        e.preventDefault();
                        if (currentQuery.length > 0) {
                            window.location = "/search/" + encodeURIComponent(currentQuery);
                        }
                    });

                    $(".search-bar__link").on("click", function(e) {
                        e.preventDefault();
                        if (currentQuery.length > 0) {
                            window.location = "/search/" + encodeURIComponent(currentQuery);
                        }
                    });

                    var close = function() {
                        willClose = true;
                        setTimeout(function() {
                            if (willClose) {
                                iconElement.removeClass("active");
                                resultslElement.removeClass("active");
                                cancelElement.removeClass("active");
                                cancelElement2.removeClass("search-bar__cancel--active");
                                isOpen = false;
                            }
                        }, 300);
                    };

                    inputElement.on("focus", function() {
                        willClose = false;
                        iconElement.addClass("active");
                        render();
                        resultslElement.addClass("active");
                        isOpen = true;
                        if (currentQuery.length > 0) {
                            cancelElement.addClass("active");
                            cancelElement2.addClass("search-bar__cancel--active");
                        }

                        menuDropdown.close();
                        window.scrollTo(0,0);

                    }).on("blur", function() {
                        close();
                    });

                    inputElement.on("keydown", function(e) {
                        var code = e.keyCode || e.which;
                        if (code == 13) {
                            window.location = "/search/" + encodeURIComponent(currentQuery);
                        }
                    });

                    inputElement.on("input", function(e) {

                        currentQuery = $(this).val();
                        window.scrollTo(0,0);
                        render();

                        if (currentQuery.length > 0) {
                            currentlySearching = true;
                            render();
                            $.get("/search/ajax/" + encodeURIComponent(currentQuery), function(response) {
                                if (response.query === currentQuery) {

                                    currentlySearching = false;
                                    currentTags = response.tags;
                                    currentProducts = response.products;
                                    currentTotalProducts = response.totalProducts;

                                    render();
                                }
                            });

                            // Send to Google
                            if (window.dataLayer !== undefined) {
                                dataLayer.push({
                                    event: 'searchBox',
                                    query: currentQuery
                                });
                            }

                        } else {
                            // clear results
                            currentTags = [];
                            currentProducts  =[];

                            render();
                        }

                        if (currentQuery.length > 0) {
                            cancelElement.addClass("active");
                            cancelElement2.addClass("search-bar__cancel--active");
                        } else {
                            cancelElement.removeClass("active");
                            cancelElement2.removeClass("search-bar__cancel--active");
                        }
                    });

                    cancelElement.on("click", function() {
                        // Clear the search..
                        inputElement.val("");

                        currentQuery = "";
                        currentlySearching = false;
                        currentTags = [];
                        currentProducts  = [];
                        currentTotalProducts = 0;

                        close();
                    });

                    cancelElement2.on("click", function() {
                        $(".search-bar__input").val("");
                        currentQuery = "";
                        currentlySearching = false;
                        currentTags = [];
                        currentProducts  = [];
                        currentTotalProducts = 0;
                        close();
                    });

                    // Private methods

                    var render = function() {
                        // Render the react search results component
                        window.renderSearch(currentQuery, currentlySearching, currentTags, currentProducts, currentTotalProducts);
                    };

                    // Public methods
                    return {
                        open: function() {
                            if (!isOpen) {
                                menuDropdown.close();
                                isOpen = true;
                                iconElement.addClass("active");
                                panelElement.addClass("visible");
                                panelElement.find("input").val("").focus();
                            }
                        },
                        close: function() {
                            if (isOpen) {
                                isOpen = false;
                                iconElement.removeClass("active");
                                resultslElement.removeClass("visible");
                            }
                        },
                        toggle: function() {
                            if (isOpen) {
                                this.close();
                            } else {
                                this.open();
                            }
                        }
                    }
                })();

                (function() {
                    var cursorOverButton = false;
                    var cursorOverBar = false;
                    var willHideSoon = false;
                    var handleBrowse = function() {
                        if (cursorOverButton || cursorOverBar) {
                            willHideSoon = false;
                            $("#browse-link").addClass("active");
                            $("#browse-bar").addClass("visible");
                        } else {
                            willHideSoon = true;
                            setTimeout(function() {
                                if (willHideSoon) {
                                    $("#browse-link").removeClass("active");
                                    $("#browse-bar").removeClass("visible");
                                }
                            }, 50);
                        }
                    };
                    $("#browse-link").hover(function() {
                        cursorOverButton = true;
                        handleBrowse();

                        var panelHeight = $("#browse-bar").height();

                        if (viewHeight > 0 && panelHeight > 0 && (panelHeight + 50) < viewHeight) {
                            $("#browse-bar").css('position', 'fixed');
                        } else {
                            $("#browse-bar").css('position', 'absolute');
                            window.scrollTo(0,0);
                        }
                        $(".browse-bar2").css("left", $(this).offset().left);
                    }, function() {
                        cursorOverButton = false;
                        handleBrowse();
                    });
                    $("#browse-bar").hover(function() {
                        cursorOverBar = true;
                        handleBrowse();
                    }, function() {
                        cursorOverBar = false;
                        handleBrowse();
                    });
                })();

                // Object representing the mobile/tablet dropdown menu
                var userMenuDropdown = (function() {
                    // Private state
                    var isOpen = false;
                    var iconElement = $("#user-icon");
                    var panelElement = $("#user-dropdown-panel");

                    var hoveringInIcon = false;
                    var hoveringInMenu = false;
                    var touchedToOpen = false;
                    var touchedToClose = false;
                    var willCloseSoon = false;

                    var handleMenu = function() {
                        if (touchedToOpen || touchedToClose) {
                            if (touchedToOpen) {
                                panelElement.slideDown('fast');
                            } else {
                                // Close it now
                                panelElement.slideUp('fast');
                            }

                        } else {
                            if (hoveringInIcon || hoveringInMenu) {

                                panelElement.slideDown('fast');

                            } else {
                                willCloseSoon = true;
                                setTimeout(function () {
                                    if (willCloseSoon) {
                                        panelElement.slideUp('fast');
                                    }
                                }, 150);
                            }
                        }
                    };

                    iconElement.hover(function () {
                        hoveringInIcon = true;
                        willCloseSoon = false;
                        handleMenu();
                    }, function () {
                        hoveringInIcon = false;
                        handleMenu();
                    });

                    panelElement.hover(function () {
                        hoveringInMenu = true;
                        willCloseSoon = false;
                        handleMenu();
                    }, function () {
                        hoveringInMenu = false;
                        handleMenu();
                    });

                    iconElement.on("touchstart", function(e) {
                        e.preventDefault();
                        if (touchedToOpen) {
                            touchedToOpen = false;
                            touchedToClose = true;
                            iconElement.removeClass('active');
                        } else {
                            touchedToOpen = true;
                            touchedToClose = false;
                            iconElement.addClass('active');
                        }
                        handleMenu();
                    });

                    panelElement.find(".currency").on("click", function() {
                        panelElement.find(".currency").removeClass("active");
                        $(this).addClass("active");
                        $.post("/currency/" + $(this).data("id"), function() {
                            //reload without mkt parameter
                            window.location.replace(window.location.pathname);
                        });
                    });

                    // Public methods
                    return {
                    }
                })();
            });
        })(jQuery)
    </script>
 
