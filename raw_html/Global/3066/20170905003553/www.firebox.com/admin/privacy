<SCRIPT type="text/javascript">
$(function() {
	var offset = $j("#fixer").height();
	if (window.location.hash){
		var hash = window.location.hash.substring(1);
		$j('html, body').delay(200).animate({
	      scrollTop: $j('[name="' + hash + '"]').offset().top - offset
	    }, 400);
	}
	$j('a[href*=#]').on('click', function(event){     
	    event.preventDefault();
	    var hash = $j(this).attr('href').substring(1);
		$j('html, body').animate({
	      scrollTop: $j('[name="' + hash + '"]').offset().top - offset
	    }, 400);
	});
});
</script>



