<script>
    function validateEmail(email) {
        return /^(([^<>()[\]\\.,;:\s@\"]+(\.[^<>()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/.test(email);
    }

    function newsletterSignUpValidateEmail() {
        clearNewsletterSignupValidation();
        var validationMsgs = document.querySelector('.footer__signup-validation');
        var emailInput = document.querySelector('#footer__signup_email_input');
        var agreeCheckbox = document.querySelector('#footer__signup_agreed-to-newsletter');
        var errorMsgEmail = document.querySelector('#invalidEmailAddress');
        var errorMsgCheckbox = document.querySelector('#agreeNotChecked');
        var errorMsgBoth = document.querySelector('#invalidEmailAddressAndAgreeNotChecked');
        var errorCount = 0;
        if (!validateEmail(emailInput.value) && agreeCheckbox.checked) {
            errorMsgEmail.setAttribute('style', 'display:block');
            emailInput.focus();
            errorCount++;
        }
        if (validateEmail(emailInput.value) && !agreeCheckbox.checked) {
            errorMsgCheckbox.setAttribute('style', 'display:block');
            agreeCheckbox.focus();
            errorCount++;
        }
        if (!validateEmail(emailInput.value) && !agreeCheckbox.checked) {
            errorMsgBoth.setAttribute('style', 'display:block');
            emailInput.focus();
            errorCount++;
        }
        if (errorCount > 0) {
            validationMsgs.setAttribute('style', 'display:block');
            return false;
        }
        validationMsgs.setAttribute('style', 'display:none');
        document.querySelector('.footer__signup-btn').innerText = 'Registering...';
        document.querySelector('.footer__signup-btn').setAttribute('disabled', true);
        return true;
    }

    function clearNewsletterSignupValidation() {
        var allMsgs = document.querySelector('.footer__signup-validation div.alert--error').children;
        for (var i = 0; i < allMsgs.length; i++) {
            allMsgs[i].setAttribute('style', 'display:none'); //second console output
        }
    }
</script>
