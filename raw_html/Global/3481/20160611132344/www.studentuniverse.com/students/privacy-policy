	dataLayer = [];
	
	dataLayer.push({"siteVersion":"5.5.1.0","siteEnvironment":"PROD","device":"desktop","visitorId":"XDSIRKVJLTMVKDETO","loggedIn":false,"cobrand":{"code":"studentuniverse","dk":"1000","partnerName":"StudentUniverse","partnerTracking":null}})
