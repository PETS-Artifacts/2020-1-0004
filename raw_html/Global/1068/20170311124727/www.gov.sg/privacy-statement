                                <!-- search form -->
                                <div id="searchbar_0_DivCode">

    <script type="text/javascript">
    	$(document).ready(function () {
            $('#btnGoogleSearch').click(function () {
                var searchKey = $('#txtSearchkey').val();
                if (searchKey=="") {
                    return false;
                }
                var csKey = $('#sel option:selected').val();
                	window.open('http://www.google.com/cse?q=' + searchKey + '&cx=' + csKey, "_blank");
            });
        });
        function runScript(e) {
            if (e.keyCode == 13) {//for ie7 trigger when press enter
                e.preventDefault();
                $('#btnGoogleSearch').trigger('click');
                return false;
            }
        }
    </script>
    <div class="site-search">
            <label for="txtSearchkey" style="display:none;">Search</label>
        <input type="text" class="searchinput" title="Search" placeholder="Search" id="txtSearchkey" onkeypress="return runScript(event)" />
         <label for="sel" style="display:none;">Search Within</label>
        <select id="sel" class="searchoption" >
            
            <option value="001092496951764254203:mb1pl5iphji">Within All Government Website</option>
        </select>
        <i class="glyphicon glyphicon-search" id="btnGoogleSearch"></i>
    </div>
</div>





