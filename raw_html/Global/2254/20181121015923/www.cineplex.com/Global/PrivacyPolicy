<script>
    //Search autocomplete is activated here
    $(function () {
        $('#search-query').sayt({
            subdomain: 'cineplex',
            collection: 'theatre', //Collection1 will appear first, then collection2, etc...
            collection2: 'store',
            saytCollection: 'theatreSayt',  //These collections are used for product recommendations
            saytCollection2: 'storeSayt',
            saytArea: 'ProductionTheatreWildcard',
            saytArea2: 'ProductionStoreWildcard',
            autocompleteTemplate: 'autocompleteTemplate',
            productTemplate: 'productTemplate',
            defaultImage: 'https://mediafiles.cineplex.com/Cineplex2013/postermissing_230x341.jpg',
            selectSearchTerm: function (data) {
                $('#search-query').val(data.value);
                $('#search-form').submit();
            },
            selectNavigation: function (data) {
                $('#search-query').val(data.value);
                $('#search-form').submit();
            },
            selectProduct: function (data) {
                if (data.TheatreUrlName != null && data.TheatreUrlName != '')
                    window.location.href = '/Showtimes/{0}/nearby-theatres'.replace('{0}', data.TheatreUrlName);
                else if (data.TheatreUrl != null && data.TheatreUrl != '' && data.LanguageField == 'lang_fr')
                    window.location.href = 'https://www.cineplex.com/Film/' + data.TheatreUrl;
                else if (data.TheatreUrl != null && data.TheatreUrl != '')
                    window.location.href = 'https://www.cineplex.com/Movie/' + data.TheatreUrl;
                else if (data.StoreUrlId != null && data.StoreUrlId != '')
                    window.location.href = 'https://store.cineplex.com/Product/' + data.StoreUrlId;
            }
        });
    });
</script>
