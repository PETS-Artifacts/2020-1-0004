                addSize([980, 690], [[970, 90], [728, 90]]).
                addSize([768, 576], [728, 90]).
                build();
                googletag.defineSlot("/7226/cplx.com_Global_{0}/970x90_top", [[970, 90], [728, 90]], "adblock-970x90_top").defineSizeMapping(mapping);
                googletag.defineSlot("/7226/cplx.com_Global_{0}/970x90_bottom", [[970, 90], [728, 90]], "adblock-970x90_bottom").defineSizeMapping(mapping);
                googletag.defineSlot("/7226/cplx.com_Global_{0}/320x50_top", [320, 50], "adblock-320x50_top");
                googletag.defineSlot("/7226/cplx.com_Global_{0}/320x50_bottom", [320, 50], "adblock-320x50_bottom");
                googletag.defineSlot("/7226/cplx.com_Global_{0}/300x250", [300, 250], "adblock-300x250");
