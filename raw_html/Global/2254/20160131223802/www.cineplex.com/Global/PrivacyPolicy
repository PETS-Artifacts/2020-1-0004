                googletag.defineSlot("/7226/cplx.com_Global_{0}/320x50_top", [320, 50], "adblock-320x50_top");
                googletag.defineSlot("/7226/cplx.com_Global_{0}/320x50_bottom", [320, 50], "adblock-320x50_bottom");
