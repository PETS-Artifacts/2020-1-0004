            });
        </script>
        <script type="text/javascript">
            $.when(
                //get jquery responsive ad plugin
                $.getScript("/js/jquery.responsiveAd.js"),
                //ensures the dom is fully loaded
                $.Deferred(function (deferred) {
                    $(deferred.resolve);
                })
            ).done(function () {
                //safe responsiveAd function for jquery
                $.fn.addResponsiveAd = function (screenSize) {
                    if (this.length > 0) {
                        $(this[0]).responsiveAd({ screenSize: screenSize });
                    }
                }

                //desktop
                $('#adblock-970x90_top').addResponsiveAd('desktop');
                $('#adblock-970x90_bottom').addResponsiveAd('desktop');
                $('#adblock-160x600').addResponsiveAd('desktop');
                $('#adblock-235x90').addResponsiveAd('desktop');
                $('#adblock-300x250').addResponsiveAd('desktop');
                $('#adblock-voken').addResponsiveAd('desktop');
                $('#adblock-215x552').addResponsiveAd('desktop');

                //mobile
                $('#adblock-320x50_top').addResponsiveAd('mobile');
                $('#adblock-320x50_bottom').addResponsiveAd('mobile');
                $('#adblock-300x250').addResponsiveAd('mobile');
            });
        </script>
