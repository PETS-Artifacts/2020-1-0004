<script>
jQuery(document).ready(function(e) {

      jQuery("#mainMenu li.parent").click(function(){
            jQuery(this).toggleClass("displayit");
          });

   jQuery('aside.sidebar div.sd_nav ul.menu li ul li ul li a').addClass('nolink');
        jQuery('.nolink').click(function(){
          jQuery(this).parent().find("ul").toggle("fast");
          jQuery(this).parent().toggleClass("isactive");
          return false;
        });

      jQuery( "ul.topnav li ul li, aside.sidebar div.sd_nav ul.menu li ul li ul li > span" ).click(function() {
      if(jQuery(this).parent().length){
            jQuery(this).toggleClass("isactive");
      }
      });

      if(jQuery("#mainMenu>li.active").length) {
//         jQuery(".sd_nav .topnav").html("<li>"+jQuery("#mainMenu>li.active").html()+"</li>");
      }
     jQuery( "ul.topnav li ul li, aside.sidebar div.sd_nav ul.menu li ul li ul li, aside.sidebar div.sd_nav ul.menu li ul li ul li.deeper, aside.sidebar div.sd_nav ul.menu li ul li ul li.deeper" ).click(function() {
          jQuery(this).find("ul").first().toggle("fast");
      });

    jQuery('.deeper .deeper.active').addClass("blackicon");


    jQuery('.deeper .deeper>a').click(function(){


       jQuery(this).parent("li").toggleClass("blackicon").toggleClass("active").toggleClass("hideul");

       if(jQuery(this).parent("li.active").hasClass("hideul")){ jQuery(this).parent("li").find("ul").slideUp('slow').delay( 800 ).stop(true, true); }

      return false;
     });



     jQuery('ul.menu.topnav>li>ul>li.parent>ul>li>a').click(function(){
       var goto = jQuery(this).attr("href");
     if(jQuery(this).parent("li").hasClass("deeper")){
       jQuery(this).parent("li").addClass("gotit");

         return true;
     }else{ document.location.href=goto;}

      // if(jQuery(this).hasClass("active") && jQuery(this).hasClass("current")){ jQuery(this).addClass("blackicon"); };
      return false;
     });


     jQuery(".deeper .deeper > ul>li>a").click(function(){
        var goto = jQuery(this).attr("href");
      document.location.href=goto;
       return false;
     });

     jQuery(".sd_nav .topnav li img").closest("li").remove();
    /*




  jQuery( "ul#mainMenu > li" ).click(function() {
    jQuery( this ).toggleClass( "displayit" );
  });




    */


});







      </script>

