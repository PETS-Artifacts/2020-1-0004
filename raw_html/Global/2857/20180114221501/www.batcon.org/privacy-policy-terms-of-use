<script type="text/javascript">
var jsTransOverlay = new Object();			// namespace

jsTransOverlay.showOverlay = function() {
var o = document.getElementById('translateinfo');
o.style.visibility = 'visible';
}
jsTransOverlay.hideOverlay = function() {
var o = document.getElementById('translateinfo');
o.style.visibility = 'hidden';
}
jsTransOverlay.delayHide = function() {
jsTransOverlay.delayhidevar = setTimeout("jsTransOverlay.hideOverlay();",1000)
}
jsTransOverlay.clearHide = function() {
	if (this.delayhidevar) {
		clearTimeout(this.delayhidevar);
		this.delayhidevar = null;
	}
}
