  MTFontIds.push("694021"); // Neue Helvetica® W02 55 Roman
  MTFontIds.push("694024"); // Neue Helvetica® W02 56 Italic
  MTFontIds.push("694033"); // Neue Helvetica® W02 75 Bold
  MTFontIds.push("694036"); // Neue Helvetica® W02 76 Bold Italic
  MTFontIds.push("694045"); // Neue Helvetica® W02 95 Black
  MTFontIds.push("674486"); // Neue Helvetica® W02 96 Black Italic
  (function () {
    var mtTracking = document.createElement('script');
    mtTracking.type = 'text/javascript';
    mtTracking.async = 'true';
    mtTracking.src = ('https:' == document.location.protocol ? 'https:' : 'http:') + '//fast.fonts.net/lt/trackingCode.js';

    (document.getElementsByTagName('head')[0] || document.getElementsByTagName('body')[0]).appendChild(mtTracking);
  })();
</script>

