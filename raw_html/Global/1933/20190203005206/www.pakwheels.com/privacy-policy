    <script>
      var OneSignal = window.OneSignal || [];
      OneSignal.push(["init", {
        appId: "2cca5b5e-e246-46f7-9a26-f638f0d42a03",
        safari_web_id: "web.onesignal.auto.5e769637-3ad0-4829-bde6-e4b31d264ae2",
        autoRegister: false, /* Set to true to automatically prompt visitors */
        httpPermissionRequest: {
            enable: true
        },
        notifyButton: {
          enable: false /* Set to false to hide */
        },
        promptOptions: {
            siteName: 'OneSignal Documentation',
            actionMessage: "Subscribe to our push notification for the latest news & Updates",
            acceptButtonText: "ALLOW",
            cancelButtonText: "NO THANKS"
        },
        path: '/OneSignalDesktop/',
      }]);
      OneSignal.push(function() {
          OneSignal.isPushNotificationsEnabled(function(isEnabled) {
              if (isEnabled)
                  console.log("Push notifications are enabled!");
              else{
                OneSignal.push(function() {
                  OneSignal.showHttpPrompt();
                });
              }
          });

          OneSignal.on('subscriptionChange', function (isSubscribed) {
              console.log("The user's subscription status has change, isSubscribed:", isSubscribed);
              if (isSubscribed)
              {
                trackEvents('User','From-browser PushSubscriptionDone', "main",true);
              }else{
                 trackEvents('User','From-browser PushSubscriptionClosed', "main",true);
              }
              OneSignal.getUserId( function(userId) {
                  console.log("OneSignal User ID:", userId);
                  // Make a POST call to GA with the subscription data and userId aka playerId
                  // trackEvents('User','Subscription_Change', '',true);
                  // ga('send', 'event', 'Subscription_Change', isSubscribed, userId);
              });
          });

          OneSignal.on('popoverCancelClick', function(permissionChange) {
            trackEvents('User','PushSubscriptionClosed', "main",true);
            console.log('New permission state:', permissionChange);
          });
          OneSignal.on('popoverAllowClick', function(permissionChange) {
            trackEvents('User','PushSubscriptionDone', "main",true);
            console.log('New permission state:', permissionChange);
          });

          OneSignal.on('popoverShown', function(permissionChange) {
            trackEvents('User','PushSubscriptionPopupAppear', "main",true);
            console.log('New permission state:', permissionChange);
          });

      });


    </script>
    
