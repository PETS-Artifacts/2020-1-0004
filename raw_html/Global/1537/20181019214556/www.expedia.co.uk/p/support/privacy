<script>
	$LAB.queueWait(function(){
	    $('.skip-nav a[href^="#"]').on('click', function(e) {
	      e.preventDefault();
	      e.stopPropagation();
	      var focusId = e.currentTarget.getAttribute('href');
	      uitk.utils.focusElement($(focusId));
	    });
	});
</script>


