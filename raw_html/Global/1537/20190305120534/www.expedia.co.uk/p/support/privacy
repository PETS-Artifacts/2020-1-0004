<script>
  $LAB
.queueScript("/minify/page-bundle-min-3131403097.js?v=release-2019-03-r1-8957-2138907-1")
    .queueWait(function(){
      var calConfig = exp.core.date.CalendarControl.config = exp.core.date.CalendarControl.Config;

      calConfig.setFormatConfig('ddmmyy', '/');
      calConfig.setUnitConfig(1, ['January','February','March','April','May','June','July','August','September','October','November','December'], ['S','M','T','W','T','F','S']);
      calConfig.setTextConfig('Close');
      calConfig.setNightTextConfig(['one night','nights','1 night trip','# night trip']);

      uitk.calendarObj = exp.core.date.Calendar(calConfig);
      uitk.dateUpdater = exp.core.date.DateUpdater(uitk.calendarObj, calConfig);
      uitk.init();
      uitk.refreshPlaceholders();
    });
</script>

