<a id="toTop" href="#gmt" class="scroll-top">
	<div aria-hidden="true">^</div>
	<span class="visuallyhidden">Back to top</span>
</a>

<nav class="cf cols-row" style="text-align:right;"><fieldset><a class="topOfPage offscreen" href="#gmt">Back to top</a></fieldset></nav>

<script>
	require('jquery',function($){
		 $(window).scroll(function () {
	            var winScrollToTop = $(window).scrollTop(),
	           scrollLimit = $('.site-content-wrap').eq(0).offset(),
	           toTop = $('#toTop');
	           // back to top
	        if( winScrollToTop > scrollLimit.top ) {
	           toTop.css('display','inline-block');
	       } else {
	           toTop.css('display','none');
	       };
	    });
	});
</script>

