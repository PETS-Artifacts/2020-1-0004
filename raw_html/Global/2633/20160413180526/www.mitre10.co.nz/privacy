		<script src="/assets/js/jquery.validate.pack.js" type="text/javascript"></script>
        <script type="text/javascript">
        
		$(document).ready(function(){
            $("#regionID").change(function() { 
                //get what they selected
                var selected = $("option:selected",this).val();
                $("#branchID").children().remove().end().append("<option value=\"\">Select a store</option>");
                //now load in new options
                if(selected == "") return;
               		
			 	$.ajax({
					type: 'GET',
					url: '/module/getStore.cfc?method=getstores&returnformat=json', 
					data: {"regionID":selected, "typeID":0},
					datatype:"json",
					async:false,
					success: function(res){
						res = res.substring(2);
						//convert back to objects after getting rid of the //
						var obj = jQuery.parseJSON( res );
						var newoptions = "";
                    	for(var i=0; i<obj.length; i++) {
	                        //In our result, ID is what we will use for the value, and NAME for the label
                        	newoptions += "<option value=\"" + obj[i].BRANCHID + "\">" + obj[i].NAME + "</option>";
                    	}
					
                    	$("#branchID").children().end().append(newoptions);
					},
					error: function() {
						alert('data failure');
					}
            	});
	        });
        });
        </script>

    
	

