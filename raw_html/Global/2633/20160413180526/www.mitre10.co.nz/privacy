<script>$( "[data-after],[data-before],[data-append],[data-replace]" ).ajaxInclude();</script>
<script src="/assets/v2/js/vendor/classie.js"></script>
<script src="/assets/v2/js/vendor/mlpushmenu.js"></script>
<script src="/assets/v2/js/vendor/rtlmlpushmenu.js"></script>

<script src="/assets/js/jquery-migrate-1.2.1.min.js"></script>
