		<script async>
		  function checkCookie(){
		    var puStatus = getCookie('ytStatus');
		    if (puStatus == 'closed') {
		      document.getElementById('ytplayer').className = 'ythidden'; 
		      document.getElementById('ytplayer-controls').className = 'ythidden';
		    } 
		  }

		  function getCookie(cname) {
		    var name = cname + "=";
		    var decodedCookie = decodeURIComponent(document.cookie);
		    var ca = decodedCookie.split(';');
		    for(var i = 0; i < ca.length; i++) {
		        var c = ca[i];
		        while (c.charAt(0) == ' ') {
		            c = c.substring(1);
		        }
		        if (c.indexOf(name) == 0) {
		            return c.substring(name.length, c.length);
		        }
		    }
		    return "";
		  }
		
		  var tag = document.createElement('script');
		  tag.src = "https://www.youtube.com/iframe_api";
		  var firstScriptTag = document.getElementsByTagName('script')[0];
		  firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

		  var player;
		  function onYouTubePlayerAPIReady() {
		    player = new YT.Player('ytplayer', {
		      
		      
		      	height: '260',
		      	width: '465',
		      
		      playerVars: {
			      listType:'playlist',
			      list: 'UUdd6-08yhtqjjwFVpq_u1TA',
			      //list: 'UU_SHiiPV3xftyp77EP3MDZA',
			      enablejsapi: 1,
			      controls: 0,
			      autoplay: 1,
			      modestbranding: 1,
			  },
		      events: {
		        onReady: function(e) {
		        	e.target.mute();
		        	//updateTitle();
		        }
		      }
		    });
		    checkCookie();
		  }
		  
		  var t = document.getElementById('ytTitle');
		  var v = document.getElementById('volumeButton');
		  var x = document.getElementById('ytplayer');
		  var y = document.getElementById('playButton');

		  function onPlayerReady(event) {
		    event.target.playVideo();
		    console.log('<ytplayer> Video Playing');
		  }
		  
		  function updateTitle() {
			  t.innerHTML = player.getVideoUrl();
		  }
		  
		  function closeYoutube() {
    		player.stopVideo();
    		document.getElementById('ytplayer').className = 'ythidden'; 
    		document.getElementById('ytplayer-controls').className = 'ythidden';
		    setCookie('ytStatus', 'closed');
  		  }

		  function setCookie(cname, cvalue, exdays) {
			var d = new Date();
			d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
			var expires = "expires="+d.toUTCString();
			document.cookie = cname + "=" + cvalue + ";path=/";
		  } 


		  var done = false;
		  function onPlayerStateChange(event) {
		    if (event.data == YT.PlayerState.PLAYING && !done) {
		      setTimeout(stopVideo, 6000);
		      done = true;
		    }
		  }
		  function stopVideo() {
		    player.stopVideo();
		    console.log('<ytplayer> Video Stopped');
		  }
		  
		  function volumeUpDown() {
			if(player.isMuted()) {
				player.unMute();
				v.className = 'fas fa-volume-up';
				//v.setAttribute('muted', '0');
			} else {
				player.mute(0);
				v.className = 'fas fa-volume-off';
				//v.setAttribute('muted', '1');
			}
		  }
		  
		  function playPause() {
		    if (player.getPlayerState() == 1) {
		        y.className = 'fa fa fa-play';
		        player.pauseVideo();
		        console.log('<ytplayer> Video Paused');
		    } else {
		        y.className = 'fa fa fa-pause';
		        player.playVideo();
		        console.log('<ytplayer> Video Playing');
		    }
		  }
		</script>
		

		
