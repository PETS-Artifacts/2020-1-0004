function faAnalyticsPaywall(){
  if (s.prop26 == 'premium') {
    s.prop27 = (jQuery("div.paywall").length)?"paywall":"nopaywall";
    if (s.prop24.match(/anonymous user/) || s.eVar13.match(/anonymous user/)) {
      if (s.prop27 == 'nopaywall') {
        s.events = s.events + ',event29';
      }
      else {
        s.events = s.events + ',event30';
      }
    }
    else if (s.prop24.match(/Registered User/) || s.eVar13.match(/Registered User/)) {
      jQuery.cookie.raw = true;
      s.eVar5 = jQuery.cookie('fp_count');
      jQuery.cookie.raw = false;
      if (s.prop27 == 'paywall') {
        s.events = s.events + ',event33';
        s.eVar5 = 'max';
      }
      else if (s.eVar5 == '1') {
        s.events = s.events + ',event31';
      }
    }
    s.prop6 = 'D=v5';
  }
