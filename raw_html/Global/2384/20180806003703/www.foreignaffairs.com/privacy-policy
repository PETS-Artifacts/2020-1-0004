if (!s.campaign && location.hash.indexOf("#cid=") == 0) s.campaign = location.hash.substring(5);
if (!s.campaign) s.campaign = s.Util.getQueryParam("fb_ref");
if (!s.campaign && jQuery.cookie("cid")) {
  s.campaign = jQuery.cookie("cid");
  jQuery.removeCookie("cid");
}
referrer = document.referrer;
//Otherwise assign generic social media if referrer is twitter or facebook
if (!s.campaign && referrer) {
  domain = referrer.split("/")[2];
	if (domain && domain.search("twitter.com")>=0 || domain == "t.co") {
		s.campaign = "soc-tw-gen";
	}
	if(domain && domain.search("facebook.com")>=0) {
		s.campaign = "soc-fb-gen";
	}
}

