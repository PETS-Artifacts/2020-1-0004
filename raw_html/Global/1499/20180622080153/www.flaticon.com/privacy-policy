                            <script>
                                var search_type;
                                try {
                                    search_type = localStorage.getItem("search-type");
                                } catch (e) {
                                    search_type = 'icons';
                                }

                                if (search_type)
                                {
                                    document.querySelector(".search-holder .type-selector input[name=search-type][value='"+search_type+"']").checked = true;
                                    document.querySelector(".search-holder .type-selector .popover-button").innerHTML = search_type.charAt(0).toUpperCase() + search_type.slice(1);
                                }
                                if (window.location.pathname == '/packs' || window.location.pathname.search('icon-packs') != -1) {
                                    document.querySelector(".search-holder .type-selector .popover-button").innerHTML = "Packs";
                                    document.querySelector(".search-holder .type-selector input[name=search-type][value='packs']").checked = true;
                                }
                            </script>

