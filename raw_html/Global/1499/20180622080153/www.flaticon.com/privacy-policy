<script type="text/javascript">
    var show_footer_banner = true;

    document.addEventListener("DOMContentLoaded", function(){
        
        if (!show_footer_banner) return false;

        var bannerId = 'footer_banner_campaign';

        var banner_cookie = Cookie.get(bannerId);
        var today = new Date();
        var eventDayInit = new Date("2018-06-05");
        var eventDayEnd = new Date("2018-06-07");

        if ((today.getTime() >= eventDayInit.getTime()) && 
            (today.getTime() < eventDayEnd.getTime()) &&
            (typeof gr.auth.me!='undefined') && 
            !gr.auth.me.premium) {
            
                showFooterBanner(bannerId);
                return;
        }

        if (!banner_cookie.length)
        {
            document.body.classList.add("topbar-visible");
        }

        setTimeout(function(){
            document.body.classList.remove('topbar-visible');
        }, 10000);

    });


    function footerBannerClick(bannerId) {
        if (document.body.classList.contains('topbar-visible')) {
            document.body.classList.remove('topbar-visible');
        }
        Cookie.setH(bannerId, '1', 1);
    }

    function showFooterBanner(bannerId){
        var banner_cookie = Cookie.get(bannerId);
        if (!banner_cookie.length){
            document.body.classList.add("topbar-visible");
            document.getElementById(bannerId).classList.remove('hidden');
        }
    }

</script>
<!-- End Footer Banner -->

		
