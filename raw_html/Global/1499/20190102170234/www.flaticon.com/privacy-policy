                            </div>

                            <div class="pd-lv3 pd-y-none alignc">
                                <a id="button_banner_premium_campaign" href="https://www.flaticon.com/?utm_source=freepik&utm_medium=web&utm_campaign=welcome-successful-payment" 
                                    class="button button--md button--flat button--premium">
                                    Go Premium                                </a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <button class="recommendations__close btn"></button>
    </div>

</div>



<script type="text/javascript">


$(function() {
    BannerCampaign.init();
});

var BannerCampaign = {

    banner: null,
    eventLoad: null,
    eventOpen: null,
    eventClose: null,
    currentDate: new Date(),
    buttonClose: null,
    recommendationInterval: null,
    recommendationLink: null,
    startDate: null,
    endDate: null,
    expire: 1 * 60 * 60 * 1000,
    type: false,
    cookie_data: null,
    linkEs: null,
    linkEn: null,
    
    /**
    * Inicia la ejecucion de la campaña
    */
    init: function() {
        try {

            var cookie_gr_welcome = Cookie.get('gr_welcome');
            if (!cookie_gr_welcome) {
                BannerCampaign.selectCampaignsToShow('recommendations campaign');
            } else {
                BannerCampaign.cookie_data = JSON.parse(decodeURIComponent(cookie_gr_welcome));
                var isValidSite = ['flaticon','freepik'].indexOf(BannerCampaign.cookie_data.site)!=-1;
                var isValidDate = (new Date(BannerCampaign.cookie_data.date).getTime()) > (new Date(new Date().getTime() - (24*60*60*1000)));

                if (isValidSite && isValidDate) {
                    var id_banner = 'banner-cross-selling-'+BannerCampaign.cookie_data.site;
                    BannerCampaign.setCampaignToShow(id_banner);
                } else {
                    // TODO: Borro cookie
                    BannerCampaign.selectCampaignsToShow('recommendations campaign');
                }
            }
        } catch (err) {
            console.log('err',err);
            BannerCampaign.selectCampaignsToShow('recommendations campaign');
        }
    },

    /**
    * Obtenemos el diseño y mostramos la campaña
    */
    setCampaignToShow: function(id) {
        var banner = document.getElementById(id);

        BannerCampaign.setVars(banner);

        if (BannerCampaign.isActive(banner)) {
            BannerCampaign.banner = banner;
            BannerCampaign.showBanner(BannerCampaign.banner);
        }
    },

    /**
    * Buscamos entre varios diseños y seleccionamos la campaña
    */
    selectCampaignsToShow: function(selector_class) {
        var banners = Array.prototype.slice.call(document.getElementsByClassName(selector_class));
        var bannerVisible;
        banners.forEach(function(banner) {
            BannerCampaign.setVars(banner);
            if (BannerCampaign.isActive(banner)) {
                bannerVisible = banner;
                BannerCampaign.banner = banner;
                BannerCampaign.setCampaignToShow(banner.id);
                return;
            }
        });
        BannerCampaign.setVars(bannerVisible);
    },

    /**
    * Seteamos las variables necesarias para mostrar la campaña
    */
    setVars: function(banner) {

        if (!banner) return false;

        BannerCampaign.currentDate = new Date();
        BannerCampaign.buttonClose = banner.querySelector('.recommendations__close');
        BannerCampaign.recommendationInterval;
        BannerCampaign.recommendationLink = banner.querySelector('a');  

        BannerCampaign.linkEs = banner.getAttribute('linkes');
        BannerCampaign.linkEn = banner.getAttribute('linken');

        BannerCampaign.startDate = new Date(banner.getAttribute('data-start'));
        BannerCampaign.endDate = new Date(banner.getAttribute('data-end'));
        
        BannerCampaign.type = banner.getAttribute('data-type');
        
        /**
         * Events
         */
        BannerCampaign.eventLoad  = banner.dataset.onload;
        BannerCampaign.eventOpen  = banner.dataset.open;
        BannerCampaign.eventClose = banner.dataset.close;   

        
        setCountdown(banner, BannerCampaign.recommendationInterval, BannerCampaign.startDate, BannerCampaign.endDate, banner.dataset.countdownTotal);

        BannerCampaign.recommendationInterval = setInterval(function () {
            setCountdown(banner, BannerCampaign.recommendationInterval, BannerCampaign.startDate, BannerCampaign.endDate, banner.dataset.countdownTotal);
        }, 1000);

        BannerCampaign.recommendationLink.addEventListener('click', function () {
            ga.apply(null, BannerCampaign.eventOpen.split(','));
            //window.location.href = (LANGUAGE=='spanish') ? BannerCampaign.linkEs : BannerCampaign.linkEn;
        });

        if (BannerCampaign.type == 'cross-selling') {
            BannerCampaign._setVarsCrossSelling();
        } else {
            BannerCampaign._setVarsDefaultCampaigns();
        }
    },

    /**
    * Mostramos el banner
    */
    showBanner: function(banner) {
        ga.apply(null,  BannerCampaign.eventLoad.split(','));


        banner.style.display = 'block';
    },

    /**
    * Ocultamos el banner
    */
    hideBanner: function() {
        BannerCampaign.banner.style.display = 'none';
    },

    /**
    * Revisamos si la campaña es la que debe mostrarse
    */
    isActive: function(banner) {
        if (BannerCampaign.type == 'cross-selling') {
            return BannerCampaign._visibleCrossSellingCampaign(banner);
        } else {
            return BannerCampaign._visibleDefaultCampaign(banner);
        }        
    },

    _setVarsDefaultCampaigns: function() {
        if (BannerCampaign.buttonClose) {
            BannerCampaign.buttonClose.addEventListener('click', function() {
                BannerCampaign.hideBanner();

                Cookie.setH(BannerCampaign.banner.id, '1', {
                    expires: new Date(new Date().getTime() + (BannerCampaign.expire)).toGMTString(),
                    path: '/'
                });

                ga.apply(null, BannerCampaign.eventClose.split(','));
            });
        }
    },

    _setVarsCrossSelling: function() {
        if (BannerCampaign.cookie_data) {

            BannerCampaign.startDate = (BannerCampaign.cookie_data.date)?new Date(BannerCampaign.cookie_data.date):new Date();
            BannerCampaign.endDate = new Date(BannerCampaign.startDate.getTime() + (1000*60*60*24)); 

            if (BannerCampaign.buttonClose) {
                BannerCampaign.buttonClose.addEventListener('click', function() {
                    BannerCampaign.hideBanner(BannerCampaign.banner);

                    Cookie.setH(BannerCampaign.banner.id + '_count', '1', {
                        expires: new Date(new Date().getTime() + (1000*60*30)).toGMTString(),
                        path: '/'
                    });

                    ga.apply(null, BannerCampaign.eventClose.split(','));
                });
            }            
        }
    },

    _visibleCrossSellingCampaign: function(banner) {
        var condition = (Cookie.get('gr_welcome') && !Cookie.get(banner.id + '_count'));
        return condition;
    },

    _visibleDefaultCampaign: function(banner) {
        var condition = (( !Cookie.get(banner.id) &&
                    BannerCampaign.recommendationLink &&
                    (BannerCampaign.currentDate.getTime() >= BannerCampaign.startDate.getTime() && BannerCampaign.currentDate.getTime() <= BannerCampaign.endDate.getTime()) && 
                    (window.innerWidth >= 768) &&
                    ('undefined' !== typeof gr && !gr.auth.me.premium) &&
                    (BannerCampaign.eventLoad && BannerCampaign.eventOpen && BannerCampaign.eventClose)));
        return condition;
    },

    _setCookie: function() {
                Cookie.setH('gr_welcome', JSON.stringify({'site':'flaticon','date': new Date() }), {
                    expires: new Date(new Date().getTime() + (1000*60*60*24)).toGMTString(),
                    path: '/'
                });        
    }

}

</script>
	<div class="loader" id="loader" style="display: none;">
		<div class="circle-spinner"><img src="https://media.flaticon.com/img/loader-logo.svg" title="Flaticon logo"></div>
	</div>

</body>
</html>
