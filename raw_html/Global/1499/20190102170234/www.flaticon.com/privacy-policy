        data-type="cross-selling"
        >

        <div class="paragraph-readable push-center-i alignc">
            <div class="container">
                <div class="cross-selling-content">
                    <div class="container wrapper">
                        <div class="row row--vertical-center row--horizontal-center alignc">
                            <div class="percentage pd-lv2">
                                <p class="mg-none font-h2 bold text--inverted pd-lv3 pd-y-none">30% DTO</p>
