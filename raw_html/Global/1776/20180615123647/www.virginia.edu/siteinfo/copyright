	document.getElementById( "edit-search-block-form--2" ).oninput = function () {
		myFunction();
	}
	
	function myFunction() {
		var value = document.getElementById( "edit-search-block-form--2" ).value;
		var value2 = value.replace( /[&\/\\_\-|=+()$~%^'\[\]";*<>{}]/g, "" );
		document.getElementById( "edit-search-block-form--2" ).value = value2;
		CludoSearch.searchUrl = '/search/site/'+value2;
