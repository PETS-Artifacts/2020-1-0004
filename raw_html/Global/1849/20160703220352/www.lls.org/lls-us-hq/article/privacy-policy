<script type="text/javascript">
    var js = jQuery.noConflict();
    var clientPopupOpt;
    js(document).ready(function (js) {
        if (js(window).width() < 991) {
            js("body.home.html").css("margin-top", "44px");
        }
                initLlsPage();
                js(".search-terms").focus(function(){ advancedSearch();});
                js(".search-terms").blur(function(){ advancedSearch();});
                js(".search-terms").change(function(){ advancedSearch();});
                article_page_init();
                js(".title.print.pull-right.hidden-sm.hidden-xs a").attr("target", "");
        homeCloseMenu = function () {
            closeMenu();
            if (js(window).width() < 991 && js(window).width() > 767) {
                js("body.home.html").css("margin-top", '44px');
            }
            else {
            	var headerHeight = jQuery("body>header .module-navigation").outerHeight();
            	if(js("body").hasClass("node-type-blank-page"))
            		js("body.home.html").css("margin-top", headerHeight);
            	else
                	js("body.home.html").css("margin-top", '130px');
            }
            js("section.module.module-hero-slider .bx-viewport .slide").css("width", js(window).width() + 'px');

        };

        /*-------------GTM Custom Start--------------*/


        audio = js('body').find("audio").get(0);
        js('audio').bind('play pause ended', function(e){
            var int;
            //dataLayer.push({'eventCategory': 'Audio','eventAction': e.type,'eventLabel': js(this).children(":first").attr('src')});
            var Filename= js(this).children(":first").attr('src').split('/').pop();
            if(e.type == 'play'){
                int = window.setInterval(function() { updateProgress(Filename); }, 1000);
                dataLayer.push({'event': 'audioPlay', 'fileName':Filename, 'percentagePlayed': e.type});
            }
            else if(e.type == 'pause'){
                dataLayer.push({'event': 'audioPlay', 'fileName':Filename, 'percentagePlayed': e.type});
                //window.clearInterval(int);
            }
            else if(e.type == 'ended'){
                dataLayer.push({'event': 'audioPlay', 'fileName':Filename, 'percentagePlayed': "completed"});
                window.clearInterval(int);
            }
        });
        var isDownloadPdf = new RegExp('.pdf$');
        js('a').each(function(){
            var url = js(this).attr("href");
            var fileName = js(this).attr("alt");
            if(fileName == '' || fileName == 'undefined')
                fileName = url.split('/').pop();
            if (isDownloadPdf.test(url)){
                //js(this).attr("onclick","dataLayer.push({'event': 'Pdf Download','DownloadURL': '"+ url +"', 'fileName':'"+ url.split('/').pop() +"'});");
                js(this).attr("onclick","dataLayer.push({'event': 'Pdf Download','DownloadURL': '"+ url +"', 'fileName':'"+ fileName +"'});");
            }
            else { }

        }); //End each function

        /*-------------GTM Custom Stop--------------*/
        js(".article-content a").each(function(){
            if ((js(".article-content a").attr("name") != '') || (js(".article-content a").attr("name") != null)) {
                                js(".article-content a").addClass("jumptarget");
                js(".article-content li a").removeClass("jumptarget");
                
            }
        });
        if(!isEmptyOrNull(clientPopupOpt))
            js(".jumptarget").removeClass("cn");
        window.addEventListener("orientationchange", homeCloseMenu);
        closeMenu = function(){
            if (js(window).width() < 991) {
                var nav = js(".collapsed-nav"),
                    menuLink = js('ul.small-nav li.menu');
                nav.removeClass('bounceInDown').addClass('collapse');
                menuLink.removeClass('active');
                js('body').css('position', 'static');
            }
        };
    });
    function isEmptyOrNull(v)
    {
        if(v == null || v == "null" || v == "-1")
            return true;
        return false;
    }
    function updateProgress(Filename) {
        progress_percent = Math.ceil((audio.currentTime / audio.duration) * 100);
        if(progress_percent > 24 && progress_percent < 26 ){
            dataLayer.push({'event': 'audioPlay', 'fileName':Filename,'percentagePlayed': '25'});
        }
         else if(progress_percent > 49 && progress_percent < 51){
            dataLayer.push({'event': 'audioPlay', 'fileName':Filename,'percentagePlayed': '50'});
        }
         else if(progress_percent > 74 && progress_percent < 76){
            dataLayer.push({'event': 'audioPlay', 'fileName':Filename,'percentagePlayed': '75'});
        }
    }
</script>

