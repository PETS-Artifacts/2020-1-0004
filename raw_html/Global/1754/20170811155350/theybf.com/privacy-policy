  $(document).ready(function () {
    var results= document.cookie.match ( '(^|;) ?ybfEmailAalerts=([^;]*)(;|$)' );
    if (!results) {
      $('#overlay').show();
      $('#popupBox').show();
    }
  });

  function closePopup() {
    $('#overlay').hide();
    $('#popupBox').hide();
    
    var cookieDate = new Date(2030, 01, 01);
    document.cookie = "ybfEmailAalerts=1; path=/; expires=" + cookieDate.toGMTString();
  }
</script>
