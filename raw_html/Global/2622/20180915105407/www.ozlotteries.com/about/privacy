<script>
	(function() {

		function addFont() {
			var style = document.createElement('style');
			style.rel = 'stylesheet';
			style.id = 'font-loaded';
			document.head.appendChild(style);
			style.textContent = localStorage.dataFont;
		}

		if (localStorage.dataFont) {
			addFont();
		}
		else {
			var request = new XMLHttpRequest();
			var fontFile = '//media-jl-353936.c.cdn77.org/45d45bb5b4/fonts/pub/data-woff.css';

			request.open('GET', fontFile, true);

			request.onload = function() {
				if (request.status >= 200 && request.status < 400) {
					// Save to localStorage
					localStorage.dataFont = request.responseText;
					// Load font
					addFont();
				}
			};
			request.send();
		}
	}());
</script>

