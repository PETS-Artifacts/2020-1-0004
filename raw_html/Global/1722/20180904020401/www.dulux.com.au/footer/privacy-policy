<script>
  // Geo services
  var geo = localStorage.getItem('GeoLocation');

  if (geo) {
    geo = JSON.parse(geo);

    if (geo.latitude && geo.longitude) {
      digitalData.user.geoLat = geo.latitude;
      digitalData.user.geoLon = geo.longitude;

      if (geo.postcode) {
        digitalData.user.customerPostalCode = geo.postcode;
      }

      localStorage.removeItem('GeoLocation');
    }
  }

  // Logged in status
  var loggedIn = false;
  var path     = window.location.pathname;
  var slashes  = path.match(/\//g);

  if (slashes && slashes.length) {
    path = slashes > 1 ? path.substr(1, path.indexOf('/') + 1) : path.substr(1);
    path = path !== 'specifier' ? '' : path;
  }

  var cookies = document.cookie.split('; ').filter(function(cookie) {
    var expression = new RegExp('LoginStatus' + (path.length ? '_' + path : '') + '=({.*})');
    var matches    = cookie.match(expression);

    return matches && matches.length === 2;
  });

  if (cookies && cookies.length === 1) {
    var user = JSON.parse(decodeURIComponent(cookies[0].split('=')[1]));
    loggedIn = user.logged;
  }

  digitalData.user.loggedInStatus = loggedIn;
</script>


