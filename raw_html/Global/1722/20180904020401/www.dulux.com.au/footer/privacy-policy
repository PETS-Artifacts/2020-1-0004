  <script>
    var products = document.querySelectorAll('[data-sugprod]');
    var product;
    for (var i = 0; i < products.length; i++) {
      var name, nameArr, type, coat;
      product = products[i];
      name = product.getAttribute('data-sugprod');
      nameArr = name.split('-');
      globalConfig.favourite.contentTypes.calculator.suggestedProducts[nameArr[0]][nameArr[1]][nameArr[2]] = {
          id: product.getAttribute('data-id'),
          name: product.getAttribute('data-name'),
          link: product.getAttribute('data-link'),
          image: product.getAttribute('data-image')
      }
    }
  </script>

	

