    <article class="tertiary-landing">
        <section class="tertiary-header">
            <div class="background-image">
                <div class="background-fade"></div>
                
<div class="sfimageWrp">
	        
<img id="MainContent_HeaderImage_C002_ctl00_ctl00_imageItem" title="HEADER-SM-SEH-2016-1467" src="/images/default-source/patients-and-visitors-images/patient-and-visitor-headers/header-sm-seh-2016-1467.jpg?sfvrsn=2" alt="HEADER-SM-SEH-2016-1467" />


