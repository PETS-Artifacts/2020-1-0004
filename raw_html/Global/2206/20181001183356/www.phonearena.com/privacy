	function refreshAdUnitsBid() {
 pbjs.que.push(function() {
 let adUnitCodes = ['div-gpt-ad-1522750965466-0', 'div-gpt-ad-1522750719631-0', 'div-gpt-ad-1522750998581-0', 'div-gpt-ad-1522751046357-0', 'div-gpt-ad-1522751095299-0', 'div-gpt-ad-1523952123156-0', 'div-gpt-ad-1522750796568-0', 'div-gpt-ad-1522750868231-0', 'div-gpt-ad-1522750915662-0', 'div-gpt-ad-1523951950246-0'];

 pbjs.requestBids({
 timeout: TIMEOUT,
 adUnitCodes: adUnitCodes,
 bidsBackHandler: function() {
 pbjs.setTargetingForGPTAsync(adUnitCodes);

 let refreshSlots = [],
 adUnitCodesLength = adUnitCodes.length;
 for (i = 0; i < adUnitCodesLength.length; i++)

 { refreshSlots[i] = slots[adUnitCodes[i]]; }
 googletag.pubads().refresh(refreshSlots);
 }
 });
 });
	}

