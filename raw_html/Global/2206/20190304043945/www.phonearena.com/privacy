  function GA_track_view(trackerId, userType, isMobileDevice, isMobileSite) {
    trackerId = trackerId || '';
    userType = (userType == 'guest') ? userType : 'member';
    isMobileDevice = isMobileDevice || 'No';
    isMobileSite = isMobileSite || 'No';

    ga('create', trackerId);
    ga('require', 'GTM-PKFQT4P');
    ga('set', 'User Type', userType);
    ga('set', 'Mobile Device', isMobileDevice);
    ga('set', 'Mobile Site', isMobileSite);
    ga('create', 'UA-134906-1', { cookieDomain: 'phonearena.com', 'allowAnchor': true });
    ga('send', 'pageview', { 'page': location.pathname + location.search + location.hash});
  }
</script>

