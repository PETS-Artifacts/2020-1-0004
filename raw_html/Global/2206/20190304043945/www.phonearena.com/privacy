                        timeout: TIMEOUT,
                        adUnitCodes: ["div-gpt-ad-1544018074218-0"],
                        bidsBackHandler: function() {
                            console.log("RefreshBids.bidsBackHandler", slot3x6_1), pbjs.setTargetingForGPTAsync(["div-gpt-ad-1544018074218-0"]), console.log("Refresh Ad slot3x6_1"), googletag.pubads().refresh([slot3x6_1])
                        }
                    })
                })
            }
        })
    } catch (n) {
        console.log("slot3x6_1 not on page")
    }	
