	<script>
	function fetchHeaderBids(apstagSlots, adUnits, TIMEOUT) {
	var bidders = ['a9', 'prebid'];

    var requestManager = {
        adserverRequestSent: false,
        apstagSlots: apstagSlots,
        adUnits: adUnits
    };

    bidders.forEach(function(bidder) {
        requestManager[bidder] = false;
    })

    function allBiddersBack() {
        var allBiddersBack = bidders
            .map(function(bidder) {
                return requestManager[bidder];
            })
            .filter(Boolean)
            .length === bidders.length;
        return allBiddersBack;
    }

    function headerBidderBack(bidder) {
        if (requestManager.adserverRequestSent === true) {
            return;
        }
        if (bidder === 'a9') {
            apstag.setDisplayBids();
            requestManager.a9 = true;
        } else if (bidder === 'prebid') {
            requestManager.prebid = true;
        }

        if (allBiddersBack()) {
            sendAdserverRequest();
        }
    }

    function sendAdserverRequest() {

        if (requestManager.adserverRequestSent === true) {
            return;
        }

        requestManager.adserverRequestSent = true;

         googletag.cmd.push(function() {
            apstag.setDisplayBids();
            pbjs.que.push(function() {
            pbjs.setTargetingForGPTAsync();
            googletag.pubads().refresh();
 });
        });
    }

    function requestBids(apstagSlots, adUnits, bidTimeout) {
        apstag.fetchBids({
            slots: apstagSlots,
            timeout: bidTimeout
        }, function(bids) {
            headerBidderBack('a9');
        });
        
        pbjs.que.push(function() {
                pbjs.addAdUnits(adUnits);
                 pbjs.setConfig({
  priceGranularity: customConfigObject,  
  maxRequestsPerOrigin: 1,	 	 
  bidderTimeout: 1500,
  timeoutBuffer: 400,
  bidderSequence: "random",
	userSync: {
 filterSettings: {
 iframe: {
 bidders: '*',
 filter: 'include'
 }
 },
	syncsPerBidder: 50,
	syncDelay: 4000
	}
  });
                pbjs.requestBids({
                    bidsBackHandler: function (bidResponses) {
                        headerBidderBack('prebid');
                    }
                });
        });
}

requestBids(apstagSlots, adUnits, TIMEOUT)
    window.setTimeout(function() {
    sendAdserverRequest();
}, TIMEOUT);
};

fetchHeaderBids(apstagSlots, adUnits, TIMEOUT);

var $jscomp = $jscomp || {};
$jscomp.scope = {}, $jscomp.ASSUME_ES5 = !1, $jscomp.ASSUME_NO_NATIVE_MAP = !1, $jscomp.ASSUME_NO_NATIVE_SET = !1, $jscomp.defineProperty = $jscomp.ASSUME_ES5 || "function" == typeof Object.defineProperties ? Object.defineProperty : function(d, n, o) {
    d != Array.prototype && d != Object.prototype && (d[n] = o.value)
}, $jscomp.getGlobal = function(d) {
    return "undefined" != typeof window && window === d ? d : "undefined" != typeof global && null != global ? global : d
}, $jscomp.global = $jscomp.getGlobal(this), $jscomp.SYMBOL_PREFIX = "jscomp_symbol_", $jscomp.initSymbol = function() {
    $jscomp.initSymbol = function() {}, $jscomp.global.Symbol || ($jscomp.global.Symbol = $jscomp.Symbol)
}, $jscomp.Symbol = function() {
    var d = 0;
    return function(n) {
        return $jscomp.SYMBOL_PREFIX + (n || "") + d++
    }
}(), $jscomp.initSymbolIterator = function() {
    $jscomp.initSymbol();
    var d = $jscomp.global.Symbol.iterator;
    d || (d = $jscomp.global.Symbol.iterator = $jscomp.global.Symbol("iterator")), "function" != typeof Array.prototype[d] && $jscomp.defineProperty(Array.prototype, d, {
        configurable: !0,
        writable: !0,
        value: function() {
            return $jscomp.arrayIterator(this)
        }
    }), $jscomp.initSymbolIterator = function() {}
}, $jscomp.initSymbolAsyncIterator = function() {
    $jscomp.initSymbol();
    var d = $jscomp.global.Symbol.asyncIterator;
    d || (d = $jscomp.global.Symbol.asyncIterator = $jscomp.global.Symbol("asyncIterator")), $jscomp.initSymbolAsyncIterator = function() {}
}, $jscomp.arrayIterator = function(d) {
    var n = 0;
    return $jscomp.iteratorPrototype(function() {
        return n < d.length ? {
            done: !1,
            value: d[n++]
        } : {
            done: !0
        }
    })
}, $jscomp.iteratorPrototype = function(d) {
    return $jscomp.initSymbolIterator(), d = {
        next: d
    }, d[$jscomp.global.Symbol.iterator] = function() {
        return this
    }, d
}, $jscomp.makeIterator = function(d) {
    $jscomp.initSymbolIterator(), $jscomp.initSymbol(), $jscomp.initSymbolIterator();
    var n = d[Symbol.iterator];
    return n ? n.call(d) : $jscomp.arrayIterator(d)
}, $jscomp.arrayFromIterator = function(d) {
    for (var n, o = []; !(n = d.next()).done;) o.push(n.value);
    return o
}, $jscomp.arrayFromIterable = function(d) {
    return d instanceof Array ? d : $jscomp.arrayFromIterator($jscomp.makeIterator(d))
};
var AutoReload = function() {
    this.slots = []
};
AutoReload.prototype.addSlot = function(d) {
    return d = new Slot(d), this.slots.push(d), d
}, AutoReload.prototype.isTabActive = function() {
    return "visible" === document.visibilityState
}, AutoReload.prototype.enableService = function() {
    var d = this;
    setInterval(function() {
        return !!d.isTabActive() && void d.slots.forEach(function(n) {
            n.handleReloading()
        })
    }, 1E3)
};
var Slot = function(d) {
    var n = d;
    d = n.id;
    var o = n.reload;
    n = n.options, this.id = d, this.node = document.getElementById(d), this.reload = o, this.setOptions(n), this.viewableSeconds = this.visibleSeconds = 0, this.ads = [], this.addAd(this.node)
};
Slot.prototype.addAd = function(d) {
    d = new Ad(d), this.ads.push(d)
}, Slot.prototype.reloadAd = function() {
    this.ads[this.ads.length - 1].removeListeners(), this.viewableSeconds = this.visibleSeconds = 0, this.reload(), this.addAd(this.node)
}, Slot.prototype.setOptions = function(d) {
    this.options = Object.assign({}, {
        minVisibility: .50,
        minVisibleSeconds: 1,
        reloadSeconds: 45
    }, d), this.options.minVisibility *= this.node.clientHeight
}, Slot.prototype.handleReloading = function() {
    this.isVisible() ? this.visibleSeconds++ : this.visibleSeconds = 0, this.isViewable() && (this.viewableSeconds++, this.shouldReload() && this.reloadAd())
}, Slot.prototype.isVisible = function() {
    var d = this.node.getBoundingClientRect();
    return isRectVisible(d, {
        top: 0,
        bottom: window.innerHeight
    }, this.options.minVisibility)
}, Slot.prototype.isViewable = function() {
    return this.visibleSeconds >= this.options.minVisibleSeconds
}, Slot.prototype.shouldReload = function() {
    return this.viewableSeconds >= this.options.reloadSeconds
};
var Ad = function(d) {
    this.id = Date.now(), this.node = d, this.meta = {
        click: 0,
        hover: 0
    }, this.registerListeners()
};
Ad.prototype.countHover = function() {
    this.meta.hover++
}, Ad.prototype.countClick = function() {
    this.meta.click++
}, Ad.prototype.registerListeners = function() {
    this.node.addEventListener("mouseover", this.countHover.bind(this)), this.node.addEventListener("click", this.countClick.bind(this))
}, Ad.prototype.removeListeners = function() {
    this.node.removeEventListener("mouseover", this.countHover), this.node.removeEventListener("click", this.countClick)
};
var isRectVisible = function(d, n, o) {
        return d.top >= n.top && d.bottom <= n.bottom || d.top < n.top && d.bottom >= o || d.bottom > n.bottom && n.bottom - d.top >= o
    },
    autoReload = new AutoReload;
document.addEventListener("DOMContentLoaded", function() {
    try {
        autoReload.addSlot({
            id: "div-gpt-ad-1544018074218-0",
            reload: function() {
                console.log("Refresh Bids Initialized for slot3x6_1"), pbjs.que.push(function() {
