  <script type="text/javascript">
    $(document).ready(function() {

      propertytype_dropdown = BuildTypeSelection('Property Types', '#property_type .dropdown-menu', '#property_type .dropdown');
      feature_dropdown = BuildTypeSelection('Features', '#property_features .dropdown-menu', '#property_features .dropdown');

      $('#property_type .dropdown-menu input:checkbox').click(function(e) {
        propertytype_dropdown.updateState($(this).val());
        e.stopPropagation();
      });
      $('#property_type .dropdown-menu label').click(function(e) {
        propertytype_dropdown.updateState($(this).val());
        e.stopPropagation();
      });

      $('#property_features .dropdown-menu input:checkbox').click(function(e) {
        feature_dropdown.updateState($(this).val());
        e.stopPropagation();
      });
      $('#property_features .dropdown-menu label').click(function(e) {
        feature_dropdown.updateState($(this).val());
        e.stopPropagation();
      });

      AddAutocomplete($('#refine_search input[name=q]'), true);
      feature_dropdown.updateState('');
      propertytype_dropdown.updateState('all');
    });
  </script>

