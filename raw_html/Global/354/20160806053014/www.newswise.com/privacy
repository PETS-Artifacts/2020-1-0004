        <script>

                                            jQuery(document).ready(function ($) {
                                                $('#menu').slicknav();
                                                $('.slicknav_menu ul li > a').each(function () {
                                                    if ($(this).parent().find('.sub-menu').length !== 0) {

                                                        $(this).append('<span class="mobilemenuopen">+</span>');
                                                    }

                                                });
                                                var click1 = 1;
                                                $('.slicknav_menu ul li > a > span').click(function () {
                                                    if ($(this).parent().parent().find('.sub-menu').length !== 0) {
                                                        $(this).parent().parent().find('.sub-menu').slideToggle();
                                                        if ($(this).hasClass('menu_open')) {
                                                            $(this).removeClass('menu_open');
                                                            $(this).html("+");
                                                        } else {

                                                            $(this).addClass('menu_open');
                                                            $(this).html("-");
                                                        }
                                                        return false;
                                                    }
                                                });

                                                $('.filter_image').click(function () {
                                                    $('#search-sorts').slideToggle();

                                                });

                                            });
        </script>


