        <script>
                                            jQuery(document).ready(function ($) {

                                                // scrolling-to-fixed sidebar menu
                                                var nav = $('.sidebar-float');
                                                if (/Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(navigator.userAgent)) {
                                                    // don't float sidebar on mobile OS (covers content)
                                                } else {
                                                    $(window).scroll(function () {
                                                        if ($(this).scrollTop() > 55) {
                                                            nav.addClass("fixed-sidebar");
                                                        } else {
                                                            nav.removeClass("fixed-sidebar");
                                                        }
                                                    });
                                                }
                                            });
        </script>
        
