  var google_tag_params = {
    ecomm_prodid: getProductCode,
    ecomm_pagetype: getPageType,
    ecomm_totalvalue: getTotalValue,
    ecomm_category: getCategory
  };
  function getProductCode() {
    if(gaProductCode != null) {
      return gaProductCode;
    } else {
      return "";
    }
  }
  function getPageType() {
    if(gaPageType != null) {
      return gaPageType;
    } else {
      return "";
    }
  }
