  /*  Rich Auto-Complete Tags */

  $autocomplete.on('click', '.ra-product-name', function() {
    var raLinkText = $(this).text();
    var searchInputTerm = $searchInput.val();
    var eventLabel = searchInputTerm + ' : ' + raLinkText;
    ga('send', 'event', 'Search', 'RA - Product Click', raLinkText);
    ga('send', 'event', 'RA - Search', 'Product Click', eventLabel);
  });
  $autocomplete.on('click', '.ra-facet-value', function() {
    var raLinkText = $(this).text();
    var raLinkTextWithoutProductCount =  raLinkText.substring( 0, (raLinkText.indexOf( "(" ) - 1));
    var searchInputTerm = $searchInput.val();
    var eventLabel = searchInputTerm + ' : ' + raLinkTextWithoutProductCount;
    var raFacetType = $(this).closest('.ra-facet-result').find('p').text();
    ga('send', 'event', 'RA - Search', 'RA - ' + raFacetType + ' Click', eventLabel);
    ga('send', 'event', 'Search', 'RA - ' + raFacetType + ' Click', raLinkTextWithoutProductCount);
  });
  $autocomplete.on('click', '.ra-suggestion', function() {
    var raLinkText = $(this).text();
    var searchInputTerm = $searchInput.val();
    var eventLabel = searchInputTerm + ' : ' + raLinkText;
    ga('send', 'event', 'Search', 'RA - Suggestion Click', raLinkText);
    ga('send', 'event', 'RA - Search', 'Suggestion Click', eventLabel);
  });

  /* END Rich Auto-Complete Tags */

  /* GA Search Report Tags */
  $(document).ready(function() {
    var searchTerm = $('#ga-search-report').data('search-term');
    var report = $('#ga-search-report').data('search-report');

    if(searchTerm) {
      $.each(report, function(term, fields) {
        $.each(fields, function(index, value) {
          ga('send', 'event', 'Search Report', term, value);
        })
      });
    }
  });
  /* End GA Search Report Tags */

  /* GA Product Impressions */
  var impressionList = [];
  var products = $('#products-container').find('.product-card');
  var urlPath = window.location.pathname;

  function isScrolledIntoView(element) {
    var docViewTop = $(window).scrollTop();
    var docViewBottom = docViewTop + $(window).height();
    var elementTop = $(element).offset().top;
    var elementBottom = elementTop + $(element).height();

    return ((elementBottom <= docViewBottom) && (elementTop >= docViewTop));
  }

  function checkForImpression() {
    products.each(function(index, value){

      var productCode = $(value).find('[data-productcode]').attr('data-productcode');
      var productName = $(value).find('.product-name').text();
      if(isScrolledIntoView(value) && impressionList.indexOf(productCode) === -1) {
        impressionList.push(productCode);
        sendProductImpressionToGA(productName, productCode, urlPath, (index + 1));
      }
    });
  }

  function sendProductImpressionToGA(productName, productCode, urlPath, position) {
    ga('ec:addImpression', {              // Provide product details in an impressionFieldObject.
      'id': productCode,                  // Product ID (string).
      'name': productName,                // Product name (string).
      //'category': 'Apparel/T-Shirts',   // Product category (string).
      //'brand': 'Google',                // Product brand (string).
      //'variant': 'Black',               // Product variant (string).
      'list': urlPath,                    // Product list (string).
      'position': position                // Product position (number).
      //'dimension1': 'Member'            // Custom dimension (string).
    });
  }

  function onProductClick(e) {
    if ($(e.target).hasClass('quick-view')) {
      return;
    }
    var productCode = $(this).find('[data-productcode]').attr('data-productcode');
    var productName = $(this).find('.product-name').text();
    var position = impressionList.indexOf(productCode) + 1;
    ga('ec:addProduct', {                 // Provide product details in a productFieldObject.
      'id': productCode,                  // Product ID (string).
      'name': productName,                // Product name (string).
      //'category': 'Apparel',            // Product category (string).
      //'brand': 'Google',                // Product brand (string).
      //'variant': 'Black',               // Product variant (string).
      'position': position                // Product position (number).
      //'dimension1': 'Member'            // Custom dimension (string).
    });
    ga('ec:setAction', 'click', {         // click action.
      list: urlPath                       // Product list (string).
    });
    ga('send', 'event', 'Product Card Clicked', 'click', urlPath);
  }
  $(products).on('click', onProductClick);
  $(window).on('load scroll', checkForImpression);
  $(window).on('beforeunload', function(){
    if ($(products).length > 0){
      ga('send', 'event', 'Product Impression', 'impression', {nonInteraction: true});
    }
  });
  /* END GA Product Impressions */

  /* GA Quick Shop Tags */
  setTimeout(function() {
    if ($('#quickViewModal').length > 0) {
      $('.quick-view').on('click', function() {

        var productCode = $(this).closest('.product-card').attr('data-productcode');
        ga('send', 'event', 'Quick Shop', 'Viewed Modal', productCode);
      });

      $(document).on('shown.bs.modal', '#quickViewModal', function () {
        setTimeout( function() {
          var quickShopAddToBagButton = $('#quickViewModal').find('#add-to-bag-button');
          var quickShopViewItemDetailsButton = $('#quickViewModal').find('.view-details-button').find('a');
          var productCode = $('#quickViewModal').find('.productCode').attr('data-product-code');
          quickShopAddToBagButton.on('click', function() {
            ga('send', 'event', 'Quick Shop', 'Add to Bag Clicked', productCode);
          });
          quickShopViewItemDetailsButton.on('click', function() {
            ga('send', 'event', 'Quick Shop', 'View More Details Clicked', productCode);
          });
        }, 250);
      });
    }
  }, 250);
  /* End GA Quick Shop Tags */

  /* GA Pageview Tag - Needs to be last GA tag on page */
  if(typeof(searchTerm) != 'undefined' && searchTerm != null) {
    ga('send', 'pageview', '/search?q=' + searchTerm);
  } else { ga('send', 'pageview'); }
  /* End GA Pageview Tag*/

  // ]]-->
</script>
