    <div data-quickedit-entity-id="block_content/4" id="block-adaa-block-2" class="contextual-region block block-block-content block-block-contentca810f8d-5422-4776-8a00-f76e1d5c5c0d">
  
    <div data-contextual-id="block:block=adaa_block_2:langcode=en|block_content:block_content=4:changed=1452660506&amp;ds_bundle=basic&amp;ds_view_mode=default&amp;langcode=en"></div>
      <div class="content">
      
            <div data-quickedit-field-id="block_content/4/body/en/full" class="clearfix text-formatted field field--name-body field--type-text-with-summary field--label-hidden field__item"></div>
      
