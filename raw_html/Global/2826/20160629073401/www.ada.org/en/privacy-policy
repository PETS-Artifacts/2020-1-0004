            <div class="addthis_toolbox custom_toolbox">
			    

	            <script type="text/javascript">
	                function AddToFavorites(addfav) {
	                    //var onOff = $('#addToFavoritesLink').attr('class') == "addfav";
	                    var onOff = addfav;
	                    $.ajax({
	                        type: "POST",
	                        url: "/webservice/ADA/Favorites/UserFavoritesService.asmx/AddRemoveFavorite",
	                        data: "{item:'{EA18ACCD-6B5A-41EE-A337-5A6DDCB467D0}', onOff:'" + onOff + "'}",
	                        contentType: "application/json; charset=utf-8",
	                        dataType: "json",
	                        success: function (response) {
	                            toggleFavStatus(response['d']);
	                        }
	                    });
	                    return false;
	                }

	                function FavoriteListAdd(addfav, itemId, delClass) {

	                    var onOff = addfav;

	                    $.ajax({
	                        type: "POST",
	                        url: "/webservice/ADA/Favorites/UserFavoritesService.asmx/AddRemoveFavorite",
	                        data: "{item:'" + itemId + "', onOff:'" + onOff + "'}",
	                        contentType: "application/json; charset=utf-8",
	                        dataType: "json",
	                        success: function (response) {
	                            //  window.location.reload();
	                            if (delClass != "") {
	                                $("." + delClass).closest("li").hide();
	                            }
	                        }
	                    });
	                    return false;

	                }

	                function toggleFavStatus(onOff) {
	                    var addToFavoritesLink = $('#addToFavoritesLink');
	                    if (onOff) {
	                        //   addToFavoritesLink.attr('class', 'fav');
	                        addToFavoritesLink.text('Remove Favorite');
	                        addToFavoritesLink.attr('title', 'Remove Favorite');
	                        addToFavoritesLink.attr('onclick', 'AddToFavorites(false);');
	                        addToFavoritesLink.addClass('added');
	                    } else {
	                    //    addToFavoritesLink.attr('class', 'addfav');
	                        addToFavoritesLink.text('Add to Favorites');
	                        addToFavoritesLink.attr('title', 'Add to Favorites');
	                        addToFavoritesLink.attr('onclick', 'AddToFavorites(true);');
	                        addToFavoritesLink.removeClass('added');
	                    }
	                }
	            </script>

