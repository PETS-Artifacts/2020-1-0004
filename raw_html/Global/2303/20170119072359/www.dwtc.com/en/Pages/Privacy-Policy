    function addURL(element) {

        //$(element).attr('href', function () {
        var searchtext = $('#txtSiteSearch').val();
        var link = $(element).data("link");
        if (searchtext.length != 0) {
            var redirectTo = link + '?k=' + encodeURIComponent(searchtext);
            window.location.href = redirectTo;
        }

        // });
    }

    $(document).ready(function () {

        var langSwitcherId = 'ctl00_PageHeaderPlaceHolder_ctl00_HeaderTopSection_lnkLang';
        var lcid = _spPageContextInfo.currentLanguage;

        if (lcid == 1033) {
            var arabicSVGText = '<svg viewBox="0 0 30 11" width="30" height="11" class="lang l-icon">'
                                +    '<use xmlns:xlink="http://www.w3.org/1999/xlink" xlink:href="/_layouts/15/DWTC.COM/css/images/icons-svg.svg#icon_arabic"></use>'
                               + '</svg>';
            $("#" + langSwitcherId).html(arabicSVGText);
            $("#" + langSwitcherId).attr("title", "Arabic");
        }
        else
        {
            $("#" + langSwitcherId).html('English');
            $("#" + langSwitcherId).attr("title", "الأنكليزي");
        }
       

        $("#site_search").click(function () {
            $("#txtSiteSearch").focus();
        });

        $('#txtSiteSearch').keypress(function (e) {
            if (e.which == 13) {
                var searchtext = $('#txtSiteSearch').val();
                if (searchtext != "") {
                    var redirectTo = $('#lnkSearch').data("link") + '?k=' + encodeURIComponent(searchtext);
                    window.location.href = redirectTo;
                }
            }
        })
    });
</script>


