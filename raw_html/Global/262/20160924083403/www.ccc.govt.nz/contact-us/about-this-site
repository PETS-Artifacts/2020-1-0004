		<style>
@media (max-width: 767px) {
    /* Start Emergency Mobile */
    .alert-banner-wrapper  .alert-vertical-divider {
         border-left :0;
		 padding :0;
		 margin :0;
    }
    
	.alert-banner-wrapper .alert-title, .alert-nav-section, .alert-summary-section {
         padding :0;
		 margin :0;
    }
	
	.alert-banner-wrapper .alert-nav-section .alert-nav li {
		  display: block;
	}
	
	.alert-banner-wrapper .alert ul.alert-nav li {
	  padding-left: 0;
	  padding-right: 0;
	  padding-top:8px;
	}
	
	.alert-banner-wrapper .alert-summary-section{
		padding-top:8px;
	}
	
	.alert-banner-wrapper span.alert-label {
	    display: inline-block;
	}
    
    /********End Emeregency*************/
}
</style>
<div class="alert-banner-wrapper">
	<ul class="alert-banner">
	
	</ul>
</div>

<script type="text/javascript" src="/assets/_combinedfiles/cwp-ccc-default/scripts.js?m=1474538796"></script><script type="text/javascript">
	$(document).ready(function () {
	 	// adds toggle button to the 1st alert item.
		$('ul.alert-banner li:lt(1)').find('.alert-toggle')
			.html("<button class='arrow' data-toggle='collapse'><span class='glyphicon glyphicon-chevron-down' aria-hidden='true'></span></button>");
		
		// binds the button onclick event.
		$(".arrow").click(function () {
			
			var buttonIcon = $(this).find('span');
			var alerts = $('ul.alert-banner li');
			var firstAlert = $('ul.alert-banner li:lt(1)');
			
			if(buttonIcon.hasClass("glyphicon-chevron-down")){
				
				buttonIcon.removeClass("glyphicon-chevron-down").addClass("glyphicon-chevron-up");
				
				// Display all listitem
				alerts.show();
				
				firstAlert.find('.collapse-alert').addClass('in');
				firstAlert.css('border-bottom','1px solid #ffffff');
			} 
			else {
				buttonIcon.removeClass("glyphicon-chevron-up").addClass("glyphicon-chevron-down");
				
				// shows the most severe alert only
				alerts.hide();
				firstAlert.show();  
				
				firstAlert.find('.collapse-alert').removeClass('in');
				firstAlert.css('border-bottom','none');
			}	
		});
	});
</script>
