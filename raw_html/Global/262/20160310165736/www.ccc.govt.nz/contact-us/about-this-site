<script type="text/javascript" src="http://www.ccc.govt.nz/themes/ccc-default/js/navigation.js"></script>

	<script>
		var _gaq = _gaq || [];
		_gaq.push(['_setAccount', 'UA-2061991-40']);
		_gaq.push(['_trackPageview']);

		(function() {
			var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
			ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
			var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
		})();
	</script>



<script type="text/javascript" src="http://www.ccc.govt.nz/themes/ccc-default/js/iframeResizer.min.js"></script>
<script>
    
	// Create the link you want to click on that makes something appear. Give it the class "showSingle" and an id # (e.g. 1 here):
	//<a href="/contact-us/about-this-site#" class="showSingle" id="1">Phone Christchurch City Council</a>
	//For the object appearing, give it class "targetDiv" and the id "div#", where # is the id above, e.g. 1 here:
	//<div id="div1" class="targetDiv">Phone Christchurch City Council</div>
	//The code below will close all elements with class "targetDiv" and open the div# that was clicked on.
	$(window).load(function(){
		jQuery(function(){
		         jQuery('#showall').click(function(){
		               jQuery('.targetDiv').show();
		        });
		        jQuery('.showSingle').click(function(){
					  jQuery('.targetDiv').not('#div'+$(this).attr('id')).hide();
					  jQuery('#div'+$(this).attr('id')).slideToggle('600');
					  jQuery(this).toggleClass('active');
					  //alert('#div'+$(this).attr('id')+' should have appeared');
		        });
		});
	});
	
	(function($) {
	    $(document).ready(function(){
	     $('#main-menu > li').has('ul').hover(function(){
      		$(this).addClass('showBottomBorder');
		    	}, function(){
		   		$(this).removeClass('showBottomBorder');
			});

	     $('#main-menu > li').hover(function(){
        	$('#main-menu').find('.current').removeClass('active');
        		}, function(){
	        	$('#main-menu').find('.current').addClass('active');
        	});
	   });
	})(jQuery);

    iFrameResize({
        log: false,
        enablePublicMethods: true,
        checkOrigin: false,
		autoResize: true,
		scrolling: false,
		minHeight: 150
    });

    jQuery.noConflict(true);

    
    $(".slidesearch").click(function(){
        if ( $('.search-wrapper').is(':visible') == false ) {
			$('.slidesearch-wrap').addClass('slidesearch-wrap-grey');
		} else {
            $('.slidesearch-wrap').removeClass('slidesearch-wrap-grey');
		}
		$( ".search-wrapper" ).slideToggle( "fast", function() {
			$('#SearchForm_SearchForm_Search').focus();
		});
    });
	$("#clear-search-mobile").click(function(){
        	$('#SearchForm_SearchForm_Search').val('');
			$('#SearchForm_SearchForm_Search').focus();
    });
	$(".section-content-wrapper").hide();
	$('.section-header-wrapper div.clickable').on("click", function (e) {
		if ($(this).hasClass('section-collapsed')) {
			$(this).parent('.section-header-wrapper').find('.section-content-wrapper').slideUp();
			$(this).removeClass('section-collapsed');
			$(this).find('i').removeClass('glyphicon-menu-up').addClass('glyphicon-menu-down');
		}
		else {
			$(this).parent('.section-header-wrapper').find('.section-content-wrapper').slideDown();
			$(this).addClass('section-collapsed');
			$(this).find('i').removeClass('glyphicon-menu-down').addClass('glyphicon-menu-up');
		}
	});
    
    $(document).ready(function () {
        $('.footer-accordian').click(function () {
            if ( $('.popular-links-list').is(':visible') == false ) {
                $('.footer-accordian > div> ul > div').slideUp(300);
                $('.footer-accordian').addClass('footer-accordian-on');
                $('#footer-accordian-arrow').addClass('fa-chevron-up').removeClass('fa-chevron-down');
            } else {
                $('.footer-accordian').removeClass('footer-accordian-on');
                $('#footer-accordian-arrow').addClass('fa-chevron-down').removeClass('fa-chevron-up');
            }
            $('.popular-links-list').slideToggle(300);
        });
        
    });
	$(function() {
		var byRow = $('body').hasClass('Page');
		$('.image_gallery').each(function() {
			$(this).find('.gallery-holder').matchHeight({
				byRow: byRow
			});
		});
	});
    
    document.documentElement.className = 'js'; // adds class="js" to <html> element
    function uncheckboxes(nav){
        var navarray = document.getElementsByName(nav);
        for(var i = 0; i < navarray.length; i++){
            navarray[i].checked = false
        }
    }
</script>
<script>
	
	function hoverTouchUnstick() {
	  if (('ontouchstart' in window) || window.DocumentTouch && document instanceof DocumentTouch) {
	    for(var sheetI = document.styleSheets.length - 1; sheetI >= 0; sheetI--) {
	      var sheet = document.styleSheets[sheetI];
	      if(sheet.cssRules) {
	        for(var ruleI = sheet.cssRules.length - 1; ruleI >= 0; ruleI--) {
	          var rule = sheet.cssRules[ruleI];
	          if(rule.selectorText) {
	            rule.selectorText = rule.selectorText.replace(":hover", ":active");
	          }
	        }
	      }
	    }
	  }
	}
	hoverTouchUnstick();


