<script type="text/javascript">
    //<![CDATA[
    $(document).ready(function() {
        $(function() {
            var byRow = $('body').hasClass('LandingPage');
            $('.landing-container').each(function() {
                $(this).find('.landing-item').matchHeight({
                    byRow: byRow
                });
            });
        });
        $(".landing-body").hide();
        $('.landing-item-collapse span.clickable').on("click", function (e) {
            if ($(this).hasClass('landing-collapsed')) {
                $(this).parents('.landing-item-collapse').find('.landing-body').slideUp();
                $(this).removeClass('landing-collapsed');
                $(this).find('i').removeClass('glyphicon-chevron-down').addClass('glyphicon-chevron-down');
            }
            else {
                $(this).parents('.landing-item-collapse').find('.landing-body').slideDown();
                $(this).addClass('landing-collapsed');
                $(this).find('i').removeClass('glyphicon-chevron-up').addClass('glyphicon-chevron-up');
            }
        });
    });
    //]]>
</script>
