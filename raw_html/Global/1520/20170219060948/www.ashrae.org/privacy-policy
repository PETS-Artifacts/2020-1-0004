<script language="javascript" type="text/javascript">
    jQuery.redirect = function (obj, url, params) {
        url = url || window.location.href || '';
        url = url.match(/\?/) ? url : url + '?';

        for (var key in params) {
            var re = RegExp(';?' + key + '=?[^&;]*', 'g'); //RegExp('[;&]' + key + '=?[^&;]*', 'g')

            url = url.replace(re, '');
            url += '&' + key + '=' + params[key];
        }
        url = url.replace(/[;&]$/, '');
        url = url.replace(/\?[;&]/, '?');
        url = url.replace(/[&]{2}/g, '&');
        url = url.replace(/[;&]{2}/g, ';');
        obj.href = url;
    };
    $(document).ready(function () {
        $("a[href*='techstreet.com']").each(function () {
            var token = '';
            //if(token != '')
            $.redirect(this, this.href, { ashrae_auth_token: token });
        });
    });

</script>


        
        
        
        
        
        
