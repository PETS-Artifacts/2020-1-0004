<script type="text/javascript">
    var axel = Math.random() + "";
    var ord = axel * 1000000000000000000;
    $(document).ready(function () {
        $("#ctl01_Header_txtUser").keypress(function (e) {
            if (e.which == 13) return LoginClick(e);
        });
        $("#ctl01_Header_txtPwd").keypress(function (e) {
            if (e.which == 13) return LoginClick(e);
        });

        $("#searchTextBox").focus(function () {
            $(this).filter(function () {
                return $(this).val() == "" || $(this).val() == "Search on Ashrae"
            }).removeClass("watermarkOn").val("");
        });
        $('#searchTextBox').blur(function () {
            $(this).filter(function () {
                return $(this).val() == ""
            }).addClass("watermarkOn").val("Search on Ashrae");
        });
        $("#btnSearch").bind("click", SearchClick);
        $("#searchTextBox").keypress(function (e) {
            if (e.which == 13) return SearchClick(e);
        });
    });
    function SearchClick(e) {
        e.preventDefault();
        var searchText = jQuery("#searchTextBox").val();
        var SearchUrl = '/home/search' + '?k=' + searchText;
        if (searchText != "" && searchText != "Search on Ashrae")
            window.location.href = SearchUrl;
        return false;
    }
    function LoginClick(e) {
        e.preventDefault();
        $("#ctl01_Header_ibtnLogin").trigger('click');

        return false;
    }
</script>
