    $('.lightbox').each(function () {
        var image = $(this).children('img').attr('src');
        var text = $(this).find('.galleryMain-caption').text();
        $(this).magnificPopup({
            mainClass: 'mfp-fade',
            items:
            [{
                src: image,
                title: text
            }],
            gallery: { enabled: true },
            type: 'image'
        });
    });
