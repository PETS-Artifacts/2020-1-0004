    <script type="text/javascript">
    function loadPartialView() {
        var urls = '/Home/Testimonials';

        $.ajax({
            url: urls,
            type: 'post',
            cache: false,
            async: true,
            success: function (result) {
                $('.customer-testimony').html(result);
            }
        });

    }

    function HideStoreProducts(Id) {
        $('[id^="Store_Products_"]').hide();
        var selectedStore = "#Store_Products_" + Id;
        if (Id !== "") {
            $(selectedStore).show();
            AddToBasket(0);
        }
    }


    function UpdateQuantity(guid, quantity, storeId) {
        var urls = '/UpdateQuantity';
        var slug = '';

        $.ajax({
            url: urls,
            type: 'post',
            cache: false,
            async: true,
            data: { guid: guid, quantity: quantity, slug: slug, storeId: storeId },
            success: function (result) {
                $('.shopping-items').html(result);
            }

        });
    }

</script>

