<script type="text/javascript">
    var regionId = 0;
    var storeId = 0;
    var storeSelectionUrl = '';

    function FindStores(e) {
        var id = parseInt(e.value);
        var url = '/Home/FindStore';
        regionId = id;

        $.ajax({
            url: url,
            type: 'post',
            cache: false,
            async: true,
            data: { regionId: id },
            success: function(result) {
                $('.stores').html(result);
                urlBuildStore();
                $('#GetAQuoteStore').on('change', function () {
                    storeId = $('#GetAQuoteStore').val();
                    urlBuildStore();
                });
            }
        });
    }

    function FindQStores(e) {
        var id = parseInt(e.value);
        var url = '/Home/FindQStore';
        regionId = id;

        $.ajax({
            url: url,
            type: 'post',
            cache: false,
            async: true,
            data: { regionId: id },
            success: function (result) {
                $('.stores').html(result);
                urlBuildStore();
                $('#GetAQuoteStore').on('change', function () {
                    storeId = $('#GetAQuoteStore').val();
                    urlBuildStore();
                });
            }
        });
    }


    function urlBuildStore() {
        var parameters = '';
        var url = '/GetAQuoteForStore?t=1';
        if (regionId != 0) {
            parameters = "&regionId=" + regionId.toString();
        }
        if (storeId != 0) {
            parameters += "&storeId=" + storeId.toString();
        }
        $("#GetAQuoteForStore").attr("href", url + parameters);
    }
</script>



