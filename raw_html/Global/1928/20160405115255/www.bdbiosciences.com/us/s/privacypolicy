<script type="text/javascript">
	var loggedIn = '';
	var selectedRegion = 'US';
	var changeRegionUrl = '/us/changeRegion';
	var removeSessionCartUrl = '/us/changeRegion/removeCart';
	//var actionUrl = '';
			
	function beforeLogin()
	{
		var email = $('#j_username').val();
		var password = $('#j_password').val();
		document.getElementById('validatepwdlabel').style.display="none";  
		document.getElementById('validateemaillabel').style.display="none";  
		document.getElementById('validateemailllabel').style.display="none";  
	    if(email.length<=0 && password.length<=0){
			document.getElementById('validateemaillabel').style.display="";
			return false;
		}
		else if(email.length <=0){
				document.getElementById('validateemailllabel').style.display="";  
				return false;
		}
		else if(password.length <=0){
			document.getElementById('validatepwdlabel').style.display="";  
			return false;
		}
		
		
		$.ajax({
			type: 'GET',
			url: changeRegionUrl,
			data: {email:email},
			success: function(result) {
				$.modal.close();
				$('#colorbox').hide();
				if (result.msg == "")
				{
					$('#loginForm').submit();
				}
				else
				{
					$('#onLoginRegionChangePopup #message').html(result.msg);
					$('#onLoginRegionChangePopup.simplemodal-data_add').modal();
				}
			},
			error: function(result) {
				$.modal.close();
				$('#colorbox').hide();
				$('#onLoginRegionChangePopup').html('Problem in fetching user country.');
				$('#onLoginRegionChangePopup.simplemodal-data_add').modal();
				return false;
			}
		});
	}
	
	function submitLoginForm()
	{
		$.ajax({
			type: 'GET',
			url: removeSessionCartUrl,
			success: function(result) {
				$('#loginForm').submit();
				return true;
			},
			error: function(result) {
				$('#loginForm').submit();
				return false;
			}
		});
	}
</script>
<div id="onLoginRegionChangePopup" class="simplemodal-data_add">
	<p id="message">
	</p>

	<div class="com_submit">
		<input type="button" name="proceed" value="Proceed" onclick="submitLoginForm();"/>
		<input class="simplemodal-close" type="button" name="cancel" id="cancel" value="Cancel" />
	</div>
