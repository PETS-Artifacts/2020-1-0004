      <script type="text/javascript">

$( document ).ready(function() {
	try{
		var region='us';
		if(region&&region!=""){
			setOffsetTime(region);
		}
	}
	catch(exception){
		if(window.console){
			console.log(exception)
		}
	}
});

function setOffsetTime(region){
	var cosiPrefix=region=='na'?'/cosi':'';
	$.ajax({
		url: '/'+region+cosiPrefix+'/time/setOffset?offset='+new Date().getTimezoneOffset()*60*1000,
		type: 'GET',
		success: function (data)
		{
			if(window.console)console.log(data)
		},
		error: function (xht, textStatus, ex)
		{
			if(window.console)console.log("failed to set offset:"+ex)
		}
	});
}

</script>

