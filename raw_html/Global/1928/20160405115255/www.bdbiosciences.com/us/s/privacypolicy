	<script>
	var customer = $.cookie("welcome");
	var name = "";
	var accountType = "";
	var punchoutLogo = "";
	var loginHtml = '<a href="/us/login">Sign In or Create an Account</a>';
	var lpChatLoggedInHtml = "<div id='lpButtonDiv-Header'></div>";
	var lpChatContactHtml = '<a href="/us/s/contactus" >Live Chat</a>';
	if (customer != null) {
		var customerArray = customer.split("|");
		if (customerArray.length > 0) {
			if (customerArray[0] != null) {
				name = customerArray[0];
			}
		}
		if (customerArray.length > 1) {
			if (customerArray[1] != null) {
				accountType = customerArray[1];
			}
		}
		if (customerArray.length > 2) {
			if (customerArray[2] != null) {
				punchoutLogo = customerArray[2];
			}
		}
		
		if (name!= "") 	{
			$('#lpLink').html(lpChatLoggedInHtml);
		}
		
		// Display welcome message
		if (accountType != "Punchout_BDInstitutional") {
			if (accountType != "Punchout_BDIndividual") {
				if (name == "")	{
					$('#welcome').html(loginHtml);
					$('#lpLink').html(lpChatContactHtml);
				}
			}
			if (name != "") {
				$('#welcome').html(welcomeString + " " + name);
				$('#myaccount').html(" - " + myAccountHtml);
			}
			if (accountType != "Punchout_BDIndividual") {
				if (name != "")	{
					//$('#signout').html(" - " + logoutHtml);
					$('#signout').html(logoutHtml);
				}
			}
		}
		
		if (accountType == "Punchout_BDInstitutional") {
			if (name != "") {
				$('#welcome').html(welcomeString + " " + name);
			}
		}
		
		if (accountType == "Punchout_BDInstitutional" || accountType=='Punchout_BDIndividual') {
            if (isBlank(punchoutLogo)) {
                   $("#punchoutLogoImage").attr("src",punchoutLogo);
                   $("#punchoutLogoImage").css( "display", "block !important");
                   $("#punchoutLogoImage").show();
            }                   
     }
	
	}
	else {
		$('#welcome').html(loginHtml);
		$('#lpLink').html(lpChatContactHtml);
	}
	//$('#globalnav').listview('refresh');
	//alert("1234");
	var items = "items";
	var item = "item";
	var cartEmpty = "Cart Empty";
	
	//strCount = "Cart Empty";
	strCount = cartEmpty;
	var count = $.cookie("bdCartCookie");

	if (count != null) {
		var strCount = count + "";
		if (count > 1) {
			strCount = strCount + ' '+items;
		}
		else if(count==0) {
			/* strCount = "Cart Empty"; */
			strCount = cartEmpty;
		}
		else {
			strCount = strCount + ' '+item;
		}
	}
	$('#cartTotalItems').html(strCount);
</script>		 
