    <div class="journal-content-article" id="article_10154_33113_160451284_1.1">
        






   















































   

































































        





    <section class="mod-wrapper ui-small-carousel mod-trending delay-bg-load" id="mod-trending-160451284">
        <div class="fullWidth"></div>
		<div class="mod-header">
			<h3>Trending</h3>
			<div class="sponsor">
								<img src="http://media.ksat.com/photo_qa/2015/06/08/poweredByTrove_1433816690153_2902_ver1.0.png">
			</div>
		</div>
		<div class="mod-body owl-carousel owl-theme small-carousel"></div>
		<footer class="mod-footer">
			<div class="loading-animation center-block"></div>
		</footer>
	</section>
		<script type="text/javascript">
	   (function($, A) {
			var mod=$('#mod-trending-160451284'), carousel=mod.children('.owl-carousel'), footer=mod.children('.mod-footer');
			var placeholder = "http://media.clickorlando.com/theme/images/placeholder-16x9-wkmg.png";
			var thumbnail = "http://media.clickorlando.com/theme/images/placeholder-16x9-wkmg.png";

			var onCarouselReady = function(){
				var owl = carousel.data('owlCarousel');

						
				$.ajax({
					dataType: 'jsonp',
					url: 'https://trove-proxy.s3.amazonaws.com/${site}_trending.json',
					jsonpCallback: "renderTrendingContent",
				})
				.done(function(data) {
					footer.hide();
					var items = (data && data.trending && data.trending.items) ? data.trending.items : null;
					if( items && items.length) {
						$.each(items, function(idx, item){
							createOwlItem(item);
						});
					} else {
						mod.hide();
						lakana.utils.log("No Trove data return.");
					}
					carousel.addClass('owl-ready');
					EP.Util.dotdotdot.add( carousel.find('.owl-item .item a article h5') );
				})
				.fail(function() {
					lakana.utils.log("Get Trove data error.");
					mod.hide();
				});

				var $div = $("<div />");
				var escapeAttribute = function( val ){
					$div.attr('title', val);
					return $div.attr('title');
				};

				var createOwlItem = function( item ) {
					var thumb = "";
					var photoCopyright = "";
					if(item.image) {
						if(item.image.thumbnails[2]) {
							thumb = item.image.thumbnails[2].url;
						} else if(item.image.thumbnails[1]) {
							thumb = item.image.thumbnails[1].url;
						} else if(item.image.fullImage) {
							thumb = item.image.fullImage.url;
						}
					}
					if(item.copyright) {
						photoCopyright = item.copyright;
					}
					var content = "";
					content += "<div class=\"item trending\">";
					content += "	<a href=\"" + item.url + "\" title=\"" + escapeAttribute(item.displayName) + "\">";
					content += "		<div class=\"image-container\">";
					content += "			<img src=\"" + placeholder + "\" alt=\"\">";
					content += "			<span class=\"image placeholder-16x9\" style=\"background-image: url(" + thumb + ") !important;\"></span>";
					content += "				<small class=\"copyright\">"+photoCopyright+"</small>";
					content += "		</div>";
					content += "		<article>";
					content += "			<h5>" + item.displayName + "</h5>";
					content += "		</article>";
					content += "	</a>";
					/*content += "	<a href=\"#\" class=\"carousel-article-category text-uppercase\">";
					content += "		<h6>Local</h6>";
					content += "	</a>";*/
					content += "</div>";

					owl.addItem( content );
				};
		   };

		   carousel.one('owl-carousel-ready', onCarouselReady);
		    	   })(jQuery, AUI());
	</script>

    </div>

    











    

    

    

	

					
				
			
			
		
	
	



	</div>

				</div>

				
			
		
	


	





			
		</div>
	</div>





	
	
</div>
        </div>
    </section>

