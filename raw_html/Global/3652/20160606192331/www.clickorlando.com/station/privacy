   mediaType = 'Home';
  }else{
   mediaType = $('meta[name="page-type"]').attr('content');
  }
  ga('create', 'UA-17717614-1', 'auto',{'name': 'trackerEvent'});
  ga('trackerEvent.set', 'dimension1', mediaType);
  ga('trackerEvent.set', 'dimension2', section);
  ga('trackerEvent.set', 'dimension3', pageAuthor);
    
