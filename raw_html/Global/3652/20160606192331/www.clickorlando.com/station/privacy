   var $el = $(this);
   var gaSection = $el.attr('ga-section');
   var gaCategory = $el.attr('ga-category');
   var gaAction = $el.attr('ga-action');
   var gaLabel = $el.attr('ga-label');
   var gaValue = "";
   switch (gaCategory) {
    case "Breaking News Bar": 
     var count = $el.parent().children(".count").html();
     gaValue = count.split('/')[0];
     if(gaAction == "Right Scroll"){
      gaValue = eval(gaValue) + 1;
     }else{
      gaValue = eval(gaValue) - 1;
     }
     break;
    case "Lazy Load": 
     gaValue = $('#' + gaSection).attr('pageNum');
     break;
    default: 
     gaValue = $('#' + gaSection + ' .ga-value').val();
   }
   
   var customFields = {                    
    'hitType': 'event',
    'eventCategory': gaCategory, 
    'eventAction': gaAction,     
    'eventLabel': gaLabel,
    'eventValue': gaValue
   };
   ga('trackerEvent.send', customFields);
   ga('trackerEvent2.send', customFields);
  });
  
 //Timers
 var setLater = function(timer, delay, ctx, fnt, data, periodic){
  killLater(timer);
  return A.later(delay, ctx, fnt, data, periodic);
 };
 var killLater = function(timer){
  if(timer){
   timer.cancel();
  }
  return null;
 };
 
 var trackOnOpen = function(e) {
  var $el = $(this);
  var gaSection = $el.attr('ga-section');
  var gaCategory = $el.attr('ga-category');
  var gaAction = $el.attr('ga-action');
  var gaLabel = $el.attr('ga-label');
  var gaValue = $el.attr('ga-value');

  var customFields = {
   'hitType': 'event',
   'eventCategory': gaCategory,
   'eventAction': gaAction,
   'eventLabel': gaLabel,
   'eventValue': gaValue
  };
  ga('trackerEvent.send', customFields);
 };
 var DELAY = 500;
 var debounceTrackOnOpen = function(el, showEvent, hideEvent){
  var timer;
  
  $(el).on(showEvent, function(e){
   if(e.target===e.currentTarget && !timer){
    timer = setLater(timer, DELAY, el, trackOnOpen, arguments);
   }
  }).on(hideEvent, function(e){
   if(e.target===e.currentTarget){
    timer = killLater(timer);
   }
  });
 };
 $('.dropdown.ga-open-event').each(function(idx, el){
  debounceTrackOnOpen(el, 'show.bs.dropdown', 'hide.bs.dropdown');
 });
 $('.collapse.ga-open-event').each(function(idx, el){
  debounceTrackOnOpen(el, 'show.bs.collapse', 'hide.bs.collapse');
 });
  
 })(jQuery, AUI(), _sf_async_config.sections, _sf_async_config.authors);
