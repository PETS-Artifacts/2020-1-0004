    <script>

        function blockSpecialChar(e) {
            var k;
            document.all ? k = e.keyCode : k = e.which;
            return ((k > 64 && k < 91) || (k > 96 && k < 123) || k == 8 || k == 32 || (k >= 48 && k <= 57));
        }

      
    </script>

    <script type="text/javascript">
        $('#BrokingMobileNo, #StickyMobileNo').on('keypress', function (event) {
            var regex = new RegExp("^[0-9]+$");
            var key = String.fromCharCode(!event.charCode ? event.which : event.charCode);
            if (!regex.test(key)) {
                event.preventDefault();
                return false;
            }
        });
        $(document).ready(function () {
            $('#RegDiv').hide();
            $('#loginDiv').show();
            $('#btnReg').on('click', function () {
                $('#RegDiv').show();
                $('#loginDiv').hide();
            });
            $('#btnLog').on('click', function () {
                $('#RegDiv').hide();
                $('#loginDiv').show();
            });

            $('.modal-close').on('click', function () {
                $('#RegDiv').hide();
            });
        });
    </script>
    
    <script type="text/javascript">
        var state = "", city = "";
        function LoadGeoloc() {
            var url = "https://api.ipgeolocation.io/ipgeo?apiKey=d7a00180faa14d5b89906d9710e6bfdd"
            $.ajax({
                type: "get",
                async:false,
                url: url,
                success: function (data) {
                    state = data.state_prov;
                    city = data.city;
                },
                error: function (resp) {
                }
            });
        }
        $(document).ready(function () {
            $('#btnWebSite').on('click', function () {
                window.location.reload(true);
            });
        });
    </script>
    <script>
        function fetchIP() {
            return { getState: state, getCity: city }
        }

        function getParameterByName(name, url) {
            if (!url) url = window.location.href;
            name = name.replace(/[\[\]]/g, "\\$&");
            var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"),
                results = regex.exec(url);
            if (!results) return null;
            if (!results[2]) return '';
            return decodeURIComponent(results[2].replace(/\+/g, " "));
        }
       
        function ValidateMobileSticky(event, IsStickyBroking, errLbl) {
            var StickyMobileNumber = "";
            var age = "";
            localStorage.setItem("langFlagFooter", $('#otherLangfooter').is(":checked"));
            if (IsStickyBroking) StickyMobileNumber = $('#StickyMobileNo').val().trim();
            var OnlyNumber = new RegExp('^[0-9]*$');
            var phoneRg = new RegExp('^[6-9]{1}\\d*$');
            if (StickyMobileNumber == '') {
                $('#' + errLbl).addClass('form-input-error');
                return false;
            }
            else if (!OnlyNumber.test(StickyMobileNumber)) {
                $('#' + errLbl).addClass('form-input-error');
                return false;
            }
            else if (StickyMobileNumber.length != 10) {
                $('#' + errLbl).addClass('form-input-error');
                return false;
            }
            else if (!phoneRg.test(StickyMobileNumber)) {
                $('#' + errLbl).addClass('form-input-error');
                return false;
            }
            else {
                if (IsStickyBroking) {
                    $('.sticky__wait').css('display', 'block');
                    LoadGeoloc();
                    CallMeFunFooter(event, StickyMobileNumber, 'OnlineStore', 'OnlineStoreSticky', getParameterByName('utm_campaign'), getParameterByName('utm_source'), getParameterByName('campaign'), getParameterByName('utm_medium'), getParameterByName('utm_term'), getParameterByName('utm_content'), getParameterByName('gclid'), getParameterByName('network'), getParameterByName('matchtype'), getParameterByName('campaignid'), getParameterByName('adgroupid'), getParameterByName('keyword'), getParameterByName('adposition'), getParameterByName('random'), getParameterByName('placement'), getParameterByName('device'), getParameterByName('creative'), getParameterByName('targetid'));
                }
                else {
                }
            }
        }
       
        function CallMeFunFooter(event, StickyMobileNumber, leadsource,leadProduct, utm_campaign, utm_source, campaign, utm_medium, utm_term, utm_content, gclid, network, matchtype, campaignid, adgroupid, keyword, adposition, random, placement, device, creative, targetid) {
            event.preventDefault();
            var geoLoc = fetchIP();
            utm_campaign = (utm_campaign == "" || utm_campaign == null) ? "sticky" : utm_campaign;
            var ResultURL = window.location.href;
            $.ajax({
                type: 'Post',
                url: '/Home/LeadGeneration',
                data: {
                    'MobileNo': StickyMobileNumber, UTM_campaign: utm_campaign, 'LeadSource': leadsource, 'otherlang': localStorage.getItem("langFlagFooter"), 'state': geoLoc.getState, 'city': geoLoc.getCity, 'LeadProduct': leadProduct, 'utm_source': utm_source, 'campaign': campaign, 'utm_medium': utm_medium, 'utm_term': utm_term, 'utm_content': utm_content, 'gclid': gclid, 'network': network, 'matchtype': matchtype, 'campaignid': campaignid, 'adgroupid': adgroupid, 'keyword': keyword, 'adposition': adposition, 'random': random, 'placement': placement, 'device': device, 'creative': creative, 'targetid': targetid, 'ResultURL': ResultURL },
                success: function (data) { },
                error: function (data) { }
            });
            setTimeout("location.replace('/open-demat-account?LeadMob=" + btoa(StickyMobileNumber) + "');", 1000);
        }
    </script>
    <script>
        $(document).ready(function () {
            $("#StickyMobileNo").keyup(function (e) {
                var charCode = (e.which) ? e.which : e.keyCode;
                if (charCode == 37 || charCode == 38 || charCode == 39 || charCode == 40 || charCode == 9 || charCode == 13 || charCode == 35 || charCode == 36 || charCode == 45 || charCode == 33 || charCode == 34 || charCode == 44 || charCode == 145 || charCode == 19 || charCode == 113 || charCode == 115 || charCode == 118 || charCode == 119 || charCode == 120 || charCode == 122 || charCode == 123) {
                }
                else {
                    $('#StickyMobileNo').removeClass('form-input-error');
                }
            });
        });
    </script>
