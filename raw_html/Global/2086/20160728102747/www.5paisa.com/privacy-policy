<script type="text/javascript">

    function isMiniNumberKey(evt) {

        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57))
            return false;

        //CharCodes
        //Number  keyboard  numpad
        //7         55        103
        //8         56        104
        //9         57        105

        if ($("#txtMiniMobile").val() == "") {
            if (charCode == 55 || charCode == 103 || charCode == 56 || charCode == 104 || charCode == 57 || charCode == 105) {
                //number : 7
                return true;
            }
            else {
                return false;
            }
        }


        return true;
    }


    $("#txtMiniName,#txtMiniMobile,#txtMiniEmail,#txtMinipwd").keydown(function (event) {

        if (event.keyCode == 13) {
            isSuccess = true;
            isSuccess = MiniRegisterValidate();

            if (isSuccess == true) {
                //$("#btnRegister").click();
                isMiniRegistered = true;

                isMiniRegistered = MiniRegisterClick();
                event.preventDefault();
                return false;

            }

            return isSuccess;
        }
        else {
            return true;
        }

    });

    function MiniRegisterValidate() {

        bchk = true;
        if (requiredfield($('#txtMiniName'), "Please enter your Name")) {
            $('#txtMiniName').next(".error").hide();
        } else bchk = false;
        if (requiredfield($('#txtMiniMobile'), "Please enter valid Mobile No")) {
            $('#txtMiniMobile').next(".error").hide();
        } else bchk = false;
        if (requiredfield($('#txtMiniEmail'), "Please enter valid Email Address")) {
            $('#txtMiniEmail').next(".error").hide();
        } else bchk = false;
        if (requiredfield($('#txtMinipwd'), "Please enter valid Password")) {
            $('#txtMinipwd').next(".error").hide();
            //if (ValidatePassword($('#txtMinipwd'), "Password has to be minimum 8 characters, with 1 Upper Case, 1 Special Character, 1 Alphanumeric value.")) {
            //    $('#txtMinipwd').next(".error").hide();
            //}
            //else bchk = false;
        } else bchk = false;

        if (bchk == false) {
            return bchk;
        }

        return bchk;

    }



    function MiniRegisterClick()
    {
        isSuccess = true;

        $('#hfMiniName').val(EncryptString($('#txtMiniName').val().trim()));
        $('#hfMiniMobile').val(EncryptString($('#txtMiniMobile').val().trim()));
        $('#hfMiniEmail').val(EncryptString($('#txtMiniEmail').val().trim()));
        $('#hfMinipwd').val(EncryptString($('#txtMinipwd').val().trim()));

        //$('#txtMiniName').val("");
        //$('#txtMiniMobile').val("");
        //$('#txtMiniEmail').val("");
        //$('#txtMinipwd').val("");



        $.ajax({
            type: "POST",

            data: "{'EnMiniEmail':'" + $('#hfMiniEmail').val().trim() + "','EnMiniPassword':'" + $("#hfMinipwd").val().trim()
                + "','EnMiniName':'" + $("#hfMiniName").val().trim() + "','EnMiniMobile':'" + $("#hfMiniMobile").val().trim() + "'}",
            url: "BackPage.aspx/MiniRegistrationButtonClick",
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            async: true,
            success: function (result) {

                if (result.d.toLowerCase() == "true") {
                    //Success

                    $("#dvMiniReg").hide();
                    $("#dvMiniMobileOTP").show();
                    $("#dvMiniEmailOTP").hide();
                    $("#dvMiniThanks").hide();

                    $("#miniregx").hide();
                    $("#minireg2x").show();
                    $("#minireg3x").hide();
                    $("#minireg4x").hide();
                    $("#minireg5x").hide();
                    $('#reg-x').hide();


                    $("#lblMiniErr").hide();
                    $("#lblMiniErr").html("");

                    isSuccess = true;

                }
                else if (result.d.toLowerCase() == "exception") {
                    //Exception
                    isSuccess = false;
                    $("#lblMiniErr").show();
                    $("#lblMiniErr").html("Error while Registering user. Please try again later!");

                }
                else {
                    //Login Failed
                    isSuccess = false;
                    $("#lblMiniErr").show();
                    $("#lblMiniErr").html(result.d);
                }

            },
            error: function () {

                //Error while Registration
                isSuccess = false;
                $("#lblMiniErr").show();
                $("#lblMiniErr").html("Error while Registering user. Please try again later!");


            }

        });

        return isSuccess;
    }

    $(document).ready(function () {


        $("#btnMiniRegister").click(function () {

            isSuccess = true;

            isSuccess = MiniRegisterValidate();
            if (isSuccess==false) {
                return isSuccess;
            }

            MiniRegisterClick();

            return false;
        });



        $("#txtMiniMOTP").keydown(function (event) {

            if (event.keyCode == 13) {
                isSuccess = true;
                isSuccess = isMiniMVerifyEmpty();

                if (isSuccess == true) {
                    //$("#btnRegister").click();
                    isMiniMVerified = true;

                    isMiniMVerified = MiniMVerifyClick();
                    event.preventDefault();
                    return false;

                }

                return isSuccess;
            }
            else {
                return true;
            }

        });

        function isMiniMVerifyEmpty() {
            bchk = true;

            if ($("#txtMiniMOTP").val() == "") {
                $("#lblMiniMErr").html("Please enter mobile OTP.");
                bchk = false;
            }

            return bchk;
        }

        function MiniMVerifyClick() {
            isSuccess = true;

            $.ajax({
                type: "POST",

                data: "{'MOTP':'" + $('#txtMiniMOTP').val().trim() + "'}",
                url: "BackPage.aspx/RegisterationMiniMVerifyClick",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (result) {

                    if (result.d.toLowerCase() == "true") {
                        //Success
                        isSuccess = true;

                        $("#dvMiniReg").hide();
                        $("#dvMiniMobileOTP").hide();
                        $("#dvMiniEmailOTP").show();
                        $("#dvMiniThanks").hide();

                        $("#miniregx").hide();
                        $("#minireg2x").hide();
                        $("#minireg3x").show();
                        $("#minireg4x").hide();
                        $("#minireg5x").hide();


                        $("#lblMiniMErr").hide();
                        $("#lblMiniMErr").html("");

                        window.location.href = "/homepage";


                    }
                    else if (result.d.toLowerCase() == "exception") {
                        //Exception
                        isSuccess = false;
                        $("#lblMiniMErr").show();
                        $("#lblMiniMErr").html("Error while verifying mobile OTP. Please try again later!");

                    }
                    else {
                        //Login Failed
                        isSuccess = false;
                        $("#lblMiniMErr").show();
                        $("#lblMiniMErr").html(result.d);
                    }

                },
                error: function () {

                    //Error while Registration
                    isSuccess = false;
                    $("#lblMiniMErr").show();
                    $("#lblMiniMErr").html("Error while verifying mobile OTP. Please try again later!");


                }

            });

            return isSuccess;
        }


        $("#btnMiniMVerify").click(function () {

            isSuccess = true;

            isSuccess = isMiniMVerifyEmpty();

            if (isSuccess == false) {
                return isSuccess;
            }

            MiniMVerifyClick();

            return false;


        });

        $("#lnkMiniMresend").click(function (e) {
            e.preventDefault();

            $.ajax({
                type: "POST",
                url: "BackPage.aspx/RegistrationMiniMResendClick",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
