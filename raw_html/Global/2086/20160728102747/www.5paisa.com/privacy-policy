<script type="text/javascript">
    $(document).ready(function () {

        //$('#miniFPx').show();
        //$('.backdrop').show();


        /*forget password*/
        $('#lnkFPLogin').click(function (e) {
            e.preventDefault();
            $(this).parents('#dvFPThanks').slideUp();
            $('#miniFPx').hide();
            $('#login-x').show();
            $('#login-panel-1').show();
            $('.backdrop').show();
            $('#login-btn,#reg-btn').removeClass('inactive');

        });

        $("#txtFPEmail").keydown(function (event) {

            if (event.keyCode == 13) {
                isSuccess = true;
                isSuccess = isFPGetEmailempty();

                if (isSuccess == true) {
                    //$("#btnRegister").click();
                    isFPGetEmailed = true;

                    isFPGetEmailed = FPGetEmailClick();
                    event.preventDefault();
                    return false;

                }

                return isSuccess;
            }
            else {
                return true;
            }

        });

        function isFPGetEmailempty() {
            bchk = true;
            if (requiredfield($('#txtFPEmail'), "Please enter Email ID")) {
                $('#txtFPEmail').next(".error").hide();
            } else bchk = false;

            if (bchk == false) {
                return bchk;
            }
            return bchk;
        }

        function FPGetEmailClick() {
            isSuccess = true;

            $('#hfFPEmail').val(EncryptString($('#txtFPEmail').val().trim()));

            //$('#txtFPEmail').val("");

            $.ajax({
                type: "POST",
                data: "{'EnEMail':'" + $('#hfFPEmail').val().trim() + "'}",
                url: "BackPage.aspx/GetFPMobileOTP",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (result) {

                    if (result.d.toLowerCase() == "true") {
                        //true
                        isSuccess = true;
                        $("#EmailFPOTP").hide();
                        $("#dvFPVerifyOTP").show();
                        $("#dvFPPWD").hide();
                        $("#dvFPThanks").hide();
                        //$('#login-btn,#reg-btn').addClass('inactive');


                    }

                    else if (result.d.toLowerCase() == "exception") {
                        //exception
                        isSuccess = false;
                        $("#lblFPOTPMsg").html("Error while getting OTP. Please try again later!");

                    }
                    else {
                        //Get mobile OTP Failed.
                        isSuccess = false;
                        $("#lblFPOTPMsg").html(result.d);
                    }
                },

                error: function () {
                    //Error while Login
                    isSuccess = false;
                    $("#lblFPOTPMsg").html("Error while getting OTP. Please try again later!");
                }
            });

            return isSuccess;
        }

        $("#btnFPgetEmail").click(function () {

            isSuccess = true;

            isSuccess = isFPGetEmailempty();

            if (isSuccess == false) {
                return isSuccess;
            }

            FPGetEmailClick();

            return false;
        });



        $("#txtFPOTP").keydown(function (event) {

            if (event.keyCode == 13) {
                isSuccess = true;
                isSuccess = isFPVerifyOTPEmpty();

                if (isSuccess == true) {
                    //$("#btnRegister").click();
                    isFPOTPVerified = true;

                    isFPOTPVerified = FPVerifyOTPClick();
                    event.preventDefault();
                    return false;

                }

                return isSuccess;
            }
            else {
                return true;
            }

        });


        function isFPVerifyOTPEmpty() {
            bchk = true;
            if (requiredfield($('#txtFPOTP'), "Please enter OTP")) {
                $('#txtFPOTP').next(".error").hide();
            } else bchk = false;

            if (bchk == false) {
                return bchk;
            }
            return bchk;
        }

        function FPVerifyOTPClick() {
            isSuccess = true;

            $.ajax({
                type: "POST",
                data: "{'EnEMail':'" + $('#hfFPEmail').val().trim() + "','OTP':'" + $('#txtFPOTP').val().trim() + "'}",
                url: "BackPage.aspx/VerifyFPMobileOTP",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (result) {

                    if (result.d.toLowerCase() == "true") {
                        //true
                        isSuccess = true;
                        $("#EmailFPOTP").hide();
                        $("#dvFPVerifyOTP").hide();
                        $("#dvFPPWD").show();
                        $("#dvFPThanks").hide();
                    }

                    else if (result.d.toLowerCase() == "exception") {
                        //exception
                        isSuccess = false;
                        $("#lblFPVerifyMsg").html("Error while verifying OTP. Please try again later!");

                    }
                    else {
                        isSuccess = false;
                        //Verify mobile OTP Failed.
                        $("#lblFPVerifyMsg").html(result.d);
                    }
                },

                error: function () {
                    //Error while Login
                    isSuccess = false;
                    $("#lblFPVerifyMsg").html("Error while verifying OTP. Please try again later!");
                }
             });

            return isSuccess;
        }



        $("#btnFPVerifyOTP").click(function () {
            
            isSuccess = true;

            isSuccess = isFPVerifyOTPEmpty();

            if (isSuccess == false) {
                return isSuccess;
            }

            FPVerifyOTPClick();

            return false;

        });




        $("#txtFPNpwd,#txtFPCpwd").keydown(function (event) {

            if (event.keyCode == 13) {
                isSuccess = true;
                isSuccess = isFPSubmitEmpty();

                if (isSuccess == true) {
                    //$("#btnRegister").click();
                    isFPSubmit = true;

                    isFPSubmit = FPSubmitClick();
                    event.preventDefault();
                    return false;

                }

                return isSuccess;
            }
            else {
                return true;
            }

        });


        function isFPSubmitEmpty() {
            bchk = true;
            if (requiredfield($('#txtFPNpwd'), "Please enter New Password")) {
                $('#txtFPNpwd').next(".error").hide();
            } else bchk = false;
            if (requiredfield($('#txtFPCpwd'), "Please enter Confirm Password")) {
                $('#txtFPCpwd').next(".error").hide();
            } else bchk = false;
            if (bchk) {
                //if (ValidatePassword($('#txtFPNpwd'), "Password has to be minimum 8 characters, with 1 Upper Case, 1 Special Character, 1 Alphanumeric value.")) {
                //    $('#txtFPNpwd').next(".error").hide();
                //}
                //else bchk = false;
                if ($('#txtFPNpwd').val() != $('#txtFPCpwd').val()) {
                    $('#txtFPCpwd').next(".error").show();
                    $('#txtFPCpwd').next(".error").html("New Password should be same as Confirm Password.");
                    bchk = false;
                }
            }

            if (bchk == false) {
                return bchk;
            }
            return bchk;
        }


        function FPSubmitClick() {
            isSuccess = true;

            $('#hfFPNpwd').val(EncryptString($('#txtFPNpwd').val().trim()));
            $('#hfFPCpwd').val(EncryptString($('#txtFPCpwd').val().trim()));

            //$('#txtFPNpwd').val("");
            //$('#txtFPCpwd').val("");


            $.ajax({
                type: "POST",
                data: "{'EnEMail':'" + $('#hfFPEmail').val().trim()
                    + "','EnNewPassword':'" + $('#hfFPCpwd').val().trim()
                    + "','OTP':'" + $('#txtFPOTP').val().trim() + "'}",
                url: "BackPage.aspx/SubmitFPNewPassword",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (result) {

                    if (result.d.toLowerCase() == "true") {
                        //true
                        isSuccess = true;
                        $("#EmailFPOTP").hide();
                        $("#dvFPVerifyOTP").hide();
                        $("#dvFPPWD").hide();
                        $("#dvFPThanks").show();
                    }

                    else if (result.d.toLowerCase() == "exception") {
                        //exception
                        isSuccess = false;
                        $("#lblFPMessage").html("Error while setting new password. Please try again later!");

                    }
                    else {
                        //Verify mobile OTP Failed.
                        isSuccess = false;
                        $("#lblFPMessage").html(result.d);
                    }
                },

                error: function () {
                    //Error while Login
                    isSuccess = false;
                    $("#lblFPMessage").html("Error while setting new password. Please try again later!");
                }
            });

            isSuccess = false;
        }




        $("#btnFPSubmit").click(function () {
            
            isSuccess = true;

            isSuccess = isFPSubmitEmpty();

            if (isSuccess == false) {
                return isSuccess;
            }

            FPSubmitClick();

            return false;
        });




        $("#lnkFPOTPresend").click(function (e) {
            e.preventDefault();



            $.ajax({
                type: "POST",
                data: "{'EnEMail':'" + $('#hfFPEmail').val().trim() + "'}",
                url: "BackPage.aspx/OTPFPResend",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (result) {

                    if (result.d.toLowerCase() == "exception") {
                        //exception
                        $("#lblFPVerifyMsg").html("Error while resending OTP. Please try again later!");

                    }
                    else {
                        //Resend MOTP.

                        $("#lblFPVerifyMsg").html(result.d);
                    }
                },

                error: function () {
                    //Error while Login

                    $("#lblFPVerifyMsg").html("Error while resending OTP. Please try again later!");
                }
            });


        });
    });
</script>

</div>


    <script type="text/javascript">



        function validateDate() {

            var currVal = $('#txtdatePick').val();
            if (currVal == '')
                return false;

            //Declare Regex  
            var rxDatePattern = /^(\d{1,2})(\/|-)(\d{1,2})(\/|-)(\d{4})$/;
            var dtArray = currVal.match(rxDatePattern); // is format OK?

            if (dtArray == null)
                return false;

            //Checks for dd/mm/yyyy format.
            var dtDay = dtArray[1];
            var dtMonth = dtArray[3];
            var dtYear = dtArray[5];

            if (dtMonth < 1 || dtMonth > 12)
                return false;
            else if (dtDay < 1 || dtDay > 31)
                return false;
            else if ((dtMonth == 4 || dtMonth == 6 || dtMonth == 9 || dtMonth == 11) && dtDay == 31)
                return false;
            else if (dtMonth == 2) {
                var isleap = (dtYear % 4 == 0 && (dtYear % 100 != 0 || dtYear % 400 == 0));
                if (dtDay > 29 || (dtDay == 29 && !isleap))
                    return false;
            }

            return true;
        }

        $(document).ready(function () {



            $('#txtdatePick').keypress(function (event) {
                // "/" 47
                // "-" 45

                var charCode = (event.which) ? event.which : event.keyCode
                // Allow: home, end, left, right, down, up
                if (event.keyCode >= 35 && event.keyCode <= 40) {
                    return true;
                }
                //delete
                if (event.keyCode == 46) {
                    return true;
                }

                //ctrl+v
                if (event.ctrlKey == true && charCode == 118) {
                    return true;
                }

                if (charCode > 31 && (charCode < 48 || charCode > 57))
                    return false;


                return true;
            });


            $("#txtMiniUID,#txtMiniPWD,#txtdatePick").keydown(function (event) {

                if (event.keyCode == 13) {
                    isSuccess = true;
                    isSuccess = LoginMiniValidate();

                    if (isSuccess == true) {
                        //$("#btnRegister").click();
                        isLoggedIn = true;

                        isLoggedIn = MiniLoginClick();
                        event.preventDefault();
                        return false;

                    }

                    return isSuccess;
                }
                else {
                    return true;
                }

            });


            function LoginMiniValidate() {
                bchk = true;
                if (requiredfield($('#txtMiniUID'), "Please enter User ID")) {
                    $('#txtMiniUID').next(".error").hide();
                } else bchk = false;
                if (requiredfield($('#txtMiniPWD'), "Please enter Password")) {
                    $('#txtMiniPWD').next(".error").hide();
                } else bchk = false;
                if ($('#txtdatePick').val() == '') {
                    $("#spnDatePck").show();
                    $("#spnDatePck").html("Please enter date(ddMMyyyy)");
                    bchk = false;
                }


                if (bchk == false) {
                    return bchk;
                }

                if ($('#txtdatePick').val().trim().length < 8) {
                    $("#spnDatePck").show();
                    $("#spnDatePck").html("Please enter date in (ddMMyyyy) format.");
                    bchk = false;
                }
                else {
                    $("#spnDatePck").hide();
                    $("#spnDatePck").html("");
                    bchk = true;
                }
                return bchk;
            }

            function MiniLoginClick() {

                isSuccess = true;

                $('#hfMiniUID').val(EncryptString($('#txtMiniUID').val().trim()));
                $('#hfMiniPWD').val(EncryptString($('#txtMiniPWD').val().trim()));

                //$('#txtMiniUID').val("");
                //$('#txtMiniPWD').val("");

                $.ajax({
                    type: "POST",
                    data: "{'EnLoginEmail':'" + $('#hfMiniUID').val().trim()
                        + "','EnLoginPassword':'" + $('#hfMiniPWD').val().trim() + "','DOB':'"
                        + $('#txtdatePick').val().trim() + "'}",
                    url: "BackPage.aspx/LoginMiniClick",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async: true,
                    success: function (result) {

                        if (result.d.toLowerCase() == "personal") {
                            //Personal

                            $("#dvReg").hide();
                            $("#dvMOTP").hide();
                            $("#dvEOTP").hide();
                            $("#dvCongrats").hide();
                            $("#dvComplete").hide();
                            $("#dvResume").show();
                            $("#dvRegistered").hide();
                            isSuccess = true;
                            window.location.href = "/homepage";
                        }
                        else if (result.d.toLowerCase() == "notpersonal") {
                            //Not Personal

                            $("#dvReg").hide();
                            $("#dvMOTP").hide();
                            $("#dvEOTP").hide();
                            $("#dvCongrats").hide();
                            $("#dvComplete").show();
                            $("#dvResume").hide();
                            $("#dvRegistered").hide();
                            isSuccess = true;
                            window.location.href = "/homepage";
                        }
                        else if (result.d.toLowerCase() == "active") {
                            //Active

                            $("#dvReg").hide();
                            $("#dvMOTP").hide();
                            $("#dvEOTP").hide();
                            $("#dvCongrats").hide();
                            $("#dvComplete").hide();
                            $("#dvResume").hide();
                            $("#dvRegistered").show();
                            isSuccess = true;
                            window.location.href = "/homepage";
                        }
                        else if (result.d.toLowerCase() == "exception") {
                            //Exception
                            isSuccess = false;

                            $("#lblMiniLoginErr").html("Error while Login. Please try again later!");
                        }
                        else {
                            //Login Failed.

                            isSuccess = false;

                            $("#lblMiniLoginErr").html(result.d);
                        }
                    },

                    error: function () {
                        //Error while Login
                        isSuccess = false;

                        $("#lblMiniLoginErr").html("Error while Login. Please try again later!");
                    }
                });

                return isSuccess;
            }


            $("#btnMiniLogin").click(function () {


                isSuccess = true;

                isSuccess = LoginMiniValidate();

                if (isSuccess == false) {
                    return isSuccess;
                }


                MiniLoginClick();
                

                return false;

            });

