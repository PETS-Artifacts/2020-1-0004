        $('#txtMiniEmail').change(function () {

            if ($('#txtMiniEmail').val() == "") {
                $('#txtMiniEmail').next(".error").html("");
                return;
            }

            var input = $('#txtMiniEmail').val();
            var re = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/;
            var is_email = re.test(input);
            if (is_email) {
                $('#txtMiniEmail').next('.error').hide();
            }
            else {
                $('#txtMiniEmail').next('.error').show();
                return false;
            }

            $.ajax({
                type: "POST",
                //url: "BackPage.aspx/ValidateEmailID",
                //data: "{email:\"" + $('#txtMiniEmail').val() + "\"}",
                data: "{'email':'" + $('#txtMiniEmail').val() + "'}",
                url: "BackPage.aspx/VerifyClientExistsByEmail",
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (result) {

                    if (result.d.toLowerCase() == "1") {
                        //client is a trail user.
                        $('#txtMiniEmail').next('.error').show();
                        $('#txtMiniEmail').next(".error").html("You are already a registered user. Please login.");
                        $("#btnMiniRegister").prop('disabled', true);

                    }
                    else if (result.d.toLowerCase() == "0") {
                        //no user found.
                        $('#txtMiniEmail').next('.error').hide();
                        $('#txtMiniEmail').next(".error").html("");
                        $("#btnMiniRegister").prop('disabled', false);
                    }
                    else if (result.d.toLowerCase() == "2") {
                        //success.
                        $('#txtMiniEmail').next('.error').show();
                        $('#txtMiniEmail').next(".error").html("You are already a registered user. Please login.");
                        $("#btnMiniRegister").prop('disabled', true);

                    }
                    else {
                        $('#txtMiniEmail').next('.error').show();
                        $('#txtMiniEmail').next(".error").html(result.d.toLowerCase());
                        $("#btnMiniRegister").prop('disabled', true);
                    }

                },
                error: function () {
                    $('#txtMiniEmail').next('.error').show();
                    $('#txtMiniEmail').next(".error").html("Error while validating!");
                    $("#btnMiniRegister").prop('disabled', true);

                }

            });
        });





    });

    function VerifyMobileOTP() {
        
        jQuery.ajax({
            type: 'POST',
            contentType: 'application/json; charset=utf-8',
            url: '../Backpage.aspx/VerifyMobileOTP',
            error: function (xhr, ajaxOptions, thrownError) {
                alert("Error!");
            },
            success: function (result) {
                alert("Success!");
                var Resp = $.parseJSON(result.d);
                alert(Resp[0].Msg);
            }
        });
    }

