<script type="text/javascript">


    function isNumberKey(evt) {

        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode > 31 && (charCode < 48 || charCode > 57))
            return false;

        //CharCodes
        //Number  keyboard  numpad
        //7         55        103
        //8         56        104
        //9         57        105

        if ($("#txtFMobile").val() == "") {
            if (charCode == 55 || charCode == 103 || charCode == 56 || charCode == 104 || charCode == 57 || charCode == 105) {
                //number : 7
                return true;
            }
            else {
                return false;
            }
        }


        return true;
    }

    $(document).ready(function () {

        $("#btnFSubmit").click(function () {

            bchk = true;


            if (requiredfield($('#txtFContactName'), "Please enter Contact Name")) {
                $('#txtFContactName').next(".error").hide();
            } else bchk = false;
            if (requiredfield($('#txtFProductName'), "Please enter Product Name")) {
                $('#txtFProductName').next(".error").hide();
            } else bchk = false;
            if (requiredfield($('#txtFEmail'), "Please enter Email")) {
                $('#txtFEmail').next(".error").hide();
            } else bchk = false;
            if (requiredfield($('#txtFMobile'), "Please enter Mobile")) {
                $('#txtFMobile').next(".error").hide();
            } else bchk = false;
            if (requiredfield($('#txtFCategory'), "Please enter Category")) {
                $('#txtFCategory').next(".error").hide();
            } else bchk = false;
            if (requiredfield($('#txtFSubCategory'), "Please enter Sub Category")) {
                $('#txtFSubCategory').next(".error").hide();
            } else bchk = false;
            if (requiredfield($('#txtFSubject'), "Please enter Subject")) {
                $('#txtFSubject').next(".error").hide();
            } else bchk = false;
            if (requiredfield($('#txtFDescription'), "Please enter Description")) {
                $('#txtFDescription').next(".error").hide();
            } else bchk = false;

            if (bchk == false) {
                return bchk;
            }


            $.ajax({
                type: "POST",

                data: "{'contactName':'" + $('#txtFContactName').val() + "','productName':'" + $('#txtFProductName').val() + "','email':'" + $('#txtFEmail').val() + "','mobile':'" + $('#txtFMobile').val() + "','category':'" + $('#txtFCategory').val() + "','subCategory':'" + $('#txtFSubCategory').val() + "','subject':'" + $('#txtFSubject').val() + "','description':'" + $('#txtFDescription').val() + "'}",
                url: "BackPage.aspx/FeedbackFormSubmit",

                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (result) {
                    if (result.d.toLowerCase() == "true") {
                        $('#lblFSubmitErr').html("Thank you for your Feedback. Please try agian later!");
                    }
                    else if (result.d.toLowerCase() == "false") {
                        $('#lblFSubmitErr').html("Error while saving Feedback. Please try agian later!");
                    }
                    else {
                        $('#lblFSubmitErr').html("Error while saving Feedback. Please try agian later!");
                    }
                },
                error: function () {
                    $('#lblFSubmitErr').html("Error while saving Feedback. Please try agian later!");
                }

            });

            return false;

        });


        $('#txtFEmail').change(function () {


            var input = $('#txtFEmail').val();
            var re = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/;
            var is_email = re.test(input);
            if (is_email) {
                $('#txtFEmail').next('.error').hide();
                $('#txtFEmail').next(".error").html("");
                $("#btnFSubmit").prop('disabled', false);
            }
            else {
                $('#txtFEmail').next('.error').show();
                $('#txtFEmail').next(".error").html("Enter correct e-mail address");
                $("#btnFSubmit").prop('disabled', true);
                return false;
            }



        });


    });
</script>

