        $('.ok_btn').click(function () {
            $('.fg3, .fg2, .fg4,.fg5').hide();
            $('.fg1').show();
        });


        $("#Tick").click(function () {
            if ($(".clsMobile").val() != "") {
                var mobilenofirstchar = $(".clsMobile").val().substring(0, 1);
                if (mobilenofirstchar == "7" || mobilenofirstchar == "8" || mobilenofirstchar == "9") {

                }
                else {
                    $('.fg5').show();
                    $(".clsMobile").val("");
                    return;
                }
            }
            else {
                return;
            }

            $.ajax({
                type: "POST",
                data: "{'contactName':'" + $('#hdnFContactName').val() + "','productName':'" + $('#hdnFProductName').val() + "','email':'" + $('#hdnFEmail').val() + "','mobile':'" + $(".clsMobile").val() + "','category':'" + $('#hdnFCategory').val() + "','subCategory':'" + $('#hdnFSubCategory').val() + "','subject':'" + $('#hdnFSubject').val() + "','description':'" + $('#hdnFDescription').val() + "'}",
                url: "BackPage.aspx/FeedbackFormSubmit",

                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (result) {
                    if (result.d.toLowerCase() == "true") {
                        $('.fg3, .fg2, .fg4,.fg5').hide();
                        $('.fg3').show();
                        $(".clsMobile").val("");                        
                    }
                    else if (result.d.toLowerCase() == "false") {
                        $('.fg4').show();
                        $(".clsMobile").val("");
                    }
                    else {
                        $('.fg4').show();
                        $(".clsMobile").val("");
                    }
                },
                error: function () {
                    $('.fg4').show();
                    $(".clsMobile").val("");
                }
            });
        });
    });
    
