            $('#txtMiniUID').change(function () {


                var input = $('#txtMiniUID').val();
                var re = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/;
                var is_email = re.test(input);
                if (is_email) {
                    $('#spnUID').hide();
                }
                else {
                    $('#spnUID').show();
                    return false;
                }

                $.ajax({
                    type: "POST",
                    //url: "BackPage.aspx/ValidateEmailID",
                    //data: "{email:\"" + $('#txtMiniUID').val() + "\"}",
                    data: "{'email':'" + $('#txtMiniUID').val() + "'}",
                    url: "BackPage.aspx/VerifyClientExistsByEmail",
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    async: true,
                    success: function (result) {

                        if (result.d.toLowerCase() == "1") {
                            //client is a trail user.
                            $(".clsdatepick").hide();
                            $('#txtdatePick').val("01011900");
                                $("#btnMiniLogin").prop('disabled', false);
                            }
                            else if (result.d.toLowerCase() == "0") {
                                //no user found.
                                $(".clsdatepick").hide();
                                $('#txtdatePick').val("");
                            $("#spnUID").show();
                            $("#spnUID").html("You are not registered user. Please register.");
                            $("#btnMiniLogin").prop('disabled', true);

                        }
                        else if (result.d.toLowerCase() == "2") {
                            $(".clsdatepick").show();
                            $('#txtdatePick').val("");
                            $("#spnUID").hide();
                            $("#spnUID").html("");
                            $("#btnMiniLogin").prop('disabled', false);
                        }
                        else {
                            $(".clsdatepick").hide();
                            $("#spnUID").show();
                            $("#spnUID").html("Invalid email!");
                        }
                        },

                    error: function () {
                        $("#spnUID").show();
                        $("#spnUID").html("Error while validating!");
                    }
                });
            });
        });
    </script>


    
