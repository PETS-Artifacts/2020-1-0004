            <script>
                $(document).ready(function () {
                    $('.mgc1').show();
                    $('.mgc2').hide();
                    $('.mgrievancesClick').click(function () {

                        if ($('.mgc1').is(':visible')) {
                            $('.mgc1').hide();
                            $('.mgc2').show();
                            $('.mobile-charms .charm_window_wrap').css('top', '-415px');
                        } else {
                            $('.mgc1').show();
                            $('.mgc2').hide();
                            $('.mobile-charms .charm_window_wrap').css('top', '-313px');
                        }
                    });
                    if ($('.lblfullname').length > 0) {
                        $('#divWelcome').show();
                        $('#divLogin').hide();
                        $("#btnResumeApplication").show();
                        $("#spnLogout").show();
                        $('#spnWelcomeUser').text($('.lblfullname').text())
                    } else {
                        $('#divLogin').show();
                        $('#divWelcome').show();
                        $("#btnResumeApplication").hide();
                        $("#spnLogout").hide();
                        $('#spnWelcomeUser').text('Guest')
                    }
                    $('#mobile-charm-x').hide();
                    $("#link_charm_btn").click(function () {
                        $(".charm_window_wrap").slideDown();
                        $('#login-x').hide();
                        $('#reg-x').hide();
                        $('#reg2-x').hide();
                        $('#reg3-x').hide();
                        $('#reg4-x').hide();
                        $('#reg5-x').show();
                        $('#link_charm_btn').hide();
                        $('#login-panel-1').hide();
                        $('#dvReg').hide();
                        $('#reg-panel-2').hide();
                        $('#reg-panel-3').hide();
                        $('#reg-panel-4').hide();
                        $(".backdrop").fadeIn();
                        $("#dvLoginStatus").hide();
                        $("#dvlnkTradeNow").hide();
                    });
                    $("#mobile_charm_btn").click(function () {
                        $(".charm_window_wrap").slideDown();
                        $('#mobile_charm_btn').hide();
                        $('#mobile-charm-x').show();
                        $(".backdrop").toggle();
                    });
                    $("#mobile-charm-x").click(function () {
                        $(".charm_window_wrap").hide();
                        $('#mobile_charm_btn').show();
                        $("#mobile-charm-x").hide();
                        $(".backdrop").toggle();
                    });
                });
            </script>
            <script>
                $(document).ready(function () {
                    $("#show_footer").click(function () {
                        $(".footer_hide").toggle(500);
                        $(this).toggleClass("up_icon");
                    });
                });
            </script>
            <script>
                function scrollToAnchor(aid) {
                    var aTag = $("a[name='" + aid + "']");
                    $('html,body').animate({ scrollTop: aTag.offset().top }, 'slow');
                }

                $("#show_footer").click(function () {
                    scrollToAnchor('id3');
                });
            </script>
        </div>
    </form>
</body>
</html>
