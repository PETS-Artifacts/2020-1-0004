                    }
                },

                error: function () {
                    //Error while M Resend

                    $("#lblMiniMErr").show();
                    $("#lblMiniMErr").html("Error while resending mobile OTP. Please try agian later");
                }
            });
         });


        $("#btnMiniEVerify").click(function () {

            if ($("#txtMiniEOTP").val() == "") {
                $("#lblMiniEErr").html("Please enter email OTP.");
                return false;
            }

            $("#btnMiniEVerify").attr("disabled", true);
            $.ajax({
                type: "POST",
                
                data: "{'emailotp':'" + $("#txtMiniEOTP").val() + "'}",
                 url: "BackPage.aspx/RegisterMiniEVerifyClick",
                 contentType: "application/json; charset=utf-8",
                 dataType: "json",
                 async: true,
                 success: function (result) {

                     $("#btnMiniEVerify").attr("disabled", false);

                     if (result.d.toLowerCase() == "true") {
                         
                         //alert("true");




                         //window.location.href = "/homepage";


                         $("#dvMiniReg").hide();
                         $("#dvMiniMobileOTP").hide();
                         $("#dvMiniEmailOTP").hide();
                         $("#dvMiniThanks").hide();

                         $("#miniregx").hide();
                         $("#minireg2x").hide();
                         $("#minireg3x").hide();
                         $("#minireg4x").hide();
                         $("#minireg5x").hide();

                         $("#dvReg").hide();
                         $("#dvMOTP").hide();
                         $("#dvEOTP").hide();
                         $("#dvCongrats").show();
                         $("#dvComplete").hide();
                         $("#dvResume").hide();
                         $("#dvRegistered").hide();

                         $(".backdrop").fadeOut();


                     }
                     else if (result.d.toLowerCase() == "false") {
                         $("#lblMiniEErr").html("Please enter Correct OTP.");

                         $("#dvMiniReg").hide();
                         $("#dvMiniMobileOTP").hide();
                         $("#dvMiniEmailOTP").show();
                         $("#dvMiniThanks").hide();

                         $("#miniregx").hide();
                         $("#minireg2x").hide();
                         $("#minireg3x").show();
                         $("#minireg4x").hide();
                         $("#minireg5x").hide();
                         $(".backdrop").fadeIn();
                        

                     }
                     else {
                         
                         $("#dvMiniReg").hide();
                         $("#dvMiniMobileOTP").hide();
                         $("#dvMiniEmailOTP").show();
                         $("#dvMiniThanks").hide();

                         $("#miniregx").hide();
                         $("#minireg2x").hide();
                         $("#minireg3x").show();
                         $("#minireg4x").hide();
                         $("#minireg5x").hide();
                         $(".backdrop").fadeIn();
                     }

                 },
                 error: function () {
                     $("#lblMiniEErr").html("Error while verifying OTP.");
                     $("#dvMiniReg").hide();
                     $("#dvMiniMobileOTP").hide();
                     $("#dvMiniEmailOTP").show();
                     $("#dvMiniThanks").hide();

                     $("#miniregx").hide();
                     $("#minireg2x").hide();
                     $("#minireg3x").show();
                     $("#minireg4x").hide();
                     $("#minireg5x").hide();
                     $(".backdrop").fadeIn();

                     $("#btnMiniEVerify").attr("disabled", false);
                    

                 }

             });
       

            
            return false;

        });

