<script type="text/javascript">
    $(document).ready(function () {
        
                
            function AjaxCall(url, data, successEvent, failureEvent) {
                $.ajax({
                    async: true,
                    type: "POST",
                    url: url,
                    data: data,
                    contentType: "application/json; charset=utf-8",
                    dataType: "json",
                    success: successEvent,
                    failure: failureEvent
                });
            }

            $("#btnResumeApplication").click(function () {
                AjaxCall("BackPage.aspx/lnkResumeApp", "", SuccessResumeApplication, FailureResumeApplication);
                return false;
            });

            function SuccessResumeApplication(response) {
                window.location.href = response.d;
            }
            function FailureResumeApplication() {
                alert('Error while Resume!')
            }

            $("#spnLogout").click(function () {
                AjaxCall("BackPage.aspx/fn_Logout", "", SuccessLogout, FailureLogout);
                return false;
            });
            function SuccessLogout(response) {
                window.location.href = "/homepage";
            }
            function FailureLogout() {
                alert('Error while logout!')
            }
    });
</script>
