<script>
    $(document).ready(function () {
        $('.fg2, .fg3 ,.fg4,.fg5').hide();
        $('#MiniMobileGetCall').click(function () {
            $('.fg1, .fg3, .fg4,.fg5').hide();
            $('.fg2').show();
        });
        $('#MiniMobileCancel').click(function () {
            $('.fg3, .fg2, .fg4,.fg5').hide();
            $('.fg1').show();
        });
        $('.ok_btn').click(function () {
            $('.fg3, .fg2, .fg4,.fg5').hide();
            $('.fg1').show();
        });


        $("#MiniMobileTick").click(function () {
            if ($(".clsMobileM").val() != "") {
                var mobilenofirstchar = $(".clsMobileM").val().substring(0, 1);
                if (mobilenofirstchar == "7" || mobilenofirstchar == "8" || mobilenofirstchar == "9") {

                }
                else {
                    $('.fg5').show();
                    $(".clsMobileM").val("");
                    return;
                }
            }
            else {
                return;
            }

            $.ajax({
                type: "POST",
                data: "{'contactName':'" + $('#hdnFContactName').val() + "','productName':'" + $('#hdnFProductName').val() + "','email':'" + $('#hdnFEmail').val() + "','mobile':'" + $(".clsMobileM").val() + "','category':'" + $('#hdnFCategory').val() + "','subCategory':'" + $('#hdnFSubCategory').val() + "','subject':'" + $('#hdnFSubject').val() + "','description':'" + $('#hdnFDescription').val() + "'}",
                url: "BackPage.aspx/FeedbackFormSubmit",

                contentType: "application/json; charset=utf-8",
                dataType: "json",
                async: true,
                success: function (result) {
                    if (result.d.toLowerCase() == "true") {
                        $('.fg3, .fg2, .fg4,.fg5').hide();
                        $('.fg3').show();
                        $(".clsMobileM").val("");
                    }
                    else if (result.d.toLowerCase() == "false") {
                        $('.fg4').show();
                        $(".clsMobileM").val("");
                    }
                    else {
                        $('.fg4').show();
                        $(".clsMobileM").val("");
                    }
                },
                error: function () {
                    $('.fg4').show();
                    $(".clsMobileM").val("");
                }
            });
        });
    });

</script>
