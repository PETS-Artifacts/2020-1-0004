       <header class="header large-menu" role="banner"><script type="text/javascript">
    function setCookieAndClose(name,value,days) {
        var expires = "";
        if (days) {
            var date = new Date();
            date.setTime(date.getTime() + (days*24*60*60*1000));
            expires = "; expires=" + date.toUTCString();
        }
        document.cookie = name + "=" + (value || "")  + expires + "; path=/";
        var cbanner = document.querySelector(".cbannercontainer-" + name);
        if (cbanner && cbanner.remove) { cbanner.remove(); }
    }
</script>



