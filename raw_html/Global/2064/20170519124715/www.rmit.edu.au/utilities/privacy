<div class="standard-content container">
	<!-- To be used for TOP Row Component <div class="standard-content-top-row row"> -->
	<div class="standard-content-top-row row">
	<div class="container">
		


    

      <header class="article-masthead row no-vertical-margin">
        <div class="masthead col-xs-8 pull-left">
            <!-- navigation object : Masthead Detail (Related Content Advanced) -->

  			<p class="subtitle"></p>
  			<h1 class="">RMIT Privacy Policy Statement</h1>

		</div>

	</header>
	


	</div>
	</div>

	<div class="standard-content-main-row row mb-md-md-xs">
		<div class="standard-content-detail col-xs-12 col-md-8">
			<!-- Includes share icon -->
             
                <div class="standard-content-meta mb-md-md-xs clearfix not-hide">
                    <!-- Share Icon -->
                        



  <div class="m-share-wrapper not-hide"> 
    <!-- Include share -->
    <div class="share">
      <div class="btn-group"> <a href="#" class="dropdown-toggle no-outline" data-toggle="dropdown"> <span> <span class="meta no-outline">Share</span> <span class="grunticon-share-blk1F-15 c-icon-circle no-outline"></span> </span> </a>
        <ul class="dropdown-menu share-popup">
          <li> <a href="#" class="cta share-fb" title="Share on Facebook"><span class="grunticon-facebook-fullcol-15"></span>Share</a> </li>
          <li> <a href="#" class="cta share-twitter" title="Share on Twitter" target="_blank"><span class="grunticon-twitter-fullcol-15"></span>Tweet</a> </li>
          <li> <a href="#" class="cta share-email" title="Email" target="_blank"><span class="grunticon-mail-gry6-15-38"></span></span>Email</a> </li>
        </ul>
      </div>
