            <script src="https://maps.googleapis.com/maps/api/js?libraries=places&key=AIzaSyB2YOYgJPxz1kcB_9IjawZaWwvA7xvJnBU&v=3.26"></script>

        <script src="/js/compiled/standard_layout_javascript.js?version=---November-4-2017-3-36-am"></script>
    
    <script>
        // Make sure labroots site is not embedded in another iframe 
        $(function() {
            if (window.location !== window.top.location) {
                window.top.location = window.location;
            }
        });
        
        // Prevent forms from being double submitted
        $(function(){ 

            if(jQuery().preventDoubleSubmission) {
            	$('form').preventDoubleSubmission();
            }
            
        });
        
        // Address auto complete with Google's geocomplete API and jquery plugin
        $(function(){
            
            initGeoComplete = function() {
            
                var userDetailAddressFieldObj = $("input[name$='[address1]']");
                
                if (userDetailAddressFieldObj.length) {
                    
                    userDetailAddressFieldObj.geocomplete({
                        details: "form",
                        detailsAttribute: "data-geo"
                    })
                    .bind("geocode:result", function(event, result) {
                        $('#registration_question_userDetail_country').change();
                        var streetData = result.address_components.reduce(function(address, part) {
                            if( part.types == 'street_number' || part.types == 'route') {
                                return address + ' ' + part.long_name;
                            }

                            return address;
                        }, '');

                        $('#registration_question_userDetail_address1').val(streetData);
                    });
                
                }
            
            }
            
            initGeoComplete();
            
            $(document).on("focus", "input[name$='[address1]']", function() {
            
                initGeoComplete();            
            });

            $('body').on('change', '#registration_question_userDetail_country', function() {
                
                // If we aren't in USA or Cananda, let's hide the state
                if (this.value != "United States" && this.value != "Canada") {
                    $('#registration_question_userDetail_state')
                        .val("Outside US/Canada")
                        .parent()
                        .css('height', '0')
                        .css('overflow', 'hidden');
                } else {
                    $('#registration_question_userDetail_state')
                        .parent()
                        .css('height', 'auto');
                }
            });
             
        });
        
    </script>



