}
function emailImage(email,domain,source,style,alt) {
	document.write("<A HREF = mailto:" + email + "@" + domain + "><IMG SRC=\"" + source+ "\" STYLE=\"" + style + "\" alt=\"" + alt + "\"></A>");
}
function emailFull(email,domain,text,subject,body) {
	document.write("<A HREF = mailto:" + email + "@" + domain + "?Subject=" + subject + "&body=" + body + ">" + text + "</A>");
}
</SCRIPT>
<!-- end DAG custom scripts -->

