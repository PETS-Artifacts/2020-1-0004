        function loadAsync(url, where) {
            var script = document.createElement('script');
            script.async = true;
            script.type = 'text/javascript';
            script.src = url;
            if (where == 'head') {
                script.addEventListener('error', scriptError, false);
                var node = document.getElementsByTagName('script')[0];
                node.parentNode.insertBefore(script, node);
            } else {
                document.body.appendChild(script);
            }
        }
        function _deliverAd() {
            adDelivered = true;
            for (var tag in inHouseTags) {
                if(document.getElementById(tag)) {
                    var doc = document.getElementById(tag).contentWindow.document;
                    var styleSheet = '<style>body {margin:0;}</style>';
                    doc.open();
                    if (htAds.hasOwnProperty(tag)) {
                        doc.write(styleSheet + htAds[tag]);
                        doc.close();
