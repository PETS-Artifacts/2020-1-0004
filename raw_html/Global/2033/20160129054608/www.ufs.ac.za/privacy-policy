 function bindSpotlight() {
 jQuery.support.cors = true;
            $.getJSON("http://api.ufs.ac.za/media/api/spotlight/eng/0/0/Width637_X_Height235",
             function (data) {
 
                 var readmore = 'Read More';
                 var readmoreclass = 'SpotlightWhiteReadMore';
                 $('#ContentSlider').empty(); // Clear the table body.
                 var table = "";
                 // Loop through the list of products.
                 var count = 0;
                 if (data.length > 0) {
                     table += ("<ul class='bxslider'>");
                 }
                 $.each(data, function (key, val) {
 
				 var div = document.createElement("div");
				 div.innerHTML = val.Description;
var title = val.Heading;
 				 var text = div.textContent || div.innerText || "";
 
                    var caption ="<div class='CatHeadingSpotlight'>"+title+"</div><div class='bx-caption'><div class='SpotDescription'>"+  text + "</div>";
                     if (val.Link != "") {
                         caption +="<a class='" + readmoreclass + "' href='" + val.Link + "'>" + readmore + "</a>";
                     }
 caption += "</div>";
                     table += ("<li><img src='" + val.Image + "' />" + caption + "</li>");
 
                 });
                 if (data.length > 0) {
                     table += ("</ul>");
                 }
 
                 $("#ContentSlider").html(table);
			 if (data.length > 1)
				 {
	                 $(".bxslider").bxSlider({
                     auto: true,
                     autoDelay: 2000,
                     pause: 5500,
                     autoControls: true,
                     captions: true,
                     mode: 'fade'
                 });				 
				 }
				 else
				 {
                 $(".bxslider").bxSlider({
                     auto: false,
                     autoDelay: 0,
                     pause: 0,
                     autoControls: true,
                     captions: true,
                     mode: 'fade'
                 });					 
				 }             });
        }
 
        $(document).ready(function (e) {
            if (window.jQuery === undefined) window.$ = window.jQuery = jQuery;
            bindSpotlight();
 
        });
</script>


