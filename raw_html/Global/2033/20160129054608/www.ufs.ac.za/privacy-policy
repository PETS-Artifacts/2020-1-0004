<div class='sfContentBlock'><script>
    function PopulateNews() {
        $.getJSON("http://api.ufs.ac.za/media/api/news/actions/GetLatestNews/eng/4",
         function (data) {
             var newspage = "/templates/news-archive-item?news=";
             var readmore = 'Read More';
             var readmoreclass = 'SpotlightWhiteReadMore';
             $('#NewsContainer').empty(); // Clear the table body.
             var table = "";
             // Loop through the list of products.
             var count = 0;
             if (data.length > 0) {
                 table += ("<div class='NewsContainer_Left'>");
				 table += ("<div class='LatestNewsHeading'>Latest News</div>");				 
             }
             $.each(data, function (key, val) {
                 table += ("<div class='NewsItemArticle_Left'>");
                 table += ("<div class='NewsHeading_Left'><a href='" + newspage + val.Id + "'>" + val.Heading + "</a></div>");
                 table += ("<div class='NewsDescription_Left'>" + val.ShortDescription + "</div>");
                 table += ("</div>");
             });
             if (data.length > 0) {
                 table += ("</div>");
             }
 
             $("#NewsContainer_Left").html(table);
         });
    }
 
    $(document).ready(function (e) {
        jQuery.support.cors = true;
        if (window.jQuery === undefined) window.$ = window.jQuery = jQuery;
        PopulateNews();
    });
    function getParameterByName(name) {
        name = name.replace(/[\[]/, "\\[").replace(/[\]]/, "\\]");
        var regex = new RegExp("[\\?&]" + name + "=([^&#]*)"),
         results = regex.exec(location.search);
        return results == null ? "" : decodeURIComponent(results[1].replace(/\+/g, " "));
    }
</script> <div id="NewsRoomNews_Left"> <div id="NewsContainer_Left"></div> </div>


