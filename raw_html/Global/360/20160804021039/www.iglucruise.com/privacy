    <script type="text/javascript">

        Shadowbox.init();

            //remove anchor text and display the element
            function removeAnchoreText() {
                $(".mobInfinityIcn").text('');
                $(".mobInfinityIcn").show();
                $(".statusContainer").show();
            }

            //add function to last in queue to execute for element
            $('.mobInfinityIcn').delay(1000).queue(function () {
                removeAnchoreText();
                $(this).dequeue();
            });


        
        $(function () { 
            var divPhoneNumber = $("#callUsContainer");
			var divPhoneNumberPlaceHolder = $("#callUsPlaceHolder");
			var mainContainer = $("#mainContainer");
			var $window = $(window); // going to use in the event a lot...

			var ieV = 100;
			if ($.browser.msie) {
				ieV = parseInt($.browser.version, 10);
			}
			if (ieV > 6) {
				
				var fixedLeft = divPhoneNumberPlaceHolder.offset().left;
				var placeHolderTop = divPhoneNumberPlaceHolder.offset().top;
				$window.scroll(function () {
					var wScollTop = $window.scrollTop();
					

					if (wScollTop > placeHolderTop && (ieV < 10 || !divPhoneNumber.hasClass("fixedPhone") )) {
						divPhoneNumber.addClass("fixedPhone");
                        divPhoneNumber.css("left", fixedLeft-6).css("top", 1).css("top", 0);
					}
					else if (wScollTop < placeHolderTop && divPhoneNumber.hasClass("fixedPhone")) {
						divPhoneNumber.removeClass("fixedPhone");
						divPhoneNumber.css("left", "").css("top", "");
					}
				}); 

				$window.resize(function () {
					fixedLeft = divPhoneNumberPlaceHolder.offset().left;
					if (divPhoneNumber.hasClass("fixedPhone")) {
						divPhoneNumber.css("left", fixedLeft-6);
					}
				});
			}
        });

        // for mobile
        if (screen && screen.width <= 667) {
            document.write('<script type="text/javascript" src="/script/mobile/mobile.js?v=3"><\/script>');
        }
         
        // for counter
        if (document.getElementById('counter')) {

            var head = document.getElementsByTagName('head')[0];
            
            //css
            var lnk = document.createElement('link');
            lnk.type = 'text/css';
            lnk.href = '/css/counter/counter.css';
            lnk.rel = 'stylesheet';
            lnk.enableviewstate = 'false';

            //js
            var js = document.createElement("script");
            js.type = "text/javascript";
            js.src = "/script/counter/counter.js";

            head.appendChild(lnk);
            head.appendChild(js);
        }
       
 

    </script>
    
    
