	$(function(){
				 
			         
			        new BootstrapDialog({
			            type: BootstrapDialog.TYPE_DEFAULT,
			            size: BootstrapDialog.SIZE_NORMAL,
			            closable: true,						
						draggable: true,
			            title   :   '',
