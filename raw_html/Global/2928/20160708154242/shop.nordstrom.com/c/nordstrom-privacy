    (function() {
        // Anniversary Flag
        var settings = window.nord.config.settings;
        var categoryPath = settings.analytics.categoryPath;
        var qualifiedShopper = settings.shopper.isEarlyAccessEligible;
        var isAnniversaryCategory = categoryPath && categoryPath.split("~")[2] === "60163514";

        if (isAnniversaryCategory && !qualifiedShopper) {
            window.nord.core.dispatcher.dispatch({
                "action":"GlobalConfigPageFeaturesSync",
                "IsAnniversaryPage":true
            });
        }
