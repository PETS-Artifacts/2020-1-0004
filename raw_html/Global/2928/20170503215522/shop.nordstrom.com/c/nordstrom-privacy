	function getViewport() {
		var
		obj = window,
		propertyPrefix = 'inner';

		if (! window.innerWidth) {
			obj = document.documentElement || document.body;
			propertyPrefix = 'client';
		}

		return {
			"height": obj[propertyPrefix + 'Height'],
			"width": obj[propertyPrefix + 'Width']
		};
	}

