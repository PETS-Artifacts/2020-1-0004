    <script>
    // SITE-766 ---  ensure YT videos stop playing when the AdFrames unit is expanded
	function addApiToVideos() {
	    //find all iframes
	    var ifs = document.getElementsByTagName("iframe");
	    //each iframe
	    for (var i in ifs) {
	        var iframe = ifs[i];
	        //if it has a src
	        if (iframe.src) {
	            //assume we don't already have a query
	            var append = "?";
	            //if we find a ?, we use a &
	            if (iframe.src.search(/\?/) != -1) {
	                append = "&";
	            }
	            //if it's a vimeo page
	            if (iframe.src.search(/^http[s]?:\/\/.+\.vimeo.com/) != -1 && iframe.src.search("api=1") == -1) {
	                //reload the video with the new query
	                iframe.src += append + "api=1";
	            }
	            //if it's a youtube page
	            else if (iframe.src.search(/^http[s]?:\/\/.+\.youtube.com/) != -1 && iframe.src.search("jsapi=1") == -1) {
	                //reload the video with the new query
	                iframe.src += append + "jsapi=1";
	            }
	        }
	    }
	}
	addApiToVideos();
	
	/* FIX VIDEO HEIGHT */
	(function($) {
		// Find all YouTube videos
		var $allVideos = $("iframe[src^='//www.youtube.com']"),

			// The element that is fluid width
			$fluidEl = $("body");

		// Figure out and save aspect ratio for each video
		$allVideos.each(function() {

		  $(this)
			.data('aspectRatio', this.height / this.width)

			// and remove the hard coded width/height
			.removeAttr('height')
			.removeAttr('width');

		});

		// When the window is resized
		$(window).resize(function() {

		  var newWidth = $fluidEl.width();

		  // Resize all videos according to their own aspect ratio
		  $allVideos.each(function() {

			var $el = $(this);
			$el
			  .width(newWidth)
			  .height(newWidth * $el.data('aspectRatio'));

		  });

		// Kick off one resize to fix all videos on page load
		}).resize();	
	})(jQuery);	
	
	
	
	</script>
	
