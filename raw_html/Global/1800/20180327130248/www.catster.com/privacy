    	<script>
			//jQuery(".gimmemore > li:nth-child(3)").after();

	    	jQuery('#mce-footer-EMAIL').keypress(function(e) {
				if (e.which == 13) {
					e.preventDefault();
					var email = jQuery("input#mce-footer-EMAIL").val();
					subscribe(email);
				}
			});


		    jQuery('#footer-subscribe').click(function(e) {
				e.preventDefault();
				var email = jQuery("input#mce-footer-EMAIL").val();
				subscribe(email);
			});

			function subscribe(sEmail){
				if ($.trim(sEmail).length == 0) {
					errormessage(2);
					return false;
		        }
		        else if (validateEmail(sEmail)) {
					$.ajax({
						type: "GET",
					    crossDomain: true,
					    jsonp: false,
						url: " /_orion/.hydra_services/mailchimp/listSubscribe",
						data: { email: sEmail, list_id: "{{ _conf.mailchimp_list }}", MERGE4 : "{{ _conf.mailchimp_merge }}" },
						success:function(data){

							if(data.length == 0){
								errormessage(1);
								return false;
							}
							else{
								var json = data,
									obj = JSON.parse(data);
								if(obj.error == "false"){
									errormessage(0);
									return false;
								}
								else{
									errormessage(2);
								}
							}
						},
						error: function () {
							errormessage(2);
						}
					});
					return false;
		        }
		        else {
					errormessage(2);
					return false;
		        }
			}

			function errormessage(num){
				if(num == 0){
					jQuery('div#newsletter-column')
						.html('<h2>Thank you for subscribing!</h2><p>Just one more step! To start your subscription to the Catster Meow, please click the link in the email we just sent you.<p>')
						.hide()
						.fadeIn(1750);
				}
				else if(num == 1){
		            jQuery('#email-message-footer')
		            	.html('<img src="http://www.catster.com/wp-content/themes/catster/assets/images/error.png" width="17px"/> You are already on our mailing list!')
						.fadeIn('fast')
						.delay(1800)
						.fadeOut('slow');
				}
				else{
		            jQuery('#email-message-footer')
		            	.html('<img src="http://www.catster.com/wp-content/themes/catster/assets/images/error.png" width="17px"/> Invalid email address. Please try again.')
						.fadeIn('fast')
						.delay(1800)
						.fadeOut('slow');
				}
			}

			function validateEmail(sEmail) {
			    var filter = /^([\w-\.]+)@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
			    if (filter.test(sEmail)) {
			        return true;
			    }
			    else {
			        return false;
			    }
			}
	    </script>

    <!--Start image overlay Sharing Javascript -->
    <script type="text/javascript">
        function pinIt(e,obj) {
            e.preventDefault();
            popupwindow = window.open(jQuery(obj).data('gotourl'),'dummy',"width=700,height=350");
            if (window.focus) {popupwindow.focus()}
            return false;
        }

        function shareIt(e,obj) {
            e.preventDefault();
            popupwindow = window.open(jQuery(obj).data('gotourl'),'dummy',"width=600,height=450");
            if (window.focus) {popupwindow.focus()}
            return false;
        }

        function overlay_display(obj) {
            jQuery(obj).find('div.image_overlay').css('visibility','visible');
        }

        function overlay_hide(obj) {
            jQuery(obj).find('div.image_overlay').css('visibility','hidden');
        }
    </script>
    <!--End image overlay Sharing Javascript -->

