<script type="text/javascript">
$(document).ready(function(){
/******************************************
this is for the side nav li's to do the following: 

-it will only give the selected class to the current page you are on
-it will give the class 'down' (for arrow styling) to all the parent pages that have children
-it will give the class 'down' to current page li if it has children
-it will hide all other children pages not in the scope of the current page and its parents

******************************************/
	var list = $('.side-nav.hc1 ul li'),
		selected = $('.side-nav.hc1 ul li.selected'),
		currentPage = selected.last();
		
		//returns the current class of element
		function findCurrentLevelClass(elSelector){
			//grabs the classes of the current selected li and puts into array
			var classes = $(elSelector).prop('class').split(' ');
			var levelClass;
			
			//grabs the current level class eg. 'level2'
			$.each(classes, function(index, value){
				if(value.includes('level')){
					levelClass = value;
				}
			});
			
			return levelClass;
		}
		
		//returns the class of next or previous level depending on amount
		function findLevel(classSelector, direction, amount){
			var split = classSelector.slice(5);
			var nextClassNum = direction == 'next' ? parseInt(split) + amount : parseInt(split) - amount;
			var nextClass = 'level' + nextClassNum.toString();
			return nextClass;
		}
		
		//returns the number of the level above or below the given amount
		function findLevelNum(classSelector, direction, amount){
			var split = classSelector.slice(5);
			var nextClassNum = direction == 'next' ? parseInt(split) + amount : parseInt(split) - amount;
			return nextClassNum;
		}
	
	//timeout created to wait for CMS javascript to run and add the selected classes
	setTimeout(function(){
		//grabs index of first selected li and the last selected li
		var firstIndex = list.index($('li.selected:first'));
		var lastIndex = list.index($('li.selected:last'));
		
		//holds an array of all the indexes of all the selected li
		var selectedArray = [];
		list.each(function(){
			if($(this).hasClass('selected')){
				selectedArray.push($(this).index());
			}
		});
		
		var currentSIndex = null;
		var continueFunction = true;
		var foundLast = false;
		//runs through the whole list to figure out which ones to hide or set to class 'down'
		list.each(function(index, el){
			//will continue this function as long as its still in a level other than 'level1'
			if(continueFunction){
				if(currentSIndex !== null){
						var currentParent = list[currentSIndex];
						var currentParentClass = findCurrentLevelClass(currentParent);
						var childClass = findLevel(currentParentClass, 'next', 1);
						var nextTwoClass = findLevel(currentParentClass, 'next', 2);
						var prevClass = findLevel(currentParentClass, 'prev', 1);
						var prevTwoClass = findLevel(currentParentClass, 'prev', 2);
						var parentHasChildren = findCurrentLevelClass(list[currentSIndex + 1]) == childClass;
						var currentLvlNum = findLevelNum(currentParentClass, 'next', 0);
						var itemLvlNum = findLevelNum(findCurrentLevelClass($(this)), 'next', 0);
						var foundLastSelected = currentSIndex == selectedArray[selectedArray.length - 1];
					}
				
				//will run as long as it has not found its last selected item	
				if(!foundLast){
					//hides any children that are showing before we hit the scope of the selected parents
					if(currentSIndex == null && !$(this).hasClass('level1')){
						$(this).hide();
					}
					//if current item is 'selected' it will check if needs to add 'down' class and then resets the currentSIndex to current index
					else if($(this).hasClass('selected')){
						if($(this).hasClass('minus')){
							$(this).addClass('down');
						}
						currentSIndex = $(this).index();
					}
					//if it isn't selected checks the rest of the logic
					else if(!$(this).hasClass('selected')){
						//hides any grandchildren of current scope
						if($(this).hasClass(nextTwoClass)){
							$(this).hide();
						}
						//if its already on the last selected li
						else if (foundLastSelected){
							//checks if it is already level1, then bumps you out and sets foundLast = true
							if(prevClass == 'level0' && $(this).hasClass('level1')){
								foundLast = true;
							}
							
							else if(parentHasChildren){
								//if item is higher or same as current level of the last selected li
								if(itemLvlNum <= currentLvlNum){
									foundLast = true;
									currentSIndex = $(this).index();
								}
							}
							else if(!parentHasChildren){
								//if item is less than current level of last selected li
								if(itemLvlNum < currentLvlNum){
									foundLast = true;
									currentSIndex = $(this).index();
								}
							}
						}
					}
				}
				else{
					//at this point it is going backwards in the levels and hiding any children of current level
					if(itemLvlNum > currentLvlNum){
						$(this).hide();
					}
					// and reseting the currentSIndex if the level is lower than the current level (eg. level1 < level2)
					else if (itemLvlNum < currentLvlNum){
						currentSIndex = $(this).index();
					}
					//once you reach level1, it jumps you out of the function......
					else if($(this).hasClass('level1')){
						continueFunction = false;
					}
				}
			}
			else {
				//hides everything else that is not level1
				if(!$(this).hasClass('level1')){
					$(this).hide();
				}
			}
			
		});
		
		//removes the selected class from all of them
		list.removeClass('selected');
		//add the 'selected' class only to the current page li
		if(selectedArray.length > 0){
			$('.side-nav.hc1 ul').find('li:eq(' + lastIndex + ')').addClass('selected');
		}
		
	}, 100);
});</script>
