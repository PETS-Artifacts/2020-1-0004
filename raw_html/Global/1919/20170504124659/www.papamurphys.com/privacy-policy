	<script>
	window.dataLayer=window.dataLayer || [];
	$('document').ready(function(){
		
		if($('.order-page-identifier').length > 0){	
			
			
			(function(){
				
				var userType = document.getElementById('userType').value;
				var obj = {};
				var dataLayerObject = {};
				var dataArr = [];
				var orderDataObject =  JSON.parse($('my-text').html());
				
				dataLayerObject.ecommerce = {};
				
				
				dataLayerObject.ecommerce.purchase = {};
				
				dataLayerObject.ecommerce.purchase.actionField = {};
				dataLayerObject.ecommerce.promoView = {};
				
				dataLayerObject.ecommerce.promoView.promotions = {};
				
				dataLayerObject.ecommerce.purchase.actionField["id"] = orderDataObject.code;
				dataLayerObject.ecommerce.purchase.actionField["affiliation"] = orderDataObject.storeName;
				dataLayerObject.ecommerce.purchase.actionField["revenue"] = orderDataObject.revenue;				
				dataLayerObject.ecommerce.purchase.actionField["tax"] = orderDataObject.tax;
				dataLayerObject.ecommerce.purchase.actionField["shipping"] = orderDataObject.shipping;
				
				
				//var entries = ;
				Array.prototype.forEach.call(orderDataObject.products,function(value,index){
					obj = {};					
					obj["id"] = value.code;
					obj["name"] = value.name;
					//obj["category"] = value.product.categories[0].name;
					obj["price"] = value.price;
					obj["quantity"] = value.quantity;
					
					
					
					dataArr.push(obj);
				});
				
				
				
				dataLayerObject["dimension1"] = orderDataObject.dimension1;
				dataLayerObject["dimension2"] = userType;
				dataLayerObject["dimension3"] = orderDataObject.dimension3;
				dataLayerObject["metric1"] = orderDataObject.metric1;
				dataLayerObject.ecommerce.purchase["products"] = dataArr;
				
				
				dataLayerObject["event"] = "TransactionComplete";
					 
			
				
				
				
				
			
				
				
				
				
				window.dataLayer.push( dataLayerObject);
				
	        })()
		}
		
	})
	
	
	
		 
	</script>	
 

				

 
