<script type="text/javascript">
    /* <![CDATA[ */
    var HawkSearchUrl = 'http://manage.hawksearch.com/sites/apics';
    var BaseHawkSearchUrl = '/searchproxy';
    if ("https:" == document.location.protocol) {
        HawkSearchUrl = HawkSearchUrl.replace("http://", "https://");
        BaseHawkSearchUrl = BaseHawkSearchUrl.replace("http://", "https://");
    }

    if (HawkSearch === undefined) {
        var HawkSearch = {};
        HawkSearch.TrackingUrl = HawkSearchUrl;
        HawkSearch.BaseUrl = BaseHawkSearchUrl;
    }

    HawkSearch.initAutoSuggest = function () {
        HawkSearch.suggestInit('#txtKeyword', {
            lookupUrlPrefix: HawkSearchUrl + '?fn=ajax&f=GetSuggestions',
            hiddenDivName: '',
            isAutoWidth: false
        });
    };

    function SetHdnKeyword() {
        $("#hdnhawkkeyword").val($('#txtKeyword').val());
        return true;
    };

    function ViewDetail() {
        var url = window.location.href;
        $.ajax({
            type: "GET",
            url: '/ae-test/portfolio/Ajax.aspx?ListingUrl=' + encodeURIComponent(url),
            async: false,
            success: function (returnedData) {
                console.log(returnedData);
            }
        });
        document.cookie = "detail=" + true;
        if (detectIE() && window.location.href.indexOf("#") > 0) {
            document.cookie = "ieback=" + true;
        }
    };
    /* ]]> */
    $(document).keypress(function (e) {
        if (e.which == 13) {
            if (e.srcElement == document.getElementById('user-location')) {
                e.preventDefault();
                changeLocation();
            } else if (e.srcElement == document.getElementById('txtKeywordPage')) {
                e.preventDefault();
                onButtonSearchClickPage();
            } else if (e.srcElement != document.getElementById('txtKeyword')) {
                e.preventDefault();
            }
        }
    });

    function onButtonSearchClick(keyword) {
        var Url = "/search";
        var pos = Url.indexOf("?");
        if (pos != -1) {
            Url = Url.substring(0, pos);
        }

        if (!keyword) {
            keyword = document.getElementById('txtKeyword').value;
        }

        window.location.href = window.location.origin + Url + "#keyword=" + keyword;
        fillSearchBoxes();
        return false;
    }

    function getParameterByName(name) {
        var url = window.location.href.toLowerCase();
        name = name.replace(/[\[\]]/g, "\\$&");
        var regex = new RegExp("[?&]" + name + "(=([^&#]*)|&|#|$)"), results = regex.exec(url);
        if (!results) {
            regex = new RegExp("[#&]" + name + "(=([^&#]*)|&|#|$)"), results = regex.exec(url);
        }
        if (!results) return null;
        if (!results[2]) return '';
        return decodeURIComponent(results[2].replace(/\+/g, " "));
    }

    function fillSearchBoxes() {
        var searchwithin = getParameterByName("searchwithin");
        var keyword = getParameterByName("keyword");
        if (document.getElementById('txtKeywordPage')) {
            if (searchwithin) {
                document.getElementById('txtKeywordPage').value = searchwithin;
                document.getElementById('hdnSearchWithin').value = searchwithin;				
            } else if (keyword){
                document.getElementById('txtKeywordPage').value = keyword;
            } else {
                document.getElementById('txtKeywordPage').value = "";
            }
        }
    }

    $(document).ready(function () {
        fillSearchBoxes();
        $("#hawkdistanceselection").hide();		
    });

</script>

