 }</script><script type="text/javascript">$(document).ready(function(){ 

 /* header links tracker */
$('.header-top .header-top-inner .header-links > li:not(#wantTo) > a').click(function() {
          var headerlinkitem = $(this).html();
          ga('apicsTracker.send', 'event', 'Header Links', 'Click', headerlinkitem);      
});


/* i want to tracker */
$('.header-top .header-top-inner .header-links li#wantTo > ul > li').click(function() {
          var iwanttoitem = $(this).children('a').html();
          ga('apicsTracker.send', 'event', 'I Want To', 'Click', iwanttoitem);      
});

/* megamenu tracker */
$('nav:not(.magazine-menu) > ul.cm-menu .custom-sub a').click(function() {
     var toplevel = "";
     //if its a toplevel link
     if ($(this).parents(".link-landing").length > 0){
            toplevel = $(this).parents(".link-landing").find("a span").html();
            ga('apicsTracker.send', 'event', 'Megamenu', 'Top Level:' + toplevel, toplevel);      
     }    
     //if its a dropdown, not a link
     else if (!$(this).hasClass("has-sub")){
            var megamenuitem = $(this).text();
            toplevel = $(this).parents(".container").siblings(".link-landing").find("a span").html();
            ga('apicsTracker.send', 'event', 'Megamenu', 'Top Level:' + toplevel, megamenuitem);      
     }  
});
    //if its myapics
    $('ul.cm-menu li[data-type="mega"] > a[href*="myapics"]').click(function() {
            ga('apicsTracker.send', 'event', 'Megamenu', 'Top Level:My APICS', 'My APICS');
    });
  
/* footer tracker */
$('.footer-bottom a').click(function() {
     var toplevel = "";
     //if its a toplevel link
     if ($(this).parents(".footer-heading").length > 0){
            toplevel = $(this).parents(".footer-heading").find("a span").html();
     
       ga('apicsTracker.send', 'event', 'Footer', 'Top Level:' + toplevel, toplevel);        
     }    
     //if its a dropdown, not a link
     else if (!$(this).hasClass("has-sub")){
            var footeritem = $(this).find("span").html();
            toplevel = $(this).parents(".footer-links").siblings(".footer-heading").find("a span").html();
        
       ga('apicsTracker.send', 'event', 'Footer', 'Top Level:' + toplevel, footeritem);      
     }  
});
});</script>
