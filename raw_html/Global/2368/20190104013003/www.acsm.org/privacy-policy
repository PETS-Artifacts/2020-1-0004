<script type="text/javascript">

    function eraseCookie() {
        var res = document.cookie;
        var multiple = res.split(";");
        for (var i = 0; i < multiple.length; i++) {
            var key = multiple[i].split("=");
            document.cookie = key[0] + " =; expires = Thu, 01 Jan 1970 00:00:00 UTC";
        }
    }

    function getUserToken() {
        var cookieStart = 'UserOn=';
        var ca = document.cookie.split(';');
        for (var i = 0; i < ca.length; i++) {
            var c = ca[i];
            while (c.charAt(0) == ' ') c = c.substring(1, c.length);
            if (c.indexOf(cookieStart) == 0) {
                var cv = c.substring(cookieStart.length, c.length);
                userToken = cv;
                return true;
            }
        }

        return false;
    }
    $(document).ready(function () {

        $.getJSON("https://applications.acsm.org/sitefinitysso/ATS.Utilities.Handlers.jsonpSSO.ashx?callback=?",
            function (data) {
                if (data.UserToken && data.UserToken.length > 0) {

                } else {

                    if (getUserToken()) {
                        var cookies = document.cookie.split(";");

                        eraseCookie();

                        window.location.href = "/Sitefinity/SignOut?sts_signout=true&redirect_uri=";
                    }

                }
            }
        );
    });

           
