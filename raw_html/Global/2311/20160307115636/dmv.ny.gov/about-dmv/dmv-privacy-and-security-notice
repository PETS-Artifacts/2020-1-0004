<script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/jquery-cookie/1.3.1/jquery.cookie.min.js"></script>
<ul class="language-controls">
<li style="display:none;" class="notranslate language-switcher">
    <a class="switcher" href="#en" title="English">English</a>
    </li>
<li style="display:none;" class="notranslate language-switcher">
    <a class="switcher" href="#es" title="Espa&ntilde;ol">Espa&ntilde;ol</a>
    <a href="/spanish">(Disclaimer)</a></li>
</ul>
<script type="text/javascript">
    jQuery(document).ready(function($) {
        var current_domain = $(location).attr('href').split('/')[2];
        var active_lang = $.cookie("googtrans");
        var main_lang = 'en';
        active_lang = active_lang ? active_lang.split('/').reverse()[0] : main_lang;
        var switchers = $('.language-controls .language-switcher a.switcher');
        switchers.each(function(i){
            var lang = $(this).attr('href').replace('#', '');
            lang == active_lang ? $(this).parent().hide() : $(this).parent().show();

            $(this).click(function(e){
                var that = this;
                e.preventDefault();
                $(this).parent().hide();
                switchers.each(function(j){
                    if($(this).attr('href') != $(that).attr('href')){
                        $(this).parent().show();
                    }
                });
                doGTranslate(main_lang + '|' + lang);
            });
        });
        var at_spanish_page = $.inArray('spanish', $(location).attr('href').split('/')) > -1;
        if(at_spanish_page){
            $('.pane-node-title').each(function(i){
                $(this).addClass('notranslate');
            });
            $('.pane-node-body .field-items .field-item').each(function(i){
                if(!$(this).hasClass('notranslate')) {
                    $(this).addClass('notranslate');
                    $(this).append('<p><a id="body-lang-switch" href="#">Continuar en Español</a></p>');
                    $(this).append('<p>Translations powered by Google Translate.</p>');
                    $('#body-lang-switch').click(function(e){
                        doGTranslate(main_lang + '|es');
                    });
                }
            });
        }
    });
</script>
