                                <select class="form-control" data-bind="visible: railcard.showAdults,  attr: { tabindex: '2' + $index() + '3' }, value: railcard.adults, options: $parent.numberOfAdultsList, fancySelect:{ setWidth : true, suffixes : { none : ' Adults', singular : ' Adult', multiple : ' Adults' }  },
                                        event: {
                                        focus: $parent.dlCaptureCurrentFieldValue.bind($data, 'qtt_railcard_qty_' + $index() + '_adults', railcard.adults()), 
                                        blur: $parent.dlPushNewFieldValue.bind($data, false, 'qtt_railcard_qty_' + $index() + '_adults', railcard.adults) 
                                    }"></select>
                            </div>
                        </div>
                        <div class="col-xs-7 col-md-5 with-caret" data-bind="if: railcard.showPassengersSelection, css: { 'hidden': !railcard.showPassengersSelection() }">
                            <div data-bind="if: railcard.showChildren">
                                <select class="form-control" data-bind="visible: railcard.showChildren,  attr: { tabindex: '2' + $index() + '4' }, value: railcard.children, options: $parent.numberOfChildrenList, fancySelect:{ setWidth : true, suffixes : { none : ' Children (5-15)', singular : ' Child (5-15)', multiple : ' Children (5-15)' }},
                                        event: {
                                        focus: $parent.dlCaptureCurrentFieldValue.bind($data, 'qtt_railcard_qty_' + $index() + '_children', railcard.children()), 
                                        blur: $parent.dlPushNewFieldValue.bind($data, false, 'qtt_railcard_qty_' + $index() + '_children', railcard.children) 
                                    }"></select>
                            </div>
                        </div>
                        <div class="hidden-xs col-md-4">
                        </div>
                    </div>
                    <div class="col-xs-12 col-md-12 validation with-caret" data-bind="visible: !railcard.isValid()">
                        <span class="validation-error-message" data-bind="validationMessage: railcard.code"></span>
                    </div>
                    <div class="hidden visible-xs col-md-12 mobile-remove with-caret">
                        <a data-bind="click: railcard.remove.bind(railcard), attr: { tabindex: '2' + $index() + '6' }, event: {click: $parent.dlTriggerEvent.bind($data, false, 'qttRailcardsRemove')}"><i class="icon-close_no_circle"></i> Remove</a>
                    </div>
                    <div class="col-xs-12 col-md-12 with-caret" data-bind="visible: railcard.isValid() && railcard.isLast">
                        <a data-bind="click: railcard.add.bind(railcard), attr: { tabindex: '2' + $index() + '7' }, event: {click: $parent.dlTriggerEvent.bind($data, false, 'qttRailcardsAddAnother')}"><i class="icon-add"></i> Add another railcard</a>
                    </div>
                </div>
            </div>

            <div class="row">
                <div class="col-xs-6 col-md-6 cancel">
                    <a data-bind="click: $data.railcardsCancel(), event: {click: $data.dlTriggerEvent.bind($data, false, 'qttRailcardsCancel')}">Cancel</a>
                </div>
                <div class="col-xs-6 col-md-6 done">
                    <button type="button" class="btn btn-custom btn-search-custom" data-bind="click: $data.railcardsDone(), css: { disabled: invalidRailcardsParams }, event: {click: $data.dlTriggerEvent.bind($data, false, 'qttRailcardsDone')}">Done</button>
                </div>
            </div>

        </div>
    </div>

</div>

            </span>
        
            <div class="qtt--v2__footer-links qtt--v2__visible link-container">
                    <a href="/plan-journey/tickets-railcards-and-season-tickets/season-tickets" target="_self">Season tickets</a> <span class="divider"></span>
                    <a href="http://tickets.gwr.com/gw/en/JourneyPlanning/Wizard/" target="">Accessible booking tool</a> <span class="divider"></span>
                    <a href="/plan-journey/tickets-railcards-and-season-tickets/touch" target="_self">Touch smartcard</a> <span class="divider"></span>
            </div>

             <div class="qtt--v2__spacer qtt--v2__visible"></div>

            <div class="col-xs-12 col-sm-5 col-md-3 col-lg-2 form-group text-right btn-wrapper qtt--v2__submit qtt-find-ticket-button-container show-wrapper">
                <button tid="findTicketQtt" class="col-xs-8 col-xs-offset-2 col-sm-12 col-sm-offset-0 btn btn-custom btn-revenue-custom" data-bind="click: submit,  clickBubble: false, css: { disabled: invalidSearchParams }, event: {click: dlTriggerEvent.bind($data, false, 'qttSubmit')}" data-elid="qtt-find">Find tickets</button>
            </div>

            <div class="footer-links-box-in-simple-form show-wrapper col-xs-12">
                <a href="/plan-journey/tickets-railcards-and-season-tickets/season-tickets" target="_self" class="links-in-simpliform-on-bottom">Season tickets</a>
