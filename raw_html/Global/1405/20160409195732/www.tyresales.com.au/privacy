<script type="text/javascript">
    function SubmitToGigyaActivity() {
        try {
            publishToInternalFeed("http://www.tyresales.com.au/help/ask-an-expert", "Ask Greg", 'question was asked', "", "");
        } catch (e) {

        }
        $('input[id*="txt"], textarea').each(function () {

            if (this.value == $(this).attr('title')) {
                this.value = '';
            }
        });
    }

    $(document).ready(function () {

        $('.questionForm input[id*="txt"], textarea').each(function () {

            this.value = $(this).attr('title');

            $(this).focus(function () {
                if (this.value == $(this).attr('title')) {
                    this.value = '';
                }
            });

            $(this).blur(function () {
                if (this.value == '') {
                    this.value = $(this).attr('title');
                }
            });
        });

    });
    
</script>

        

