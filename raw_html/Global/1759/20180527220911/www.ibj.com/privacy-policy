<script>
    var policy = '<p class="policy-notice" style="clear:both; max-width: 400px; margin: auto; background-color: #eeeeee; padding: 10px;">This process collects personal data and is stored only for IBJ Media Corp. use. Read our <a href="https://www.ibj.com/privacy-policy" class="ibj-blue-link" target="_blank" style="color:#0352c5;">privacy policy</a> on how we protect and manage your data.</p>';

    addPolicy();

    function addPolicy() {
        $(".policy-notice").remove();

        keys = ["Register", "Continue", "Process Order", "Update", "Submit"];

        has_key = false;

        $("input").each(function() {
            if (keys.indexOf($(this).val()) != -1) {
                has_key = true;
            }
        });

        if (has_key) {
            $('#section-2').append(policy);
        }
    }
