    <a href="/user/logout" class="logout">Logout</a>
    <a href="/user/profile" class="register">Your Account</a>
  </div>
  <div class="mini-cart">
  <a href="/cart">View Cart</a>
  <span id="cart-count">&nbsp;</span>
</div>

</div>


<script>
  (function($){
    var username = $.cookie('user_name'),
      $login_nav = $("#login-nav"),
      timeout_at = $.cookie('timeout_at');

    if (timeout_at !== null && Date.parse(timeout_at) > Date.now()) {
      $login_nav.find('.user-logged-out').hide();
      $login_nav.find('.user-logged-in').show();
      $("#welcome-username").html(username);
      $("#logout-username").html(username);
    }
  })(jQuery);
</script>

      </div><!--END of HEADER-USER-NAV-->

    </header>

