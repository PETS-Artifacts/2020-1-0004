    $(document).ready(function () {
        $('#searchDropdownBox').on("change", function () {
            contentTypeSelected = $('#searchDropdownBox option:selected').val();
            $('#mainSearchBox').coveo('state', 'contentitemtype', contentTypeSelected);
        });
        $('#globalSearchBoxButton').on("click touchstart", function() {
            var val = $('#twotabsearchtextbox').val();
