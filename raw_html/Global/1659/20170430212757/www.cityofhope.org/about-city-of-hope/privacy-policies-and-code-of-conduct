							function goLocalized(locale) {
							var url = window.location.href;    
							if (url.indexOf('?') > -1){
								if (url.indexOf('locale') > -1){ 
									url = url.substring(0, url.lastIndexOf('locale')-1) + '?locale=' + locale;
								} else {
									url += '&locale=' + locale	
								}
							}else{ 
							   url += '?locale=' + locale
							}
							window.location.href = url;
							}
							</script>
