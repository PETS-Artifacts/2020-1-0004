$('.scroll-links a').click(function(e){
  e.preventDefault();
  var scroll_id = $(this).attr('href').replace('#', '');
  $('html, body').animate({scrollTop: $('#'+scroll_id).offset().top - 80 }, 400); // - 70}, 400);
});
