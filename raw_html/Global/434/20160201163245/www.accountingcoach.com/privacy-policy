<script>
var $overlay_wrapper;
$(function() {
  $overlay_wrapper = $('<div id="close_overlay"></div>').appendTo( $('BODY') );
  $overlay_wrapper.css('height',$(document).height());
});



     $("#member-menu-toggle").click(function(e) {
      e.preventDefault();
      $overlay_wrapper.show();
      $("#member-menu-box").toggle();
      $overlay_wrapper.click(function(){
        $(this).hide();
        $("#member-menu-box").hide();
       });
    });
   /* ================= Removes branding padding top if window is enlarged while mobile search bar is visible == */
  	$(window).resize(function() {
  		var width = $(window).width();
  		if(width > 767) {
  			$('#branding').attr('style','');
  			$('#site-search').attr('style','');
  			$('#mobile-search-toggle').attr('style', '');
  		}
  			
  		if($('.topic-downloads').length) {
  		  if(width > 767) {
  		    $("#page-heading .topic-download").css('width', ($('.container').width()-380)+'px');
  		  }
  		  else {
  		    $("#page-heading .topic-download").css('width', 'auto');
  		  }
  		}
  		if($('.exam-overview').length) {	
  		  var max_height = 0;
  		  $('.exam-overview').each(function(){
  		    $(this).css('height', 'auto');
  		    if($(this).outerHeight() > max_height) max_height = $(this).outerHeight();
  		  });
  		  $('.exam-overview').css('height', max_height + 'px');
  		}	
  	});
  	$(window).trigger('resize');
</script> 
