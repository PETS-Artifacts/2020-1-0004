         *
         *  that the mobile menu will be 80% width or 350px max
         */

        function menuWidth(){
            window_Width = (window.innerWidth * 0.8) - 30;
            width = (window_Width < 350) ? window_Width : 350;
            $('.page .mobile-nav').css({"width": width});

            var pageleft = $('.page').offset();
            if (pageleft.left > 0) {
                $('.page').css({"left" : width});
            };
        }

    })(jQuery)

