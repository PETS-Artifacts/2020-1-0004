        })

        $(document).ready(function(){
            menuWidth();
        })

        $(window).resize(function(){
            menuWidth();
            if (window.innerWidth > 991){
                $('.mobile-nav.side-block ').css({"display":"none"});
                $('.page').css({"left" : 0});
            } else {
                $('.mobile-nav.side-block ').css({"display":"block"});
            }
