<script>

        affirm.ui.ready(function () {

            var itemDiv = document.createElement('div');
            itemDiv.className = "bronto-cart-recovery-line-item";

            var parent =  document.getElementById("bronto-cart-recovery-line-items");
            parent.insertBefore(itemDiv,parent.firstChild);
            var apr = ".1"; // percentage assumed APR for loan
            var months = "12"; // can be 3, 6, or 12

            //prep div for global affirm price
            var gDiv = document.createElement('div');
            gDiv.className = 'bronto-cart-recovery-line-item-other';
            itemDiv.appendChild(gDiv);

            //Global affirm price for whole cart
            affirm.ui.payments.get_estimate(
                {
                    apr: apr,
                    months: months,
                    amount: "0"
                },
                function (payment_estimate) {
                    //set div price and trigger bronto cart recovery script
                    gDiv.innerHTML = payment_estimate.payment_string;
                    updateBrontoRecovery();
                }
            );



            //Iterate all cart items and set affirm price under other element
            var lineItems = document.getElementById('bronto-cart-recovery-line-items');
            processItems(lineItems,1);

            /**Process each cart item and set their individual affirm price
             * in bronto cart recovery data
             */
            function processItems(lineItems, i) {
                //get current line item and its price
                var lineItem = lineItems.children[i];
                var price = lineItem.getElementsByClassName("bronto-cart-recovery-line-item-total-price")[0].innerHTML;

                var args = [ lineItem , i ];

                //run affirm get estimate
                affirm.ui.payments.get_estimate(
                    {
                        apr: apr,
                        months: months,
                        amount: price*100
                    },
                    function (payment_estimate) {
                        var lineItem = args[0];
                        var i = args[1];

                        //set affirm price under items "other" value
                        lineItem.getElementsByClassName("bronto-cart-recovery-line-item-other")[0].innerHTML =
                            payment_estimate.payment_string;

                        //check whether to finish with triggering bronto or processing next item
                        i += 1;
                        if(i === lineItems.children.length){
                            updateBrontoRecovery();
                        } else if(i < lineItems.children.length) {
                            processItems(lineItems, i);
                        }
                    }
                );
            }

            /**
             * function triggered after bronto recovery elements are updated
             */
            function updateBrontoRecovery() {
                var _bsw = _bsw || [];
                _bsw.push(['_bswId', '48021501248116e9fc07fb7ccbd5287ba0a8dbd697d3bb0d6d6da453c5977436']);
                (function() {
                    var bsw = document.createElement('script'); bsw.type = 'text/javascript'; bsw.async = true;
                    bsw.src = ('https:' == document.location.protocol ? 'https://' : 'http://') + 'js.bronto.com/c/7bg7xtbcgiu85tlwfy75w0zj7ux8qdk5ej32756rcevo8769hu/48021501248116e9fc07fb7ccbd5287ba0a8dbd697d3bb0d6d6da453c5977436/s/b.min.js';
                    var t = document.getElementsByTagName('script')[0]; t.parentNode.insertBefore(bsw, t);
                })();
            }

        });
    </script>
