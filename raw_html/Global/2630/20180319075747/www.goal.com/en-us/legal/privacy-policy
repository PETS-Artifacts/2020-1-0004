<script id="dfpHeadScript" data-dfp='{"ads":[{"isOneToOne":false,"isOutOfPageSlot":false,"getAdUnitPath":"\/67970281\/DISPLAY_OWNED_GBL\/goalcom_responsive\/ros\/topbanner","getSlotSizes":[[970,250],[970,90],[728,90]],"getHtmlId":"div-gpt-ad-topbanner-0","getResolutionMappings":[[[1021,90],[[970,250],[970,90],[728,90]]],[[660,90],[728,90]],[[0,0],[]]],"additionalData":[],"keys":{"Display_Ad_Size":"Top_Banner","Display_Ad_Position":"ATF"}},{"isOneToOne":false,"isOutOfPageSlot":true,"getAdUnitPath":"\/67970281\/DISPLAY_OWNED_GBL\/goalcom_responsive\/ros\/highimpact","getSlotSizes":null,"getHtmlId":"div-gpt-ad-highimpact-1","getResolutionMappings":[],"additionalData":[],"keys":{"Display_Ad_Position":"ATF","Display_Ad_Size":"Skin"}}],"keys":{"pageType":"legal","goalversion":"legacy","edition":"en-us","environment":"production","language":"en"},"activeKeys":{"goal_layout":"(($(window).width() > 1020) ? \"desktop\" : \"tablet\")","amznslots":"(window.amzValues || \"\")"},"resizeBreakpoints":[300,320,620,728,940,970,1020]}'></script>

<script>

(function () {
'use strict';
var headScript = document.getElementById('dfpHeadScript');
var dfpData = JSON.parse(headScript.getAttribute('data-dfp'));
var outOfPage = false;
var outOfPageId = '';
var currentDevice = (window.innerWidth > 1020 ? 'desktop' : 'tablet');

window.googletag = window.googletag || {};
window.movedByJsGoogleSlots = [];
window.googletag.cmd = window.googletag.cmd || [];
var googleConfig = {
gptAdSlots: [],
outOfPageSlot: [],
noRefreshableSlots: [],
currentWidth: window.innerWidth,
currentGoogleTagDfpBreak: -1,
googleTagDfpBreakpoints: dfpData.resizeBreakpoints,
screenGoogleTagDfpBreak: 0,

sizeGoogleTagDfpCheck: function () {
var screenGoogleTagDfpW = window.innerWidth;
var googleTagDfpBreakpointsCount = googleConfig.googleTagDfpBreakpoints.length;
for (var i = 0; i < googleTagDfpBreakpointsCount; i++) {
if (googleConfig.googleTagDfpBreakpoints[i] > screenGoogleTagDfpW) {
break;
}
googleConfig.screenGoogleTagDfpBreak = i;
}

if (googleConfig.screenGoogleTagDfpBreak != googleConfig.currentGoogleTagDfpBreak) {
googleConfig.currentGoogleTagDfpBreak = googleConfig.screenGoogleTagDfpBreak;

window.googletag.cmd.push(function () {
window.googletag.pubads().refresh(googleConfig.gptAdSlots);
});
}
},
heightIframeAd: 0,
widthIframeAd: 0,
timeToHide: 0,
closeIframeAd: false,
iframe: {}
};

window.googletag.cmd.push(function () {
for (var keys in dfpData.keys) {
window.googletag.pubads().setTargeting(keys, dfpData.keys[keys]);
}

for (var activeKeys in dfpData.activeKeys) {
window.googletag.pubads().setTargeting(activeKeys, eval(dfpData.activeKeys[activeKeys]));
}

for (var ad in dfpData.ads) {

var adSpecificDevice = dfpData.ads[ad].additionalData.specificDevice !== 'undefined' ? dfpData.ads[ad].additionalData.specificDevice : false;
if (adSpecificDevice && currentDevice !== adSpecificDevice) {
continue;
}

var movedByJs = dfpData.ads[ad].additionalData.movedByJs !== 'undefined' ? dfpData.ads[ad].additionalData.movedByJs : false;

if (!dfpData.ads[ad].isOutOfPageSlot) {

var adMappingSlot = googletag.sizeMapping();

for(var addResolutions in dfpData.ads[ad].getResolutionMappings){
var addResolution=dfpData.ads[ad].getResolutionMappings[addResolutions];
adMappingSlot = adMappingSlot.addSize(addResolution[0], addResolution[1]);
}

adMappingSlot = adMappingSlot.build();


if (!dfpData.ads[ad].isOneToOne) {
googleConfig.gptAdSlots.push(
window.googletag.defineSlot(dfpData.ads[ad].getAdUnitPath, dfpData.ads[ad].getSlotSizes, dfpData.ads[ad].getHtmlId)
.defineSizeMapping(adMappingSlot)
.addService(window.googletag.pubads())
.setCollapseEmptyDiv(true)
);

for (var slotTargeting in dfpData.ads[ad].keys) {
googleConfig.gptAdSlots[googleConfig.gptAdSlots.length-1].setTargeting(slotTargeting, dfpData.ads[ad].keys[slotTargeting]);
}


if(movedByJs){
window.movedByJsGoogleSlots.push(googleConfig[googleConfig.gptAdSlots.length-1]);
}

}
else {
googleConfig.noRefreshableSlots.push(
window.googletag.defineSlot(dfpData.ads[ad].getAdUnitPath, eval(dfpData.ads[ad].getSlotSizes), dfpData.ads[ad].getHtmlId)
.addService(window.googletag.pubads())
.setCollapseEmptyDiv(true)
);

for (var slotTargeting in dfpData.ads[ad].keys) {
googleConfig.noRefreshableSlots[googleConfig.noRefreshableSlots.length-1].setTargeting(slotTargeting, dfpData.ads[ad].keys[slotTargeting]);
}

if(movedByJs){
window.movedByJsGoogleSlots.push(googleConfig[googleConfig.noRefreshableSlots.length-1]);
}
}
}
else {
outOfPage = true;
outOfPageId = dfpData.ads[ad].getHtmlId;
googleConfig.outOfPageSlot.push(
window.googletag.defineOutOfPageSlot(dfpData.ads[ad].getAdUnitPath, dfpData.ads[ad].getHtmlId)
.addService(window.googletag.pubads())
);

for (var slotTargeting in dfpData.ads[ad].keys) {
googleConfig.outOfPageSlot[googleConfig.outOfPageSlot.length-1].setTargeting(slotTargeting, dfpData.ads[ad].keys[slotTargeting]);
}
}
}

window.googletag.pubads().enableSingleRequest();
window.googletag.enableServices();
});

if (outOfPage) {
window.addEventListener('message', function (event) {
googleConfig.heightIframeAd = event.data['height'];
googleConfig.widthIframeAd = event.data['width'];
googleConfig.timeToHide = event.data['timeToHide'] * 1000;
googleConfig.closeIframeAd = event.data['close'];
googleConfig.iframe = $('#' + outOfPageId + ' > div iframe');

if (googleConfig.heightIframeAd) {
googleConfig.iframe.css({
'height': googleConfig.heightIframeAd,
'width': googleConfig.widthIframeAd,
'display': 'block',
'position': 'fixed',
'top': '0',
'z-index': '100'
});
setTimeout(function () {
googleConfig.iframe.css({'display': 'none'});
}, googleConfig.timeToHide);
}
if (googleConfig.closeIframeAd) {
googleConfig.iframe.css({'display': 'none'});
}
});
}

window.addEventListener("load", function () {
window.addEventListener("resize", _.throttle(function () {
if (googleConfig.currentWidth != window.innerWidth) {
googleConfig.currentWidth = window.innerWidth;
googleConfig.sizeGoogleTagDfpCheck();
}
}, 200), false);

window.addEventListener("orientationchange", _.throttle(function () {
googleConfig.sizeGoogleTagDfpCheck();
}, 200), false);
});
})();
</script>

