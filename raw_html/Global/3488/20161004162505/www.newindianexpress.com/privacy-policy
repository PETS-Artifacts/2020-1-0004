<script>
 function show_accordian_articles(content_type){
		 $.ajax({
			url			: 'http://www.newindianexpress.com/user/commonwidget/get_editor_pick_content',
			method		: 'get',
			data		: { type: content_type, param: 'home',mode:'live'},
			beforeSend	: function() {				
			},
			success		: function(result){ 
			console.log(result);
				   $('#'+content_type+'_content').html(result).hide().fadeIn({ duration: 1000 });
				  $('#'+content_type).removeAttr('onclick');
				   console.clear();
				   }			
		});
	   }

$(document).ready(function(){
show_accordian_articles('most_read');
});	   
</script></div></div></div><div class="row"><div class=" col-lg-12 col-md-12 col-sm-12 col-xs-12  "><div class="widget-container widget-container-5">
