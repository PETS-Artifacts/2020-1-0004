<script>
function sectionas_12210(){
$.ajax({
	type:'post',
	cache:false,
	url:'http://www.newindianexpress.com/user/commonwidget/fetchadv',
	data:{'widgetinstance':'12210'},
	dataType:'html',
	success:function(result){
