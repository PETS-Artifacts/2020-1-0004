function updateRatings() {
	//console.log('updateRatings()');
	var ratings = {que:'QC',ont:'ON',alb:'AB',bri:'BC',man:'MB',sco:'NS',bru:'NS',fou:'NS',pei:'NS',nwt:'BC',nun:'BC',sas:'BC',yuk:'BC',qc:'QC',on:'ON',ab:'AB',bc:'BC',mn:'MB',ns:'NS',nb:'NS',nl:'NS',pe:'NS',nt:'BC',nu:'BC',sk:'BC',yt:'BC'}
	var CCrating=ratings[CCr];
	if (typeof CCrating !== 'undefined') {
		console.log('CCrating='+CCrating);
		if (CCrating.length==2) {
			var rtclass=".rt"+CCrating;
			var myClasses = document.querySelectorAll(rtclass),i = 0,l = myClasses.length;
			for (i; i < l; i++) {
				myClasses[i].style.display = 'inline-block';
			}
		}
	}
	else {
		var myClasses = document.querySelectorAll('.rtUS'),i = 0,l = myClasses.length;
		for (i; i < l; i++) {
			myClasses[i].style.display = 'inline-block';
		}
	}
}
