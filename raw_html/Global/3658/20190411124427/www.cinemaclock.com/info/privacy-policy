	$('#sitetitle').removeClass('sitetitleover');
	$('#sitetitle').css('height','130px');
	$('#WALL').css('height','250px');
	$('#menubar').appendTo('body').removeClass('menubarover');
	$('#tdsearch').animate({opacity:"1"}, 500 );
	$('#MISIA').animate({opacity:"1"}, 500 );
	$('#topbarinner').css('max-width','1600px');
	var vtagba='';
	if (Bannerid>0) {
		vtagba = '<a href="/aw/bannadsc.aw?zid=' + Zoneid + '&bid=' + Bannerid + '" target="' + BannerTarget + '" style="display: block;width: 100%;height: 100%;text-decoration: none;"></a>';
		$("#WALL").append(vtagba);
	}
	setCookie('CCVokenCap',1,1);
}
