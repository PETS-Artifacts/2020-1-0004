function qsearch() {
	var qurl = document.getElementById('key').value;
	var keysel = document.getElementById('keysel');
	if (keysel === undefined || keysel === null) {
	}
	else {
		if (keysel.value.length>2){
			window.location.href = keysel.value;
			return false;
		}
	}
	window.location.href = CCpath + '/search/' + qurl;
	return false;
}
