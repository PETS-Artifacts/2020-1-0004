			$("#TRvideo").append(vtag);
			loadScript("/html/ccjs/clockvideo20181129.js",function () {if (SkinVideoWaitPlay>0) {clockplayer.stop();}})
		});	
	}
	setTimeout(function() {
		//delayLazyLoadAll=1;
		lazyLoad();
	}, 1000);
}
</script>

