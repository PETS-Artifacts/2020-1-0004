function ValidateEmail(valemail) {
	if (/^\w+([\.-]?\w+)*@\w+([\.-]?\w+)*(\.\w{2,3})+$/.test(valemail))
	{return (true)}
	//console.log('User email NOT good: '+valemail);
	return (false)
}
