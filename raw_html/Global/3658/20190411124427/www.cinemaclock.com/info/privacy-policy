function logOut() {
	GGloggingout=1;
	$('#LOGUJ').html('<div class="BD loginbox" style="text-align:center;"><h1>Logout</h1><p class="form">You are now logged out.</p><div id="CCloginunderemail"><button class="btn btnccinv btnenter" onClick="closeLogin();">OK</button></div></div>');
	askingPassword=0;
	CCusertoken='';
	setCookie('ccuserid','',-1);
	setCookie('ccusertoken','',-1);
	setCookie('ccusername','',-1);
	setCookie('ccuseremail','',-1);
	setCookie('uc','',-1);
	setCookie('uf','',-1);
	setCookie('um','',-1);
	setCookie('ccuserlogincancel','',-1);
	$('#ccmainlogout').fadeOut();
	$('#ccmainlogin').fadeIn();
	$('#LOGUJ').fadeIn('fast');$('div.BD.loginbox').css('top','20px');
	//Google sign out
	if (GGappLaunched) {
		GGappStart();
	}
	else {
		GGappLaunch();			
	}
}
