function offs() {
	if (SearchBoxY == 0) {
		var p = $("#key");	
		var offset = p.position();
		var SearchBoxX=Math.round(offset.left);
		SearchBoxY=Math.round(offset.top+40);
		$('#results').css('top',SearchBoxY+'px').css('left',SearchBoxX+'px');
	}
}
</script>
