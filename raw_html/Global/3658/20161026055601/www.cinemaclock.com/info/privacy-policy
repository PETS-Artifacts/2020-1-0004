<script>
function showDetailedView() {
	classicview=0;
	setCookie('filmbuttons','1',1024);
	$("div.buttons15").css("display","block");
	$(".posterstickers").css("display","block");
	//$("div.tagm").css("display","block");
	$('.shortvalue').css("display","none");
	$('.shortcinemas').css("display","none");
	$('.shortrat').css("display","none");
	$("div.moviegenre").css("padding","5px 10px 0");
	if (wwi > 479) {
		$(".smallposter img").css("width","120px").css("height","171px");
		$("a.reviewsnip").css("display","table-cell");
	}
	else	if (wwi > 399) {
		$(".smallposter img").css("width","100px").css("height","142px");
	}
	else	{
		$(".smallposter img").css("width","80px").css("height","114px");
	}
	$('.moviedesc').css("cursor","default");
//	$('#checkbuttons').html(' Detailed view (with buttons)');
	$('.ccfont.butmatrix').css("color","#5bbade");
}
function showClassicView() {
	classicview=1;
	setCookie('filmbuttons','0',1024);
	$("div.buttons15").css("display","none");
	$(".posterstickers").css("display","none");
	//$("div.tagm").css("display","none");
	$('.shortvalue').css("display","block");
	$('.shortcinemas').css("display","inline");
	$('.shortrat').css("display","inline");
	$("div.moviegenre").css("padding","14px 2px 0 7px");
	if (wwi > 479) {
		$(".smallposter img").css("width","80px").css("height","114px");
		$("a.reviewsnip").css("display","none");
	}
	else	{
		$(".smallposter img").css("width","60px").css("height","86px");
	}
	$('.moviedesc').css("cursor","pointer");
//	$('#checkbuttons').html(' Classic view (without buttons)');
	$('.ccfont.butmatrix').css("color","#aaa");
}
function toggleMatrix() {
	if (classicview) {
		showDetailedView();
		//setTimeout(function(){togmatrix=1;}, 100);
	}
	else {
		showClassicView();
	}
}
</script>










