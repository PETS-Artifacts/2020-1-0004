				if (hilite.next().length == 0) {
					divline.eq(0).addClass('hilite')
				}
				e.preventDefault();
				return false;
			} else if (e.which === 38) {
				hilite.removeClass('hilite').prev().addClass('hilite');
				if (hilite.prev().length == 0) {
					divline.eq(-1).addClass('hilite')
				}
				e.preventDefault();
				return false;
			}
