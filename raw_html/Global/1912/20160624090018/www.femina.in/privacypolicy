<script>
$(document).ready(function() {
    $(".right_nav ul li").hover(function() {
        $(this).find("ul").css("visibility", "visible")
    }, function() {
        $(this).find("ul").css("visibility", "hidden")
    });
	
	$('.right_nav .search').click(function(){
		$('.searchui').fadeIn();
		$('.search_overlay').fadeIn();
		$(this).hide();
		$('.right_nav .closesearch').fadeIn();
	});
	$('.right_nav .closesearch').click(function(){
		$('.searchui').fadeOut();
		$('.search_overlay').fadeOut();
		$(this).hide();
		$('.right_nav .search').fadeIn();
	});
	$('.search_overlay').click(function(){
		$('.searchui').fadeOut();
		$('.right_nav .closesearch').hide();
		$(this).hide();
		$('.right_nav .search').fadeIn();
	});
	$(window).scroll(function(){
		$('.searchui').fadeOut();
		$('.search_overlay').fadeOut();
		$('.right_nav .closesearch').hide();
		$('.right_nav .search').fadeIn();
	});
	$(document).keyup(function(e) {
		if (e.keyCode == 27) // for esc key
		{
			$('.searchui').fadeOut();
			$('.search_overlay').fadeOut();
			$('.right_nav .closesearch').hide();
			$('.right_nav .search').fadeIn(); 
		}
	});	
});
</script>
