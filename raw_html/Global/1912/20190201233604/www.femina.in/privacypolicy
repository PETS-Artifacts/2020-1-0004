 <script>
function fireEvent(place,section,subsection){
		console.log(ga);
	   if ( ga != undefined ) {
		   console.log('before event fired');
		   if(subsection!=""){
			   section = subsection;
		   }
           ga('send', 'event', place, 'Navigation click', section);
		    console.log('after event fired');
          }
}
</script>

<script>
function fireSEO_Event(e_cat,e_action,e_label,e_value){		
	   if ( ga != undefined ) {		   
           ga('send', 'event', e_cat, e_action,e_label,e_value);	    
          }
}
</script>


