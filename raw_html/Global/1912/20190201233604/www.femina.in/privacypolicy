<script>
function validateSearch(){
	var searchv = $("#topq").val();
	var searchvlength = $("#topq").length;
	if(searchv == 'Search Femina' || searchv.trim()==''){
	return false;
	}
	 if ( ga != undefined ) {
		ga('send', 'event', 'Search Click', 'click', 'Desktop Search Click');
	  }
	submitSearchForm(true);
}
function submitSearchFormTop(bool) {
  if ( bool ) {
    fieldId = 'topq'
  } else {
    fieldId = 'searchTags'
  }
  var searchQuery = '';
  if ( $("#" + fieldId).length > 0 ) {
    if ( $("#" + fieldId).val() == 'Search' ) {
      $("#" + fieldId).val('')
    }
    var topqsearch = jQuery("#" + fieldId).val();
    if ( topqsearch != '' ) {
      searchQuery = topqsearch
    }
  }
  var searchv = $("#" + fieldId).val();
  var searchvlength = $("#topq").length;
  if ( searchv == 'Search Femina' || searchv.trim() == '' ) {
    $("#" + fieldId).focus();
    return !1
  }
  var url = SITEJSROOTPATH + 'search/';
  if ( searchQuery != "" ) {
    var searchTags = searchQuery.toLowerCase();
    searchTags = $.trim(searchTags);
    searchTags = searchTags.replace("'", '');
    searchTags = searchTags.replace(/[^A-Za-z0-9]/gi, '-');
    searchTags = searchTags.replace(/[-]+/g, '-');
    searchTags = searchTags.replace(/[-]$/g, '');
    searchTags = searchTags.replace(/^[-]/g, '');
    url += 'tag_' + searchTags + '/form'
  }
  document.location.href = encodeURI(url)
}
function eventFire(language,langcode){
	if ( ga != undefined ) {
		ga('send', 'event', 'Desktop lang dropdown', 'language click', language, langcode); // 1 for hindi 0 for english 2 for bangla
	}
}
</script>
