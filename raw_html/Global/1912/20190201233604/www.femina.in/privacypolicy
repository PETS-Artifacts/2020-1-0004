		//search
	var arrowclick = 0;
	$('body').on('click','.search-i',function(e){				    
		var querystr = $("#topq").val().toLowerCase();
		//uncomment below when development
		// if(querystr!='' && querystr!='search'){
		//    //submitSearchForm_mob(true);
		// }		
		// else{
		    $(".right_search").toggleClass('largeWidth');
			$(".search-i").addClass('search-i-close');	
			$(".search-i").removeClass('search-i');
			$("#topq-mob").focus();
			$('.btn-search').show();	
		//}
	});  
	$('body').on('click','.search-i-close',function(e){	
			$(".right_search").toggleClass('largeWidth');
			$("#topq-mob").val('');
			$(".search-i-close").addClass('search-i');
			$(".search-i-close").removeClass('search-i-close');
			$('.btn-search').hide();			
	});
	
	$('#topq-mob').on('keypress', function (e) {
		 if(e.which === 13){
			//Disable textbox to prevent multiple submit
			var querystr = $("#topq-mob").val().toLowerCase();
				if(querystr!='' && querystr!='search'){
				   submitSearchForm_mob(true);
				}else{
					e.preventDefault();
					return false;
				}
			
		 }
		 $('.btn-search').show();
   });
   function submitSearchForm_mob(){
        submitSearchForm_mob(true); 
    }
    /* this is for search box */           
    function submitSearchForm_mob(bool){     
      if(bool){
          fieldId = 'topq-mob';
      } else {
          fieldId = 'searchTags';
      }
      var searchQuery='';
      if($("#"+fieldId).val().trim()=="") return false;
      if($("#"+fieldId).length>0){
        if($("#"+fieldId).val() == 'Search'){
          $("#"+fieldId).val('');
        }
        var topqsearch=jQuery("#"+fieldId).val();
        if(topqsearch!=''){
          searchQuery=topqsearch;
        }
      }
      //var url = 'http://localhost/filmfare/contentsearch.php';
       var url = SITEJSROOTPATH+'/search/';
      if(searchQuery !=""){
        var searchTags = searchQuery.toLowerCase(); 
        searchTags = searchTags.replace("'",'');
        searchTags = searchTags.replace(/[^A-Za-z0-9]/gi,'-');
        searchTags = searchTags.replace(/[-]+/g,'-');
        searchTags = searchTags.replace(/[-]$/g,'');//end with-
        searchTags = searchTags.replace(/^[-]/g,'');//start with -
        url += 'tag_'+searchTags;       
      }
      document.location.href= encodeURI(url);
    }
   	$('#btn_search').on('click', function (e) {
		//Disable textbox to prevent multiple submit
		var querystr = $("#topq-mob").val().toLowerCase();
		if(querystr!='' && querystr!='search'){
		 if ( ga != undefined ) {
			ga('send', 'event', 'Search Click', 'click', 'Mobile Search Click');
          }
		   submitSearchForm_mob(true);
		}else{
			$("#topq-mob").focus();
			e.preventDefault();
			return false;
		}
	});
   
   $(".left-menu li a.drop").click(function() {
     $(this).parent("li").children("ul").toggle(500); 
     $(this).toggleClass("drop");
        return false;
	});
</script>	<!--</div>
