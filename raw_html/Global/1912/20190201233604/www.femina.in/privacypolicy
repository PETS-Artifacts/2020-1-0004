<script>
$(document).ready(function() {
	/*
    $(".right_nav ul li").hover(function() {
        $(this).find("ul").css("visibility", "visible")
    }, function() {
        $(this).find("ul").css("visibility", "hidden")
    });
	*/
	$(".right_nav ul li").hover(function() {
		if($(this).hasClass("active") === false){
		       $(this).find("ul").css("visibility", "visible");
		}
		   }, function() {
		       $(this).find("ul").css("visibility", "hidden");
		   });
	// $('.top_hdr .search').click(function(){
	// 	$('.searchui').fadeIn();
	// 	$('.search_overlay').fadeIn();
	// 	$(this).hide();
	// 	$('.top_hdr .closesearch').fadeIn();
	// 	$('#topq').focus();
	// });
	$('.top_hdr .search').click(function(){
		//$('.searchui').fadeIn();
		if($(document).width() >=981){
			$('.searchui').css('width','400px');
		}else{
			$('.searchui').css('width','300px');
		}
		//$('.searchui .searchbtn').css('width','95px');
		setTimeout(function(){
			$('.searchui .searchbtn,.searchui .searchinput').fadeIn();	
		},150);
		


		//$('.search_overlay').show();
		$(this).hide();

		$('.top_hdr ul li a.instagram,.top_hdr ul li a.twitter,.top_hdr ul li a.facebook,.top_hdr ul li a.youtube,.top_hdr ul li a.gplus').hide();
		$('.top_hdr .closesearch').show();
		$('#topq').focus();
	});
	// $('.top_hdr .closesearch').click(function(){
	// 	$('.searchui').fadeOut();
	// 	$('.search_overlay').fadeOut();
	// 	$(this).hide();
	// 	$('.top_hdr .search').fadeIn();
	// 	$('#topq').blur();
	// });
	$('.top_hdr .closesearch').click(function(){
		//$('.searchui').hide();
		$('.searchui').css('width','0');
		setTimeout(function(){				
		$('.searchui .searchbtn,.searchui .searchinput').hide();
		},150);
		setTimeout(function(){
		$('.top_hdr ul li a.instagram,.top_hdr ul li a.twitter,.top_hdr ul li a.facebook,.top_hdr ul li a.youtube,.top_hdr ul li a.gplus').show();
		},200);
		//$('.search_overlay').hide();
		$(this).hide();
		$('.top_hdr .search').show();
		$('#topq').blur();
	});
	$('.search_overlay').click(function(){
		//$('.searchui').hide();
		$('.searchui').css('width','0');
		setTimeout(function(){
		$('.searchui .searchbtn').hide();	
		$('.top_hdr ul li a.instagram,.top_hdr ul li a.twitter,.top_hdr ul li a.facebook,.top_hdr ul li a.youtube,.top_hdr ul li a.gplus').show();
		},200);
		//$('.search_overlay').hide();
		$('.top_hdr .closesearch').hide();
		$('.top_hdr .search').show();
		$('#topq').blur();
	});
	$(window).scroll(function(){
		//$('.searchui').hide();
		$('.searchui').css('width','0');
		setTimeout(function(){				
		$('.searchui .searchbtn,.searchui .searchinput').hide();
		},150);
		setTimeout(function(){
		$('.top_hdr ul li a.instagram,.top_hdr ul li a.twitter,.top_hdr ul li a.facebook,.top_hdr ul li a.youtube,.top_hdr ul li a.gplus').show();
		},200);
		//$('.search_overlay').hide();
		$('.top_hdr .closesearch').hide();
		$('.top_hdr .search').show();
		$('#topq').blur();
	});
	$(document).keyup(function(e) {
		if (e.keyCode == 27) // for esc key
		{
			//$('.searchui').hide();
		$('.searchui').css('width','0');
		setTimeout(function(){				
		$('.searchui .searchbtn,.searchui .searchinput').hide();
		},150);
		setTimeout(function(){
		$('.top_hdr ul li a.instagram,.top_hdr ul li a.twitter,.top_hdr ul li a.facebook,.top_hdr ul li a.youtube,.top_hdr ul li a.gplus').show();
		},200);
		//$('.search_overlay').hide();
		$('.top_hdr .closesearch').hide();
		$('.top_hdr .search').show();
		$('#topq').blur();			
		}
	});	
		
	//Disable cut copy paste
	$('img').bind('cut copy paste', function (e) {
		e.preventDefault();
	});	
	//Disable mouse right click
	$("img").on("contextmenu",function(e){
		return false;
	});

});

function checkUserSSOLogin() {
	var userId = readCookie("MSCSAuthID");		
	if(userId == "" || userId == null || typeof(readCookie("MSCSAuthID")) == 'undefined' ) {
		//openSSOPopup();
		return false;
	}
	return userId;	
}

$(window).load(function() {
	checkLoginOnLoad();
});
