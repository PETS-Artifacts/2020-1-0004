    function findBaseUrl() {
        var url = '/';
        var base = window['_app_base']
        if (base) {
            switch (base) {
                case '/nz':
                    url = base + '/';
                    break;
                case '/my':
                    url = base + '/';
                    break;
                case '/sg':
                    url = base + '/';
                    break;
            }
        }
        return url;
    }

    (function() {
	if (window.location.pathname && window.location.pathname.length > 0) {
	    window['_app_base'] = '/' + window.location.pathname.split('/')[1];
	}

    document.write("<base href='"+ findBaseUrl() + "' >");
  })();
</script>
