<script type="text/javascript">
	dataLayer = [];
	var dlPageType = 'OTHER';
	var dlCustomerId = '';
	<!-- Standard Variables -->
	dataLayer.push({
		'site-name'				:  'Spotlight Australia',
		'country-code'			:  'AU',
		'currency-code'			:  'AUD',
		'lang-code'				:  'en',
		'pageType'				:   dlPageType,
		'customer-id'			:  '',
		'customer-email'        :  '',
		'customer-email-sha256' :  '',
		'ui-experience-level'	:  'Desktop',
		'ga-on-page-ua-enabled'  : false
	});
    
    <!-- Events -->
	
	
	    
	    
		 function pushGTMDataLayerEvent(category, action, name, value) {
	  		dataLayer.push({
		  		'event' : 'custom.generic.event',
		  		'eventCategory' : category,
		  		'eventAction' : action,
		  		'eventLabel' : name,
		  		'eventValue' : value,
	  		});
	   	}
	        
	    window.mediator.subscribe('trackAddToCart', function(data) {
	        if (data.productCode && data.quantity)
	        {
	             pushGTMDataLayerEvent('Cart', 'AddToCart', data.productCode, data.quantity);
	        }
	    });
	    
	    window.mediator.subscribe('trackUpdateCart', function(data) {
	        if (data.productCode && data.initialCartQuantity && data.newCartQuantity && data.initialQuantity != data.newQuantity)
	        {
	            if (data.initialQuantity > data.newQuantity) {
	                pushGTMCartEvent('Cart', 'RemoveFromCart', productCode, initialQuantity - newQuantity);
	            } else {
	                pushGTMDataLayerEvent('Cart', 'AddToCart', productCode, newQuantity - initialQuantity);
	            }
	        }
	    });
	    
	    window.mediator.subscribe('trackRemoveFromCart', function(data) {
	        if (data.productCode && data.initialCartQuantity)
	        {
	            pushGTMDataLayerEvent('Cart', 'RemoveFromCart', data.productCode, data.initialCartQuantity);
	        }
	    });
	    
    </script><noscript><iframe src="//www.googletagmanager.com/ns.html?id=GTM-5FFFRK"
