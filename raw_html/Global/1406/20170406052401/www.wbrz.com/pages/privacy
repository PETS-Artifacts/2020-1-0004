	$("#search-dialog").dialog({
		autoOpen: false,
		modal: true,
		closeOnEscape: true,
		width: 750,
		height: 80,
		position: ['top', 200]
	});	
	$(".menu-search").click(function(e){
		e.preventDefault();
		$("#search-dialog").dialog("open");
	});
	$("#search-dialog").on( "dialogopen", function( event, ui ) { 
		$('.ui-dialog-overlay').bind('click', function(){ 
			$("#search-dialog").dialog('close'); 
		});
	});
