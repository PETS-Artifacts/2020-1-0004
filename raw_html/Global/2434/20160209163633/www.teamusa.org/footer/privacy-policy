    <script type="text/javascript">
        jQuery.noConflict();

        jQuery(document).ready(function () {
            // steal enter key from search boxes
            jQuery('.navbar-form input[type=text], .search-box input[type=text]').bind('keypress', function (e) {
                if (e.which == 13) {
                    document.location.href = '/search?i=home&q=' + jQuery(this).val();
                    e.preventDefault();
                }
            });

            // steal click from buttons (related to the above)
            jQuery('.navbar-form input[type=image], .search-box input[type=image]').bind('click', function (e) {
                document.location.href = '/search?i=home&q=' + jQuery(this).prev('input[type=text]').val();
                e.preventDefault();
            });
        });
    </script>
<!-- Used to force submenu display on click for touch -->
<script type="text/javascript">
    (function ($) {
        $('ul.dropdown-menu [data-toggle=dropdown]').on('click', function (event) {
            event.preventDefault();
            event.stopPropagation();
            $(this).parent().siblings().removeClass('open');
            $(this).parent().toggleClass('open');
        });
    })(jQuery);
</script>

