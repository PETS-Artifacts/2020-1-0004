    // trigger on active page change.
    (function() {
      $(window).on('activePageChange', function (w, context) {
        switch(context.type) {
          case 'Section':
            var
              pageType = 'Landing Page',
              sectionName = context.name + ' ' + pageType,
              // Example:
              // Original  = "Integrators > Dealers, Integrators & Installing Companies Landing Page"
              // Converted = "Integrators, Dealers Integrators & Installing Companies"
              category = context.name.replace(",", "").replace(" > ", ", "),
              tag = category,
              sectionId = context.id,
              channelName = (context.parent.name) ? context.parent.name + ' ' + pageType : 'Unassigned ' + pageType,
              channelId = (context.parent.id) ? context.parent.id : context.id,
              data = {
                behaviorId: "4892I1376578A2N",
                category: category,
                pageType: pageType,
                tag: tag,
                section: sectionName,
                sectionId: sectionId,
                channel: channelName,
                channelId: channelId,
              } 
            ;
            olytics.fire(data);
            break;
          case 'Content':
            var
              pageType = 'Content Page',
              sectionName = (context.section.name) ? context.section.name + ' ' + pageType : 'Unassigned ' + pageType,
              // Example:
              // Original  = "Integrators > Dealers, Integrators & Installing Companies Landing Page"
              // Converted = "Integrators, Dealers Integrators & Installing Companies"
              category = context.section.name.replace(",", "").replace(" > ", ", "),
              tag = context.tagNames.join(", "),
              sectionId = context.section.id,
              channelName = (context.section.parent.name) ? context.section.parent.name + ' ' + pageType : 'Unassigned ' + pageType,
              channelId = (context.section.parent.id) ? context.section.parent.id : context.id,
              data = {
                behaviorId: "4892I1376578A2N",
                category: category,
                tag: tag,
                pageType: pageType,
                name: context.name,
                contentId: context.id,
                contentType: context.contentType,
                section: sectionName,
                sectionId: sectionId,
                channel: channelName,
                channelId: channelId,
              }
              if (context.companyName) {
                data.companyName = context.companyName;
              }
              if (context.primaryCategoryName) {
                data.primaryCategoryName = context.primaryCategoryName;
              }
            ;
            olytics.fire(data);
            break;
          default: 
            break;
        }
      });
    })();
  </script>
