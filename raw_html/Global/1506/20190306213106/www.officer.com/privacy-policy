<script type="text/javascript">
  $(function(){
    $('[data-bvo-type="content.body"]').fitVids();

    if(window.location.hash && typeof $('.element[data-bvo-param-field-key="bodyWebsite"]') !== 'undefined') {
      $('.element[data-bvo-param-field-key="bodyWebsite"]').addClass('show');
    } else {
      $('.element[data-bvo-param-field-key="bodyWebsite"]').on('click', function() {
         $(this).addClass('show');
      })
    }

    if ($('.element[data-bvo-param-field-key="bodyWebsite"]').height() < 500) {
      $('.element[data-bvo-param-field-key="bodyWebsite"]').addClass('show')
    }
  })
</script>

  <script type="text/javascript" src="https://radix.officer.com/lib/radix.min.js?x-radix-appid=b105cce3-d815-4347-9ae4-b203d58347a4"></script>
  <script type="text/javascript">

    Radix.on('AccountManager.identity.loaded', function() {
      var identityId = Radix.getIdentity();
      if (identityId) {
        ga('set', 'userId', identityId);
                if (typeof Sapience !== 'undefined') {
          Sapience.Tracker.setIdentity(identityId);
        }
              }
    });

    $(window).on('wufoo.form.loaded', function() {
      StickySidebar.updateStickyHeight();
    });
    Radix.on('GatedDownload.form.loaded', function() {
      StickySidebar.updateStickyHeight();
    });
    Radix.on('GatedDownload.form.updated', function() {
      StickySidebar.updateStickyHeight();
    });

    Radix.init({
      appId    : "b105cce3-d815-4347-9ae4-b203d58347a4",
      host     : "https://radix.officer.com",
      debug    : false,
      logLevel : "error",
    });
    function setRadixGateByRegistrationInfo() {
      $('[data-bvo-type="radix.gate-by-registration"]').attr({
        'radix-app-id'    : "b105cce3-d815-4347-9ae4-b203d58347a4",
        'radix-host'     : "https://radix.officer.com",
      });
      if (null !== Radix.getIdentity() && typeof $('[data-bvo-type="radix.gate-by-registration"]') !== 'undefined') {
        $('[data-bvo-type="radix.gate-by-registration"]').attr({
          'radix-identity' : Radix.getIdentity()
        });
      }
    }
    Radix.on('AccountManager.identity.loaded', function() {
      setRadixGateByRegistrationInfo();
    });
    Radix.on('AccountManager.account.loaded', function() {
      setRadixGateByRegistrationInfo();
    });
    Radix.on('AccountManager.account.unloaded', function () {
      $('[data-bvo-type="radix.gate-by-registration"]').attr({
        'radix-identity' : null
      });
    });
  </script>

                    
