  }, 400);
</script>


  <script src="https://www.baseplatform.io/lib/reskin.js"></script>
  <script>
      var adIframe;
      $('.ad.wallpaper-ad').bind("DOMSubtreeModified", function() {
          if(null == adIframe && typeof $('.ad.wallpaper-ad iframe') !== 'undefined') {
              function IsJsonString(str) {
                  try {
                      JSON.parse(str);
                  } catch (e) {
                      return false;
                  }
                  return true;
              }
              // addEventListener support for IE8
              function bindEvent(element, eventName, eventHandler) {
                  if (element.addEventListener){
                      element.addEventListener(eventName, eventHandler, false);
                  } else if (element.attachEvent) {
                      element.attachEvent('on' + eventName, eventHandler);
                  }
              }
              // Listen to message from child window
              bindEvent(window, 'message', function (e) {
                  var vars = IsJsonString(e.data) ? JSON.parse(e.data) : {};
                  if (typeof vars.backgroundImagePath == 'undefined') return;
                  Reskin.init(vars);
              });
          }
      });
  </script>
                        </body>
</html>
