<script>
  // Track if AdBlocker is running.
  var checkForAdBlock = document.createElement('div');
  checkForAdBlock.innerHTML = '&nbsp;';
  checkForAdBlock.className = 'advertisement';
  document.body.appendChild(checkForAdBlock);
  window.setTimeout(function() {
    if (checkForAdBlock.offsetHeight === 0) {
      console.warn('Disable Ad Blocker for the best user experience');
      document.body.classList.add('adblock');
      ga('send', 'event', 'Ad Setting', 'Adblock', 'Enabled');
    }
    checkForAdBlock.remove();
  }, 400);
</script>


