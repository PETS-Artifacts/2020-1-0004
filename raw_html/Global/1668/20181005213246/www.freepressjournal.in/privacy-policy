<script>
jQuery(window).scroll(function() {    
    var scroll = jQuery(window).scrollTop();

    if (scroll >= 150) {
        jQuery("#header-top").addClass("goToTop");
    } else {
        jQuery("#header-top").removeClass("goToTop");
    }
});
