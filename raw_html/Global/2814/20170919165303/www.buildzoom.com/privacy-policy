<script>
  (function(w, d, s) {
    function loadJsFile(){
      var js, fjs = d.getElementsByTagName(s)[0], load = function(url, id) {
        if (d.getElementById(id)) {return;}
        js = d.createElement(s); js.src = url; js.id = id;
        if(fjs) {fjs.parentNode.insertBefore(js, fjs);}
        else {d.body.appendChild(js);}
      };
//      load('file url here', 'html element id here');
    }
    if (w.addEventListener) { w.addEventListener("load", loadJsFile, false); }
    else if (w.attachEvent) { w.attachEvent("onload",loadJsFile); }
  }(window, document, 'script'));
</script>
<script>
  (function(w, d, s) {
    function createStyleTag(){
      var css, st = d.getElementsByTagName(s)[0], load = function(url, id, delay) {
      	delay = delay || 0;
      	setTimeout(function () {
          if (d.getElementById(id)) {return;}
          css = d.createElement(s); css.innerHTML = url; css.id = id;
          if (st) {st.parentNode.insertBefore(css, st);}
          else { d.head.appendChild(css)}
        }, delay);
      };

        load('@import url("//fast.fonts.net/t/1.css?apiType=css&projectid=d4ad9bcf-f4b5-42c8-9a4d-513f441ac979");', 'fast-fonts-pixel', 1000);
    }
    if (w.addEventListener) { w.addEventListener("load", createStyleTag, false); }
    else if (w.attachEvent) { w.attachEvent("onload",createStyleTag); }
  }(window, document, 'style'));
</script>
