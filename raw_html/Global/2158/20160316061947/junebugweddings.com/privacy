<script type="text/javascript">
  $(function(){

      $("nav ul li").hover(function(){
      
          $(this).addClass("hover");
          $('ul:first',this).css('visibility', 'visible');
      
      }, function(){
      
          $(this).removeClass("hover");
          $('ul:first',this).css('visibility', 'hidden');
      
      });
      
      $("nav ul li ul li:has(ul)").find("a:first").append(" &raquo; ");

  });
</script>

<script>
    $(document).ready(function() {
            $('.linkable-div').hover(function () {
                    $(this).find("img").css({'opacity' : 0.7});
                },
                function () {
                    $(this).find("img").css({'opacity' : 1});
                }
            );
        }
    );
</script>

<script type="text/javascript">
	$( "a" ).bind( "click", function() {

        var linkHrefValue = " "+$(this).attr('href');
        var linkTypeText = "Internal Link";

        if(linkHrefValue.indexOf("www") > 0)
        {
          if(linkHrefValue.indexOf("junebug") > 0)
          { 
            //Do Nothing 
          }
          else
          {
            linkTypeText = "outbound-article";
          }
        }

        //line below functional
        //ga('send', 'event', 'button', 'click', linkTypeText, $(this).attr('href'));
        //_gaq.push(['_trackEvent', 'Outbound Links', $(this).attr('href'), '',, true]);
        _gaq.push(['_trackEvent', linkTypeText, $(this).attr('href'), '',, true]);

      });
</script>


<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-963949-1']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>


    
        <style>
        #smart_footer_container { z-index: 50; background: rgba(0, 0, 0, 0.9); border:1px solid black; bottom:0; left:0; position:fixed; width:100%; display: none; }
        #smart_footer_wrapper { display: flex; justify-content: center; height: 65px; }
        #smart_footer_content { margin:0 auto; font-family: Proxima-Nova, Arial, Helvetica, sans-serif; color: white; text-align:center; align-self: center; }
        #smart_footer_close { float: right; margin-right: 5px; margin-top: 5px; font-family: Arial, Helvetica, sans-serif; font-size: 9px; cursor: pointer; border: 1px solid black; color: black; background-color: white; padding: 2px 3px 2px 3px }
        #smart_footer_close:hover { border: 1px solid #666; background-color: black; color: white; }
        #smart_footer_teaser { font-size: 15px; margin-bottom: 5px; }
    </style>

    <div id="smart_footer_container">
        <div id="smart_footer_close">X</div>
        <div id="smart_footer_wrapper">
            <div id="smart_footer_content">
                <form id="smart_footer_form" method="POST" action="/action/ajax_newsletter_subscribe">
                    <div id="smart_footer_teaser">
                        Sign up for our Love Letter today for a chance to win a $200 J.Crew gift card!                     </div>
                        <span id="smart_footer_form_html">
                            <input id="smart_footer_email" name="email" type="text" size="50" placeholder="Enter your email address"> <button id="smart_footer_submit" type="submit">Submit</button>
                        </span>
                    <span id="smart_footer_form_error" style="display: none"></span>
                </form>
            </div>
        </div>
    </div>

    <script>
        jQuery(document).ready(function ($) {
            function setcookie(key, value, days) {
                var expires = new Date();
                expires.setTime(expires.getTime() + (days * 24 * 60 * 60 * 1000));
                document.cookie = key + '=' + value + ';expires=' + expires.toUTCString() + '; path=/';
            }

            function getcookie(key) {
                var keyValue = document.cookie.match('(^|;) ?' + key + '=([^;]*)(;|$)');
                return keyValue ? keyValue[2] : null;
            }

            function clear_smart_footer_cookies() {
                setcookie('jb_hide_smart_footer_close_time', 0, 1);
                setcookie('jb_hide_smart_footer_close_count', 0, 1);
                setcookie('jb_email_signup_success', 0, 1);
            }

            var last_closed = getcookie('jb_hide_smart_footer_close_time');
            var next_showable_millis = 0;
            if(last_closed != null) {
                var last_closed_millis = new Date(last_closed).getTime();
                next_showable_millis = last_closed_millis + (24 * 60 * 60 * 1000);
            }
            var now_millis = new Date().getTime();

            if(getcookie('jb_email_signup_success') != 1 && getcookie('jb_hide_smart_footer_close_count') < 3 && now_millis >= next_showable_millis) {
                setTimeout(function() {
                    $('#smart_footer_container').slideDown(500);
                    _gaq.push(['_trackEvent', 'smart-footer', 'impression', 'email']);
                }, 2000);
            }

            $("#smart_footer_close").click(function() {
                var count = getcookie('jb_hide_smart_footer_close_count');
                if(count == null) {
                    count = 1;
                }
                else {
                    count++;
                }

                setcookie('jb_hide_smart_footer_close_count', count, 365*10);
                setcookie('jb_hide_smart_footer_close_time', new Date(), 365*10);
                $("#smart_footer_container").fadeOut(500);

                _gaq.push(['_trackEvent', 'smart-footer', 'close']);
            });

            $("#smart_footer_form").submit(function(event) {
                var form = $(this);
                $.ajax({
                    type: form.attr('method'),
                    url: form.attr('action'),
                    data: form.serialize(),
                    dataType: 'json'
                }).done(function(data) {
                    if(!data.error) {
                        setcookie('jb_email_signup_success', 1, 365*10);
                        $("#smart_footer_teaser").empty();
                        $("#smart_footer_form_html").empty();
                        $("#smart_footer_teaser").hide();
                        $("#smart_footer_teaser").append('<span>Thank you! Winners will be announced on 3/16/2016.</span>');
                        $("#smart_footer_teaser").fadeIn(500);

                        setTimeout(function() {
                            $("#smart_footer_container").fadeOut(1000);
                        }, 2000);

                        var emailaddr = $("#smart_footer_email").val();
                        _gaq.push(['_trackEvent', 'smart-footer', 'email-submit-success', emailaddr]);
                    }
                    else {
                        $("#smart_footer_form_error").append('<span style="color: red; text-align: left">' + data.msg + '</span>');
                        $("#smart_footer_form_html").hide();
                        $("#smart_footer_form_error").fadeIn();
                        setTimeout(function() {
                            $("#smart_footer_form_error").hide();
                            $("#smart_footer_form_error").empty();
                            $("#smart_footer_form_html").fadeIn();
                        }, 2000);
                        _gaq.push(['_trackEvent', 'smart-footer', 'email-submit-fail', emailaddr]);
                    }
                });
                event.preventDefault();
            });
        });
    </script>
    
</div>


<script type="text/javascript">/* <![CDATA[ */(function(d,s,a,i,j,r,l,m,t){try{l=d.getElementsByTagName('a');t=d.createElement('textarea');for(i=0;l.length-i;i++){try{a=l[i].href;s=a.indexOf('/cdn-cgi/l/email-protection');m=a.length;if(a&&s>-1&&m>28){j=28+s;s='';if(j<m){r='0x'+a.substr(j,2)|0;for(j+=2;j<m&&a.charAt(j)!='X';j+=2)s+='%'+('0'+('0x'+a.substr(j,2)^r).toString(16)).slice(-2);j++;s=decodeURIComponent(s)+a.substr(j,m-j)}t.innerHTML=s.replace(/</g,'&lt;').replace(/>/g,'&gt;');l[i].href='mailto:'+t.value}}catch(e){}}}catch(e){}})(document);/* ]]> */</script></body>
