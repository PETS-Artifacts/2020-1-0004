    if (propertiesStr) {
            propertiesStrArr = propertiesStr.split(',');
            if (propertiesStrArr) {
                for (var i in propertiesStrArr) {
                    var arr = propertiesStrArr[i].split(':');
                    ga('create', arr[1], 'auto', {'name': arr[0]});
                    ga(arr[0]+'.send', 'pageview');
                }
            }
        }
</script>






		<!--end googleAnalytic-->

	
