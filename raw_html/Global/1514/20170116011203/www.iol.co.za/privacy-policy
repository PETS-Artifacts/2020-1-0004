function mini_movieGuide_init() {
   var daysOfWeek = ["Sun", "Mon", "Tue", "Wed", "Thu", "Fri", "Sat"];
   var monthNames = ["Jan", "Feb", "Mar", "Apr", "May", "Jun", "Jul", "Aug", "Sep", "Oct", "Nov", "Dec"];
   var icon = $(this).find('.icon-down-open');
   var today = daysOfWeek[(new Date()).getDay()] + " " + (new Date()).getDate() + " " + monthNames[(new Date()).getMonth()];

   var onReady = function () {
      $(".movieGuidelogo").on('click', function () {
         $('.movieGuidelogo').css('border-color', '#aaa');
         $(this).css('border', '4px solid #2c83b5');
         $('.movieGuideResults').hide();
      });
      $("#cinema-all").on('click', function () {
         $('#movieguide-results-all').slideToggle(10, function () {
         });
      });
      $("#cinema-ster").click(function () {
         $('#movieguide-results-nume').slideToggle(10, function () {
         });
      });
      $("#cinema-nume").on('click', function () {
         $('#movieguide-results-ster').slideToggle(10, function () {
         });
      });
      $("#cinema-cine").on('click', function () {
         $('#movieguide-results-cine').slideToggle(10, function () {
         });
      });
      $('#searchCinemaBtn').click(function () {
         $('#searchMovieContainer').hide();
         $('#searchMovieBtn').removeClass('active');
         $('#searchCinemaContainer').slideToggle(100, function () {
            $('#searchCinemaBtn').toggleClass('active', '');
         });
      });
      $('#searchMovieBtn').click(function () {
         $('#searchCinemaContainer').hide();
         $('#searchCinemaBtn').removeClass('active');
         $('#searchMovieContainer').slideToggle(100, function () {
            $('#searchMovieBtn').toggleClass('active', '');
         });
      });
      $('#regionSelect').click(function () {
         if ($('.regionName').not(':visible')) {
            $('#regionSelect').toggleClass('active', '');
         }
         mini_Accordion($('#regionSelect'), $('.regionName'));
      });
      $('.synopsisSelector').click(function () {
         $(this).toggleClass('active', '');
         var target = $(this).next('.showSynopsis');
         mini_Accordion(this, target);
      });
      $('.movieSchedule').click(function () {
         $(this).find('.movieGuideDayHeading').toggleClass('active', '');
         var target = $(this).find($('.movieShowTimes'));
         mini_Accordion(this, target);
      });
      $('.movieGuideVenueDay').click(function () {
         $(this).toggleClass('active', '');
         var target = $(this).next('.dayShow');
         mini_Accordion(this, target);
      });
   };
   if ('.movieGuide') {
      onReady();
      $('.movieSchedule').first().click();
      $('.movieGuideVenueDay').first().click();
   }
}

function olympicRemainingTime(endTime) {
   var d = new Date();
   var utc = d.getTime();
   var t = Date.parse(endTime) - new Date(utc + (3600000 * 3));
   var seconds = Math.floor(t / 1000) % 60;
   var minute = Math.floor((t / 1000) / 60) % 60;
   var hours = Math.floor((t / 1000 / 60 / 60) % 24);
   var days = Math.floor((t / 1000 / 60 / 60 / 24));
   return {
      'total': t, 'days': days, 'hours': hours, 'minutes': minute, 'seconds': seconds
   };
}

// Todo - we need to initialise these types of methods only on the page/s which require them
function mini_olympicsClock_init() {
   var deadline = '8/5/2016 23:00:00 GMT-0300';
   var clock = $('#olympics-clock-div');
   var timeInterval = setInterval(function () {
      var t = olympicRemainingTime(deadline);
      clock.html('<ul><li><p>' + t.days + '</p><span> DAYS </span>' + '</li><li><p>' + t.hours + '</p><span> HRS </span>' + '</li><li><p>'
         + t.minutes + '</p><span> MINS </span>' + '</li><li><p>' + t.seconds + '</p><span>SECS </span>' + '</li></ul> ');
      if (t.total <= 0) {
         clearInterval(timeInterval);
         $(".OlympicsClockWidget").css('display', 'none');
      }
   }, 1000);

   function tick() {
      $('#olympicsTicker li:first').slideUp(function () {
         $(this).appendTo($('#olympicsTicker')).slideDown();
      });
   }

   setInterval(function () {
      tick()
   }, 5000);

   if ('#OlympicsCalendar') {
      $('#datePicker').datepicker({
         dateFormat: 'd-m-yy',
         setDate: new Date(),
         minDate: (new Date(2016, 8 - 1, 5)),
         maxDate: (new Date(2016, 8 - 1, 21)),
         showOn: "button",
         buttonImage: "/img/olympics/calander-img.png",
         buttonImageOnly: true,
         onSelect: function (dateText) {
            $(this).change();
         }
      }).change(function () {
         window.location = '/olympics-rio-2016/schedules?date=' + this.value;
      });
   }
}

