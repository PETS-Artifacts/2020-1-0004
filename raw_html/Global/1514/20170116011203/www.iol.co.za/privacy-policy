$('.subnav-menu').hide();
$('.headnav-menu').hide();

var newspaperNames = [
   "capeargus",
   "capetimes",
   "dailynews",
   "ios",
   "isolezwe",
   "mercury",
   "pretoria-news",
   "the-star",
   "thepost",
   "sundaytribune",
   "sundayindependent",
   "saturday-star",
   "weekend-argus"
];

var ion = {
   ua: '',
   uri: location.pathname.substring(1),
   uri0: '',
   redirect: true,
   redirected: false,
   screenWidth: 768,
   screenHeight: 1024,
   logging: false,
   logs: [],
   startTime: window.ion_startTime,
   inlineTime: Date.now(),
   ads: {
      status: {},
      listeners: []
   }
};

if (ion.uri.length) {
   var index = ion.uri.indexOf('/');
   if (index > 0) {
      ion.uri0 = ion.uri.substring(0, index);
      var lastIndex = ion.uri.lastIndexOf('/');
      if (lastIndex >= index) {
         ion.articleSection = ion.uri.substring(0, lastIndex);
      }
   } else {
      ion.uri0 = ion.uri;
   }
}

if (location.search) {
   if (/^nor/.test(location.search.substring(1))) {
      ion.redirect = false;
   }
}

if (navigator.userAgent) {
   ion.ua = navigator.userAgent;
}

function console_log() {
   if (!ion.logging) {
   } else if (arguments.length > 4) {
      console.log(arguments);
   } else if (arguments.length === 4) {
      console.log(arguments[0], arguments[1], arguments[2], arguments[3]);
   } else if (arguments.length === 3) {
      console.log(arguments[0], arguments[1], arguments[2]);
   } else if (arguments.length === 2) {
      console.log(arguments[0], arguments[1]);
   } else if (arguments.length === 1) {
      console.log(arguments[0]);
   }
}

function console_state() {
   if (!ion.logging) {
   } else if (arguments.length > 4) {
      console.log(arguments);
   }
   if (arguments.length == 4) {
      console.log(arguments[0], arguments[1], arguments[2], arguments[3]);
   } else if (arguments.length === 3) {
      console.log(arguments[0], arguments[1], arguments[2]);
   } else if (arguments.length === 2) {
      console.log(arguments[0], arguments[1]);
   } else if (arguments.length === 1) {
      console.log(arguments[0]);
   }
   ion.logs.push(arguments);
}

function console_warn() {
   if (ion.logging && arguments && arguments.length > 0) {
      if (arguments.length > 4) {
         console.warn(arguments);
      }
      if (arguments.length == 4) {
         console.warn(arguments[0], arguments[1], arguments[2], arguments[3]);
      } else if (arguments.length === 3) {
         console.warn(arguments[0], arguments[1], arguments[2]);
      } else if (arguments.length === 2) {
         console.warn(arguments[0], arguments[1]);
      } else if (arguments.length === 1) {
         console.warn(arguments[0]);
      }
      if (arguments.length > 0) {
         ion.logs.push(arguments);
         if (arguments[0] && arguments[0].stack) {
            console.error(arguments[0].stack);
         }
         if (arguments.length > 1) {
            var lastArg = arguments[arguments.length - 1];
            if (lastArg && lastArg.stack) {
               console.error(lastArg.stack);
            }
         }
      }
   }
}

if (location.hostname === 'localhost' || location.search === '?logging'
   || (/Chrome/.test(ion.ua) && /Linux/.test(ion.ua) && !/Android/.test(ion.ua))) {
   ion.logging = true;
   console_log('logging enabled', ion.ua);
}

console_log('inline', location.pathname, document.title);

if (document.documentElement && document.documentElement.clientWidth) {
   ion.screenWidth = document.documentElement.clientWidth;
   ion.screenHeight = document.documentElement.clientHeight;
} else if (window.screen) {
   ion.screenWidth = screen.width;
   ion.screenHeight = screen.height;
} else if (typeof window.innerWidth === 'number') {
   ion.screenWidth = window.innerWidth;
   ion.screenHeight = window.innerHeight;
} else if (document.body && document.body.clientWidth) {
   ion.screenWidth = document.body.clientWidth;
   ion.screenHeight = document.body.clientHeight;
} else {
   console_log('WARN screenWidth none');
}

function extractArticleId(name) {
   if (/[0-9]{5}$/.test(name)) {
      return name.match(/([0-9]*)$/)[1];
   }
}

function extractBetaArticleId() {
   var index = location.pathname.lastIndexOf('-');
   if (/\-1\.[0-9]+[0-9]{4}$/.test(location.pathname)) {
      return location.pathname.substring(index + 3);
   } else if (/\-[0-9]+[0-9]{4}$/.test(location.pathname)) {
      return location.pathname.substring(index + 1);
   }
}

function getArticleId() {
   var mobiArticleId = extractArticleId(location.hash);
   if (mobiArticleId) {
      return mobiArticleId;
   }
   var betaArticleId = extractBetaArticleId();
   if (betaArticleId) {
      return betaArticleId;
   }
   return extractArticleId(location.pathname);
}

ion.articleId = getArticleId();

function getMobiPath() {
   if (location.pathname === '/') {
      return '/';
   }
   if (ion.articleId) {
      return '/#!/article/' + ion.articleId;
   }
   var paths = location.pathname.substring(1).split('/');
   if (paths.length === 1) {
      return '/#!/section/' + paths[0];
   } else if (paths.length > 2 && paths[2] === 'tv-guide') {
      return '/#!/tvguide';
   } else if (paths.length > 2 && paths[2] === 'movie-guide') {
      return '/#!/movieguide-region-select';
   } else { // subsection
      return '/#!/section/' + paths[0];
   }
}

function getBetaPath() {
   if (/\-1\.[0-9]+[0-9]{4}$/.test(location.pathname)) { // is beta article link
      var index = location.pathname.lastIndexOf('-');
      if (index > 0) {
         return location.pathname.substring(0, index + 1) + location.pathname.substring(index + 3);
      }
   }
   return location.pathname;
}

function getClassicPath() {
   if (/\-[0-9]+[0-9]{4}$/.test(location.pathname)) {
      var index = location.pathname.lastIndexOf('-');
      if (index > 0) {
         return location.pathname.substring(0, index + 1) + '1.' + location.pathname.substring(index + 1);
      }
   }
   return location.pathname;
}

function isClassicOnly() {
   if (!ion.uri0) {
      console_warn('uri0 empty', ion.uri);
      return false;
   }
   if (ion.uri0 === 'search-page-results') {
      //return true;
   }
   if (ion.uri0 === 'feedback') {
      return true;
   }
   if (ion.uri0 === 'blogs') {
      return true;
   }
   if (ion.uri0 === 'polopoly_fs') {
      return true;
   }
   return false;
}

ion.betaPath = getBetaPath();
ion.classicPath = getClassicPath();
ion.mobiPath = getMobiPath(); // TODO decommish mobi

function redirect(href) {
   if (href) {
      ion.redirected = true;
      console_log('redirect', href);
      if (location.hostname !== 'localhost') {
         $('#mobileNav-items').remove(); // TODO
         location.href = href;
      }
   }
}

if (ion.redirect) {
   var locationParts = location.pathname.substring(1).split('/');
   if (isClassicOnly()) {
      redirect('http://classic.iol.co.za' + location.pathname); // not for articles
   } else if (/ MSIE [5-7]/.test(ion.ua)) { // older IE to classic
      redirect('http://classic.iol.co.za' + getClassicPath());
   } else if (/ MSIE 8/.test(ion.ua)) { // IE 8 to sbeta
      redirect('http://www.iol.co.za' + getBetaPath());
   } else if (/ Opera Mini/.test(ion.ua) && ion.mobiPath) { // opera mini to mobi
      //redirect('http://mobi.iol.co.za' + ion.mobiPath);
   } else if (ion.screenWidth < 300 && ion.mobiPath) { // mobi
      //redirect('http://mobi.iol.co.za' + ion.mobiPath);
   } else {
      console_log('redirect none', ion.uri0);
   }
}

function headText(text) { // TODO deprecate
   return text;
}

function mini_scrollTop() {
   $('html, body').animate({scrollTop: 0}, 200);
   setTimeout(function () {
      window.scrollTo(0, 0);
   }, 200);
}

function mini_menu() {
   console_log('mini_menu');
   $('.subnav-item a').on('click', function (e) {
      if ($(this).attr('target') === '_blank') {
      } else {
         $(this).css('color', '#dd1b23');
         //$(this).css('border-bottom', 'solid 1px #dd1b23');
         setTimeout(function () {
            $('body').css('opacity', '0.7');
         }, 100);
      }
   });
   $('html').removeClass('no-touchevents');
   $('a.headnav-menu').on('click', function (e) {
      console_log('mini_menu headnav menu clicked');
   });
   if (false) {
      if (ion.screenWidth >= 600) {
         $('.headnav-news').text(headText('NEWS'));
         $('.headnav-sport').text(headText('SPORT'));
         $('.headnav-business').text(headText('BUSINESS'));
         $('.headnav-motoring').text(headText('MOTORING'));
         $('.headnav-travel').text(headText('TRAVEL'));
      }
   }
   if (ion.screenWidth >= 600) {
      if (ion.screenWidth < 1024) {
         $('.subnav-top .subnav-more span').text('NEXT');
      }
   }
   if (ion.screenWidth >= 768) {
      if (false) {
         $('.headnav-tonight').text(headText('TONIGHT'));
         $('.headnav-lifestyle').text(headText('LIFESTYLE'));
         $('.headnav-scitech').text(headText('SCITECH'));
      }
   }
   if (ion.screenWidth >= 1024) {
      if ($('.subnav-more span').text() === 'NEXT') {
         $('.subnav-more span').text('NEXT PAGE');
      }
      if (false) {
         $('.headnav-weather').text(headText('WEATHER'));
         $('.headnav-tvguide').text(headText('TV GUIDE'));
         $('.headnav-marketindicators').text(headText('MARKET IND.'));
         $('.headnav-movieguide').text(headText('MOVIE GUIDE'));
      }
   }
   var displayed = false;

   function displayItems(recurseCount) {
      if (!displayed) {
         var width = $('.headnav ul').width();
         console_log('displayItems', width);
         if (width < 400) {
            displayed = true;
            //displayItemsImpl();
         } else if (recurseCount < 10) {
            setTimeout(function () {
               displayItems(recurseCount + 1);
            }, 1000);
         }
      }
   }

   if (ion.screenWidth >= 540 && ion.screenHeight >= 480) {
      $('.headnav').css('display', 'inline');
      $('.headnav ul li a').on('click', function (e) {
         if ($(this).attr('target') !== '_blank') {
            $(this).css('color', '#dd1b23');
            $('footer').hide();
            setTimeout(function () {
               $('body').css('opacity', '0.7');
            }, 100);
         }
      });
      displayItems(0);
   }
   $('.headnav-menu').show();
}

function mini_article() {
   if (ion.screenWidth < 768) {
      var elements = $('.article-more p');
      if (elements.length > 9) {
         elements = elements.slice(4);
         console_log('article-more', elements.length);
         $(elements).hide();
         $('.article-more-link').css('display', 'block');
         $('.article-more-link').on('click', function (e) {
            e.preventDefault();
            $('.article-more-link').hide();
            $(elements).show();
         });
      }
   } else if (ion.screenWidth >= 1280) {
      var height = $('.article-column').height();
      if (height > 1500) {
         $('.articleLeft-menuCards-container').css('padding-top', '600px');
      }
   }
}

function mini_onClickCard(that) {
   console_log('mini_onClickCard', that);
   console_log('mini_onClickCard', $(that).find('.card-header'));
   $(that).find('.card-title').css('color', '#dd1b23');
   $('body').css('opacity', '0.7');
}

function inline_init() {
   ion.screenWidth = window.innerWidth
      || document.documentElement.clientWidth
      || document.body.clientWidth;
   // TODO register on scrolling cards
   $('.card-container').on('click', function (e) {
      mini_onClickCard(this);
   });
   $('p.link a').on('click', function (e) {
      window.ion.plink = this;
      if ($(this).attr('target')) {
         $(this).css('color', '#808080');
      } else {
         $(this).css('color', '#dd1b23');
         $('body').css('opacity', '0.7');
      }
   });
   $('.menuCard-header-link').on('click', function (e) {
      console_log('menuCard-header-link click');
      $(this).find('.menuCard-header').css('color', '#dd1b23');
      $('body').css('opacity', '0.7');
      //$('footer').hide();
   });
   $('.logo-link').on('click', function (e) {
      $('body').css('opacity', '0.7');
   });
   $('.menuCard-sectionLink').on('click', function (e) {
      $(this).css('color', '#dd1b23');
      $('body').css('opacity', '0.7');
   });
}

function mini_servicesGuide_init() {
   var servicesGuide = $('#servicesGuide');
   var getValues = function () {
      return {
         titleId: $(servicesGuide).find('.titleId').val(),
         servicesId: $(servicesGuide).find('.servicesId').val(),
         searchInput: $(servicesGuide).find('.searchInput').val()
      }
   };
   var isValues = function (values) {
      return (values.searchInput && values.servicesId && values.titleId);
   };
   var onChange = function () {
      var disabled = true;
      var values = getValues();
      if (isValues(values)) {
         disabled = false;
      }
      console_log('onChange', disabled, values);
      $('#servicesGuide .button').prop('disabled', disabled);
   };
   var onSubmit = function (e) {
      var values = getValues();
      var location = '/services-guide/'
         + values.servicesId
         + '?z='
         + values.titleId
         + '+'
         + values.servicesId
         + '+'
         + values.searchInput;
      console_log('location', location, values);
      window.location = location;
   };
   var onReady = function () {
      $('#servicesGuide .searchButton').on('click', function (e) {
         onSubmit();
      });
      $('#servicesGuide select').on('change', function (e) {
         onChange();
      });
      $('#servicesGuide .searchInput').on('keydown', function (e) {
         if (e.keyCode == 13) {
            onSubmit();
         }
      });
      onChange();
   };
   if (servicesGuide) {
      onReady();
   }
}

function mini_tvGuide_init() {
   var scroller = $('scroller');
   var date = new Date();
   var minutesSinceMidnight = Math.floor((date.getTime() - date.setHours(0, 0, 0, 0)) / 60000);
   var position = (minutesSinceMidnight * 8640 / (24 * 60)) - 90;
   var scroll = position;
   var int100;
   var onReady = function () {
      $('#scroller').scrollLeft(position);
      $('#tvGuide-btn-next').on('touchstart click', function (e) {
         e.stopPropagation();
         e.preventDefault();
         if (event.type == "touchstart") {
            $(this).off('click');
         }
      });
      $('#tvGuide-btn-next').on('touchstart mousedown', function () {
         int100 = setInterval(function () {
            repeatingScroll(scroll += 40);
         }, 40);
      }).on('touchend mouseup mouseout', function () {
         clearInterval(int100);
      });
      $('#tvGuide-btn-prev').on('touchstart mousedown', function () {
         int100 = setInterval(function () {
            repeatingScroll(scroll -= 40);
         }, 40);
      }).on('touchend mouseup mouseout', function () {
         clearInterval(int100);
      });
   }
   if (scroller) {
      onReady();
   }
   function repeatingScroll(scroll) {
      $('#scroller').scrollLeft(scroll);
   }
}

function mini_jseSharesAccordion_init() {
   $('.shareCategoryTable').each(function () {
      var thead = $(this).find('thead');
      var iconSVG = $(this).find('.icon-svg-chevron-down');
      var tbody = $(this).find('tbody');
      tbody.hide();
      thead.click(function () {
         tbody.slideToggle(10, function () {
            if (tbody.is(':visible')) {
               iconSVG.attr('class', 'icon-svg-chevron-up');
               iconSVG.html("<path d='M1683 1331l-166 165q-19 19-45 19t-45-19l-531-531-531 531q-19 19-45 19t-45-19l-166-165q-19-19-19-45.5t19-45.5l742-741q19-19 45-19t45 19l742 741q19 19 19 45.5t-19 45.5z'/>");
            } else {
               iconSVG.attr('class', 'icon-svg-chevron-down');
               iconSVG.html("<path d='M1683 808l-742 741q-19 19-45 19t-45-19l-742-741q-19-19-19-45.5t19-45.5l166-165q19-19 45-19t45 19l531 531 531-531q19-19 45-19t45 19l166 165q19 19 19 45.5t-19 45.5z'/>");
            }
         });
      })
   })
}

function mini_Accordion(trigger, target) {
   var iconSVG = $(trigger).find('.chevron-down');
   target.slideToggle(400, function () {
      if (target.is(':visible')) {
         iconSVG.html("<div className='chevron-up'><svg className='icon-svg-chevron-up' viewBox='0 0 1792 1792'><path d='M1683 1331l-166 165q-19 19-45 19t-45-19l-531-531-531 531q-19 19-45 19t-45-19l-166-165q-19-19-19-45.5t19-45.5l742-741q19-19 45-19t45 19l742 741q19 19 19 45.5t-19 45.5z'/></svg></div>");
      } else {
         iconSVG.html("<div className='chevron-down'><svg className='icon-svg-chevron-down' viewBox='0 0 1792 1792'><path d='M1683 808l-742 741q-19 19-45 19t-45-19l-742-741q-19-19-19-45.5t19-45.5l166-165q19-19 45-19t45 19l531 531 531-531q19-19 45-19t45 19l166 165q19 19 19 45.5t-19 45.5z'/></svg></div>");
      }
   });
}

function mini_subNavDropDown_init() {
   $('#subNavDropToggle').click(function () {
      mini_Accordion($('#subNavDropToggle'), $('#subNavDropDown'));
   })
}


