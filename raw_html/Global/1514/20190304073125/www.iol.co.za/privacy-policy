             byRow: true,
             property: 'height'
        });
        $('.more-from .one-quarter .content').matchHeight({
             byRow: true,
             property: 'height'
        });
        $('.feature-height').matchHeight({
             byRow: true,
             property: 'height'
        });
       var zimH = $('.trends-wrap').height();
       $('.trending-tweets.zim').height(zimH - 30);
    }
    if ($(document).width() < 767) {
        $('.slick-slide .content').matchHeight({
             byRow: true,
             property: 'height'
        });
        $('.trend').matchHeight({
             byRow: true,
             property: 'height'
        });
    }
});

