        });
        $('article a p').matchHeight({
             byRow: true,
             property: 'height'
        });
        $('.equal').matchHeight({
             byRow: true,
             property: 'height'
        });
        $('.more-from .one-quarter').matchHeight({
             byRow: true,
             property: 'height'
        });
        $('.feature-height').matchHeight({
             byRow: true,
             property: 'height'
        });
    }
