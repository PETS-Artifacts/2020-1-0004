var ion = {
    ua: '',
    uri: location.pathname.substring(1),
    uri0: '',
    redirect: true,
    redirected: false,
    screenWidth: 768,
    screenHeight: 1024,
    logging: false,
    logs: [],
    startTime: window.ion_startTime,
    inlineTime: Date.now(),
    ads: {
        status: {},
        listeners: []
    }
};

const postCompetition = '/competition'

if (ion.uri.length) {
    var index = ion.uri.indexOf('/');
    if (index > 0) {
        ion.uri0 = ion.uri.substring(0, index);
        var lastIndex = ion.uri.lastIndexOf('/');
        if (lastIndex >= index) {
            ion.articleSection = ion.uri.substring(0, lastIndex);
        }
    } else {
        ion.uri0 = ion.uri;
    }
}

if (location.search) {
    if (/^nor/.test(location.search.substring(1))) {
        ion.redirect = false;
    }
}

if (navigator.userAgent) {
    ion.ua = navigator.userAgent;
}

function console_log() {
    if (!ion.logging) {} else if (arguments.length > 4) {
        console.log(arguments);
    } else if (arguments.length === 4) {
        console.log(arguments[0], arguments[1], arguments[2], arguments[3]);
    } else if (arguments.length === 3) {
        console.log(arguments[0], arguments[1], arguments[2]);
    } else if (arguments.length === 2) {
        console.log(arguments[0], arguments[1]);
    } else if (arguments.length === 1) {
        console.log(arguments[0]);
    }
}

function console_state() {
    if (!ion.logging) {} else if (arguments.length > 4) {
        console.log(arguments);
    }
    if (arguments.length == 4) {
        console.log(arguments[0], arguments[1], arguments[2], arguments[3]);
    } else if (arguments.length === 3) {
        console.log(arguments[0], arguments[1], arguments[2]);
    } else if (arguments.length === 2) {
        console.log(arguments[0], arguments[1]);
    } else if (arguments.length === 1) {
        console.log(arguments[0]);
    }
    ion.logs.push(arguments);
}

function console_warn() {
    if (ion.logging && arguments && arguments.length > 0) {
        if (arguments.length > 4) {
            console.warn(arguments);
        }
        if (arguments.length == 4) {
            console.warn(arguments[0], arguments[1], arguments[2], arguments[3]);
        } else if (arguments.length === 3) {
            console.warn(arguments[0], arguments[1], arguments[2]);
        } else if (arguments.length === 2) {
            console.warn(arguments[0], arguments[1]);
        } else if (arguments.length === 1) {
            console.warn(arguments[0]);
        }
        if (arguments.length > 0) {
            ion.logs.push(arguments);
            if (arguments[0] && arguments[0].stack) {
                console.error(arguments[0].stack);
            }
            if (arguments.length > 1) {
                var lastArg = arguments[arguments.length - 1];
                if (lastArg && lastArg.stack) {
                    console.error(lastArg.stack);
                }
            }
        }
    }
}

if (location.hostname === 'localhost' || location.search === '?logging' || (/Chrome/.test(ion.ua) && /Linux/.test(ion.ua) && !/Android/.test(ion.ua))) {
    ion.logging = true;
    console_log('logging enabled', ion.ua);
}

console_log('inline', location.pathname, document.title);

if (typeof window.innerWidth === 'number') {
    ion.screenWidth = window.innerWidth;
    ion.screenHeight = window.innerHeight;
} else if (window.screen) {
    ion.screenWidth = screen.width;
    ion.screenHeight = screen.height;
} else if (document.body && document.body.clientWidth) {
    ion.screenWidth = document.body.clientWidth;
    ion.screenHeight = document.body.clientHeight;
} else if (document.documentElement && document.documentElement.clientWidth) {
    ion.screenWidth = document.documentElement.clientWidth;
    ion.screenHeight = document.documentElement.clientHeight;
} else {
    console_log('WARN screenWidth none');
}

function extractArticleId(name) {
    if (/[0-9]{5}$/.test(name)) {
        return name.match(/([0-9]*)$/)[1];
    }
}

function extractBetaArticleId() {
    var index = location.pathname.lastIndexOf('-');
    if (/\-1\.[0-9]+[0-9]{4}$/.test(location.pathname)) {
        return location.pathname.substring(index + 3);
    } else if (/\-[0-9]+[0-9]{4}$/.test(location.pathname)) {
        return location.pathname.substring(index + 1);
    }
}

function getArticleId() {
    var mobiArticleId = extractArticleId(location.hash);
    if (mobiArticleId) {
        return mobiArticleId;
    }
    var betaArticleId = extractBetaArticleId();
    if (betaArticleId) {
        return betaArticleId;
    }
    return extractArticleId(location.pathname);
}

ion.articleId = getArticleId();

function mini_servicesGuide_init() {
    var servicesGuide = $('#servicesGuide');
    var getValues = function() {
        return {
            titleId: $(servicesGuide).find('.titleId').val(),
            servicesId: $(servicesGuide).find('.servicesId').val(),
            searchInput: $(servicesGuide).find('.searchInput').val()
        }
    };
    var isValues = function(values) {
        return (values.searchInput && values.servicesId && values.titleId);
    };
    var onChange = function() {
        var disabled = true;
        var values = getValues();
        if (isValues(values)) {
            disabled = false;
        }
        console_log('onChange', disabled, values);
    };
    var onSubmit = function() {
        var values = getValues();
        var location = '/services-guide/' + values.servicesId + '?z=' + values.titleId + '+' + values.servicesId + '+' + values.searchInput;
        console_log('location', location, values);
        window.location = location;
    };
    var onReady = function() {
        $('.searchBar .red-btn').on('click', function() {
            onSubmit();
        });
        $('.servicesGuideSearch select').on('change', function() {
            onChange();
        });
        $('.searchBar .searchInput').on('keydown', function() {
            if (e.keyCode == 13) {
                onSubmit();
            }
        });
        onChange();
    };
    if (servicesGuide) {
        onReady();
    }
}

function mini_tvGuide_init() {
    var scroller = $('scroller');
    var date = new Date();
    var minutesSinceMidnight = Math.floor((date.getTime() - date.setHours(0, 0, 0, 0)) / 60000);
    var position = (minutesSinceMidnight * 8640 / (24 * 60)) - 90;
    var scroll = position;
    var int100;
    var onReady = function() {
        $('#scroller').scrollLeft(position);
        $('#tvGuide-btn-next').on('touchstart click', function(e) {
            e.stopPropagation();
            e.preventDefault();
            if (event.type == "touchstart") {
                $(this).off('click');
            }
        });
        $('#tvGuide-btn-next').on('touchstart mousedown', function() {
            int100 = setInterval(function() {
                repeatingScroll(scroll += 40);
            }, 40);
        }).on('touchend mouseup mouseout', function() {
            clearInterval(int100);
        });
        $('#tvGuide-btn-prev').on('touchstart mousedown', function() {
            int100 = setInterval(function() {
                repeatingScroll(scroll -= 40);
            }, 40);
        }).on('touchend mouseup mouseout', function() {
            clearInterval(int100);
        });
    };
    if (scroller) {
        onReady();
    }

    function repeatingScroll(scroll) {
        $('#scroller').scrollLeft(scroll);
    }
}

function searchNav_init() {
    $('.iconSvgSearch').on('click', function(e) {
        e.preventDefault();
        if ($('#commercialNavSearchForm').is(':visible')) {
            var inputText = $('#commercialNavSearch').val();
            if (inputText) {
                $('body').css('opacity', '0.7');
                var searchHref = '/search/?q=' + encodeURIComponent(inputText);
                console_log('searchSubmit', inputText, searchHref);
                window.location.href = searchHref;
            } else {
                $('#commercialNavSearchForm').animate({ width: 'toggle' });
            }
        } else {
            $('#commercialNavSearchForm').animate({ width: 'toggle' });
        }
    });
}

function accordion_Down(trigger, target) {
    var iconSVG = $(trigger).find('.chevronUp');
    if (iconSVG.length < 1) {
        iconSVG = $(trigger).find('.chevronDown');
    }
    target.slideToggle(300, function() {
        if (target.is(':visible')) {
            target.addClass('active');
            iconSVG.html("<div class='chevronUp'><svg class='iconSvgChevronUp' viewBox='0 0 1792 1792'><path d='M1683 1331l-166 165q-19 19-45 19t-45-19l-531-531-531 531q-19 19-45 19t-45-19l-166-165q-19-19-19-45.5t19-45.5l742-741q19-19 45-19t45 19l742 741q19 19 19 45.5t-19 45.5z'/></svg></div>");
        } else {
            target.removeClass('active');
            iconSVG.html("<div class='chevronDown'><svg class='iconSvgChevronDown' viewBox='0 0 1792 1792'><path d='M1683 808l-742 741q-19 19-45 19t-45-19l-742-741q-19-19-19-45.5t19-45.5l166-165q19-19 45-19t45 19l531 531 531-531q19-19 45-19t45 19l166 165q19 19 19 45.5t-19 45.5z'/></svg></div>");
        }
    });
}

function mini_movieGuide_init() {
    var icon = $(this).find('.icon-down-open');

    var onReady = function() {
        $(".movieGuidelogo").on('click', function() {
            $('.movieGuidelogo').css('border-color', '#aaa');
            $(this).css('border', '4px solid #2c83b5');
            $('.movieGuideResults').hide();
        });
        $("#cinema-all").on('click', function() {
            $('#movieguide-results-all').slideToggle(10, function() {});
        });
        $("#cinema-ster").click(function() {
            $('#movieguide-results-ster').slideToggle(10, function() {});
        });
        $("#cinema-nume").on('click', function() {
            $('#movieguide-results-nume').slideToggle(10, function() {});
        });
        $("#cinema-cine").on('click', function() {
            $('#movieguide-results-cine').slideToggle(10, function() {});
        });
        $('#searchCinemaBtn').click(function() {
            $('#searchMovieContainer').hide();
            $('#searchMovieBtn').removeClass('active');
            $('#searchCinemaContainer').slideToggle(100, function() {
                $('#searchCinemaBtn').toggleClass('active', 200);
            });
            if (window.bLazy) {
                window.bLazy.revalidate();
            }
        });
        $('#searchMovieBtn').click(function() {
            $('#searchCinemaContainer').hide();
            $('#searchCinemaBtn').removeClass('active');
            $('#searchMovieContainer').slideToggle(100, function() {
                $('#searchMovieBtn').toggleClass('active', 200);
            });
            if (window.bLazy) {
                window.bLazy.revalidate();
            }
        });
        $('#regionSelect').click(function() {
            if ($('.regionName').not(':visible')) {
                $('#regionSelect').toggleClass('active', 200);
            }
            accordion_Down($('#regionSelect'), $('.regionName'));
        });
        $('.synopsisSelector').click(function() {
            $(this).toggleClass('active', 200);
            var target = $(this).next('.showSynopsis');
            accordion_Down(this, target);
        });
        $('.movieSchedule').click(function() {
            $(this).find('.movieGuideDayHeading').toggleClass('active', 200);
            var target = $(this).find($('.movieShowTimes'));
            accordion_Down(this, target);
        });
        $('.movieGuideVenueDay').click(function() {
            $(this).toggleClass('active', 200);
            var target = $(this).next('.dayShow');
            accordion_Down(this, target);
        });
    };
    if ('.movieGuide') {
        onReady();
        $('.movieSchedule').first().click();
        $('.movieGuideVenueDay').first().click();
    }
}

