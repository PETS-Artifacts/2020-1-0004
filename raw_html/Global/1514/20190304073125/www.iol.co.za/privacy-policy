   });

    $('.scroll').on('click', function(){
       $('html, body').animate({
           scrollTop: $( $.attr(this, 'href') ).offset().top - 96
       }, 500);
       return false;
    });

    $('.show-popup').on('click', function(e){
      e.preventDefault()
      var url = $(this).attr('href');
      $(url).show();
      // $('main').css('z-index', '302');
   });

   $('.close-btn').on('click', function(e){
      e.preventDefault()
      var url = $(this).attr('href');
      $(url).hide();
      // $('main').removeAttr('style');
   });


	if ($(document).width() > 767) {
		$(window).scroll(function() {
			if ($('#read-next').hasClass('closed')) {
				$('#read-next').hide(500);
         } else if ($(document).scrollTop() > $(window).height() + 500 && !$('#read-next').hasClass('closed')) {
            $('#read-next').fadeIn(500);
			} else {
				$('#read-next').fadeOut(500);
			}
		});
		$('.close-next-btn').on('click', function(){
			$('#read-next').addClass('closed').fadeOut(500);
		});
   }

   show_hide();
   show_hide_search();
   sub_nav();
   main_click();
   show_hide_content();
   show_hide_content_faq();
   remove();
});

function mini_inline_start() {
    console_log('mini_inline_initial_start', location.pathname, navigator.userAgent);
    try {
        if (ion.screenWidth < 768) {
            mobileTvBoxAd_init();
            mobileHighlightsAd_init();
        }
        searchNav_init();
        mini_mobileStickyAdClose();
        if (location.pathname.match('/movie-guide')) {
            mini_movieGuide_init();
        }
        if (location.pathname.match('/services-guide')) {
            mini_servicesGuide_init();
        }
        if (location.pathname.match('/tv-guide')) {
            mini_tvGuide_init();
        }
    } catch (err) {
        console_warn(err);
    }
}

if (!ion.redirected) {
    mini_inline_start();
}
