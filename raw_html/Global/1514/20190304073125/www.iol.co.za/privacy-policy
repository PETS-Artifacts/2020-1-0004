             byRow: true,
             property: 'height'
        });
    }
});

// Scroll to Top of Page
$(window).scroll(function() {
    if ($(this).scrollTop()) {
        $('#toTop:hidden').stop(true, true).fadeIn();
    } else {
        $('#toTop').stop(true, true).fadeOut();
    }
    if ($(document).width() > 767) {
        var scroll = $(window).scrollTop();
        if (scroll > 1) {
            $("header.black-bottom-border").addClass("scrolled");
            $(".ad-wallpaper-container").addClass("scrolled");
        } else {
            $("header.black-bottom-border").removeClass("scrolled");
            $(".ad-wallpaper-container").removeClass("scrolled");
        }
    }
});

function moveRelatedArticlesCarousel() {
    $('.related-article-inner').appendTo('#second-ad-slot')
}

function setUpcompetition () {
    var form = document.querySelector('[name = "competitiondetector"]');
    $('#' + form.id).on('submit', function(e){
        e.preventDefault()
        handleCompetitionSubmit(form.id)
    })
}

function handleCompetitionSubmit (id) {
    let elements = document.getElementById(id).elements
    submitCompetition(getAllFormFields(elements), id)
}

function getFormFields (formElements) {
    var formElementsArray = []
    for (var i in formElements) {
      var element = {}
      element.name = formElements[i].name
      element.value = formElements[i].value
      formElementsArray.push(element)
    }
    return formElementsArray.filter(function(element) {
      return element.name ? element.name.match('fields') !== 'null' : null
    })
}
  
function getAllFormFields (formElements) {
    var formEntries = {}
    for (var i = 0; i < formElements.length; i++) {
      if (formElements[i].type === 'checkbox') {
        if (!formEntries[formElements[i].name]) {
          formEntries[formElements[i].name] = []
        }
        if (formElements[i].checked) {
          formEntries[formElements[i].name].push(formElements[i].value)
        }
      } else if (formElements[i].type === 'radio') {
        if (formElements[i].checked) {
          formEntries[formElements[i].name] = formElements[i].value
        }
      } else {
        formEntries[formElements[i].name] = formElements[i].value
      }
    }
    return formEntries
}

function showErrorMessage (id, result, formId) {
    let competitionFormNode = document.getElementById(formId)
    if (competitionFormNode) {
      let input = document.getElementById(id)
      if (input) {
        input.insertAdjacentHTML('afterend', '<p class="form-error-validation" style="color:red">' + result + '</p>')
      }
    }
  }

function submitCompetition (formData, id) {
    var errorNode = document.getElementsByClassName('form-error-validation')
    while (errorNode.length > 0) {
        errorNode[0].parentNode.removeChild(errorNode[0])
    }
    $.post(postCompetition, {data: $.param(formData)}, function(data) {
        if (!data.success) {
            var errors = Object.keys(data.errors)
            for (var i = 0; i < errors.length; i++) {
                var messages = data.errors[errors[i]]
                for (var j = 0; j < messages.length; j++) {
                   showErrorMessage('fields-' + errors[i], messages[j], id)
                }
            }
        } else {
            console_log('Competition submitted succefully');
            $('#'+id).html('<p>Thank you for your submission</p>');
        }
    })
}

jQuery(document).ready(function($) {
    if ($('.related-articles-carousel').length) {
       moveRelatedArticlesCarousel();
    }
