     $('.footer-content').matchHeight({
          byRow: true,
          property: 'height'
     });
    if ($(document).width() > 767) {
        $('article:not(.bottom-article, .side-article, .article-headline, .small-article-square-image-left, .item, .trend, .related-article-slide) h3').matchHeight({
             byRow: true,
             property: 'height'
        });
       $('.related-article-slide h3, .trends article h3').removeAttr('style');
        $('.news-south-africa-gauteng .large-article-image-bottom h3, .news-south-africa-eastern-cape .large-article-image-bottom h3, .news-south-africa-north-west .large-article-image-bottom h3,'
         + ' .travel-south-africa-western-cape .large-article-image-bottom h3, .travel-south-africa-limpopo .large-article-image-bottom h3, .travel-south-africa-north-west .large-article-image-bottom h3').matchHeight({
             byRow: true,
             property: 'height'
        });
        $('.one-half .small-article-rect-image-left, .one-half .small-article-square-image-left').matchHeight({
            byRow: true,
            property: 'height'
        });
        $('.medium-article-image-top .content h3').matchHeight({
             byRow: true,
             property: 'height'
        });
        $('.medium-article-image-top .content p').matchHeight({
             byRow: true,
             property: 'height'
        });
        $('article a p').matchHeight({
             byRow: true,
             property: 'height'
        });
        $('.equal').matchHeight({
             byRow: true,
             property: 'height'
        });
