    <div class="container">
      <div class="row">
                  <div class="sidebar sidebar-first col-xs-12 col-sm-4 col-md-4 col-lg-4">
            <div class="panel-pane pane-custom pane-1 guide-ref active first nav-tabs-content sidebar-menu-table pane-toc"  >
  
      
  
  <div class="pane-content">
    <p>TOC</p>
  </div>

  
