  <div class="standardSubnav_header_fmt">
    <div class="standardSubnav_headerText_fmt">Policies</div>
  </div>
  <div id="standardSubnav_fmt">
    <ul id="st-polnav">
      <li class="standardSubnav_subhead_fmt">
        <a href="http://www.webmd.com/about-webmd-policies/about-advertising-policy" onclick="return sl(this,'','st-polnav_1');">Advertising Policy</a>
      </li>
      <li class="standardSubnav_subhead_fmt">
