<script type="text/javascript">
(function(){
	// duckpunching s_before_pv as some assets are using that vs our event driven system. This lets the event system get triggered as well as the other functions	
	var _old = window.s_before_pv;	
	
	window.s_before_pv = function() {
		var timestamp = Number(new Date());
		if (window.location.protocol === 'http:') {
			webmd.load({image: '//b.scorecardresearch.com/b?c1=2&c2=6035829&c3=&c4=&c5=&c6=&c7=' + encodeURIComponent(window.location.href) + '&c9=' + encodeURIComponent(window.document.referrer) + '&c15=&cv=2.0&cj=1&cb='+ timestamp});
		}
		if (window.location.protocol === 'https:') {
			webmd.load({image: '//sb.scorecardresearch.com/b?c1=2&c2=6035829&c3=&c4=&c5=&c6=&c7=' + encodeURIComponent(window.location.href) + '&c9=' + encodeURIComponent(window.document.referrer) + '&c15=&cv=2.0&cj=1&cb='+ timestamp});
		}
		
		_old();
	}

	if (webmd && webmd.ads2Consumer) {

		var $mainContainer = $('#mainContentContainer_area'),
		_tmp = $('#bannerAd_rdr').clone(true),
		isSymtomCheckerSEO = $('.symptoms_seo_titlebar').length>0 ? true : false;
		
        if($mainContainer.length > 0) {
            $('#bannerAd_rdr').remove();
        	$mainContainer.prepend(_tmp);
        }
        else if($('header').length > 0) {
            $('#bannerAd_rdr').remove();
        	$(_tmp).insertAfter($('header').eq(0));
        }

		if(isSymtomCheckerSEO) {
        	$('html').addClass('rising-star-banner symptom-checker-seo');
        	$('#breadcrumb_ctr').append($('#bannerAd_rdr'));
		}
		else {
			$('html').addClass('rising-star-banner');
		}

		webmd.ads2Consumer.display();
	}
})();
</script>

<!-- Forteo health solutions footer functionality -->
<script>
	(function() {
	// banned topic ids
	var topic_ids = [1844, 2726, 1662, 2946, 3937, 1811, 1816, 2729, 4290, 1822, 1824, 3645, 1828, 2730, 1831, 1688, 1839, 3573, 3572];
	var health_solutions = document.getElementById('sp_ft_rdr') || document.getElementsByClassName('healthSolutions').item(0);

	if (health_solutions) {
		var i;
		var li;
		var els_off = $('.forteo-off',health_solutions);
		var els_on = $('.forteo-on',health_solutions);

		// show default hidden elements if current topic id is not found in the banned topic ids array
		if (!topic_ids.filter(function(id) {
			return id == s_topic;
		}).length) {
			// show default hidden elements
			for (i=0; i<els_off.length; i++) {
				els_off[i].style.display = 'inline';
			}

			deleteListItems(els_on);
		}
		else {
			deleteListItems(els_off);
		}
	}

	function deleteListItems(elementArray) {
		// delete extra class elements parent list items
		for (i=0; i<elementArray.length; i++) {
			// capture li element: span < a < li
			li = elementArray[i].parentNode.parentNode;

			// if li parent exists and is a ul delete the list item
			if (li.parentNode && li.parentNode.tagName === 'UL') {
				li.parentNode.removeChild(li);
			}
		}
	}
})();
</script>
<style>
	#sp_ft_rdr .forteo-off,.healthSolutions .module-content a .forteo-off{display:none}
</style></div>
