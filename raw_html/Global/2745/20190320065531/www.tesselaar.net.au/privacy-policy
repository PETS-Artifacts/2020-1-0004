  // Is the browser <= IE8
  outdated_browser = (window.attachEvent && !window.addEventListener);
  if (outdated_browser) {
    document.getElementById("outdated_browser_message").style.display = 'block';
  }

  // Are cookies enabled?
  if (!Tesselaar.cookies_enabled()) {
    document.getElementById("cookies_disabled").style.display = 'block';
  }
</script>

