      var regExp2 = /A>/gi;
      var regExp = /<\/?[^>]+>/gi;
      function ReplaceTags(xStr){
        xStr = xStr.replace(regExp2,"A>|");
        xStr = xStr.replace(regExp,"");
        xStr = xStr.replace(/&nbsp;/g, " ");
        xStr = xStr.replace(/&gt;/g, ">");
        return xStr;
      }
      function trim(stringToTrim) {
          return stringToTrim.replace(/^\s+|\s+$/g,"");
      }
