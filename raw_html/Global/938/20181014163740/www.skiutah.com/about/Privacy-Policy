    <script type="text/javascript">
      function googleTranslateElementInit() {
        var opts = {pageLanguage: 'en', includedLanguages: 'en,de,es,fr,it,pt,ru,zh-CN', layout: google.translate.TranslateElement.InlineLayout.SIMPLE, gaTrack: true, gaId: 'UA-94387-10'};
        var el = document.getElementById('google_translate_element-in-header-aux') || document.getElementById('google_translate_element-in-header-main');
        if(getComputedStyle(el)['display'] === "block") {
          new google.translate.TranslateElement(opts, el.attributes.id.nodeValue);

          var sendGAEvent = function(e){
            if (ga) {
              ga('send', 'event', "Google Website Translator", "Translate", el.textContent);
            }
            // Only listen for the event after a click was made.
            // However, if the user initiates a change from the sticky bar this won't catch it.
            el.removeEventListener('DOMNodeInserted', sendGAEvent);
          };
          // Add event listener for changes to translate and pass them to the universal google analytics.
          if (el && el.addEventListener) {
            el.addEventListener('click', function(e){
              el.addEventListener('DOMNodeInserted', sendGAEvent, false);
            });
          }
        }
      }
    </script>
