	function ValidateComments(source, args) {
		var banned;
		var _comments = document.getElementById($j(".comments").attr("id"));
		var isValid = true;
		var containsBannedWords = false;
		source.errormessage = "";

		if ($j(".display-name").val() == "" || $j(".display-name").val() == "Your name") {
			source.errormessage += "Please enter a name to be displayed with your comment\n";
			isValid = false;
		}


		if ($j(".comments").val() == "" || $j(".comments").val() == "Your comments") {
			source.errormessage += "Please add your comments";
			isValid = false;
		}
		else {

		
			var re = new RegExp("[^\\w\\s]", "g");
			var cleanedComments = $j(".comments").val().replace(re, "");
			
			var words = cleanedComments.split(' ');
			var banned;				
			$j.ajax({
				url: '/banned.xml',
				async:false,
				dataType: "xml",
				success: function (xmlResponse) {
					banned = $j("BannedWord", xmlResponse).map(function () {
						return {
							value: $j(this).text()
						};
					}).get();
					
				}
			});

			var bannedArray = $j.map(banned, function (tag) {
				return tag.value.toLowerCase();
			});

			$j.each(bannedArray, function (index, value) {
				if ($j.inArray(value.toLowerCase(), words) != -1) {
					containsBannedWords = true;
					return false;
				}
			});

			if (containsBannedWords) {
				source.errormessage += "Sorry, your comment contains a banned word. Please edit your message and resubmit.";
				isValid = false;
			}

		}

		args.IsValid = isValid;
	}

	function checkCaptcha() {
		return ($j(".yourname").val() == "");
	}

	function GetFormValues() {
		if (!checkCaptcha) {
			return false;
		}
		else {
			
			$j(".reporter-name").val($j(".report-comment-name").val());
			$j(".reporter-email").val($j(".report-comment-email").val());
			$j(".reporter-comment").val($j(".report-comment-comments").val());
			$j(".reporter-reason").val($j(".report-reasons option:selected").text());

			return true;
		}
		


	}



	

	function closeDialog() {
		$j(".report-comment-name").val("");
		$j(".report-comment-email").val("");
		$j(".report-comment-comments").val("");

		$j(".report-reasons").selectmenu("destroy");
		$j('.report-reasons > option:first').attr("selected", true);
		$j(".report-reasons").selectmenu({ style: 'dropdown', menuWidth: 467, transferClasses: true });
		

		$j(".comment-reported").remove();
		$j(".reported-comment").val("");
		$j('.report-comment-form').dialog('destroy');

		$j(".watermark").each(function () {
			if ($j(this).val() == "") {
				$j(this).val($j(this).attr("title"));
				$j(this).css("color", "#333");
			}

			$j(this).focusout(function () {
				if ($j(this).val() == "") {
					$j(this).val($j(this).attr("title"));
					$j(this).css("color", "#333");
				}
			});

			$j(this).focus(function () {
				if ($j(this).val() == "" || $j(this).val() == $j(this).attr("title")) {
					$j(this).val("");
					$j(this).css("color", "#000");
				}
			});
		});
	}
