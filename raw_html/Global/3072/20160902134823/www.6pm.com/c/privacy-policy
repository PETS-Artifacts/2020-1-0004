<script>
  (function(welg) {
    if (typeof(welg) !== 'undefined' && welg) {
      var originalHeader = document.getElementById('legacyHeader');
      var refreshHeader = document.getElementById('refreshHeader');

      if (originalHeader && refreshHeader) {
        var originalHeaderClass = originalHeader.className;
        originalHeader.className = "hidden";
        refreshHeader.className = originalHeaderClass;

        document.body.className += ' welg'; // add welg class to body
      }
    }
  })(window.welg);
</script>
