<!-- Check if window.welg variable is set. If yes, display new footer -->
<script>
  (function(welg) {
    if (typeof(welg) !== 'undefined' && welg) {
      var originalFooter = document.getElementById('legacyFooter');
      var refreshFooter = document.getElementById('refreshFooter');

      if (originalFooter && refreshFooter) {
        var originalFooterClass = originalFooter.className;
        originalFooter.className = "hidden";
        refreshFooter.className = originalFooterClass;

        document.body.className += ' welg'; // add welg class to body
