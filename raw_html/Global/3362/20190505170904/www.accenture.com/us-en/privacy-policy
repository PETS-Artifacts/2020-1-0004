    <script type="text/javascript">
        function OptanonWrapper() {
        }
        (function () {
            // check if sat_track cookie exists
            var check = getCookie('sat_track');

            if (check === undefined) {
                document.cookie = "sat_track=true;path=/";
                console.log("creating sat_track");
            }

            //check if the banner has been closed already
                var minimizeBanner = getCookie('OptanonAlertBoxClosed');
                var localStorageName = "https://cdn.cookielaw.org/consent/1f50ee75-8cf1-481b-bba2-4f28aa30b796.js";
                var substringStart = localStorageName.lastIndexOf("/")+1;
                var substringEnd = localStorageName.lastIndexOf(".");
                localStorageName = localStorageName.substr(substringStart, substringEnd);
                localStorageName = localStorageName.replace(/-/g, '');
                var minimizeButtonKey = localStorageName;
            //if not closed yet delegate event to the buttons
            if (minimizeBanner === undefined) {
                $(document).one("click", "button[onclick*='Optanon.TriggerGoogleAnalyticsEvent(\'OneTrust Cookie Consent\'']", function (event) {
                    var minimizeButtonText = $("button.optanon-toggle-display.cookie-settings-button").text();
                    localStorage.setItem(minimizeButtonKey, minimizeButtonText);
                    $("body").append("<div id=\"optanon-minimize-wrapper\" class=\"optanon-toggle-display\"><button id=\"optanon-minimize-button\" title=\"" + minimizeButtonText + "\" aria-label=\"" + minimizeButtonText + "\" >" + minimizeButtonText + "</button></div>");
                });
            }//show the minimized button if the banner is closed already
            else {
                $(function () {
                    $("body").append("<div id=\"optanon-minimize-wrapper\" class=\"optanon-toggle-display\"><button id=\"optanon-minimize-button\" title=\"" + localStorage.getItem(minimizeButtonKey) + "\" aria-label=\"" + localStorage.getItem(minimizeButtonKey) + "\" >" + localStorage.getItem(minimizeButtonKey) + "</button></div>");
                });
            }
        }());
    </script>

