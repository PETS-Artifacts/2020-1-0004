                <div class="item active background parallax img-align-middle" data-image-override="" data-mobile-image="" data-background-image="" >
                    <section>
                        <div class="hero-title-wrapper">
                            <div class="page-title">
                                <h1 class="">Accenture Privacy Statement</h1>
                                
                            </div>
                        </div>
                    </section>
