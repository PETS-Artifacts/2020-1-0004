<!-- Merge Main Release: Changeset 724330 to 728897 -->
<div id="block-general" class="ui-container   row white"
     data-shape="delta"
     data-shape-color="white"
     data-show-top-line="False"
     data-scroll-speed="1"
     data-background-align=""
     data-background-cover="False"
     data-background-playerId=""
     data-background-mediaId=""
