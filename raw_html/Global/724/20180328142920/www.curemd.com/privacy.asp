<script>
$(document).ready(function () {  
  var top = $('.right-col').offset().top - 130;
  var scrollBottom = $(document).height() + $(window).height() - 2100 ;
  $(window).scroll(function (event) {
    var y = $(this).scrollTop();
	
    // alert(top);
	if (y > top && y < scrollBottom ) {
      $('.right-col').addClass('fixed');
    }
	else {
      $('.right-col').removeClass('fixed');
    }
	
	
  });
});
</script>
