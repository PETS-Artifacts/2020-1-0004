<script type="text/javascript">
jQuery(document).ready(function(){
    jQuery('.search-btn').click(function(){
        if(jQuery(this).hasClass('open'))
        {
            jQuery(this).removeClass('open');
            jQuery('.search-container').slideUp('slow');
        }
        else
        {
           jQuery(this).addClass('open');
           jQuery('.search-container').slideDown('slow');
        }
    });
    jQuery('#a-search-button').click(function(){
        var type  =  jQuery('#aircraft-type').val().split('__');
        var man   =  jQuery('#aircraft-manufacturer').val().split('__');
        var model =  jQuery('#aircraft-model').val().split('__');
        if(model[0] != '' && man[0] != '' && type[0] != '')
        {
            top.location = "http://www.avbuyer.com/aircraft-for-sale/"+man[1]+"/"+model[1];
        }
        else if(model[0] == '' && man[0] != '' && type[0] != '')
        {
            top.location = "http://www.avbuyer.com/aircraft-for-sale/"+man[1];
        }
        else if(model[0] == '' && man[0] == '' && type[0] != '')
        {
            top.location = "http://www.avbuyer.com/aircraft/"+ type[1];
        }
    });
    
    jQuery('#aircraft-type').change(function(){
        getAircraftFilter('manufacturer');
    });
    jQuery('#aircraft-manufacturer').change(function(){
        getAircraftFilter('model');
    });

});
</script>
<script type="text/javascript">
    function getAircraftFilter (ajaxType) {
        if(ajaxType == 'manufacturer' )
        {
            var type =  jQuery('#aircraft-type').val().split('__');;
            $.post("/wp-content/themes/WP-Skeleton/templates/ajax/mobile_header_search.php", {
                "type"      : type[0],
                "ajax_type" : ajaxType
            })
            .done(function (data) {
                //alert('suc'+jQuery("#aircraft-model").html());
                //display new results now
                jQuery('#aircraft-manufacturer').append(data).selectric('refresh');
            });
        }
        if(ajaxType == 'model' )
        {
            var type =  jQuery('#aircraft-type').val().split('__');;
            var man =  jQuery('#aircraft-manufacturer').val().split('__');
            $.post("/wp-content/themes/WP-Skeleton/templates/ajax/mobile_header_search.php", {
                "type"      : type[0],
                "man_id"    : man[0],
                "ajax_type" : ajaxType
            })
            .done(function (data) {
                //display new results now
                jQuery("#aircraft-model").append(data).selectric('refresh');
            });
        }
    }
</script>

