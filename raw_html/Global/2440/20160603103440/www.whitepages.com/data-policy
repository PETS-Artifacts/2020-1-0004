    <script type="text/javascript">
  window.bk_async = function(){
    bk_addPageCtx('__bk_l', document.location.toString());
    bk_addPageCtx('__bk_t', document.title.toString());
    bk_addPageCtx('__bk_pr', document.referrer.toString());
    bk_addPageCtx('domain', document.location.hostname);
    bk_addPageCtx('site', WP.dfpAdUnitSite());  // Requires rebuild of head script!!!
    var page = WP.dfpAdUnitPage();
    var parts = page && page.split('.');
    var search = (parts && parts[1]) ? parts[0] : '';
    bk_addPageCtx('page', page);
    bk_addPageCtx('search_type', search);

    var today = moment().format("YYYY-MM-DD");

    // Overall Searches
    if(search === "person" || search === "phone" || search === "address"){
      bk_addPageCtx('sw1', WP.usage.searchesThisWeek().length);
      bk_addPageCtx('sm1', WP.usage.searchesThisMonth().length);
    }
    // People Searches
    if(search === "person"){
      bk_addPageCtx('psw1', WP.usage.searchesThisWeek('people search').length);
    }
    // Phone Searches
    if(search === "phone"){
      bk_addPageCtx('phsw1', WP.usage.searchesThisWeek('phone search').length);
    }
    // Address Searches
    if(search === "address"){
      bk_addPageCtx('asw1', WP.usage.searchesThisWeek('address search').length);
    }
    if(WP.storage().getItem("lastUsageHistory") !== today){
      bk_addPageCtx('sw2', WP.usage.searchesLastWeek().length);
      bk_addPageCtx('sw3', WP.usage.searchesWeekThree().length);
      bk_addPageCtx('sw4', WP.usage.searchesWeekFour().length);
      bk_addPageCtx('sm2', WP.usage.searchesLastMonth().length);
      bk_addPageCtx('psw2', WP.usage.searchesLastWeek('people search').length);
      bk_addPageCtx('psw3', WP.usage.searchesWeekThree('people search').length);
      bk_addPageCtx('psw4', WP.usage.searchesWeekFour('people search').length);
      bk_addPageCtx('phsw2', WP.usage.searchesLastWeek('phone search').length);
      bk_addPageCtx('phsw3', WP.usage.searchesWeekThree('phone search').length);
      bk_addPageCtx('phsw4', WP.usage.searchesWeekFour('phone search').length);
      bk_addPageCtx('asw2', WP.usage.searchesLastWeek('address search').length);
      bk_addPageCtx('asw3', WP.usage.searchesWeekThree('address search').length);
      bk_addPageCtx('asw4', WP.usage.searchesWeekFour('address search').length);

      WP.storage().setItem("lastUsageHistory", today);
    }

    if (typeof dfpSetTargetingParams == "undefined") {
      dfpSetTargetingParams = {};
    }

    bk_addPageCtx('cat', dfpSetTargetingParams.cat || '');
    bk_addPageCtx('sub_cat', dfpSetTargetingParams.SubCat || '');
    bk_addPageCtx('eb_value', monster.get('eb') || '');
    bk_addPageCtx('is_logged_in', false ? 1 : 0);
    bk_addPageCtx("id", monster.get('wp_pid') || '');
    bk_allow_multiple_calls = true;
    bk_ignore_meta = true;
    bk_send_statid_payload = true;
    bk_use_first_party = true;
    bk_use_multiple_iframes = true;
    BKTAG.doTag(19777, 10);
  };

  async_inject('//tags.bkrtx.com/js/bk-coretag.js')
</script>

    <div id="pro-upsell-modal" class="modal hide fade upsell-modal" tabindex="-1" role="dialog" data-backdrop="static" data-keyboard="false"></div>
