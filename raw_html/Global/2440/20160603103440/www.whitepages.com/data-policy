    <script id="flexads-dfp-trigger">
  var dfp_ad_set_ads = dfp_ad_set.getAds();
for (var i = 0; i < dfp_ad_set_ads.length; i++) {
  (function(ad){
    var adslot = function(){
      var tag = googletag.defineSlot(ad.baseUri, ad.sizes, ad.divId).addService(googletag.pubads());
      if(ad.outOfPage){
        var out_of_page_tag = googletag.defineOutOfPageSlot(ad.baseUri, ad.divId + '-oop').addService(googletag.pubads());
        dfp_ad_set_slots.push(tag);
      }
      for (var attr in ad.targetParams) {
        var val = ad.targetParams[attr];
        tag.setTargeting(attr, val);
      };
      dfp_ad_set_slots.push(tag);
      if (ad.isRefreshable) {
        DfpIframeCoordinator.pushRefreshableSlot(tag);
      }
    };
    googletag.cmd.push(adslot);
  })(dfp_ad_set_ads[i]);
};

