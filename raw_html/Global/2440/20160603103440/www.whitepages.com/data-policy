      <script type="text/javascript">
    $(function() {
      var citiesPath = '/autocomplete/cities';
      var businessPath = '/autocomplete/business';
      var updatePrepend = function(text, removeClass, addClass) {
        cityAutocomplete.setPrependList(text ? [text] : []);
        if (removeClass) cityAutocomplete.removePrependClass(removeClass);
        if (addClass) cityAutocomplete.addPrependClass(addClass);
      }

      var cityAutocomplete = new WP.Autocomplete({
        url: citiesPath + '?query=',
        $el: $('.city-typeahead'),
        label: 'cities',
      });

      var businessAutocomplete = new WP.Autocomplete({
        url: businessPath + '?query=',
        $el: $('.business-typeahead'),
        label: 'categories'
      });
    });
  </script>
