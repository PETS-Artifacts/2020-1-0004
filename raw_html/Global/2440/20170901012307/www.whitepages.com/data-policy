      <script type="text/javascript">
    $(function() {
      var citiesPath = '/autocomplete/cities';
      var businessPath = '/autocomplete/business';
      var updatePrepend = function(text, removeClass, addClass) {
        cityAutocomplete.setPrependList(text ? [text] : []);
        if (removeClass) cityAutocomplete.removePrependClass(removeClass);
        if (addClass) cityAutocomplete.addPrependClass(addClass);
      }

      var cityAutocomplete = new WP.Autocomplete({
        url: citiesPath + '?query=',
        $el: $('.city-typeahead'),
        label: 'cities',
      });

      var businessAutocompleteMobile = new WP.Autocomplete({
        url: businessPath + '?query=',
        $el: $('.business-typeahead.mobile-typeahead'),
        label: 'categories'
      });

      var businessAutocompleteNonMobile = new WP.Autocomplete({
        url: businessPath + '?query=',
        $el: $('.business-typeahead.non-mobile-typeahead'),
        label: 'categories'
      });

      businessAutocompleteMobile.addPrependClass('mobile-typeahead');
      businessAutocompleteNonMobile.addPrependClass('non-mobile-typeahead');

      $('.mobile-menu-text').on('click', function(){
        if ($('.typeahead.dropdown-menu.non-mobile-typeahead').is(':visible')) {
          businessAutocompleteNonMobile.addPrependClass('hide');
        }
      });

      $(window).on('resize', function(){
        if ($('.mobile-search-button').is(':hidden') && $('.non-mobile-typeahead').hasClass('hide')) {
          $('.typeahead.dropdown-menu.non-mobile-typeahead').css('display', 'none');
          businessAutocompleteNonMobile.removePrependClass('hide');
        }
      })

      if ($('body').hasClass('home-page')) {
        $('.donation').removeClass('hide');
      }

    });
  </script>
