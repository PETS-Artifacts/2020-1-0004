function getMLBDate() {
	
	//Day Start: 10
	try {
		var sysDate = new Date("March 24, 2019");
		sysDate.toDirectoryPath = function() {
			var mm = (this.getMonth() + 1);
				mm = (mm < 10)?"0" + mm:mm;
			var dd = this.getDate();
				dd = (dd < 10)?"0"+dd:dd;
			return this.getFullYear() + "/" + mm + "/" + dd;
		}
		return sysDate;
	} catch(e) {
		return new Date();
	}
}
