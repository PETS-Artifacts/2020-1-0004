            // do something here
            device += "iphone";
        }
        if ($('html').hasClass('ipod')) {
            // do something here
            device += "ipod";
        }
        if ($('html').hasClass('android')) {
            // do something here
            device += "android";
        }

        if ($('html').hasClass('blackberry')) {
            // do something here
            device += "blackberry";
        }

        function memberViewCount(setplay) {
            var result = false;

            $.ajax({
                type: 'GET',
                url: '//data.coastalwatch.com/json/count.aspx',
                data: {
                    id: 'Editorial Privacy Policy &amp; Disclaimer | Coastalwatch.com',
                    device: device,
                    site: 'www.coastalwatch.com',
                    setplay: setplay
                },
                dataType: 'json',
                success: function (rep) {
                },
                error: function () {
                }
            });
        }
    </script>
