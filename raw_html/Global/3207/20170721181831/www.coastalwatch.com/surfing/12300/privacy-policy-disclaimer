    <script>
        $(document).ready(function () {
            if ($.cookie("cw-cclater") == null) {
                $('#update-cc-card').show();
            }
            if ($.cookie("cw-cclater") == "1") {
                $('#update-cc-card').hide();
            }
        });
        $("#remindmelatertab").click(function () {
            $.cookie("cw-cclater", "1", { expires: 2, path: "/" });
            $("#update-cc-card").hide();
        });
    </script>
    
    <script type="text/javascript">
        var adddiv = $("#div-gpt-ad-1494377656432-0");
        if (adddiv.length >= 0) {
            var lastHeight = $(adddiv).height();
            var firstheight = lastHeight;
            var changed = false;
            function checkForChanges() {
                if ($(adddiv).height() != lastHeight) {
                    changed = true;
                    $("addlabel").fadeIn();
                    lastHeight = $(adddiv).height();
                }
                if (lastHeight == firstheight && changed) {
                    $("#addlabel").fadeOut();
                } else {
                    if (!$("#addlabel").is('visible')) {
                        $("#addlabel").fadeIn(0);
                    }
                    setTimeout(checkForChanges, 500);
                }
            }
            checkForChanges();
        }
    </script>
    

