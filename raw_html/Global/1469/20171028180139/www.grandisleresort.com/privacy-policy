<script type="text/javascript">
    $(window).load(function () {
        if ($.cookie('local_traffic_ad') != '1') {
            var date = new Date();
            var minutes = 30;
            var delay_time = 0;
            delay_time = 0;
            console.log(delay_time);
            date.setTime(date.getTime() + (minutes * 60 * 1000));

            $.cookie('local_traffic_ad', '1', {expires: date});
            setTimeout(function() {
                $('.popup-overlay').fadeIn(function(){
                    resizeWindow();
                });
            }, delay_time);
            $('.popup-close').on('click', function() {
                $('.popup-overlay').fadeOut();
            });

            $(window).resize(function (event) {
                resizeWindow();
            });
            function resizeWindow() {
                setTimeout(function() {
	                $('.popup-inner').css('margin-top',($(window).height()-$('.popup-inner').height())/2);
                }, 300);
                
            }
        }
    });
    $(document).click(function(event) {
        if(!$(event.target).closest('.popup-overlay').length) {
            if($('.popup-overlay').is(":visible")) {
                $('.close-widget').trigger("click");
            }
        }
    });
