					$.each($("#CheckInAir"), function(){
						if($(this).data('default-val')!= ""){
							$(this).val($(this).data('default-val'));
						}
						
					});
						
				
				}
				});
				}
	
				
				if($("#CheckOutAir").length>0){
					
				$("#CheckOutAir").attr("autocomplete", "off");
					
				//document.title = wpdp_refresh_first=='yes';
				force = true;
				if(wpdp_refresh_first=='yes' || force){
					
					if(typeof $.datepicker!='undefined')
					$("#CheckOutAir").datepicker( "destroy" );
					
					$("#CheckOutAir").removeClass("hasDatepicker");
					wpdp_refresh_first = 'done';
					
				}
				$("#CheckOutAir").on('mouseover, mousemove', function(){
									
				if ($(this).val()!= "") {
					$(this).attr('data-default-val', $(this).val());
				}		
							
				if(wpdp_counter>2)
				clearInterval(wpdp_intv);		
				
				if(!$("#CheckOutAir").hasClass('hasDatepicker')){

				
					
				$("#CheckOutAir").datepicker($.extend(  
					{},  // empty object  
					$.datepicker.regional[ "en-GB" ],       // Dynamically  
					{  
 					dateFormat: wpdp_dateFormat
  } // your custom options 
				)); 
				
				$("#CheckOutAir").attr('readonly', 'readonly');
				
				
				
				$("#CheckOutAir").datepicker( "option", "dateFormat", "dd-MM-yy" );
				$("#CheckOutAir").datepicker( "option", "monthNamesShort", wpdp_month_array );
									
					$.each($("#CheckOutAir"), function(){
						if($(this).data('default-val')!= ""){
							$(this).val($(this).data('default-val'));
						}
						
					});
						
				
				}
				});
				}
	
				
				if($("#CheckInno").length>0){
					
				$("#CheckInno").attr("autocomplete", "off");
					
				//document.title = wpdp_refresh_first=='yes';
				force = true;
				if(wpdp_refresh_first=='yes' || force){
					
					if(typeof $.datepicker!='undefined')
					$("#CheckInno").datepicker( "destroy" );
					
					$("#CheckInno").removeClass("hasDatepicker");
					wpdp_refresh_first = 'done';
					
				}
				$("#CheckInno").on('mouseover, mousemove', function(){
									
				if ($(this).val()!= "") {
					$(this).attr('data-default-val', $(this).val());
				}		
							
				if(wpdp_counter>2)
				clearInterval(wpdp_intv);		
				
				if(!$("#CheckInno").hasClass('hasDatepicker')){

				
					
				$("#CheckInno").datepicker($.extend(  
					{},  // empty object  
					$.datepicker.regional[ "en-GB" ],       // Dynamically  
					{  
