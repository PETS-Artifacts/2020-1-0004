	<script type="text/javascript" language="javascript">
	

	jQuery(document).ready(function($){
		
		if($('.wpcf7-form-control.wpcf7-repeater-add').length>0){
			$('.wpcf7-form-control.wpcf7-repeater-add').on('click', function(){
				wpdp_refresh(jQuery, true);
			});
		}
		
	
});
var wpdp_refresh_first = 'yes';
var wpdp_counter = 0;
var wpdp_month_array = [];
var wpdp_dateFormat = "dd-MM-yy";
function wpdp_refresh($, force){
				if(typeof $.datepicker!='undefined' && typeof $.datepicker.regional["en-GB"]!='undefined'){
								wpdp_month_array = $.datepicker.regional["en-GB"].monthNamesShort;
									
				}
		
		
				
	
				
				if($("#CheckIn").length>0){
					
				$("#CheckIn").attr("autocomplete", "off");
					
				//document.title = wpdp_refresh_first=='yes';
				force = true;
				if(wpdp_refresh_first=='yes' || force){
					
					if(typeof $.datepicker!='undefined')
					$("#CheckIn").datepicker( "destroy" );
					
					$("#CheckIn").removeClass("hasDatepicker");
					wpdp_refresh_first = 'done';
					
				}
				$("#CheckIn").on('mouseover, mousemove', function(){
									
				if ($(this).val()!= "") {
					$(this).attr('data-default-val', $(this).val());
				}		
							
				if(wpdp_counter>2)
				clearInterval(wpdp_intv);		
				
				if(!$("#CheckIn").hasClass('hasDatepicker')){

				
					
				$("#CheckIn").datepicker($.extend(  
					{},  // empty object  
					$.datepicker.regional[ "en-GB" ],       // Dynamically  
					{  
 					dateFormat: wpdp_dateFormat
  } // your custom options 
				)); 
				
				$("#CheckIn").attr('readonly', 'readonly');
				
				
				
				$("#CheckIn").datepicker( "option", "dateFormat", "dd-MM-yy" );
				$("#CheckIn").datepicker( "option", "monthNamesShort", wpdp_month_array );
									
					$.each($("#CheckIn"), function(){
						if($(this).data('default-val')!= ""){
							$(this).val($(this).data('default-val'));
						}
						
					});
						
				
				}
				});
				}
	
				
				if($("#CheckOut").length>0){
					
				$("#CheckOut").attr("autocomplete", "off");
					
				//document.title = wpdp_refresh_first=='yes';
				force = true;
				if(wpdp_refresh_first=='yes' || force){
					
					if(typeof $.datepicker!='undefined')
					$("#CheckOut").datepicker( "destroy" );
					
					$("#CheckOut").removeClass("hasDatepicker");
					wpdp_refresh_first = 'done';
					
				}
				$("#CheckOut").on('mouseover, mousemove', function(){
									
				if ($(this).val()!= "") {
					$(this).attr('data-default-val', $(this).val());
				}		
							
				if(wpdp_counter>2)
				clearInterval(wpdp_intv);		
				
				if(!$("#CheckOut").hasClass('hasDatepicker')){

				
					
				$("#CheckOut").datepicker($.extend(  
					{},  // empty object  
					$.datepicker.regional[ "en-GB" ],       // Dynamically  
					{  
 					dateFormat: wpdp_dateFormat
  } // your custom options 
				)); 
				
				$("#CheckOut").attr('readonly', 'readonly');
				
				
				
				$("#CheckOut").datepicker( "option", "dateFormat", "dd-MM-yy" );
				$("#CheckOut").datepicker( "option", "monthNamesShort", wpdp_month_array );
									
					$.each($("#CheckOut"), function(){
						if($(this).data('default-val')!= ""){
							$(this).val($(this).data('default-val'));
						}
						
					});
						
				
				}
				});
				}
	
				
				if($("#CheckInAir").length>0){
					
				$("#CheckInAir").attr("autocomplete", "off");
					
				//document.title = wpdp_refresh_first=='yes';
				force = true;
				if(wpdp_refresh_first=='yes' || force){
					
					if(typeof $.datepicker!='undefined')
					$("#CheckInAir").datepicker( "destroy" );
					
					$("#CheckInAir").removeClass("hasDatepicker");
					wpdp_refresh_first = 'done';
					
				}
				$("#CheckInAir").on('mouseover, mousemove', function(){
									
				if ($(this).val()!= "") {
					$(this).attr('data-default-val', $(this).val());
				}		
							
				if(wpdp_counter>2)
				clearInterval(wpdp_intv);		
				
				if(!$("#CheckInAir").hasClass('hasDatepicker')){

				
					
				$("#CheckInAir").datepicker($.extend(  
					{},  // empty object  
					$.datepicker.regional[ "en-GB" ],       // Dynamically  
					{  
 					dateFormat: wpdp_dateFormat
  } // your custom options 
				)); 
				
