<script id="dropdown-menu-template" type="text/x-handlebars-template">
{{#each this}}
{{#startsWith "*" @key}}
{{else}}
  <li>
  {{#compare this "==" ""}}<hr class="dropdown-menu_divider"><p class="dropdown-menu_divider-text">{{@key}}<hr class="dropdown-menu_divider">{{/compare}}
  {{#compare this "!=" ""}}
    <a class="js- dropdown-menu_link"
      {{#startsWith "ai." this}}onclick="{{this}};ai.ui.menu.hide()"{{/startsWith}}
      {{#startsWith "/" this}}href="{{this}}"{{/startsWith}}
      {{#startsWith "http" this}}href="{{this}}" target="_blank"{{/startsWith}}
      {{#startsWith "#" this}}data-action="{{this}}"{{/startsWith}} >{{@key}}
    </a>
  {{/compare}}
{{/startsWith}}
{{/each}}
</script>



