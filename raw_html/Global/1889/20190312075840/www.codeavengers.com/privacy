<script>
  function displayApp() {
    var previewFrame = document.getElementById('myApp');
    var preview = previewFrame.contentDocument
        || previewFrame.contentWindow.document;
    preview.open();
    preview.write(document.querySelector('#code textarea').value);
    preview.close();
  }
</script>

<div id="code">
{content}
</div>

