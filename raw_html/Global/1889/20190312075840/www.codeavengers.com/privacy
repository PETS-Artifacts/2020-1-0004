      {{#startsWith "ai." this}}onclick="{{this}};ai.ui.menu.hide()"{{/startsWith}}
      {{#startsWith "/" this}}href="{{this}}"{{/startsWith}}
      {{#startsWith "http" this}}href="{{this}}" target="_blank"{{/startsWith}}
