<script>
function loadReference() {
  ai.loadHtml('/reference.html', '#reference-content', function() {
    ai.referenceLoaded = true;
    if (ai.afterReferenceLoaded) ai.afterReferenceLoaded();
    ai.events.trigger('reference-loaded');
  }, true);
}

loadReference();
//Test statement for checking if the loading works when the .html file takes a while to load
//setTimeout(loadReference, 5000);
</script>

