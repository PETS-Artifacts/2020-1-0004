(function ($) {
  var old_func = GMG.Util.embedKalturaVideo
  window.GMG.Util.embedKalturaVideo = function (el, config, something) {
    config.flashvars = config.flashvars || { "autoplay" : false };
    config.flashvars.doubleClick = {
      customParams : 'category=' + $('meta[property="article:section"]').attr('content').replace(/[\"\'\=\!\+\#\*\~\;\^\(\)\<\>\[\]]/, ""),
