var GIGYA_DEVICE_TYPE = mobileAndTabletcheck();

$(document).ready(function() {
    loadGigya();
});

var loadGigya = function() {
    var s = document.createElement('script');
    s.type='text/javascript';
    s.async=true;
    s.src = "http://cdn.gigya.com/js/gigya.js?apiKey=" + GIGYA_API + "&services=socialize.share,accounts.screenset,socialize.shareCounts,socialize.simpleShare,socialize.reactions";
    s.text = '{lang: ' + GIGYA_LANG +' }';
    document.getElementsByTagName('head')[0].appendChild(s); 
};

var onGigyaServiceReady = function(serviceName) {
    initShareBar();
    initComments();
    initSimpleShare();
    /*
    initShareCounts();
    */
    gigya.accounts.addEventHandlers({ 
        onLogin: handleLogin,
        onLogout: evalSession
       }
    );
    evalSession();
};

var initShareBar = function() {
    if( $( '#' + GIGYA_SHAREBAR_CONTAINER ).length) {
        var ua = new gigya.socialize.UserAction();
        ua.setTitle(GIGYA_SHARE_TITLE);
        ua.setLinkBack(window.location.origin + window.location.pathname);
        
        var params ={ 
            userAction: ua,
            shareButtons: GIGYA_SHAREBAR_PROVIDER,
			buttonTemplate: '<a href="#" class="gigya-button $text" onclick="$onClick"><i class="fa"></i></a>',
            buttonWithCountTemplate: '<a href="#" class="gigya-button $text" onclick="$onClick"><i class="fa"></i><span>$count</span></a>',
            containerID: GIGYA_SHAREBAR_CONTAINER,
            wrap: true,
            onSendDone: trackShare,
            deviceType: GIGYA_DEVICE_TYPE
        };
        gigya.socialize.showShareBarUI(params); 
    }
};

var initSimpleShare = function() {
    if( $( '.' + GIGYA_SIMPLESHARE_CONTAINER ).length) {
        $( '.' + GIGYA_SIMPLESHARE_CONTAINER ).click(function(){
            var ua = new gigya.socialize.UserAction();
            var articleUrl = $(this).find("h6").attr("title");
            if(articleUrl.indexOf("http") != -1) {
                var linkBackUrl = articleUrl;
            } else {
                var linkBackUrl = window.location.protocol + '//' + window.location.hostname + articleUrl;
            }
            ua.setLinkBack(linkBackUrl);
            var params ={
                snapToElementID: $(this).attr('id'),
                deviceType: GIGYA_DEVICE_TYPE,
                userAction: ua,
                operationMode: "simpleShare",
                cid:'',
                context: $(this).find("h6").attr("id"),
                onSendDone: onSendDone,
                simpleShareConfig: {
                    postShareAction:"connect",
                    postShareConnectCaptionText:"Add the caption here",
                    postShareConnectBodyText:"Add the body text here"
                }
            };
            gigya.socialize.showShareUI(params); 
        }); 
    }
};

var initComments = function() {

    if( $( '#' + GIGYA_COMMENTS_CONTAINER ).length) {
        var params = {
            categoryID: GIGYA_COMMENTS_CATEGORY_ID,
            streamID: STREAM_ID,
            version: 2,
            containerID: GIGYA_COMMENTS_CONTAINER,
            cid:'',
            enabledShareProviders: 'facebook,twitter,yahoo,linkedin',
            enabledProviders: 'facebook,twitter,googleplus,linkedin',
            deviceType: GIGYA_DEVICE_TYPE,
            showLoginBar: true,
        };
        
        gigya.comments.showCommentsUI(params);
        /*
        if($( '#commentCount' ).length) {
            var params2 = {
                categoryID: 'story comments',
                streamID: STREAM_ID,
                callback: showCommentCount,
            };
            gigya.comments.getComments(params2)
        }*/
    }
};

var showCommentCount = function(comments) {
    $( '#commentCount' ).text(comments.commentCount);
};

var initShareCounts = function() {
    var articleArray = $(".article-social-count");
    var x = 0;
    while (x < articleArray.length) {
        if(articleArray[x].id != "" && articleArray[x].title != "") {
            getShareCounts(articleArray[x].title, articleArray[x].id);
        }
        x++;
    }
};

var printShareCountsResponse = function (response) {
    if ( response.errorCode == 0 ) {   
        var countContainer = response.context;
        var shareFacebookCounts = 0;
        var shareTwitterCounts = 0;
        var totalShareCounts = 0;
        if (!isNaN(response.shareCounts.facebook)) {
            shareFacebookCounts = parseInt(response.shareCounts.facebook);
        }
        if (!isNaN(response.shareCounts.twitter)) {
            shareTwitterCounts = parseInt(response.shareCounts.twitter);
        }
        totalShareCounts = shareFacebookCounts + shareTwitterCounts;
        if (totalShareCounts > 0) {
            $("#" + countContainer).text(totalShareCounts);
        } 
    }
};

var getShareCounts = function(articleUrl, countContainer) {
    var url = window.location.protocol + '//' + window.location.hostname + articleUrl;
    var params = {
        URL: url,
        callback:printShareCountsResponse,
        context: countContainer,
    };
    gigya.socialize.getProviderShareCounts(params);
};

var trackShare = function(shareObject) {

};

var onSendDone = function(event) {
    var addCountContainer = 0;
    var totalShareCounts = 0;
    /*
    addCountContainer = $("#" + event.context).text();
    if (addCountContainer == "") {
        totalShareCounts = 1;
    } else if (!isNaN(addCountContainer)) {
        totalShareCounts = parseInt(addCountContainer) + 1;
    }
    $("#" + event.context).text(totalShareCounts);
    */
};

var handleLogin = function(loginEvent) {
    evalSession();
    trackLogin(loginEvent);
};

var evalSession = function() {

    var evalResponse = function(response){
        if ( response.errorCode == 0 ) { 

            GIGYA_IS_LOGGED = true;
            GIGYA_PROFILE = response;

            buildControls(GIGYA_IS_LOGGED, response);
        } 
        else {
            GIGYA_IS_LOGGED = false;
            buildControls(GIGYA_IS_LOGGED, response);
        } 
    };

    gigya.accounts.getAccountInfo({ callback: evalResponse });
};

var buildControls = function(logged, response) {

    if(logged) {
        $("#ksat-navbar>ul.navbar-nav>li.user-nav").addClass("hidden");
        $("#ksat-navbar>ul.navbar-nav>li.user-logout").removeClass("hidden");       
        $("#mobile-edit-profile").removeClass("hidden");
        $("#mobile-login").addClass("hidden");
        $("#mobile-logout").removeClass("hidden");
    } else {
        $("#ksat-navbar>ul.navbar-nav>li.user-nav").removeClass("hidden");
        $("#ksat-navbar>ul.navbar-nav>li.user-logout").addClass("hidden");
        $("#mobile-edit-profile").addClass("hidden");
        $("#mobile-login").removeClass("hidden");
        $("#mobile-logout").addClass("hidden");
    }
};

var showLogin = function(){
    var loginParams={
        screenSet: GIGYA_LOGIN_SCREEN_SET,
        mobileScreenSet: GIGYA_LOGIN_MOBILE_SCREEN_SET,
        onAfterScreenLoad: createOverlay,
        onHide: removeOverlay
    };
    gigya.accounts.showScreenSet(loginParams);
};

var showLoginForMobile = function(){
    var loginParams={
        screenSet: GIGYA_LOGIN_MOBILE_SCREEN_SET,
        deviceType: 'mobile',
        mobileScreenSet:GIGYA_LOGIN_MOBILE_SCREEN_SET,
        onAfterScreenLoad: createOverlay,
        onHide: removeOverlay
    };
    gigya.accounts.showScreenSet(loginParams);
};

var showEditProfile = function(){
    var loginParams={
        screenSet: 'Default-ProfileUpdate',
        onAfterScreenLoad: createOverlay,
        onHide: removeOverlay
    };
    gigya.accounts.showScreenSet(loginParams);
};

var showEditProfileForMobile = function(){
    var loginParams={
        screenSet: 'DefaultMobile-ProfileUpdate',
        onAfterScreenLoad: createOverlay,
        onHide: removeOverlay
    };
    gigya.accounts.showScreenSet(loginParams);
};

var logOut = function() {
    gigya.accounts.logout();
};

var createOverlay = function() {
    var docHeight = $(document).height();
    $(".gigya-input-submit").attr("value","");
    $("body").append("<div id='gigya-overlay'></div>");

    $("#gigya-overlay")
       .height(docHeight)
       .css({
          'opacity' : 0.4,
          'position': 'absolute',
          'top': 0,
          'left': 0,
          'background-color': 'black',
          'width': '100%',
          'z-index': 5000
       });
 };

 var removeOverlay = function() {
    $('#gigya-overlay').remove();
    var customFields = {
     'hitType': 'event',
     'eventCategory': 'Modal', 
     'eventAction': 'Closed',     
     'eventLabel': 'Gigya Modal Closed'
    };
    ga('trackerEvent.send', customFields);
 };
 
</script>
</body>
</html>


