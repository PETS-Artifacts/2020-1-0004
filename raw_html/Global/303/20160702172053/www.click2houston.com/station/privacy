      <section class="bg-gray-l" id="get-alerts">
 <div class="container"><div class="row"><div class="col-xs-12"><h2>Get Alerts</h2><p>Want the latest news and weather updates?</p>
 <form action="http://link.click2houston.com/manage/54x/manage-form" class="ui-form" id="get-alert-form" method="get" role="search">
  <div class="form-group"><input class="form-control" id="get-alert-email-input" name="email" placeholder="Enter your email here" type="text" /></div><button class="submit-btn" id="get-alert-submit-btn" type="button"><span class="glyphicon glyphicon-circle-arrow-right"></span></button></form>
 <div class="error-msg hide alert alert-danger" role="alert">Invalid email address</div></div></div></div><script type="text/javascript">
  (function($) {
   $("#get-alert-submit-btn")
     .bind(
       'click',
       function() {
        var field = $("#get-alert-email-input").val();
        var myreg = /^([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+@([a-zA-Z0-9]+[_|\_|\.]?)*[a-zA-Z0-9]+\.[a-zA-Z]{2,3}$/;
        if (!myreg.test(field)) {
         $("#email-input").addClass("error");
         $(".alert-danger").removeClass("hide");
         return false;
        } else {
         $("#email-input").removeClass("error");
         $(".alert-danger").addClass("hide");
         $("#get-alert-form").submit();
        }
       });

   $("#get-alert-email-input").bind('click', function() {
    $("#email-input").removeClass("error");
    $(".alert-danger").addClass("hide");
   });

  })(jQuery);
 </script></section>
