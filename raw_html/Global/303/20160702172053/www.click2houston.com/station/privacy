var GIGYA_API = '3_6SkewH7ZVsmoRq7jVBuokyAs77oEvj3jp5nVTEPECYo5I5e37GgjtOnXkWmnyp8t';
var GIGYA_LANG = 'en';
var GIGYA_COMMENTS_CATEGORY_ID = 'story comments';
var GIGYA_SHAREBAR_CONTAINER = 'gigya-share-bar';
var GIGYA_COMMENTS_CONTAINER = 'gigya-comments';
var GIGYA_SIMPLESHARE_CONTAINER = 'gigya-simple-share';
var GIGYA_SHARE_TITLE = 'Sharing with Gigya:';
var GIGYA_SHAREBAR_PROVIDER = 'facebook,twitter,share';
var STREAM_ID = window.location.protocol + '//' + window.location.hostname + window.location.pathname;

var GIGYA_IS_LOGGED = false;
var GIGYA_PROFILE = null;
var GIGYA_LOGIN_SCREEN_SET = 'Default-RegistrationLogin';
var GIGYA_LOGIN_MOBILE_SCREEN_SET = 'DefaultMobile-RegistrationLogin';

var mobileAndTabletcheck = function() {
    var check = 'auto';
