												<script>
    (function(A, $){
        window.GMG = window.GMG || {};
        GMG.Util = GMG.Util || {};
    })(AUI(), jQuery);
</script>
<script>
    (function(A, $){
        /*
         * GMG UI site-specific definitions
         */
        GMG.Util = A.mix(GMG.Util, {
            embedKalturaVideo: function( container, config, lazyLoad ){
                var auiLoader = A.Env._loader;
                var partnerID = config['partner_id'] || '1636891';
                var uiconfID = config['uiconf_id'] || '30507271';
                if(config['kaltura_host'] == undefined || config['kaltura_host'] == null || config['kaltura_host'] == '') {
                    config['kaltura_host'] = 'http://cdnapi.kaltura.com';
                }
                var protocol = (window.location.protocol === 'https:') ? 'https' : 'http';
                kalturaHost = (protocol === 'https') ? 'https://cdnapisec.kaltura.com' : config['kaltura_host'];
                container = (container instanceof jQuery) ? container : $(container);
                if( !auiLoader.groups[ 'kaltura-video-story' ] ){
                    EP.Loader.addGroup({
                        name : 'kaltura-video-story',
                        modules : {
                            'kaltura-video-story' : {
                                name : 'kaltura-video-story',
                                type : 'js',
                                fullpath : kalturaHost + '/p/'+partnerID+'/sp/'+partnerID+'00/embedIframeJs/uiconf_id/'+uiconfID+'/partner_id/'+partnerID,
                                requires : [],
                                version: false
                            }
                        }
                    });
                }

                var id = container.attr('id');
                if(id==null || $.trim(id)===''){
                    id = A.guid('kaltura-player-');
                    container.attr('id', id);
                }

                var getEntryId = function(url){
                    var entry;
                    if( url ){
                        var bits = url.split('/');
                        for (var i = 0; i < bits.length; i += 1) {
                            if (bits[i] === "entryId" || bits[i] === "entry_id") {
                                entry = bits[i+1];
                                break;
                            }
                        }
                    }
                    return entry;
                };

                var superReadyCallback = config.readyCallback;

                var flashvarsOrig = config.flashvars;

                config =  A.mix({
                    wid : '_'+partnerID,
                    uiconf_id : uiconfID,
                    entry_id : config['entry_id'] || getEntryId(config['videoUrl']),
                    adsOnReplay : true,
                    flashvars: {
                        autoPlay : false,
                        thumbnailUrl: config['poster'],
                        deliveryCode: config['deliveryCode'],
                    },
                    captureClickEventForiOS: true,
                    poster: null
                }, config, true, null, 0, true);

                config.flashvars = A.mix(config.flashvars, flashvarsOrig, true, null, 0, true);

                config.targetId = id;

                var autoPlay = config.flashvars.autoPlay;

                config.readyCallback = function (playerId) {
                    // issue a play ( since we already clicked the play button )
                    var kdp = document.getElementById(playerId);
                    var onFirstPlay = function(){
                        kdp.kUnbind('doPlay', onFirstPlay);
                        kdp.kUnbind('adStart', onFirstPlay);
                        kdp.kUnbind('playerReady', onFirstPlay);
                        var controls = $(kdp).find('iframe').contents().find('.controlBarContainer');
                        $(kdp).parents('.kaltura-video-wrapper, .media-placeholder-wrapper').toggleClass('no-controls', controls.length===0);
                    };
                    kdp.kBind('doPlay', onFirstPlay);
                    kdp.kBind('adStart', onFirstPlay);
                    kdp.kBind('playerReady', onFirstPlay);

                    if(superReadyCallback){
                        superReadyCallback.apply(this, arguments);
                    }
                };

                var render = function(play){
                    config.flashvars.autoPlay = play || false;
                    A.use('kaltura-video-story', function(){
                        container.addClass('player');
                        kWidget.embed( config );
                    });
                };

                /* autoPlay || touch || phone */
                if(autoPlay || $('html').hasClass('touch') || Modernizr.mq('only screen and (max-width: 767px)') ){
                    render( config.flashvars.autoPlay );
                } else {
                    if(typeof(config.copyright) == "undefined") {
                        config.copyright = "";
                    }
                                        var posterStyle = config.poster ? " style=\"background-image: url('"+config.poster+"');\"" : "";
                    var poster = $("<div class=\"poster\"><div class=\"crop-photo\""+posterStyle+"><img src=\"http://www.click2houston.com/gmg-base-theme/images/placeholder/placeholder-16x9.jpg?minifierType=js&languageId=en_US&b=6110&t=1483738933000\" /><small class=\"copyright\">"+config.copyright+"</small><i class=\"playBtn abs-center\"></i></div></div>");
                    if(lazyLoad){
                        poster.find('.crop-photo').addClass(lazyLoad['scroll'] ? 'delay-bg-load ratio-16x9' : 'placeholder-16x9');
                    }
                    container.append( poster );

                    var preRender = function(){
                        poster.find('.playBtn').attr('class', 'loading-animation abs-center');
                        render(true);
                    };
                    poster.one('click', preRender);
                }
            },
            lazyLoadAssets: function(images){
                images.each(function(idx, img) {
                    var jImg = $(img);
                    var dataAttr = 'data-src';
                    var dataSrc = jImg.attr(dataAttr);
                    var src = jImg.attr('src');
                    if((src==null || $.trim(src)==='') && dataSrc){
                        jImg.attr('src', dataSrc).css('visibility', 'visible').removeAttr(dataAttr).removeClass('delay-img-load delay-load');
                    }
                });
            },
            findLazyLoadAssets: function(container){
                GMG.Util.lazyLoadAssets( container.find('img.delay-img-load[data-src], iframe.delay-img-load[data-src], img.delay-load[data-src], iframe.delay-load[data-src]') );
                var placeCls1 = 'placeholder-16x9', placeCls2 = 'placeholder-1x1';
                container.find('.'+placeCls1+', .'+placeCls2).removeClass(placeCls1+' '+placeCls2);
            },
            supportsHistoryAPI: !!(window.history && history.pushState),
            /* Setting when to refresh ads on resize */
            resizeRefreshAdsValue: 2 /* [0=never, 1=always, 2=only major media query change] */
        }, true);

        // IE Detection
        var isIE =  (navigator.userAgent.indexOf('MSIE') !== -1 || navigator.appVersion.indexOf('Trident/') > 0);
        if(isIE){
            $('html').addClass('ie');
        }

        

        var kalturaHost = 'https://cdnapisec.kaltura.com',
                uiconfID = null,
                partnerID = null;

                    uiconfID = 32105241;
        
                    partnerID = 2031081;
                if (uiconfID && partnerID) {
            EP.Loader.addGroup({
                name : 'generic-video-element',
                modules : {
                    'generic-video-element' : {
                        name : 'generic-video-element',
                        type : 'js',
                        fullpath : kalturaHost + '/p/'+ partnerID +'/sp/'+ partnerID +'00/embedIframeJs/uiconf_id/'+ uiconfID +'/partner_id/'+ partnerID,
                        requires : [],
                        version: false
                    }
                }
            });
        } else {
            console.debug("Missing configuration for head.vm");
        }
    })(AUI(), jQuery);
</script>

