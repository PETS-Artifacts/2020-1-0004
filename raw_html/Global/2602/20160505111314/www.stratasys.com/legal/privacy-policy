    <!-- removes horizontal rule on pages with tabs and pages without billboard content -->
	<script type="text/javascript">
	    $(document).ready(function () {
	        function isEmpty(el) {
	            return !$.trim(el.html())
	        }

	        if (document.getElementById("content_0_divTabContainer") || isEmpty($('.billboard'))) {
	            $("div#billboard").css("background", "none");
	            $("div#billboard").css("margin-bottom", "0px");
	        }
	        // 
	        if (document.getElementById("content_0_divTabContainer")) {
	            $(".sidecolumn").css("margin-top", "49px");
	            $(".sidecolumn").css("padding-top", "18px");
	            $(".sidecolumn").css("border-top", "1px solid #B7B7B7");
	        }
	    });
	</script>
	
	<!-- makes large buttons same flexible and same height -->
	<script type="text/javascript">
	    $(document).ready(function () {
	        var myarr = [];

	        $('#sameHeight li a').each(function () {
	            myarr[myarr.length] = $(this).height();
	        });

	        $('.calltoactionshort').each(function () {
	            myarr[myarr.length] = $(this).height();
	        });

	        $('.calltoactionlong').each(function () {
	            myarr[myarr.length] = $(this).height();
	        });

	        var maxValueInArray = Math.max.apply(Math, myarr);

	        $('#sameHeight li a').each(function () {
	            myarr[myarr.length] = $(this).height(maxValueInArray);
	        });

	        $('.calltoactionshort').each(function () {
	            myarr[myarr.length] = $(this).height(maxValueInArray);
	        });

	        $('.calltoactionlong').each(function () {
	            myarr[myarr.length] = $(this).height(maxValueInArray);
	        });

	    });
	</script>

