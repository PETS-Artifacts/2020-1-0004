<script>
    function search(id) {
        var query = $("#" + id).val();

        if (query.length > 2) {
            var mode = getQueryByName("m");
            var newurl = 'http://www.stratasys.com//search' + '?q=' + encodeURIComponent(query);

            if (mode.length > 0) {
                newurl = newurl + '&m=' + mode;
            }

            window.location = newurl;
        }
    }

    $(document).ready(function () {
        $(".typeahead").keypress(function (e) {
            if (e.which == 13) {
                e.preventDefault();
                search(e.target.id);
            }
        });
    });
</script>
