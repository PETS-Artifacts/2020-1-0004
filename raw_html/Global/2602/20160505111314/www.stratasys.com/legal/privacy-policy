        $(document).ready(function () {
            var head = document.getElementsByTagName('head')[0];
            var script = document.createElement('script');
            script.type = 'text/javascript';

            if (document.location.hostname == 'www.stratasys.com.cn') {
                script.src = "/StratasysGlobal/Scripts/cn-baidu-remarketing-tag.js";
                head.appendChild(script);
            }
            if (document.location.hostname == 'www.stratasys.co.kr') {
                script.src = "/StratasysGlobal/Scripts/kr-ace-counter-tracking-code.js";
                head.appendChild(script);
            }
        });
    </script>
