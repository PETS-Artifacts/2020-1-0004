<div class="content-section ">
    <div class="left-nav-section">
        <div class="text-two-thirds panel">
            <div class="belt  ">
                <div class="about-section-parent inpage-nav-container">
                    <div class="left-content col w-33">
                        <div class="left-nav-container float-nav-col">
    <div class="pad page-nav">
        <div class="page-nav-bg"></div>
            <ul class="page-links mod-float-nav nav-parent-level">
                    <li>
                        <a data-analytics-category="LeftNavigation" href="/homepage/legal-notices">Legal notices and terms of use</a>
