    <script type="text/javascript">
    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];
    (function () {
        var gads = document.createElement("script");
        gads.async = true;
        gads.type = "text/javascript";
        var useSSL = "https:" == document.location.protocol;
        gads.src = (useSSL ? "https:" : "http:") + "//www.googletagservices.com/tag/js/gpt.js";
        var node = document.getElementsByTagName("script")[0];
        node.parentNode.insertBefore(gads, node);
    })();

    var megaevent = new Event('megabanner', {"bubbles": true, "cancelable": true});
    var galleryAdEvent = new Event('galleryAd');

    googletag.cmd.push(function () {
        googletag.pubads().addEventListener('slotRenderEnded', function (event) {

            if (event.slot.getSlotElementId() === 'adslot_desktop_top' && event.creativeId !== null && event.size !== null &&
                    ((event.size[0] === 970 && event.size[1] === 250) || event.size[0] === 728 && event.size[1] === 90)) {
                megaevent.slot = event.slot;
                megaevent.size = event.size;
                document.dispatchEvent(megaevent);
            }

            if (event.slot.getSlotElementId() === 'adslot_desktop_gallery') {
                galleryAdEvent.slot = event.slot;
                document.dispatchEvent(galleryAdEvent);
            }

        });
    });
</script>    <!-- End DFP -->

