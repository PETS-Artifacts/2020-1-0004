		<script  type="text/javascript">
		
		var digitalData;
		
         $(document).ready(function () {
            digitalData = {
                "page":
                {   
                    "pageInfo":
                    {                   
                        'pageName': "privacy"
                    },
                    "visitor":
                    {
                        'visitorID':""
                    }
                }     
            };
            
            $("#dvCategory").click(function () {
                var rdCategory = $("#dvCategory input[name='rdCategory']").filter(':checked').val();
                digitalData["page"]["pageInfo"]["pageName"] = "search-results:"+rdCategory;
                //var stringJson = JSON.stringify(digitalData);
                //console.log(stringJson);
            });

            $(".filterResults").on('change', 'input[type=checkbox]', function () {
				var rdCategory = $("#dvCategory input[name='rdCategory']").filter(':checked').val();
                var data = [];
                $("#dv" + rdCategory).find("input[type=checkbox]").each(function () {
                    if ($(this).is(":checked")) {
                        if (data.hasOwnProperty($(this).data("paramatername"))) {
                            data[$(this).data("paramatername")] += "," + $(this).val();
                        } else {
                            data[$(this).data("paramatername")] = $(this).val();
                        }
                    }
                    qs = $.grep([data["Type"], data["Region"]], Boolean).join(":");
                    if (qs != "") { qs = ":"+qs}
                    digitalData["page"]["pageInfo"]["pageName"] = "search-results:" + rdCategory + qs;
                });
                //var stringJson = JSON.stringify(digitalData);
                //console.log(stringJson);
            });
         });
        </script>
