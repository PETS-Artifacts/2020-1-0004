        $(document).ready(function () {

            $(".search-close-but").click(function (e) {
                $("#searchOverlayBox").hide();
            });

            $(".lightbox_trigger").click(function (e) {
                $("#searchOverlayBox").show();
                $(".cat-list input[name='rdCategory']:first").attr("checked", true);

                if ($(window).width() <= 767) {
                    $("html, body").animate({ scrollTop: 0 }, "slow");
                    return false;
                }
            });

            $(".btn-search-trigger").click(function (e) {
                var keywords = $("#txtSearch").val();
                //var category = $(".btn-search-trigger").filter(':checked').val();
                var category = $(".cat-list input[name='rdCategory']:checked").val();

                baseURL = window.location;
                var hostName = "http://" + baseURL.hostname;
                var url = hostName.split("?")[0];//remove everything after the first "?" if it exists
                url += "/search-results?category=" + category + "&keywords=" + keywords + "&page=1&resultsPerPage=10&run=1";
                window.location.href = url;
                return false;
            });

