try
	{
	// Make Web Part Header bottom line match the border around the web part body
	jQuery(".ms-WPHeader td").css("border-bottom-color",jQuery("#ms-rteThemeForeColor-7-3").css("color"));
	//alert(jQuery(".ms-WPHeader .ms-wpTdSpace").css("border-bottom"));
	//this seems to be the only way to set the dynamic menu background color
	jQuery(".s4-tn ul.dynamic").css("background-color",jQuery("#ms-rteThemeForeColor-4-0").css("color"));
	/* Alternating background in list views */
	jQuery(".ms-alternatingstrong").addClass("ms-rteThemeBackColor-3-0");
	// Quick Launch and Breadcrumb borders need to be a little darker
	jQuery("#breadcrumbDiv").css("border-bottom-color",jQuery("#ms-rteThemeForeColor-1-2").css("color"));
	jQuery("#s4-leftpanel-content").css("border-color",jQuery("#ms-rteThemeForeColor-1-2").css("color"));
	jQuery(".s4-specialNavLinkList").css("border-top-color",jQuery("#ms-rteThemeForeColor-1-2").css("color"));
	// Make text, web part titles, and others black	
	jQuery(".description,.ms-WPTitle,.ms-WPTitle a,.ms-rteElement-P,.article-content,.welcome-content").css("color",jQuery("#ms-rteThemeForeColor-2-0").css("color"));
	// Text in top nav menu, I decided not to do this
	//jQuery(".s4-tn .menu-item-text").css("color",jQuery("#ms-rteThemeForeColor-1-0").css("color"));
	// Dotted lines in top nav, I decided not to do this
	//jQuery(".s4-tn li.static > .menu-item").css("border-right-color",jQuery("#ms-rteThemeForeColor-1-0").css("color"));
	//jQuery(".menu-horizontal").css("border-left-color",jQuery("#ms-rteThemeForeColor-1-0").css("color"));
	}
catch(e)
	{
	// I don't do anything, but I want to make sure there are no javascript errors.
	// Colors won't get set but the page will still work.
	}

try
	{
	//Lightens an RGB color by a specified amount, returns in form #nnnnnn
	function lighterColor(colorString, lightValue)
		{
		var colors;
		
		if (colorString.substr(0, 1) === '#') // IE returns a #nnnnnn value
			{
			colors = new Array();
			if (colorString.length > 4) // Regular color #nnnnnn
				{
				colors[0] = parseInt(colorString.substr(1,2),16);
				colors[1] = parseInt(colorString.substr(3,2),16);
				colors[2] = parseInt(colorString.substr(5,2),16);
				}
			else // 3-digit color #nnn
				{
				colors[0] = parseInt(colorString.substr(1,1) + colorString.substr(1,1),16);
				colors[1] = parseInt(colorString.substr(2,1) + colorString.substr(2,1),16);
				colors[2] = parseInt(colorString.substr(3,1) + colorString.substr(3,1),16);
				}
			}
		else if (colorString.substr(0,3) === "rgb") // FireFox and Chrome return a rgb(n, n, n) value
			{
			colors = colorString.replace(/rgb\(|\)/g, "").split(",");
			}
		else // otherwise return the colorString without doing anything to it
			{
			return colorString;
			}

		for (i=0; i<3; i++)
			{
			colors[i] = parseInt(colors[i]) + lightValue;
			colors[i] = Math.max(colors[i],0); //rgb value cannot be below zero
			colors[i] = Math.min(colors[i],255); //rgb value cannot be above 255
			colors[i] = colors[i].toString(16);
			if (colors[i].length == 1) //must be two-digit, add a leading zero otherwise
				colors[i] = "0" + colors[i];
			}

		return "#" + colors[0] + colors[1] + colors[2];
		}
		
    var menuColor = jQuery("#ms-rteThemeForeColor-4-0").css("color"); // Current top nav color
	var lightColor = lighterColor(menuColor,32); // Start gradient a little lighter
	var darkColor = lighterColor(menuColor,-32); // End gradient a little darker
	
	// This is the only way to make the hover color of a menu element a little lighter
	document.write('<style type="text/css">.s4-tn li.dynamic > a:hover { background-color: ' + lightColor + ' }</style>');

	//These give the top nav bar a 3d gradient effect, different for each browser
	// IE
	jQuery("body #s4-topheader3").css("filter","progid:DXImageTransform.Microsoft.gradient(startColorstr='" + lightColor + "', endColorstr='" + darkColor + "')");
	// Mozilla FireFox
	jQuery("body #s4-topheader3").css("background","-moz-linear-gradient(center top , " + lightColor + ", " + darkColor + ") repeat scroll 0 0 transparent");
	// WebKit (Apple Safari)
	jQuery("body #s4-topheader3").css("background","-webkit-gradient(linear, left top, left bottom, from( " + lightColor + ") , to(" + darkColor + "))");
	// Opera
	jQuery("body #s4-topheader3").css("background","-o-linear-gradient(" + lightColor + "," + darkColor + ")");
	// W3C
	jQuery("body #s4-topheader3").css("background","linear-gradient(" + lightColor + "," + darkColor + ")");

	//Keeps drop down in screen on touch screen tablets
	jQuery('li.dynamic-children').doubleTapToGo();
	}
catch(e)
	{
	// I don't do anything, but I want to make sure there are no javascript errors.
	// Menu will still have color but will not have a gradient.
	}
	
// ]]>
</script>
