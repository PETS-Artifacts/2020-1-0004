    <script>
    $( document ).on( "pagechange", function() {
        var windowWidth = $(window).width();
        var maxPreviewLength = Math.ceil(windowWidth / 3.6);
        $('.articleCommentText.unprocessed,.articleCommentResponseText.unprocessed').each(function(index, value){
            var comment = $(this);
            comment.removeClass('unprocessed');
            var commentText = $(this).text();
            var length = commentText.length;
            if(length > maxPreviewLength){
                comment.addClass('original');
                commentText.substr(0,maxPreviewLength);
                var shortendComment = commentText.substr(0,maxPreviewLength);
                //re-trim if we are in the middle of a word
                shortendComment = shortendComment.substr(0, Math.min(shortendComment.length, shortendComment.lastIndexOf(" ")));
                if(comment.hasClass('articleCommentText')){
                    var commentClass = 'articleCommentText';
                }
                else{
                    var commentClass = 'articleCommentResponseText';
                }
                comment.after('<div class="' + commentClass + ' shortend">' + shortendComment + '<span class="ellipsis"> &hellip;</span></div>');
            }
        });
        
        // Add the event listeners
        $('.articleCommentResponse,.articleComment').on( 'tap', function(evt){
            // Needed since articleCommentResponse is inside of articleComment
            evt.stopPropagation();
            
            // prevent multiple taps
            if(validTap()){
                $(this).toggleClass('expanded');
            }
        });
    });
    </script>
