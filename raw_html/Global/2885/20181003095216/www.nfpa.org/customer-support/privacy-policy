                <label for="search-form-query1" class="show-for-sr">Site Search:</label>
                <div class="search-wrapper ajax-form" data-action="XXXX">
                    <input type="text" id="search-form-query1" name="q1" value="" class="populate" size="24" spellcheck="true">
                    <button name="submit1" type="button" id="search-form-submit1"><span class="show-for-sr">Search</span><i class="icon fa fa-search"></i></button>
