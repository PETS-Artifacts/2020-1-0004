    <div id="autocompleteResults"></div>
    
    <script type="text/javascript">
        $(function () {
            $("#search-form-submit1").on("click", function () {
                doHeaderSearch();
            });
            $("#search-form-query1").keyup(function (event) {
                if (!$(event.srcElement).hasClass('ui-widget-content')) {
                    if (event.keyCode == 13) {
                        $("#search-form-submit1").click();
                    } else {
                        var url = "/NFPA/Custom%20Pages/SemaphoreKeywords.ashx?prefix=" + $("#search-form-query1").val();
                        $.ajax({
                            type: "POST",
                            url: url,
                            dataType: 'html',
                            success: function (data) {
                                var availableTags = data.split("|");

                                var terms = [];
                                $.each(availableTags, function (index, item) {
                                    var tag = item.split("~");
                                    terms.push({ value: tag[0], label: tag[1] });
                                });
                                
                                $("#search-form-query1").autocomplete({
                                    source: terms,
                                    appendTo: "#autocompleteResults",
                                    focus: function (event, ui) {
                                        return false;
                                    },
                                    select: function (event, ui) {
                                        window.location = "/standard_items/search_results?term=" + ui.item.value;
                                        return false;
                                    }
                                });
                            }
                        });
                    }
                }
            });
            $("#search-form-submit2").on("click", function () {
                doCodeSearch();
            });
            $("#search-form-query2").keyup(function (event) {
                if (event.keyCode == 13) {
                    $("#search-form-submit2").click();
                }
            });
        });
        function doHeaderSearch() {
            var term = $("#search-form-query1").val().trim();
            if (term && typeof term == "string" && term.length > 0) {
                utility.showWaitOverlay("Searching...");
                window.location.href = "/standard_items/search_results?searchStr=" + encodeURIComponent(term);
            }
        };
        function doCodeSearch() {
            var term = $("#search-form-query2").val().trim();
            if (term && typeof term == "string" && term.length > 0) {
                utility.showWaitOverlay("Searching...");
                window.location.href = "/codes-and-standards/all-codes-and-standards/list-of-codes-and-standards?keyword=" + encodeURIComponent(term);
            }
        };
    </script>
</header>

