    var globalToken = '';
    var AD_BLOCK_ON = false;
    var AD_BLOCK_NUM_PAGES = '3';
    var AD_BLOCK_LIMIT_PER_SESSION = false;
    var AD_BLOCK_WHITELIST = '';
    var AD_BLOCK_VEIWPORTS = '';

