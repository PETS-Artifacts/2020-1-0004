		$.getJSON('https://www.viacharacter.org/survey/BlogCache/RecentPosts', function (data) {	
			$('#blog1').text(data['posts'][0].title)
			$('#blog1').attr('href', data['posts'][0].url)
			
			$('#blog2').text(data['posts'][1].title)
			$('#blog2').attr('href', data['posts'][1].url)
			
			$('#blog3').text(data['posts'][2].title)
			$('#blog3').attr('href', data['posts'][2].url)
			
			$('#blog4').text(data['posts'][3].title)
			$('#blog4').attr('href', data['posts'][3].url)
        })
        
		if (document.cookie.indexOf("cookieNoticedClosed=") < 0) {
            // Show cookie popup
            $("#cookieNotice").show();
        }

        $("#cookieNoticeClose").on("click", function () {
            $("#cookieNotice").hide();
            var date = new Date();
            date.setTime(date.getTime() + (365 * 24 * 60 * 60 * 1000));
            expires = "; expires=" + date.toUTCString();
            document.cookie = "cookieNoticedClosed=true" + expires + "; path=/";
        });
    })
</script>

