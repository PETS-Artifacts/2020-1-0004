function cycleImagesDesktop(){
	var $active = $('.hero-image .desktop .fade-this .active');
	var $next = ($active.next().length > 0) ? $active.next() : $('.desktop .fade-this img:first');
	$next.css('z-index',2);
	$active.fadeOut(1000,function(){
		$active.css('z-index',1).show().removeClass('active');
		$next.css('z-index',3).addClass('active');
	});
}
$(document).ready(function(){
	setInterval('cycleImagesDesktop()', 3500);
})
