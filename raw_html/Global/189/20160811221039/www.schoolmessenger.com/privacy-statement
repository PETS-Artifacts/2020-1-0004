$(document).ready(function() {
	if ($("html").hasClass("touch")) {
		
		// check for touch device behaviour and events for touch device
		// mega menu drop down
		
		var menuList = $('#menu ul li');
		var menuSubs = $('#menu ul li .sub-menu');
		var menuListSubs = $('#menu ul li .sub-menu');
		var menuSubsSubs = $('#menu ul li .sub-sub-menu');
		
		$("#menu ul li.has-child a.parent").removeAttr( "href" );
		
		$("#menu ul li.has-child a.parent").click(function(){ 
			if($(this).parent().find('.sub-menu').is(':hidden')) {
				menuSubs.hide();
				$(menuList).removeClass('active');
				$(this).parent().find('.sub-menu').show();
				$(this).parent().addClass('active');
			}
			else if($(this).parent().find('.sub-menu').is(':visible')) {
				menuSubs.hide();
				menuSubsSubs.hide();
				$(menuList).removeClass('active');
				$(menuListSubs).removeClass('active');
			}
			
			return false;
		
		});
		
		$("#menu .sub-menu li.has-child > a").removeAttr( "href" );
		
		$("#menu .sub-menu li.has-child > a").click(function(){ 
			if($(this).parent().find('.sub-sub-menu').is(':hidden')) {
				menuSubsSubs.hide();
				$(menuListSubs).removeClass('active');
				$(this).parent().find('.sub-sub-menu').show();
				$(this).parent().addClass('active');
			}
			else if($(this).parent().find('.sub-sub-menu').is(':visible')) {
				menuSubsSubs.hide();
				$(menuListSubs).removeClass('active');
			}
			
			return false;
		
		});
	}
	
	else if ($("html").hasClass("no-touch")) {
		
		$( '#menu li.has-child' ).hover(
			function(){
				$(this).addClass('active').children('.sub-menu').slideDown(200);
			},
			function(){
				$(this).removeClass('active').children('.sub-menu').slideUp(200);
			}
    	);
		
		
		 
		 $("#menu .sub-menu li.has-child").hover(
		 	function(){ $(this).addClass('active').find('.sub-sub-menu').stop().show(); },
		 	function(){ $(this).removeClass('active').find('.sub-sub-menu').stop().hide(); }
		 );
	}
}); 
