                <option value="WY">WY</option>
            </select>
            <input type="submit" id="health-submit" value="Go" class="go-btn"></div>
                    <h4 class="or-call visible-desktop">Or <span>Call</span></h4>
            <h4 class="visible-desktop">
                                    <a href="tel:+18002307526" class="find-health-center-phone">1-800-230-PLAN</a>
                            </h4>
            </form>
</article>
<script type="text/javascript">
    $(document).ready(function () {
        $("#health-center-input-arrow-wrapper-fhc-5762fd5bb37b0").click(function(e) {
            e.preventDefault();
            $("#find-health-center-fhc-5762fd5bb37b0103667").submit();
        });
        
        $("#find-health-center-fhc-5762fd5bb37b0103667").submit(function (e) {
            e.preventDefault();
            if ($("#find-health-center-fhc-5762fd5bb37b0103667 #fhc-5762fd5bb37b0health-zip").val() != '') {
                window.location.href = '/health-center/all/all/' + $("#find-health-center-fhc-5762fd5bb37b0103667 #fhc-5762fd5bb37b0health-zip").val();
                return false;
            }
            else if ($("#find-health-center-fhc-5762fd5bb37b0103667 #fhc-5762fd5bb37b0health-state").val() != "State") {
                window.location.href = '/health-center/' + $("#find-health-center-fhc-5762fd5bb37b0103667 #fhc-5762fd5bb37b0health-state").val() + '/';
                return false;
            }
        });
        $("#fhc-5762fd5bb37b0health-state").change(function (e) {
            if ($("#fhc-5762fd5bb37b0health-state").val() != "State") {
                $("#fhc-5762fd5bb37b0health-zip").val("");
            }
        });

        $("#fhc-5762fd5bb37b0health-zip").click(function () {
            $("#fhc-5762fd5bb37b0health-state").val("State");
        });

        $("#fhc-5762fd5bb37b0health-zip").keypress(function (e) {
            var char = (typeof e.which == "undefined") ? e.keyCode : e.which;
            if (!((char===08||char===13)||(char >= 48 && char <= 58))) {
                e.preventDefault();
            }
        });
    });
</script>		<article id="site-search" class="visible-desktop" method="get">
