<script type="text/javascript">
	$(document).ready(function() {
		$("#find-health-center-fhc-5762fd5c2e457103667").submit(function(e) {
			e.preventDefault();
			if ($("#find-health-center-fhc-5762fd5c2e457103667 #fhc-5762fd5c2e457health-zip").val() != '') {
				window.location.href = '/health-center/all/all/' + $("#find-health-center-fhc-5762fd5c2e457103667 #fhc-5762fd5c2e457health-zip").val();
				return false;
			}
			else if ($("#find-health-center-fhc-5762fd5c2e457103667 #fhc-5762fd5c2e457health-state").val() != "State") {
				window.location.href = '/health-center/' + $("#find-health-center-fhc-5762fd5c2e457103667 #fhc-5762fd5c2e457health-state").val() + '/';
				return false;
			}
		});
		$("#fhc-5762fd5c2e457health-state").change(function(e) {
			if ($("#fhc-5762fd5c2e457health-state").val() != "State")
			{
				$("#fhc-5762fd5c2e457health-zip").val("");
			}
		});

		$("#fhc-5762fd5c2e457health-zip").click(function() {
			$("#fhc-5762fd5c2e457health-state").val("State");
		});
        $("#fhc-5762fd5c2e457health-zip").keypress(function (e) {
            var char = (typeof e.which == "undefined") ? e.keyCode : e.which;
            if (!((char===08||char===13)||(char >= 48 && char <= 58))) {
                e.preventDefault();
            }
        });
	});
