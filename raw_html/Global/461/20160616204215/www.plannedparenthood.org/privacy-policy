<script type="text/javascript">
$(document).ready(function() {
    var countryCheck = $.cookie('countryCheck');
    // if the countryCheck cookie is already set - don't bother looking them up
    if (!countryCheck) { 
        $.ajax({
            url: '/tools/packages/geo_banner/getIPLatLng',
            type: 'GET',
            success: function(country){
                if (country == 'Nigeria') {
                    $.ajax({
                        dataType: 'html',
                        async: false,
                        type: 'get',
                        url: '/tools/packages/geo_banner/get_banner_html',
                        success: function (banner){
                            if($(window).width() > 970){
                                $("#main-header").before(banner);
                            } else {
                                $("#content").before(banner);
                            
                            }
                        }
                    });
                }
            }
        });
        $.cookie('countryCheck', 1, { expires: 30, path: '/' });
    }
});

$('#close-nigeria').live('touchstart click', function(e){
    e.preventDefault();
    $('#nigeria-redirect-banner').slideUp('fast');
});</script>
