                        <img src="http://us.christianlouboutin.com/skin/frontend/louboutin/v4/images/header/header-logo-mobile.svg" alt="Christian Louboutin">
                        <span class="circle-ln"></span>
                    </a>
                    <div id="logo-stars-mobile" class="show">
                        <a href="http://us.christianlouboutin.com/us_en/"><img src="http://us.christianlouboutin.com/skin/frontend/louboutin/v4/images/header/christmas-mobile-logo.svg" alt="Christian Louboutin"></a>
                    </div>
                </li>
                <li  class="options">
                    <div class="wrp-tr">
                        <a class="opt icon-i79 search-off search-toggler" href="javascript:void(0)">
                            <span class="hide">Search</span>
                        </a>
                        <a class="opt icon-i81 icon-bag toggle-r-sidebar toggle-basket-bag" data-name="basket-bag" href="javascript:void(0);">
                            <span class="hide">Cart</span>
                            <div class="hide bag-counter-circle"><span id="mobile-qty"></span></div>
                        </a>
                    </div>
                </li>
            </ul>
        </div>
                    <div class="search-on">
                <form id="mobile-search" name="mobile-search" action="http://us.christianlouboutin.com/us_en/corporatesearch/result/" method="get">
                    <input type="search" id="mobile-search-input-field" type="text" name="q" value="" placeholder="Search" />
                    <button type="submit" class="icon-i79"></button>
                </form>
