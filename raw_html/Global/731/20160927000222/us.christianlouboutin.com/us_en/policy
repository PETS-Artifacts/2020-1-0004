<script>
    // Check browser support

    if (typeof(Storage) != "undefined")
    {
        var currentProducts = JSON.parse(localStorage.getItem("recently-viewed-louboutin-2"))
        if(currentProducts != null){
            currentProducts.each(function(item, index){
                var img = document.createElement('img');
                img.src = item[1];
                img.alt = item[0];
                img.height = 50;
                img.width = 50;
                var a = document.createElement('a');
                a.href=item[2];
                a.writeAttribute({
                    'data-category': 'Footer',
                    'data-label': 'RV Product ' + (index + 1),
                    'data-action': 'RV Product ' + (index +1)
                });
                a.appendChild(img);
                $('recently-viewed-list').appendChild(a);
            });
            $('last-seen-products').show();
        }
    }
    else
    {
        document.getElementById("result").innerHTML="Sorry, your browser does not support Web Storage...";
    }

    Translator.add('Filters','Filters');

