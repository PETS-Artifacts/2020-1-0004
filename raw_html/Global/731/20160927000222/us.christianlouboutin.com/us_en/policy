<script type="text/javascript">
    function countryOk(){
                        Main._closeModalWindow();
        new Ajax.Request('http://us.christianlouboutin.com/us_en/geoip/ajax/visited/', {
            onSuccess: function(transport) {
                Main._closeModalWindow();
                return;
            }
        });
    }

    function countryNotOk(){
                var id = 0;
                                Main._closeModalWindow();
        new Ajax.Request('http://us.christianlouboutin.com/us_en/geoip/ajax/redirect/id/' + id, {
            onSuccess: function(transport) {
                var response = transport.responseText.evalJSON();
                window.location = response.redirect;
            }
        });
    }

    
                        
        var index_empty_modal = Mage.Cookies.get('index_empty_modal');
        if(!index_empty_modal) {
            setTimeout(function () {
                new Ajax.Request('http://us.christianlouboutin.com/us_en/geoip/ajax/index/', {
                    onSuccess: function (transport) {
                        var response = transport.responseText.evalJSON();
                        if (response.visited == '0') {
                            $('geoip-message').update(response.message);
                            $('country-update').update(response.messagenotok);
                            $('country-ok').update(response.messageok);
                            window.Main._openModalWindow('#country-not-shipped-yet');
                            prepareCompass();
                            $('countryUpdate').show();
                        }
                    }
                });
            }, 1000);
        }

        var country_empty_modal = Mage.Cookies.get('country_empty_modal');
        if(!country_empty_modal) {
            setTimeout(function () {
                new Ajax.Request('http://us.christianlouboutin.com/us_en/geoip/ajax/checkCountry/', {
                    onSuccess: function (transport) {
                        var response = transport.responseText.evalJSON();
                        if (response.country == '1') {
                            $('countrychanged-message').update(response.message);
                            $('countrychanged-notok').update(response.messagenotok);
                            $('countrychanged-ok').update(response.messageok);
                            window.Main._openModalWindow('#country-not-shipped-yet');
                            prepareCompass();
                            $('countrychanged').show();
                        }
                    }
                });
            }, 1000);
        }

    document.observe('dom:loaded', function() {
        if (Mage.Cookies.get('index_empty_modal') || Mage.Cookies.get('country_empty_modal')) {
            if (typeof NewsletterPopup !== 'undefined') {
                new NewsletterPopup();
            }
        }
    });

    
    function countrychangedOk(){
                        $('countrychanged').hide();
        new Ajax.Request('http://us.christianlouboutin.com/us_en/geoip/ajax/countryOk/', {
            onSuccess: function(transport) {
                var response = transport.responseText.evalJSON();
                window.location = response.redirect;
            }
        });
    }
    function countrychangedNotOk(){
                        $('countrychanged').hide();
        new Ajax.Request('http://us.christianlouboutin.com/us_en/geoip/ajax/redirectCountry/', {
            onSuccess: function(transport) {
                var response = transport.responseText.evalJSON();
                window.location = response.redirect;
            }
        });
    }

    function prepareCompass(){
        if( !(navigator.userAgent.match(/(iPad|iPhone|iPod)/g)) ){
            Event.observe(window, 'mousemove', function(e){
                var compass = $('modal_compass_arrow');
                var compass_layout = compass.getLayout();
                var offset = compass.viewportOffset();
                var center_x = (offset.left) + (compass.measure('width') / 2);
                var center_y = (offset.top) + (compass.measure('height') / 2);
                var radians = Math.atan2(e.pageX - center_x, e.pageY - center_y);
                var degree = (radians * (180 / Math.PI) * -1) + 180;
                TweenLite.to(compass, 0, {rotation:degree});
            });
        }
    }
</script>

