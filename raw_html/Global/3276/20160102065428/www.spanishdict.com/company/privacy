'use strict';

var googletag;
var pbjs;
var prebidAds;
var sovrnAds;
var sovrnAuctionData;
var yieldbotAds;
var bidRequestConfig;
var dacBidRequestConfigs;
var sbi_morpheus;

(function() {
  var prebidUrl;
  var indexUrl;
  var openXUrl;
  var criteoUrl;
  var amznUrl;
  var sovrnUrl;
  var yieldbotUrl;
  var mktpAdtechURL;
  var mktpPubtaglib;
  var mktpDACPlugin;
  var mktpPubTagLibHandler;
  var sonobiUrl;
  var headerBidderScripts;
  var extraMktpScripts;

  // Google.
  googletag = googletag || {};
  googletag.cmd = googletag.cmd || [];

  // Sample down and log 1% of total traffic.
  if (Math.floor(Math.random() * 100) < 1) {
    googletag.cmd.unshift(function() {
      // Hack googletag debug_log to get events.
      var googletagLog = googletag.debug_log.log;

      googletag.debug_log.log = function (level, message) {
        var slotName;
        var kvp = message.getMessageArgs();
        var messageId = message.getMessageId();

        // Page level targeting. Add kvp to all slots.
        if (messageId === 88) {
          for (slotName in SD_AD.adSlots) {
            if (SD_AD.adSlots.hasOwnProperty(slotName)) {
              SD_AD.adSlots[slotName][kvp[0]] = kvp[1];
            }
          }
        }

        // Slot level targeting. Add kvp to relevant slot only.
        if (messageId === 17) {
          slotName = kvp[2].replace('/1027916/', '');
          SD_AD.adSlots[slotName][kvp[0]] = kvp[1];
        }

        // Call original googletag log function to preserve functionality.
        googletagLog.apply(googletag.debug_log, arguments);
      };

      // Add event listeners to get events.
      googletag.pubads().addEventListener('slotRenderEnded', function(message) {
        var slotName = message.slot.getName().replace('/1027916/', '');

        // Blank message.lineItemId means AdX, AdSense won the impression.
        if (!message.lineItemId) {
          SD_AD.adSlots[slotName].is_google_ad = true;
        }

        // Let our logging code know this slot is finished rendering.
        SD_AD.logAuction(slotName);
      });
    });
  }

  googletag.cmd.push(function() {
    googletag.pubads().setTargeting('sd_is_mobile', SD_GA_IS_MOBILE);
    googletag.pubads().setTargeting('test_group', SD_TEST_GROUP);

    if (SD_AD.isMobile && SD_MOBILE_HUB) {
      googletag.pubads().setTargeting('sd_mobile_hub',
        SD_MOBILE_HUB);
    }

    if (SD_AD_TEST !== '') {
      googletag.pubads().setTargeting('sd_adtest', SD_AD_TEST);
    }
    if (!SD_AD.isMobile && SD_DESKTOP_HUB) {
      googletag.pubads().setTargeting('sd_desktop_hub',
        SD_DESKTOP_HUB);
    }
    googletag.pubads().disableInitialLoad();
  });

  // Prebid.
  pbjs = pbjs || {};
  pbjs.que = pbjs.que || [];
  prebidAds = [];
  prebidUrl = SD_AD.prebidUrl;

  // Index.
  indexUrl = SD_AD.indexUrl;

  // OpenX.
  openXUrl =
    '//ox-d.spanishdict.servedbyopenx.com/w/1.0/jstag?nc=1027916-SpanishDict';
  if (SD_AD.isMobile) {
    openXUrl += '-Mob';
  }

  // Criteo.
  criteoUrl = SD_AD.criteoUrl;

  // Amazon.
  amznUrl = '//c.amazon-adsystem.com/aax2/amzn_ads.js';

  // Sovrn.
  sovrnUrl = SD_AD.sovrnUrl;
  sovrnAds = sovrnAds || {
    'id': '1000',
    'imp': [
    ],
    'site': {
      'domain': 'spanishdict.com',
      'page': window.location.pathname
    }
  };
  sovrnAuctionData = sovrnAuctionData || {};

  // Yieldbot.
  yieldbotUrl = '//cdn.yldbt.com/js/yieldbot.intent.js';
  yieldbotAds = [];

  //- Marketplace.
  mktpAdtechURL = 'http://aka-cdn.adtechus.com/dt/common/DAC.js';
  mktpPubtaglib = SD_AD.mktpPubtaglib;
  mktpDACPlugin =
    'http://aka-cdn-ns.adtechus.com/dt/common/DACPubApiPlugin.js';
  mktpPubTagLibHandler = function (response) {
    var bidKey = response.bidKey;
    var cpm = (response.cpm ? response.cpm : 0).toFixed(2);
    var mktpKey = bidKey + '_' + response.alias;

    googletag.cmd.push(function() {
      googletag.pubads().setTargeting(mktpKey, cpm);
    });
  };
  bidRequestConfig = {
    server: 'adserver.adtechus.com',
    //Network ID
    network: '10065.1',
    //Keyvalue name used in adserver
    bidKey: 'mktp',
    loadAd: mktpPubTagLibHandler,
    //Adjust gross to net CPM (i.e. -15% for CPM value to decrease by 15%)
    adjustment: -20
  };
  dacBidRequestConfigs = [];

  // Sonobi.
  sbi_morpheus = sbi_morpheus || {registration: {}};
  sbi_morpheus.cmd = sbi_morpheus.cmd || [];
  sonobiUrl = 'http://mtrx.go.sonobi.com/morpheus.spanishdict.js';

  // Async script setup.
  headerBidderScripts = [
    indexUrl,
    sonobiUrl,
    openXUrl,
    amznUrl,
    criteoUrl,
    sovrnUrl,
    yieldbotUrl,
    mktpAdtechURL,
    prebidUrl
  ];

  extraMktpScripts = [
    mktpPubtaglib,
    mktpDACPlugin
  ];

  $LAB
    .script(headerBidderScripts)
    .wait()
    .script(extraMktpScripts)
    .wait(function () {
      var i;
      var yieldbotPubId;
      var yieldbotAd;
      var config;
      var timeoutLength;
      var mktpTryLoadPubAd;
      var SD_PREBID_SLOT_MAPPINGS;

      // Function to load gpt to init request to DFP.
      var initAdServer = function() {
        var gads;
        var useSSL;
        var node;

        if (!SD_AD.initAdServerSet) {
          SD_AD.initAdServerSet = true;
          gads = document.createElement('script');
          gads.async = true;
          gads.type = 'text/javascript';
          useSSL = 'https:' === document.location.protocol;
          gads.src = (useSSL ? 'https:' : 'http:') +
            '//www.googletagservices.com/tag/js/gpt.js';
          node = document.getElementsByTagName('script')[0];
          node.parentNode.insertBefore(gads, node);
        }
      };

      // Prebid post load setup.
      pbjs.que.push(function() {
        pbjs.addAdUnits(prebidAds);
      });
      pbjs.que.push(function() {
        pbjs.requestBids({
          timeout: 500,
          bidsBackHandler: function(bidResponses) {
            initAdServer();
          }
        });
      });

      // Mainly standard prebid KVP with one custom KVP for DFP reporting to
      // differentiate between bidders and ad slots.
      SD_PREBID_SLOT_MAPPINGS = {
        '/1027916/BSM_300_250_SIDEBAR': 'sidebar',
        '/1027916/BSM_160_600_SIDEBAR': 'siderail',
        '/1027916/BSM_300_250_SIDEBAR_2': 'sidebar2',
        '/1027916/BSM_300_250_CONTENT': 'content',
        '/1027916/BSM_728_90_FOOTER': 'footer',
        '/1027916/BSM_728_90_HEADER': 'header',
        '/1027916/SD_MOBILE_CONTENT': 'content',
        '/1027916/SD_MOBILE_HEADER': 'header',
        '/1027916/SD_MOBILE_MIDDLE': 'middle',
        '/1027916/SD_MOBILE_NEW_ADHESION': 'adhesion'
      };
      pbjs.bidderSettings = {
        standard: {
          alwaysUseBid: false,
          adserverTargeting: [{
            key: 'hb_bidder',
            val: function(bidResponse) {
              return bidResponse.bidder;
            }
          }, {
            key: 'hb_adid',
            val: function(bidResponse) {
              return bidResponse.adId;
            }
          }, {
            key: 'hb_pb',
            val: function(bidResponse) {
              return bidResponse.pbMg;
            }
          }, {
            key: 'hb_size',
            val: function(bidResponse) {
              return bidResponse.size;
            }
          }, {
            key: 'sd_hb_adslot',
            val: function(bidResponse) {
              var value = [];
              value.push(bidResponse.bidder);
              value.push(SD_AD.isMobile ? 'm' : 'd');
              value.push(SD_PREBID_SLOT_MAPPINGS[bidResponse.adUnitCode]);
              return value.join('_');
            }
          }]
        }
      };

      // Amazon post load setup.
      try {
        amznads.getAdsCallback('3286', function() {
          amznads.setTargetingForGPTAsync('amznslots');
        });
      } catch (e) { /* Ignore. */ }

      // Yieldbot post load setup.
      yieldbotPubId = SD_AD.isMobile ? '68cc' : '9146';

      yieldbot.pub(yieldbotPubId);
      for (i = 0; i < yieldbotAds.length; i++) {
        yieldbotAd = yieldbotAds[i];
        yieldbot.defineSlot(yieldbotAd.name, { sizes: [
          [yieldbotAd.width, yieldbotAd.height]
        ]});
      }
      yieldbot.enableAsync();
      yieldbot.go();

      googletag.cmd.push(function() {
        var j;
        var yieldbotKey;
        var yieldbotVal;
        var criteria = yieldbot.getPageCriteria().split(',');

        for (j = 0; j < criteria.length; j++) {
          yieldbotKey = criteria[j].split(':')[0];
          yieldbotVal = criteria[j].split(':')[2];

          if (yieldbotKey && yieldbotVal) {
            yieldbotKey = 'ybot_' + yieldbotKey;
            yieldbotVal = (50 * Math.round(yieldbotVal / 50) / 100).toFixed(2);
            googletag.pubads().setTargeting(yieldbotKey, yieldbotVal);
          }
        }
      });

      // Marketplace post load setup.
      mktpTryLoadPubAd = function(alias, adContainerId) {
        if (document.getElementById(adContainerId)) {
          ADTECH.loadPubAd(alias);
        }
      };

      for (i = 0; i < dacBidRequestConfigs.length; i++) {
        config = dacBidRequestConfigs[i];
        mktpTryLoadPubAd(config.alias, config.adContainerId);
      }

      // Sonobi post load setup.
      googletag.cmd.push(function() {
        // Calls googletag.pubads().refresh().
        // Must be last push to googletag.cmd after all targeting.
        sbi_morpheus.callOperator();
      });

      // Arbitrary timeout to give more time for header bidders to return bids
      // before we load gpt.js. Prebid should load gpt after 500 ms, so this
      // will only execute if initAdServer has not already been called.
      timeoutLength = 700;
      setTimeout(initAdServer, timeoutLength);
    });

})();

/*global SD_AD googletag sbi_morpheus sovrnAds yieldbotAds
prebidAds dacBidRequestConfigs*/

'use strict';

var SD_YIELDBOT_MAPPINGS;
var SD_MARKETPLACE_MAPPINGS;

SD_AD.adDef = function(identifier, width, height, title) {
  var SD_SONOBI_MAPPINGS;
  var SD_SOVRN_MAPPINGS;
  var SD_PREBID_MAPPINGS;

  var adDimensions = [width, height];
  if (identifier === 'adHesion') {
    adDimensions = [adDimensions, [1, 1]];
  }

  SD_AD.adSlots[title] = {
    'ad_unit': identifier,
    'is_google_ad': false,
    'ad_dimensions': [width, height]
  };

  // Register ads for Google.
  googletag.cmd.push(function() {
    googletag.defineSlot('/1027916/' + title, adDimensions, identifier)
      .addService(googletag.pubads());
  });

  //- Register ads for Sonobi.
  if (SD_AD.isMobile) {
    SD_SONOBI_MAPPINGS = {
      'adTop': '69edf6046870456f1f5c',
      'adMiddle': 'e03e03ff332ca6a08e6a',
      'adBot': '3f79b8e08e9284acd186',
      'adHesion': '30527c2377694ff84760'
    };
  } else {
    SD_SONOBI_MAPPINGS = {
      'adSideLong': '533144e843156b8400b1',
      'adSide1': '56dc3bb5f6c8e8dfe346',
      'adSide2': '9999176c8b412a283a3c',
      'adContent': '43b92720a5ba058f12ff',
      'adTop': 'f975d667234a26f83004',
      'adBot': '7c4e209f4fabee27e9e1'
    };
  }
  if (SD_SONOBI_MAPPINGS[identifier]) {
    sbi_morpheus.cmd.push(function() {
      sbi_morpheus.register(identifier, SD_SONOBI_MAPPINGS[identifier]);
    });
  }

  //- Register ads for Sovrn.
  if (SD_AD.isMobile) {
    SD_SOVRN_MAPPINGS = {
      'adTop': '306552',
      'adHesion': '306553',
      'adMiddle': '306551',
      'adBot': '307552'
    };
  } else {
    SD_SOVRN_MAPPINGS = {
      'adSide1': '306545',
      'adSide2': '306546',
      'adContent': '306547',
      'adTop': '306548',
      'adBot': '306549',
      'adSideLong': '306550'
    };
  }
  sovrnAds.imp.push({
    'id': identifier,
    'banner': {
      'w': width,
      'h': height
    },
    'tagid': SD_SOVRN_MAPPINGS[identifier]
  });

  //- Register ads for second set of Sovrn IDs.
  if (SD_AD.isMobile) {
    SD_SOVRN_MAPPINGS = {
      'adTop': '319605',
      'adHesion': '319606',
      'adMiddle': '319604',
      'adBot': '319607'
    };
  } else {
    SD_SOVRN_MAPPINGS = {
      'adSide1': '319598',
      'adSide2': '319599',
      'adContent': '319600',
      'adTop': '319601',
      'adBot': '319602',
      'adSideLong': '319603'
    };
  }
  sovrnAds.imp.push({
    'id': identifier,
    'banner': {
      'w': width,
      'h': height
    },
    'tagid': SD_SOVRN_MAPPINGS[identifier]
  });

  //- Register ads for Yieldbot.
  if (SD_AD.isMobile) {
    SD_YIELDBOT_MAPPINGS = {
      'adTop': 'MLB',
      'adBot': 'mobrec',
      'adHesion': 'adhesion',
      'adMiddle': 'mobrec2'
    };
  } else {
    SD_YIELDBOT_MAPPINGS = {
      'adSideLong': 'sky',
      'adSide1': 'SB',
      'adSide2': 'SB2',
      'adTop': 'LB'
    };
  }
  if (SD_YIELDBOT_MAPPINGS[identifier]) {
    yieldbotAds.push({
      name: SD_YIELDBOT_MAPPINGS[identifier],
      width: width,
      height: height
    });
  }

  // Register ads for Prebid.
  if (SD_AD.isMobile) {
    SD_PREBID_MAPPINGS = {
      'adTop': '5779590',
      'adMiddle': '5798797',
      'adBot': '5798790',
      'adHesion': '5798801'
    };
  } else {
    SD_PREBID_MAPPINGS = {
      'adSideLong': '5798782',
      'adSide1': '5798775',
      'adSide2': '5798776',
      'adContent': '5798780',
      'adTop': '5779546',
      'adBot': '5798781'
    };
  }
  if (SD_PREBID_MAPPINGS[identifier]) {
    prebidAds.push({
      code: '/1027916/' + title,
      sizes: [
          [width, height]
      ],
      bids: [
        {
          bidder: 'appnexus',
          params: {
            placementId: SD_PREBID_MAPPINGS[identifier]
          }
        }
      ]
    });
  }

  // Register ads for Marketplace.
  if (SD_AD.isMobile) {
    SD_MARKETPLACE_MAPPINGS = {
      'adHesion': {
        placement: 3689928,
        sizeId: 3055,
        alias: 'm320x50adhesion'
      },
      'adTop': {
