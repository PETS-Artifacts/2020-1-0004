    var scriptPath;
    function ScriptPath() {
        return document.getElementsByTagName("html")[0].attributes.getNamedItem("data-scriptpath").value;
    }
    
    function LibsPath() {
        return document.getElementsByTagName("html")[0].attributes.getNamedItem("data-libspath").value;
    }

    if (typeof jQuery == 'undefined') { document.write(unescape("%3Cscript src='" + LibsPath() + "jquery-1.11.3.min.js' %3E%3C/script%3E")); }
