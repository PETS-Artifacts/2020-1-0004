  $('.search-form .btn').click(function(e){
   if($('.search-form input[type=text]').val() == '' || $('.search-form input[type=text]').val() == ' '){
       e.preventDefault();
    }
  });
  $('.search-form input[type=text]').keypress(function(e) {
    if(e.which == 13) {
       if($('.search-form input[type=text]').val() == '' || $('.search-form input[type=text]').val() == ' '){
         e.preventDefault();
      }
    }
  });
</script>
