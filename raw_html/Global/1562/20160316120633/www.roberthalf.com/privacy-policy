<script>jQuery(function() {
  jQuery('.accordion-menu-6').accordion({ animated: 'slide', autoHeight: false, clearStyle: false, fillSpace: false, event: 'click', header: 'h3', collapsible: true, navigation: false, icons: { 'header': 'ui-icon-triangle-1-e', 'headerSelected': 'ui-icon-triangle-1-s' }, cookie: { path: '/' }, active: false });
  jQuery('.accordion-menu-6 .accordion-header.no-children').each(function(index, element) {
    jQuery(this)
      .unbind('.accordion')
      .children('.ui-icon')
      .removeClass('ui-icon-triangle-1-e')
      .addClass('ui-icon-triangle-1-s');
  });
});</script>
