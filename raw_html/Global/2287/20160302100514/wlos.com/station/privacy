sinclairDigital.dependencies.define(['Handlebars','sd.templates'],function(){
    function fetchWeather(zip) {
        var fetch = $.ajax({
            url: 'http://wjla.com/api/rest/audiencero/weather/zipcode/' + zip
        })
        return fetch;
    }
    function renderWeather(weatherZipCode) {
        fetchWeather(weatherZipCode).done(function(data){
            var obj = {
                weather: data.data[0]
            }
            $('.sd-weather-current-icon').removeClass('wi-98').addClass('wi-'+obj.weather.forecast.hourlyForecasts[0].iconCode);
            $('.sd-weather-current-temp').text(obj.weather.forecast.hourlyForecasts[0].temp); 
            $('.wx-container').css('opacity',1)
        })
    }
    $(function(){
        renderWeather('28803')
    })
