<script>

    function isIE() {
        var ie = document.documentMode;
        var edge = window.StyleMedia;

        if (ie) {//returns an integer version
            return true;
        }
        else if (window.StyleMedia) {
            return true;
        }
        else {
            return false;
        }
    }

    function validSearch() {
        var querystring = $("#utility-nav-search-input").val();
        return querystring.trim().length > 1 ? true : false;
    }

    function Search() {
        var querystring = $("#utility-nav-search-input").val();
        var url = "/en/Search-Results?query=" + querystring;
        //if (querystring != null && querystring.length > 1) {
            window.location.replace(url);
        //}
    }


    $('#utility-nav-search-button').click(function () {
        /*
        This is a hack to get by a js error in common.js where the n.forEach function is not working
        */
        if (isIE()) {
            if ($(".utility-nav__search").hasClass("open")) {

                if (validSearch()) {
                    Search();
                }
            }
            else {
                $(".utility-nav__search__input").addClass("open");
            }
        }
        else {

            if (validSearch()) {
                Search();
            }
        }
        return true;
    });

    $('#utility-nav-search-input').on('keypress', function (e) {
        if (e.which === 13) {

            if (validSearch()) {
                Search();
            }
            return false;
        }
    });
    
   
</script>
