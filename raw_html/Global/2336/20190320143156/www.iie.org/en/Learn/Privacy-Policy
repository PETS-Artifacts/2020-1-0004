<form class="utility-nav__search" id="utility-nav-search">
    <input class="utility-nav__search__input" id="utility-nav-search-input" name="query" placeholder="Search" required="" type="search">
    <button class="utility-nav__search__button" id="utility-nav-search-button" type="button" >
        <i class="icon icon-search"></i>
