function tryAnother_CRC_Customer_Feedback() {

	// random param to force refresh
		$('#captcha_CRC_Customer_Feedback').attr('src','/vgn-ext-templating/sbg/jcaptcha.jpg?'+Math.floor(Math.random()*100));

}

</script>
	
	
	
<script type="text/javascript">
	$(document).ready(function() {
	
		// initial captcha
		tryAnother_CRC_Customer_Feedback();
	
		$('#sendEnquery').click(function(e) {
			e.preventDefault();
			submitForm();   
		}); 

		validator_CRC_Customer_Feedback = $("#CRC_Customer_Feedback").validate({ 
					
		});				

		var formNames = new Array();
		

		// set up the fields
		prepareFormFields('CRC_Customer_Feedback', formNames);
	});  
</script>

<script type="text/javascript">

function submitForm(){ 
	//alert('submit form called!!');
	$.validator.addMethod(
       "regex",
        function(value, element, regexp) {
        var check = false;
        return this.optional(element) || regexp.test(value);
        },
        "Please enter 10 digit phone number."
        );

	var validator = $("#CRC_Customer_Feedback").validate({ 

    }); 
	
	
	    if(validator.form()){
    
		var formValues = $("#CRC_Customer_Feedback").serialize();
		
		$.cookie('sbgcscookie', formValues, { expires: 7 });  		
		showWaitFeedback(true);

		var postData = formValues + "&dataTable=sbgcs_feedback_form&serviceId=formServiceSaveAndEmail&successMessage=<p>Thank you for Contacting Us.</p>";
		postData=postData+"&ot_formId=e7c1c3c6ca122410VgnVCM1000007c8f910aRCRD&ot_siteName=tanzania&ot_PAGE_URL=" + window.location.href;		
		//alert('postData: '+postData);
			$.ajax({   
			  type: "post",   
			  url: '/vgn-ext-templating/sbg/cs/sbgAjaxFormProcess.jsp',   
			  data: postData,
			  success: displayFeebackFormResponse,
				error: function(jqXHR, textStatus, errorThrown) {
					showWaitFeedback(false);
					console.log(errorThrown);
					}
			});

    }
    
    	
    
}


function displayFeebackFormResponse(resp){
	var checkError = resp.indexOf('<span');

	showWaitFeedback(false);

	if(checkError == -1){
		//trackFormSuccess();
		dtmAjaxFromComplete();
		$("#CRC_Customer_Feedback").hide();

	} else {
		
	}
	$('div[id=feedbackFormMessage]').html(resp);
	$('div[id=FeedbackThankYou]').show();
	$('div[id=feedbackFormMessage]').show();	
$("div.modal-backdrop").remove();
}
		
 
 </script>
 
 
<script type="text/javascript">

function showWaitFeedback(waitStart) {
    // show/hide wait cursor, message etc

    if (waitStart) {
	//DTM Flag for form is being submitted
	dataLayer.formisSubmitted = true; 
        // show standard hourglass pointer
        $('body').css('cursor', 'wait');
		$('.loadingBackground').css('display','block');
		$('.loadingDiv').css('display','block');
    } else {
	//DTM Flag for form not submitted
	dataLayer.formisSubmitted = false; 
        // show standard pointer
        $('body').css('cursor', 'default');    
		$('.loadingBackground').css('display','none');
		$('.loadingDiv').css('display','none');
	}
}

 </script>


	
 
 
 
