                    ErrClientScripting: ''
                };
		    
		    MsgObject.MsgUnSuBr = MsgObject.MsgUnSuBr.replace('$$$BeginLink$$$', '<label id="lnkSuBr">');
		    MsgObject.MsgUnSuBr = MsgObject.MsgUnSuBr.replace('$$$EndLink$$$', '</label>');

		    dataLayer.push({ "user_id": "0" });

		    var _isSup = UnSupBr(MsgObject.MsgUnSuBr);
		    if (_isSup) // print one message at a time 
		    {
		        CookieEnabled(MsgObject.MsgCookieDisabled);
		    }
		    else
		    {
		        var lnkSuBr = document.getElementById('lnkSuBr');
		        if (lnkSuBr)
		        {
		            lnkSuBr.addEventListener('click', suBr, false);
		        }
		    }
		    
		    function suBr(e)
		    {
		        alert(MsgObject.MsgSuBr);
		        //$('#modalContentDiv').css({ 'width': '20%', 'height': '30%', 'box-shadow': 'none' });
		        //$('.modal-header').css({ 'border': 'none' });
		        //Util.showModal(MsgObject.MsgSuBr);
		    }

		    function closeModal()
		    {
		        $('#modalDiv')[0].style.display = 'none';
		        if ($().off)
		        {   // cleanup events 
		            $('#modalDiv').off('tap touchstart', handleTouch);
		            $('#modalDiv').off('click keyup', handleMouseKey);
		            $(document).off('click keyup', handleMouseKey);
		            $(document).off('tap touchstart', handleTouch);
		        }
		    }

		    function fnDownload()
		    {
		        var el ;
		        if ($('.ftContent'))
		            el = $('.ftContent');
		        else
		            el = $('.modal-body');
		        
		        var value = $(el).html();
		        if (value == null)
		        {
		            el = $('.modal-body');
		            value = $(el).html();
		        }
		        value = $("<div/>").text(el.html()).html();  //encode, decode  

		        var hdnLt = $('#hdnLt');
		        $(hdnLt).val(value);
		    }

		    function showModal(pContent, footerContent)
		    {
		        var modalBody = $('.modal-body');
		        $(modalBody).empty();

		        // place anchor at top and link at bottom to work around missing or misplaced html anchor links in document
		        // (i.g. anchor of Privacy Policy is under the header "Privacy Policy for LandmarkWorldwide.com." 
                // Course legal terms contains no anchor links at all 
		        pContent = "<a id='_top' style='display:none;'></a>" + pContent + "<a href='#_top' style='display:none;'></a>";
		        if (footerContent)
		            pContent = '<div class="ftContent" style="overflow-y:auto">' + pContent + "</div>" + footerContent;

		        $(modalBody).html(pContent);
		        $(modalBody).scrollTop(0);
		        
		        try
		        {   // scroll to top  
		            var elements = $(modalBody).find("[href='#_top']");
		            var a = $(elements).eq($(elements).length - 1);

		            if ($(a).length > 0)
		            {
		                $(a).click(function ()
		                {
		                    $(modalBody).animate({ scrollTop: 0 }, 'fast');
		                    return false;
		                });

		                if ($(a)[0].click)
		                {
		                    $(a)[0].click();
		                }
		                else
		                {   // for safari
		                    var event = document.createEvent('MouseEvent');
		                    event.initEvent('click', true, true);
		                    $(a)[0].dispatchEvent(event);
		                }
		            }
                    
		            var at = $(modalBody).find("a[id$=_top]");
		            if ($(at).length > 0)
		                $(at)[0].scrollIntoView(true);
		        }
		        catch (e) { }

		        var modalDiv = $('#modalDiv');

		        $(modalDiv).on('tap touchstart', handleTouch);
		        $(modalDiv).on('keyup click', handleMouseKey);
		        $(document).on('tap touchstart', handleTouch);
		        $(document).on('keyup', handleMouseKey);

		        $('#imgCloseModal').click(function () { closeModal() });
		        $(modalDiv)[0].style.display = 'block';

		        var __h = $(modalBody).height();
		        if (footerContent)
		            $('.ftContent').css('max-height', (__h - 140) + 'px');

		    }
		    
		    function handleTouch(e)
		    {
		        var target = e.target || e.srcElement;
		        if (target && $(target).parents('#modalContentDiv').length > 0)
		            return;
		        else
		            closeModal();
		    }

		    function handleMouseKey(e)
		    {
		        var target = e.target || e.srcElement;
		        var keyCode = e.keyCode || e.which;

		        if (keyCode == 27)
		        {
		            closeModal();
		        }
		        else if (target && $(target).parents('#modalContentDiv').length > 0)
		        {
		            return;
		        }
		        else if (keyCode == 1) 
		        {
		            closeModal();
		        }
		    }

            
		    function print(className, windowTitle)
		    {
		        var el = $(className);

		        var mywindow = window.open('about:blank', windowTitle, 'height=400,width=600');
		        mywindow.document.write('<html><head><title>' + windowTitle + '</title>');
		        mywindow.document.write('</head><body >');
		        mywindow.document.write($(el).html());
		        mywindow.document.write('</body></html>');

		        mywindow.document.close(); // necessary for IE >= 10
		        mywindow.focus(); // necessary for IE >= 10

		        mywindow.print();
		        mywindow.close();

		        return true;
		    }
            
		    var prm = Sys.WebForms.PageRequestManager.getInstance();
		    prm.add_beginRequest(EmptyLayout_BeginRequest);
		    prm.add_endRequest(EmptyLayout_EndRequest);
		    function EmptyLayout_BeginRequest(sender, e)
		    {
		    	try {
		            $get('uprMain').style.display = 'block'
		    	} catch (ex) { }
		    }
		    function EmptyLayout_EndRequest(sender, e)
		    {
		        try
		        {
		            $get('uprMain').style.display = 'none';
		        } catch (ex)    {       }
		    }

		    function preventSpace()
		    {
		        var __elements = document.getElementsByClassName('preventSpace');
		        for (var i = 0, len = __elements.length; i < len; i++)
		        {
		            var __domElement = __elements[i];
		            __domElement.onload = Util.preventSpace(__domElement);
		        }
		        $('.clearSearch').val(""); // Clear previous search text
		    }
		    

		    Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(cleanHTML);
		    Sys.WebForms.PageRequestManager.getInstance().add_pageLoaded(preventSpace);
