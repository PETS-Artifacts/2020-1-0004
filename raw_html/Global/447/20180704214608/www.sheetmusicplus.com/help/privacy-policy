							$(".gdprText").css("display", "block");
						} else {
							$(".gdprText").css("display", "none");
						}
						var site_warning = $.cookie('site_warning');
						if (null != site_warning && "true" == site_warning) {
							$(".cookie-warning").css("display", "block");
							$.cookie("site_warning", 'false', {
									path : "/"
								});
						} else {
							$(".cookie-warning").css("display", "none");
