			$(function(){  
				$.ajax({
					type: "POST",
					data: "originalUrl=http://www.sheetmusicplus.com/help/privacy-policy&responsive=true",
					url: "/cartridges/header/headerLabelEles.jsp",
					success: function(str) {
						if("true" == "true"){
							$("#rootMenu").prepend($(str).html());
						}else{
							var headerHtml = '<div class="cartBox">' + $(str).find(".cartBox").html() + '</div>';
							headerHtml += '<div class="multipleSite">' + $(str).find(".multipleSite").html() + '</div>';
							headerHtml += '<div class="acctNav">' + $(str).find(".acctNav").html() + '</div>';
							$(".toolBar").append(headerHtml);
						}
						$(".select_innerBox li a").each(function(){
							if($(this).attr('position')){
								$(this).css(
									'background-position' , '0 ' + (1-$(this).attr('position'))*14 + 'px'
								)
							}
						});

					}
				});
				
			}); 
