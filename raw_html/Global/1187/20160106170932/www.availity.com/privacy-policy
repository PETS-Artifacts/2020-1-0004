					$(this).addClass('desc');
					dir = 'desc';
				} else {
					$(this).removeClass('desc');
					$(this).addClass('asc');
					dir = 'asc';
				};
				sortUsingNestedText($('.item-wrapper'), "div", "div.rightcol:not(header) span", dir);
			});
			
			// Search
			if ($('body').hasClass('search')) {
				x = 5; // seach results to return
				
				items = $('.search-results ol.results li').size();
				
				$('.search-results ol.results li:gt(4)').hide();
				
				// Hide show more if there are less than 5 items
				if (items <= x) {
					$('.showmore').fadeOut('fast');
				}
				$('ol.results li:lt('+x+')').fadeIn("fast");
				
				$('.showmore a').click(function () {
					
					x= (x+5 <= items) ? x+5 : items;
					
					// alert(items + '~' + x);
					
					$('ol.results li:lt('+x+')').fadeIn("fast");
					
					if (items <= x) {
						// End of the line, swap out the Show More with Show Less
						$(this).parent().fadeOut("fast");
						$('.showless').fadeIn("fast");
					}
					if (x <= vis) {
						//$('.showmore').fadeIn("fast");
					}
				});
				$('.showless a').click(function () {
					
					x=(x-5<0) ? 5 : x-5;
					
					$('ol.results li').not(':lt('+x+')').fadeOut("fast");
	
					$(this).parent().fadeOut("fast");
					$('.showmore').fadeIn("fast");
				});
			}
			
			
