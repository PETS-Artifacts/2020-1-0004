      // Set default tab & content

	   $('#tabs ul li:first-child a').addClass('active');

      var default_tab = $('#tabs ul li.tab:lt(1)').find('a').attr('href');
      $(default_tab).css('left', '0px');
      var defaultHeight = $(default_tab).css('height');
	  window.defaultAccordionHeight = defaultHeight+'px';
      $('#tabbed-content').css('height', defaultHeight);
	  
	  // Fix tabbed accordion height issue on load
	  if (!$.trim( window.location.hash )) {
	  	if (default_tab) location.hash = default_tab;
	  }

      // tab click functionality
      $('li.tab').click(function(event) {
		  
		  event.preventDefault();
	  
		el = $(this).find('a');
		
		$('body,html').animate({ scrollTop: 0 });
		 
		// Set the URL
		// window.location.hash = el.attr('href');
		if(history.pushState) {
			history.pushState(null, null, el.attr('href'));
		}
		else {
			location.hash = el.attr('href');
		}

		// Collapse all accordions
		$('.accordion').accordion({
			autoHeight: true,
			active: false,
			collapsible: true,
			heightStyle: "content"	
		});

        // Hide previous link
        var prevlink;

        $('li.tab a.active').each(function() {
          prevlink = $(this).attr('href');
		  previndex = $(this).parent().index();
          $(this).removeClass('active');
        });

	    el.addClass('active');

		nextlink = el.attr('href');
		nextindex = $(this).index();
		
        if (!($(nextlink).hasClass("active"))) {
          if (previndex < nextindex) {
			  $(prevlink).animate({"left":"-700px"},500,'linear');
			  $(nextlink).css('left', '700px');
			  $(nextlink).animate({"left":"0"},500,'linear');

		  } else {
			  $(prevlink).animate({"left":"+700px"},500,'linear');
			  $(nextlink).css('left', '-700px');
			  $(nextlink).animate({"left":"0"},500,'linear');
		  }

		  nextHeight = $(nextlink).height();
		  window.defaultAccordionHeight = nextHeight+'px';

          $('#tabbed-content').animate({"height": nextHeight+'px'}, 500, 'linear');
          $(prevlink).removeClass('active');
          $(nextlink).addClass('active');
	
        }
		
      });

      $(function() {
        $( ".accordion" ).accordion({
          autoHeight: true,
          active: false,
          collapsible: true,
          heightStyle: "content"
        });
		
		// Fix extra whitespace issue on initial load
        var $tabbedContent = $('#tabbed-content');
        var defaultAccordionHeight = parseInt(window.defaultAccordionHeight);
		$tabbedContent.animate({'height': defaultAccordionHeight});
		
      });

      $('.accordion h3').click(function(){
        var $contents = $('#'+$(this).attr('aria-controls'));
        var $tabbedContent = $('#tabbed-content');
        var defaultAccordionHeight = parseInt(window.defaultAccordionHeight);
        var open = !$(this).hasClass('ui-state-active');
        if(open) {
          var accordionHeight = parseInt($contents.css('height'));
          $tabbedContent.animate({'height': defaultAccordionHeight + accordionHeight + 'px'});
        } else {
          $tabbedContent.animate({'height': defaultAccordionHeight});
        }
      });

			$("#btn1").click(function(){
				$("#box").animate({height:"300px"});
			  });
			  $("#btn2").click(function(){
				$("#box").animate({height:"100px"});
			  });
      // Check for tabbed content
	  // Check the URL for a hash, if so, trigger the appropriate tab
	  var mytab = $.trim( window.location.hash );
	  if(mytab) {
		// Trigger click
		$('li.tab').find('a[href*='+mytab+']').parent().trigger('click');
		//setTimeout("$('li.tab').find('a[href*='+mytab+']').parent().trigger('click');",10);
      }
	});
	})( jQuery );
  setTimeout(function() {
    if (location.hash) {
      window.scrollTo(0, 0);
    }
  }, 1);
</script>
<script language="javascript">
        if (typeof(window.parent.parent.resizeFrame) == 'function')
        {
            var l = location.href;
            var p = l.indexOf('?height=');
            
            if (p == -1)
                p = l.indexOf('&height=');
                
            if (p != -1)
            {
                p += 8;
                var p2 = l.indexOf('&', p);
                if (p2 == -1)
                    p2 = l.length;
                    
                window.parent.parent.resizeFrame((l.substring(p, p2) - 0) + 'px', 
                    l.indexOf('scroll=0') == -1);
            }
        }
    </script>
<script type="text/javascript">
document.write(unescape("%3Cscript src='//munchkin.marketo.net/munchkin-beta.js' type='text/javascript'%3E%3C/script%3E"));
</script>
<script>Munchkin.init('509-WWS-087');</script>

