						form.onSuccess(function(values, followUpUrl){
							//set the cookie
							$.cookie('availity_download_pdf', 'yes', { expires: 365, path: '/' });
							
							//track clicks
							munchkinClickLinkProxy();
												
							//hide the light box
							myLightBox.hide();
							
							//clear the checkboxes - NOT NEEDED
							//$('.helper-link a').click();
							
							//download/go to pdf
							$('#download').click();
							
							//return false to prevent the submission handler from taking the lead to the follow up url.
							return false;
						});
					});
				} else {
					//track clicks
					munchkinClickLinkProxy();
					$('#download').click();
				}
			});
			
			function sortUsingNestedText(parent, childSelector, keySelector, dir) {
				var items = parent.children(childSelector).sort(function(a, b) {
					var vA = $(keySelector, a).text();
					var vB = $(keySelector, b).text();
					if (dir=="desc") {
						return (vA > vB) ? -1 : (vA < vB) ? 1 : 0;
					} else {
						// asc
						return (vA < vB) ? -1 : (vA > vB) ? 1 : 0;
						
					}
				});
				parent.append(items);
			}
			
			dir = 'asc';
			
			// Type
			$( ".item.header div.leftcol span" ).click(function(e) {
				$('.item.header span').removeClass('active');
				$(this).addClass('active');
				if ($(this).hasClass('asc')) {
					$(this).removeClass('asc');
					$(this).addClass('desc');
					dir = 'desc';
				} else {
					$(this).removeClass('desc');
					$(this).addClass('asc');
					dir = 'asc';
				};
				sortUsingNestedText($('.item-wrapper'), "div", "div.leftcol:not(header)", dir);
			});
			
			// Title
			$( ".item.header div.centercol span" ).click(function(e) {
				// Clear all the spans
				$('.item.header span').removeClass('active');
				
				$(this).addClass('active');
				if ($(this).hasClass('asc')) {
					$(this).removeClass('asc');
					$(this).addClass('desc');
					dir = 'desc';
				} else {
					$(this).removeClass('desc');
					$(this).addClass('asc');
					dir = 'asc';
				};
				
				sortUsingNestedText($('.item-wrapper'), "div", "div.centercol:not(header)", dir);
				
			});
			// Date
			$( ".item.header div.rightcol span" ).click(function(e) {
