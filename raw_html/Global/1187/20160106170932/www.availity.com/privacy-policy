				// Function to make munchkin clickLink call for multiple PDF selections
				// find out which checkboxes are checked
				function munchkinClickLinkProxy()
				{
					//var boxes = $('form').find('input[type=checkbox]:checked');	//use this version if less targeted
					var boxes = $('form').find('input[name="files[]"]:checked');
					$(boxes).each(function(){
						//console.log(this.value);
						Munchkin.munchkinFunction('clickLink', {href: this.value});
					});
				}
				
				e.preventDefault();
				
				// alert('Youre trying to download something');
				// check and make sure at least 1 item is selected
				if($('form').find('input[type=checkbox]:checked').length == 0)
				{
					alert('Please select at least one file to download.');
					return false;
				}
				
				// Check for presence of cookie
				$cookie = $.cookie('availity_download_pdf');
				//alert('Cookie: ' + $cookie);
				
				if (typeof $cookie === "undefined") {
					// alert('undefined');
					// No cookie, Initiate popup
