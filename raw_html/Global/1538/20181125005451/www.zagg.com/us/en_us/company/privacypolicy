<script type="text/javascript">
    var formId = 'newsletter-validate-detail';
    var myForm = new VarienForm(formId);
    var postUrl = "https://www.zagg.com/us/en_us/newsletter/subscriber/newAjax/";
    function doAjax() {
        if (myForm.validator.validate()) {
            $('loading-form').show();
            new Ajax.Request(
                postUrl,
                {
                    method:'post',
                    asynchronous:true,
                    evalScripts:false,
                    onComplete:function(transport) {
                        if (transport && transport.responseText){
                            try{
                                response = eval('(' + transport.responseText + ')');
                            }
                            catch (e) {
                                response = {};
                            }
                        }

                        if (response.success){
                            $('loading-form').hide();
                            $('newsletter-validate-detail').hide();
                            $('formSuccess').show();

                        } else {
                            $('loading-form').hide();
                            $('newsletter-validate-detail').hide();
                            $('formError').show();
                            jQuery("<p>" + response.message + "</p>").insertAfter('#formError h3');
                        }

                    },
                    parameters: $(formId).serialize(true)
                }
            );
        }
    }
    new Event.observe(formId, 'submit', function(e){
        dataLayer.push({'event' : 'newsletterFormSubmit'});
        e.stop();
        doAjax();
    });
</script>


<script type="text/javascript">
    jQuery(document).ready(function($) {
        $(".newsletter-button").fancybox({
            maxWidth    : 500,
            minHeight   : 260,
            fitToView   : true,
            width       : '100%',
            height      : 'auto',
            autoSize    : false,
            closeClick  : false,
            autoResize  : true,
            padding     : 0,
            // afterShow   : function() {
            //     doAjax();
            // },
            afterClose  : function() {
                $('#newsletter-validate-detail').show();
                $('#formSuccess').hide();
                $('#formError').hide();
                $('#formError p').remove();
            }
        }).on('click', function(){
            $('#newsletter-origin').val($(this).data('origin'));
            $('#newsletter').val($(this).parents('form').first().find('[name=email_1]').val());
        });

        $('button.close-newsletter-signup').on('click', function(){
            $.fancybox.close(true);
        });

        $('button.reset-newsletter-signup').on('click', function(){
            $('#newsletter-validate-detail').show();
            $('#formSuccess').hide();
            $('#formError').hide();
            $('#formError p').remove();
        });

        $('form.mailing-list-subscription').on('submit', function(e){
            e.preventDefault();

            $(".newsletter-button", this).trigger('click');
        });

        $('.mailing-list-subscription').on('change', function(e) {
            emailAddress = $('#newsletter_1').val();
            newsletter = $('#newsletter');
            newsletter.val(emailAddress);
        });
    });

</script>
        
