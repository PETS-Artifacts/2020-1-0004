<script type="text/javascript">
$(document).ready(function(){
	
	$("#loginSSW").on("click", function(){
		$("#requiredOne").remove();
		if($("#screenname").val()==""){
			$("#screenname").after('<small id="requiredOne" class="help-block" data-fv-validator="notEmpty" data-fv-for="_58_firstName" data-fv-result="INVALID" style="color:red;">This field is required.</small>');
		}else{
			var screenName = $("#screenname").val();
			$("#screenname").val("");
			window.open($("#sswUrl").val() + "\/?screenName="+ screenName ,'_blank');	
		}
	});
	
});
</script>


	
	

					
				
			
		
	
	



