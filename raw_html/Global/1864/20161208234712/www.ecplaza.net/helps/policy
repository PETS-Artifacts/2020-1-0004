            logger.config('endpoint', '/clientlog');
            logger.config('logsPerPage', '6');
            logger.config('id',  ECP.Util.getClientId());
            logger.enabled('info', 1),logger.enabled('warn', 1),logger.enabled('error', 1),logger.enabled('basic', 1),logger.enabled('uncaught', 1);
