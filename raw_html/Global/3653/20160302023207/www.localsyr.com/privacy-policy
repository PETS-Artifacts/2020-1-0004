<script>
(function($, A){
		
	$('.mobile-secondary-nav-container li.level3').each(function(idx, li) {
		var $li = $(li);
		var $subList = $li.children('.sub-nav-bar');
		var $input = $li.children('input:checkbox');
		var $aLink = $li.children('a[href]');
		var cls = 'checked';
		if($subList.length && $input.length && $aLink.length){
			$aLink.click(function(e) {
				e.preventDefault();
				var checked = $input.is(':'+cls);
				$input.attr(cls, !checked);
			});
		}
	});
	 })(jQuery, AUI());
</script>						
