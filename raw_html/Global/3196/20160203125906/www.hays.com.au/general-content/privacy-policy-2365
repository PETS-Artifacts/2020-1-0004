<script type = "text/javascript">
		function HAYSCookieBannerImageonLoad(){ 
		
			if	(window.jQuery && jQuery.ui && jQuery.isReady) 
					{
				
				var img_div = document.getElementById("pl_header");				
				
				if (document.getElementById("banner") != null){			
					
					var translationOffset=$(".lang_nav").offset();
					var createAccountOffset=$(".acc_nav_display").offset(); 
					
					if(translationOffset!=null)
					{						
						var bannerOffset=$("#banner").offset();
						var bannerWidthFromLeft=$("#banner").width()+bannerOffset.left;
						var widthDifference=translationOffset.left-bannerWidthFromLeft;				
						if(translationOffset.left<bannerWidthFromLeft)
						{
							var bannerFinalWidth=bannerWidthFromLeft-translationOffset.left;					
							bannerFinalWidth=$("#banner").width()-(bannerFinalWidth+14);					
							$('#banner').attr('style','width:'+bannerFinalWidth+'px;');
							
						}
					}
					else if(createAccountOffset!=null)
					{						
						var bannerOffset=$("#banner").offset();
						var bannerWidthFromLeft=$("#banner").width()+bannerOffset.left;
						var widthDifference=createAccountOffset.left-bannerWidthFromLeft;				
						if(createAccountOffset.left<bannerWidthFromLeft)
						{
							var bannerFinalWidth=bannerWidthFromLeft-createAccountOffset.left;					
							bannerFinalWidth=$("#banner").width()-(bannerFinalWidth+14);					
							$('#banner').attr('style','width:'+bannerFinalWidth+'px;');							
						}					
					}
					
					var bannerHeight=$('#banner').height();					
					var bannercode = document.getElementById("banner");
						if(bannercode.className=='banner-hidden'){
								img_div.className='container_24 NormalTopPadding';
						}						
						else{
						
								if (bannerHeight>18){
									img_div.className='container_24 increseTopPadding';
								}						
								else {
									img_div.className='container_24 NormalTopPadding';
								}
						}
					}
				else {
					img_div.className='container_24 NormalTopPadding';
				}
			}
				else 
					{
						window.setTimeout(HAYSCookieBannerImageonLoad, 100);
					}
				}
		HAYSCookieBannerImageonLoad();
		</script>
<script type="text/javascript">	
	function cookiebannerHide() {
		if (window.jQuery && jQuery.ui && jQuery.isReady) {
			$(document).ready(function() {
				if (navigator.cookieEnabled == false)  {
				//alert("Cookie disabled");
					$('#banner').attr('style','display:none;');			
				}
		});
		 }
       else {
	     window.setTimeout(cookiebannerHide, 100);
		}
     }
	 cookiebannerHide();		
</script>	
	

