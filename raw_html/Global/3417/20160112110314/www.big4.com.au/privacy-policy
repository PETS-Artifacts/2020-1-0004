<script>
B.LoadCssList = B.LoadCssList || {};
B.LoadedCss = B.LoadedCss || {};
B.LoadCssNow = function (src, media) {
	var L = document.createElement('link'); L.type = 'text/css';
	L.rel = 'stylesheet'; L.href = src; if (media) L.media = media;
	document.head.appendChild(L);
	B.LoadedCss[src] = media;
};
B.LoadCss = function (src, media) {
	B.LoadCssList[src] = media;
	if (typeof (window.matchMedia) == "function") {
		if (!media || (media && window.matchMedia(media).matches)) B.LoadCssNow(src, media);
	} else {
		B.R.push(['matchmedia',
			function () {
				if (!media || (media && window.matchMedia(media).matches)) B.LoadCssNow(src, media);
			}
		]);
	}
}
</script>
