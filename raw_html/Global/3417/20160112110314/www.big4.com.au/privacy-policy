	B.R.push(['jquery', function () {
		B.AdsSet = {};
		if (!!$.fn.dfp) $.dfp({
			dfpID: '65911804',
			sizeMapping: {
				'HeaderLogoMobile': [
					{ browser: [768, 0], ad_sizes: [] },
					{ browser: [0, 0], ad_sizes: [148, 33] }
				],
				'HeaderLogoBigger': [
					{ browser: [768, 0], ad_sizes: [148, 33] },
					{ browser: [0, 0], ad_sizes: [] }
				],
				'FooterBanner': [
					{ browser: [728, 0], ad_sizes: [728, 90] },
					{ browser: [468, 0], ad_sizes: [468, 60] },
					{ browser: [0, 0], ad_sizes: [320, 50] }
				]
			},
			collapseEmptyDivs: true,
			enableSingleRequest: true,
			refreshExisting: true,
			afterEachAdLoaded: function (adUnit) {
				var $this = $(adUnit),
					adId = $this.attr("data-adunit"),
					adSet = !($this.hasClass("display-none")),
					$containers = $this.parents("[data-adcontainer]")
				;
				B.AdsSet[adId] = adSet;
				$containers.each(function () {
					if ($(this).find(".adunit:not(.display-none)").length == 0) $(this).hide();
					else $(this).css("display","");
				});
				if (adId == "HeaderLogo1" || adId == "HeaderLogo2") {
					if (typeof B.AdsSet["HeaderLogo1"] == "boolean" && typeof B.AdsSet["HeaderLogo2"] == "boolean") {
						if (B.AdsSet["HeaderLogo1"] == false && B.AdsSet["HeaderLogo2"] == false) {
							$("html").removeClass("header-ads").addClass("no-header-ads");
						} else {
							$("html").removeClass("no-header-ads").addClass("header-ads");
						}
					}
				}
			}
		});
	}]);
</script>
