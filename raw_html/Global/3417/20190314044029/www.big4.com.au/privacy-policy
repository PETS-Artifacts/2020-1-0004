        <script>
            // Ask Google Analytics which variation to show the user.
            var chosenVariation = cxApi.chooseVariation();
            var linkVariations = [
                '/loyalty-club',
                '/joinbig4club',
                '/loyalty-club/join'
            ];

            var experimentCodes = ['a', 'b', 'c'];

            $(document).ready(function () {

                console.log('chosen variant is ' + linkVariations[chosenVariation]);

                var $elements = $("#megamenu-loyaltyclub-col1 a").eq(1);
                $elements = $elements.add($("#global-footer-group-2 a").first());
                $elements = $elements.add($("#become-a-member-link"));

                $elements.each(function () {
                    var e = $(this);
                    e.attr('href', linkVariations[chosenVariation] + '?exp=' + experimentCodes[chosenVariation]);
                    e.on('click', function() {
                        var expires = new Date();
                        expires.setTime(expires.getTime() + (1 * 24 * 60 * 60 * 1000));
                        document.cookie = 'link-experiment' + '=' + experimentCodes[chosenVariation] + ';expires=' + expires.toUTCString();

                        return true;
                    });
                });
            });
        </script>

