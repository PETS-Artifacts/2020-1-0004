	<script type="text/javascript">
		B.R.push([
			'jquery', function() {

				$('body').on("click", "a[name='shortlistbutton']", function (e) {
					var $addBtn = $(this),
						subdomain = $addBtn.attr('data-shortlist-subdomain'),
						shortlistType = $addBtn.attr('data-shortlist-type'),
						shortlisturl = '/shortlist';

					if (!$addBtn.hasClass("saved")) {
						if ($addBtn.hasClass("logged-in")) {
							var memberId = $addBtn.attr("data-member-number");
							var shortList = {
								SubDomain: subdomain
								, ID: memberId
								, Type: shortlistType
							};
							B.addToShortList(shortList, shortlisturl, false);
							$addBtn.addClass("saved");
						} else {
							$addBtn.magnificPopup({
								items: [{
									src: '#shortlist-signin-modal', // CSS selector of an element on page that should be used as a popup
									type: 'inline'
								}],
								showCloseBtn: true,
								callbacks: {
									elementParse: function (item) {
										var $modal = $(item.src),
											form = $modal.find('#Logon'),
											$trigger = $.magnificPopup.instance.st.mainEl;

										// Add parkid to Sign Up button
										var signUpUrl = '/loyalty-club/join/FreeAccount?redirectToShortlist=True' + "&shortlistParkId=" + $trigger.attr('data-shortlist-id');
										form.find(".singup-free-btn").attr("href", signUpUrl);

										// Unbind all old form submit events
										form.off("submit");
										form.submit(function (e) {
											// Show loading
											$(this).find(".btn-signin").addClass("hidden");
											$(this).find(".lbl.loading").removeClass("hidden");

											var d = $(this).serialize();
										    d = d + '&shortListParkId=' + $trigger.attr('data-shortlist-id');
										    console.log(d);
											e.preventDefault();
											$(".field", form).removeClass("error");
											$(".validation", form).removeClass("invalid").text("").hide();
											$(".field_buttons .loading", form).show();
											$("[type='submit']", form).attr('disabled', 'disabled');
											$.ajax({
												url: 'https://www.big4.com.au/loyalty-club/account/login',
												type: "POST",
												contentType: "application/x-www-form-urlencoded;charset=utf-8",
												data: d,
												error: function () {
													$(".field", form).addClass("error");
													$("[type='submit']", form).removeAttr('disabled');
													// Hide loading
													$(".btn-signin", form).removeClass("hidden");
													$(".lbl.loading", form).addClass("hidden");
												},
												success: function (data) {
													if (data.success) {
													    $('#error-member-lookup').hide();
														$(".field", form).removeClass("error");

														// Update Park Tile and add park to shortlist
														$("a[name='shortlistbutton']").addClass("logged-in");
														$("a[name='shortlistbutton']").attr("data-member-number", data.memberNumber);
														var shortlistmemberId = data.memberNumber;
														var shortlistshortList = {
															SubDomain: $trigger.attr('data-shortlist-subdomain')
															, ID: shortlistmemberId
															, Type: $trigger.attr('data-shortlist-type')
														};
														B.addToShortList(shortlistshortList, shortlisturl, true);
													    
														if (data.redirect_url != "") {
														    //location.reload();
														    window.location.replace(data.redirect_url);
														    //window.location.href = data.redirect_url;
														} else {
														    $addBtn.magnificPopup('close');
													    }
													} else {
														$(".field", form).addClass("error");
														$("#error-member-lookup p").html(data.Message[0].Message).show();
														$('#error-member-lookup, #error-member-lookup .msgbox').show();
														$("[type='submit']", form).removeAttr('disabled');
														// Hide loading
														$(".btn-signin", form).removeClass("hidden");
														$(".lbl.loading", form).addClass("hidden");
													}
												}
											});
										});
									}
								}
							});
							$addBtn.magnificPopup('open');
						}
					}
					e.preventDefault();
					return false;
				});

				$(".close-modal").on("click", function () {
					$.magnificPopup.close();
				});

			}
		]);
	</script>
