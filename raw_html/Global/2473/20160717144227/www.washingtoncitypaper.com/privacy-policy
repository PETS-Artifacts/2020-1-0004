    <!-- Chartbeat js include -->
<script type='text/javascript'>
    var chartbeatContext = PageManager.getActiveContext(),
        sections = getChartbeatSections(chartbeatContext);

    var _sf_async_config = _sf_async_config || {};
    /** CONFIGURATION START **/
    _sf_async_config.uid = 62058;
    _sf_async_config.domain = 'washingtoncitypaper.com';
    _sf_async_config.useCanonical = true;
    _sf_async_config.sections = sections.join(',');
    // _sf_async_config.authors = 'Change this to your Author name';
    /** CONFIGURATION END **/
    (function() {
        function loadChartbeat() {
            window._sf_endpt = (new Date()).getTime();
            var e = document.createElement('script');
            e.setAttribute('language', 'javascript');
            e.setAttribute('type', 'text/javascript');
            e.setAttribute('src', '//static.chartbeat.com/js/chartbeat.js');
            document.body.appendChild(e);
        }
        var oldonload = window.onload;
        window.onload = (typeof window.onload != 'function') ?
            loadChartbeat : function() {
                oldonload();
                loadChartbeat();
            };
    })();

    // If page context changes on scroll update chartbeat with the new page info.
    $(window).on('scroll', function() {
        if (chartbeatContext.uri != PageManager.getActiveContext().uri) {
            chartbeatContext = PageManager.getActiveContext();
            sections = getChartbeatSections(chartbeatContext);
            _sf_async_config.sections = sections.join(',');
            pSUPERFLY.virtualPage(chartbeatContext.uri, chartbeatContext.title);
        }
    });

    // Return a array of section based off the current pageContext
    function getChartbeatSections(chartbeatContext) {
        var sections = [];
        // Switch case to handle how to apply sections to the tracking
        switch (chartbeatContext.type) {
            case 'Content':
                 sections.push(chartbeatContext.section.name);
                 var parentSection = chartbeatContext.section.parent;
                 // loop over the parent section and add them to the list of sections.
                 while (parentSection != null && $.inArray(parentSection.name, sections) == -1 ) {
                    sections.push(parentSection.name);
                    parentSection = parentSection.parent;
                 }
                 break;
            case 'Section':
                sections.push(chartbeatContext.name);
                break;
            default:
                break;
        }
        return sections;
    }
</script>                        </body>
