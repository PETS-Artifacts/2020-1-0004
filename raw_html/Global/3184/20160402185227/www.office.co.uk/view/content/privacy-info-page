<script type="text/javascript">
    $(document).ready(function() {
        $(".disableOnSubmit").submit(function(){
            $(this).find("input[type = 'image'],input[type = 'submit']").click(function() {
                return false;
            });   
        });
        
        QLJS.setUpMultilevelNavigation();
        
        /*
        Function to make the vertical align CSS method more flexible. Find all vertical elements and set the parent height (which needs to be fixed) to a relative height.
        */
        var pageWidth = $('.page_container').width(); /*relative value*/
        var sumEQ = pageWidth / 16;
        var checkVertExists = $('.verticalCenter').length;
        if (checkVertExists > 0) {
            $('.verticalCenter').each(function(index){
                $(this).parent().css('height',sumEQ);
            })
        }
        var paginationElements = $('.x_y_num_navigation').length;
        if (paginationElements < 1) {
            $('.x_y_string').css('width','100%');
            $('.sortComponent').css('width','100%');
            $('.productList_return_to_top').css('width','100%');
        }
    });

    function formatForTranslation(element, text, attribute) {
    	text = text.replace('messageBundle_', '');
        var index = text.split('/');
        var value = index[0];
        var key = index[1];

        element.css('background','red').addClass('translate-tooltip').attr('data-translate', key);
        if (attribute == 'directText') {
        	element.text(value);
        } else {
        	element.attr(attribute, value);
        }
    }
</script>

