<script type="text/javascript">
	window.monetateQ = window.monetateQ || [];

	var path = location.pathname;
	var currentPageType = "unknown";
	if (path.indexOf("login") != -1) {
		window.monetateQ.push([ "setPageType", "login" ]);
	} else if (path.indexOf("my-account") != -1) {
		window.monetateQ.push([ "setPageType", "account" ]);
	} else if (path.indexOf("basket") != -1) {
        window.monetateQ.push([ "setPageType", "basket" ]);
    } else if (path.indexOf("checkout") != -1 && path.indexOf("orderConfirmation") == -1) {
		window.monetateQ.push([ "setPageType", "checkout" ]);
	} else if (path.indexOf("orderConfirmation") != -1) {
		window.monetateQ.push([ "setPageType", "purchase" ]);
	}

	
	    currentPageType = "CONTENT";
	
	window.monetateQ.push([ "setPageType", currentPageType.toLowerCase() ]);

	
	
	window.monetateQ.push([ "trackData" ]);

    function trackAddToCart_monetate(cartModification) {
    	window.monetateQ = window.monetateQ || [];
        var cartDetails = [];
        cartDetails.push({
            "productId" : cartModification.entry.product.code,
            "quantity" : cartModification.entry.quantity,
            "unitPrice" : cartModification.entry.basePrice.value,
            "currency" : cartModification.entry.basePrice.currencyIso
        });

        window.monetateQ.push([ "setPageType", currentPageType.toLowerCase() ]);
        window.monetateQ.push([ "addProductDetails", [ "" ] ]);
        window.monetateQ.push([ "addCategories", [ "" ] ]);
        window.monetateQ.push([ "addCartRows", cartDetails ]);
        window.monetateQ.push([ "trackData" ]);
    };
    
