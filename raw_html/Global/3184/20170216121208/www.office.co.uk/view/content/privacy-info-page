<script type="text/javascript">
$(document).ready(function() {
    QLJS.setUpMiniBasketAjax();
});

function removeItemFromBasket(entryNumbers) {
	if ($("#cartItemForm").length == 0) {	//	if current page is not basket page
	    $.ajax({
	    	type: "POST",
	        url: "/view/basket/remove",
	        data: "cartEntry=" + entryNumbers + "&componentUid=headerbasketComponent",
	        success: function(data) {
	        	$(".myBasket_removing_error").hide();
	        	
	        	//	TODO	notify GTM of removing order entry
	        	
	        	QLJS.setUpMiniBasketAjax(true);
	        },
	        error: function(data) {
	        	$(".myBasket_removing_error").show();
	        }
	    })
	} else {	//	if current page is basket page
		window.location.href = "/view/basket/remove?cartEntry=" + entryNumbers +
			 "&pageLabel=basket&componentUid=headerbasketComponent";
	}
    return false;
}

</script>
