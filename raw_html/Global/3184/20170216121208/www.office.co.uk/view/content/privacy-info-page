<script type="text/javascript">
    $(document).ready(function() {
        $.ajax({
            cache: false,
            url: "/view/resources/getUserInfo",
            success: function(userInfo) {
                QLJS.setUpGTMUserAjax(userInfo);
                if (userInfo.anonymous) {
					$(".myAccountHeader_inner_contents").html('<p>Login if you are an existing OFFICE customer or register to make ordering simple</p> <a class="btn btn-primary btn-fluid" href="/view/secured/content/myaccount">Login/Register</a>');
                }else{
                	$(".myAccountHeader_inner_contents").html('<a class="btn btn-primary btn-fluid" href="/j_spring_security_logout">Logout</a>');
                }
            }
        });
    });
</script>

