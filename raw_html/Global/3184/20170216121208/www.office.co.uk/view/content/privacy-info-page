$(document).ready(function() {
    if (QLJS.getCookie('akamaiWrongDomain')) {
        if (QLJS.getCookie('selectedDomain')) {
            if (QLJS.getCookie('selectedDomain') != document.location.hostname) {
                document.location = "http://" + QLJS.getCookie('selectedDomain');
            }
        } else {
            var _fbCloseImage = $.facebox.settings.closeImage;
            $.facebox.settings.closeImage = '/_ui/responsive/site-office/images/fb_close_circle.png';
            $.facebox({ div: '#countrySelection'}, 'countrySelection');
            $.facebox.settings.closeImage = _fbCloseImage;
            $('#facebox').css('top', (Math.floor(($(window).height() + $('#facebox').height()) / 5)));  
            $('#facebox').css('left', (Math.floor(($(window).width() - $('#facebox').width()) / 2)));
            $('.countrySelector .country a').click(function() {
                if (document.location.hostname == $(this).attr("href").replace(/^https?:\/\//,'')) {
                    $.facebox.close();
                    return false;
                } else {
                    QLJS.setCookie('akamaiWrongDomain', '', -1);
                    QLJS.setCookie('selectedDomain', $(this).attr("href").replace(/^https?:\/\//,''));
                }
            });
        }
    }
});
