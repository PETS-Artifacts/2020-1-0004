	<script>
		// Lower case search term before submitting
		$(document).ready(function () {
			QLJS.submitToLower('#searchform');
		})
	</script>





    <script>
	    $(function() {
	        $( "#searchBox" ).autocomplete({
	          source: function( request, response ) {
	            $.ajax({
	              url: "//content.lon5.atomz.com/autocomplete/sp10/04/e2/2c",
	              dataType: "jsonp",
	              data: {
	                query: request.term
	              },
	              success: function( data ) {
	                response( data.slice(0, 10) );
	              }
	            });
	          },
	          position: {
	              //offset: '0 10' // Shift 0px left, 5px down.
	        	  my: "left-1 top-1",
	          },
	          minLength: 3,
	          appendTo: "#autocomplete",
	          select: function( event, ui ) {
	        	  $(this).val(ui.item.value);
                  $('#searchform').submit();
	          }
	        });
	        $.ui.autocomplete.prototype._resizeMenu = function () {
	        	this.menu.element.outerWidth( 300 );
        	}
	      });
    </script>

			
				

