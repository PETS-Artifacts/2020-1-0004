        var quantity = localVars.currentProducts[i].quantity;
        var price = localVars.currentProducts[i].price;
        var name = localVars.currentProducts[i].name;
        var categoryName = localVars.currentProducts[i].categoryName; 
        ga('ecommerce:addItem', {
            'id': localVars.orderId,
            'name': name,
            'sku': productCode,
            'category': categoryName, 
            'price': price, 
            'quantity': quantity
        });
    }
