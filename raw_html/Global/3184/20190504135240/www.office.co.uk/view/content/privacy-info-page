<script type="text/javascript">
    String.prototype.toProperCase = function () {
        return this.replace(/\w\S*/g, function(txt){return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase();});
    };

    window.mediator.subscribe("trackAddToCart", function(data){
        var DYEvent = {
            name: "Add to Cart",
            properties : {
                dyType: "add-to-cart-v1",
                value: data.entry.basePrice.value,
                currency: data.entry.basePrice.currencyIso,
                productId: data.entry.product.code,
                quantity: 1,
                size: data.entry.product.code.substring(10, 13),
                cart: data.cartStatus
            }
        };
        DY.API("event", DYEvent);
        
    });

    window.mediator.subscribe("trackAddToWishlist", function(data){
        var DYEvent = {
            name: "Add to Wishlist",
            properties: {
                dyType: "add-to-wishlist-v1",
                productId: data.code,
                size: data.size
            }
        };
        DY.API("event", DYEvent);
        
    });

    window.mediator.subscribe("trackChangeSize", function(data){
        var base_product = window.dataLayer[0].productId;
        if (base_product) {
            var sku_id = data.length != 13 ? base_product + data : data;
            var DYEvent = {
                context: {
                    type: 'PRODUCT',
                    data: [sku_id]
                },
                url: window.location.href.replace(base_product, sku_id),
                countAsPageview: true
            };
            DY.API('spa', DYEvent);
        }
    });

    window.mediator.subscribe("trackFilterResults", function(data){
        var DYEvent = {
            name: "Filter Items",
            properties: {
                dyType: "filter-items-v1",
                filterType: data.facet.toProperCase()
            }
        };
        if (isNaN(data.value)){
            DYEvent.properties.filterStringValue = data.value;
        } else {
            DYEvent.properties.filterNumericValue = data.value;
        }
        DY.API("event", DYEvent);
        
    });

    window.mediator.subscribe("trackKeywordSearch", function(data){
       console.log(data);
    });

    window.mediator.subscribe("trackSortResults", function(data){
        var DYEvent = {
            name: "Sort Items",
            properties: {
                dyType: "sort-items-v1",
                sortBy: data.endsWith("_r") ? data.split("_r")[0] : data,
                sortOrder: data.endsWith("_r") ? "DESC" : "ASC"
            }
        };
        DY.API("event", DYEvent);
        
    });

    window.mediator.subscribe("trackEnterPromoCode", function(data){
        var DYEvent = {
            name: "Promo Code Entered",
            properties: {
                dyType: "enter-promo-code-v1",
                code: data
            }
        };
        DY.API("event", DYEvent);
    });

    window.mediator.subscribe("trackRemoveFromCart", function(data){
        /* Create a view of the cart AFTER this item has been removed */
        var entryNumbers = Array.isArray(data) ? data : JSON.parse(data);
        var items = [];
        var removed = {};
        for (var i in QLJS.miniCartData.items){
            var cartItem = QLJS.miniCartData.items[i];
            if (cartItem.entryNumbers != entryNumbers){
                items.push({
                    productId: cartItem.product.code,
                    quantity: cartItem.quantity,
                    itemPrice: cartItem.basePrice,
                    size: cartItem.product.ukSizeCode
                });
            } else {
                removed.value=cartItem.totalPrice;
                removed.productId = cartItem.product.code;
                removed.quantity = cartItem.quantity;
                removed.size = cartItem.product.ukSizeCode;
            }
        }
        /* Create the event */
        var DYEvent = {
            name: "Remove from Cart",
            properties: {
                dyType: "remove-from-cart-v1",
                value: removed.value,
                currency: QLJS.miniCartData.selectedCurrency,
                productId: removed.productId,
                quantity: removed.quantity,
                size: removed.size,
                cart: items
            }
        };
        DY.API("event", DYEvent);
        
    });

    window.mediator.subscribe("trackNewsletterSignup", function(data){
        sha256(data).then(function(hashed){
            var DYEvent = {
                name: "Newsletter Subscription",
                properties: {
                    dyType: "newsletter-subscription-v1",
                    hashedEmail: hashed
                }
            };
            DY.API("event", DYEvent);
            
        });
    });

    window.mediator.subscribe("syncCart", function(){
        QLJS.deleteCookie("triggerCartSync");
        var items = [];
        for (var i in QLJS.miniCartData.items){
            var cartItem = QLJS.miniCartData.items[i];
            items.push({
                productId: cartItem.product.code,
                quantity: cartItem.quantity,
                itemPrice: cartItem.basePrice
            });
        }

        var DYEvent = {
            name: "Sync cart",
            properties: {
                dyType: "sync-cart-v1",
                currency: QLJS.miniCartData.selectedCurrency,
                cart: items
            }
        };

        DY.API("event", DYEvent);

    });

    window.mediator.subscribe("trackLoginEvent", function(data){
        sha256(data).then(function (uidsha) {
            var loginEvent = {
                name: "Login",
                properties: {
                    dyType: "login-v1",
                    hashedEmail: uidsha
                }
            };
            DY.API("event", loginEvent);

            $.ajax({
                cache: false,
                url: "/view/component/basket/minibasketAjax",
                success: function (miniCartData) {
                    QLJS.miniCartData = miniCartData;
                    setTimeout(function () {
                        window.mediator.publish("syncCart");
                    }, 10);
                }
            });
        });
    });

    window.mediator.subscribe("trackSignupEvent", function(data){
        sha256(data).then(function (uidsha) {
            var signupEvent = {
                name: "Signup",
                properties: {
                    dyType: "signup-v1",
                    hashedEmail: uidsha
                }
            };

            DY.API("event", signupEvent);
        });
    });

    async function sha256(message) {
        // encode as UTF-8
        const msgBuffer = new TextEncoder('utf-8').encode(message);

        // hash the message
        const hashBuffer = await crypto.subtle.digest('SHA-256', msgBuffer);

        // convert ArrayBuffer to Array
        const hashArray = Array.from(new Uint8Array(hashBuffer));

        // convert bytes to hex string
        const hashHex = hashArray.map(b => ('00' + b.toString(16)).slice(-2)).join('');
        return hashHex;
    }

    window.mediator.subscribe("trackCartRecreation", function(miniCartData) {
        if (miniCartData.createdFromCookie == true) {
            var DYEvent = {
                name: "Sync cart",
                properties: {
                    dyType: "sync-cart-v1",
                    currency: miniCartData.currency, // Optional non-default currency used
                    cart: []
                }
            };
            for (var i in miniCartData.items) {
                var cartItem = {
                    productId: miniCartData.items[i].product.code,
                    quantity: miniCartData.items[i].quantity,
                    itemPrice: miniCartData.items[i].basePrice
                }
                DYEvent.properties.cart.push(cartItem);
            }
            DY.API("event", DYEvent);
        }
    });
</script>



