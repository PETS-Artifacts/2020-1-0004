<script type="text/javascript">
$.ajax({
    cache: false,
    url: "/view/resources/getUserInfo",
    success: function(userInfo) {
        QLJS.setUpGTMUserAjax(userInfo);
        var loginStatus =  userInfo.anonymousCustomer ? '<p>Login if you are an existing OFFICE customer or register to make ordering simple</p> <a class="btn btn-primary btn-fluid" href="/view/secured/content/myaccount">Login/Register</a>' : '<a class="btn btn-primary btn-fluid" href="/j_spring_security_logout">Logout</a>';
        document.body.querySelector('.myAccountHeader_inner_contents').innerHTML = loginStatus;

        //Mentionme logic
        if (document.body.querySelector('#mmWrapper') != null){
            var mmrefUrl = document.body.querySelector('#mmWrapper').getAttribute("data-mmrefurl");
            if (!userInfo.anonymousCustomer){
                mmrefUrl += "&firstname="+userInfo.firstname+"&surname="+userInfo.lastname+"&email="+userInfo.email;
            }
            var mmReferrerScript = document.createElement('script');
            mmReferrerScript.setAttribute('src',mmrefUrl);
            document.body.appendChild(mmReferrerScript);
        }

        if (!userInfo.anonymousCustomer){
            window.analytics = {}; window.analytics.userInfo = userInfo;
        }

        if (userInfo.newlyRegistered){
            window.mediator.publish("trackSignupEvent", window.analytics.userInfo.email);
        }

        if (userInfo.userLogin){
            window.mediator.publish("trackLoginEvent", window.analytics.userInfo.email);
        }
    }
});
</script>
