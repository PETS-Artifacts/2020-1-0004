            var adSlot = document.getElementById(event.slot.getSlotElementId());
            if (!event.isEmpty) {
                var adHeader = document.createElement('h4');
                adHeader.className = 'advert-heading';
                adHeader.innerText = 'Advertisement';
                if (!adSlot.classList.contains('advert-noheading')) {
                    adSlot.parentElement.insertBefore(adHeader, adSlot);
                }
