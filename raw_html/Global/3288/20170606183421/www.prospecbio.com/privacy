                    $(document).ready(function() {

                        $('#small-searchterms_2').autocomplete
                            ({
                                delay: 500,
                                minLength: 2,

                                source: '/catalog/searchtermautocomplete',
                                appendTo: '.search-box-top',
                                select: function(event, ui) {
                                    $("#small-searchterms_2").val(ui.item.label);
                                    setLocation(ui.item.producturl);
                                    return false;
                                }
                            })
                            .keydown(function(e) {
                                if (e.keyCode === 13) {
                                    $("#small-searchterms_2").alert("asdasd");
                                }
                            })
                            .data("ui-autocomplete")._renderItem = function(ul, item) {
                                var t = item.label;
                                //html encode
                                t = htmlEncode(t);
                                return $("<li></li>")
                                    .data("item.autocomplete", item)
                                    .append("<a><span>" + t + "</span></a>")
                                    .appendTo(ul);
                            };
                    });


