<script type="text/javascript">

function displaySubCat(catIndex, menuName)
{
    var prefix = "";
    if (menuName == "top_menu")
    {
        prefix = "TopSubCat";
    }
    else if (menuName == "side_menu")
    {
        prefix = "SideSubCat";
    }
    else
    {
        prefix = "SubCat";
    }
    
    showHideElement(prefix + catIndex.toString());
}

</script>

