<script type="text/javascript">
    if (self !== top) {
        hideEverything(); // adds CSS to hide body
        top.postMessage("unblock-request", "*");
    }

    
    function hideEverything() {
        var css = "body { display: none; }";
        var style = document.createElement("style");
        style.type = "text/css";
        style.id = "antiClickjack";
        if (style.stylesheet) {
            style.stylesheet.cssText = css;
        } else {
            style.appendChild(document.createTextNode(css));
        }

        document.querySelector("head").appendChild(style);
    }

    function receiveMessage(event) {
        if (! isFromUs(event.origin)) return;

        if (event.data === "unblock-request") {
            event.source.postMessage("unblock", event.origin);
        } else if (event.data === "unblock") {
            showEverything(); // removes CSS added by hideEverything();
        }
    }

    // The "origin" on a postMessage payload will not contain the full URL.
    // We can only check if the message came from the right origin.
    function getOrigin(url) {
        var arr = url.split("/");
        return arr[0] + "//" + arr[2];
    }

    function isFromUs(origin) {
        var pattern = /^[^.]*\.donorschoose\.(org|net)$/;
        // only accept messages from donorschoose
        return pattern.test(origin) ||
                origin === getOrigin("https://storage.donorschoose.net/") ||
                origin === getOrigin("https://storage.donorschoose.net/") ||
                origin === getOrigin("https://cdn.donorschoose.net/") ||
                origin === getOrigin("https://cdn.donorschoose.net/");
    }

    function showEverything() {
        var antiClickjack = document.getElementById("antiClickjack");
        if (antiClickjack !== null) {
            antiClickjack.parentNode.removeChild(antiClickjack);
        }
    }

    if (window.addEventListener) {
        window.addEventListener("message", receiveMessage, false);
    } else if (window.attachEvent) {
        window.attachEvent("onmessage", receiveMessage);
    }
</script>


