    <script type="text/javascript">
        function fixUrlPrefix() {
            var UrlPrefix = '/';
            var elema = $('a');
            if (elema.length > 0) {
                var protocol = window.location.protocol;
                var host = window.location.host;
                var teststring = '/~/';
                var testindex = -1;
                //alert('Number of anchor elements: ' + elema.length);
                var iter = elema.length;
                for (var i = 0; i < iter; i++) {
                    if (elema[i]) {
                        if (elema[i].href) {
                            testindex = elema[i].href.indexOf(teststring)
                            if (testindex > 0) {
                                var newpath = elema[i].href.substr(testindex + 3);
                                var newUrl = protocol + "//" + host + UrlPrefix + newpath;
                                //alert('i: ' + i + 'old Url: ' + elema[i].href + ', new Url: ' + newUrl);
                                elema[i].href = newUrl;
                            }
                        }
                    }
                }
            }

            elemi = $('img');
            if (elemi.length > 0) {
                var testindex = -1;
                //alert('Number of image elements: ' + elemi.length);
                iter = elemi.length;
                for (var i = 0; i < iter; i++) {
                    if (elemi[i]) {
                        if (elemi[i].src) {
                            testindex = elemi[i].src.indexOf(teststring)
                            if (testindex > 0) {
                                var newpath = elemi[i].src.substr(testindex + 3);
                                var newUrl = protocol + "//" + host + UrlPrefix + newpath;
                                //alert('i: ' + i + 'old Url: ' + elemi[i].src + ', new Url: ' + newUrl);
                                elemi[i].src = newUrl;
                            }
                        }
                    }
                }
            }

            elemi = $('form');
            //alert('form elemi.length: ' + elemi.length);
            if (elemi.length == 0) return;
            var testindex = -1;
            //alert('Number of form elements: ' + elemi.length);
            iter = elemi.length;
            for (var i = 0; i < iter; i++) {
                if (elemi[i]) {
                    if (elemi[i].action) {
                        testindex = elemi[i].action.indexOf(teststring)
                        if (testindex > 0) {
                            var newpath = elemi[i].action.substr(testindex + 3);
                            var newUrl = protocol + "//" + host + UrlPrefix + newpath;
                            //alert('i: ' + i + 'old Url: ' + elemi[i].action + ', new Url: ' + newUrl);
                            elemi[i].action = newUrl;
                        }
                    }
                }
            }
        }

        $(document).ready(function () {
            fixUrlPrefix();
        });
    </script>
