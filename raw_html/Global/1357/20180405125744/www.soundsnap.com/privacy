				$j("#edit-keys-ojoo").on('keyup',function(){
					$j(this).removeClass("invalid");
				});
				$j("#ojoo_search_audio_only").submit(function(){
					var isFormValid = true;
					if ($j.trim($j("#edit-keys-ojoo").val()).length == 0){
						$j("#edit-keys-ojoo").addClass("invalid");
						isFormValid = false;
					} else{
						$j("#edit-keys-ojoo").removeClass("invalid");
						isFormValid = true;
					}
					return isFormValid;
					});
