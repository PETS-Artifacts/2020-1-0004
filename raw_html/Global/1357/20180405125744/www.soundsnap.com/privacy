	function createCookie(name,value,days) {
		if (days) {
			var date = new Date();
			date.setTime(date.getTime()+(days*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
		}
		else var expires = "";
		document.cookie = name+"="+value+expires+"; path=/";
	}
	
	function readCookie(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return null;
	}
	
	function findHeight(element){
		var h=0; 
		var h_desc; 
		$j(element).each(function () {
			$this = $j(this);
			if ( $this.height() > h ) {
				h_desc=this;
				h=$this.height();
			}
		}); 
		$j(element).height(h);
	} 

    $j(document).ready(function() {

		var $window = $j(window);
		var w_height=$window.height();
		
				//fix footer to bottom#maincontent
		var content_height=$j("#container").height();
		
		if(content_height<w_height){
			var newheight=w_height-$j("#footer").height()-92;
			$j("#maincontent").css({'min-height':newheight});
		}
		
				//faq collapses
		$j('.ojoo-answer').hide();
		$j('.collapsible').click(function() {			
			if($j(this).hasClass('collapsed')) { 	
				$j(this).children('.ojoo-answer').fadeOut('slow', function() {
					$j(this).hide();
				});
			} else {
				$j(this).children('.ojoo-answer').fadeIn('slow', function() {
				});
			}
		}); 
		//audio teasers
		$j('.more-info').click(function(e) {	
			e.preventDefault();	
			var row=$j(this).closest('div.audio-row').next('.audio-info');
			row.slideToggle('300');
			$j(this).toggleClass('active');
			
			$j(this).text(function(i, text){
          		return text = ($j(this).hasClass("active")) ? "- less info" : "+ more info";
      		})
		});
		//search		
				$j('#search-icon').click(function(e) {
			e.preventDefault();	
			$j('#search').slideToggle('300');
			if(!$j('#search').hasClass('active')){ //not active by default landing
				$j(this).toggleClass('active');
			}
		}); 
		$j('.search-close').click(function(e) {
			$j('#search').slideUp('300');
			$j('#search-icon').removeClass('active');
		});
		
		var search_div=$j(".ojoo-wrapper #search").html();
		$j(".ojoo-wrapper #search").remove();
		$j(".browse-header").after("<div id='search'>"+search_div+"</div>");
		
				
		//mobile menu
		$j('.responsive-menu-button').click(function() {
			if($j('nav').is(":visible")) $j("#container").animate({ left: "0"},100);
			else $j("#container").animate({ left: "-180px"},100);
			$j('nav').stop().animate({ width: "toggle"},100);
			//$j("#container").toggleClass('responsive-left');
		});
		
		$window.resize(function() {
			if($j(this).width()>768 ){
				if($j("#container").attr('style')) $j("#container").removeAttr('style');
				if($j("nav").attr('style')) $j("nav").removeAttr('style');
			}
		});
		
		$j('.menu-close').click(function(e) {
			$j('nav').animate({ width: "toggle"},100); 
			$j("#container").stop().animate({ left: "0"},100);
			//$j('#container').removeClass('responsive-left');
		});
		
		//player stuck
		var player_header=$j(".jp-type-playlist").offset();
		if(player_header){
			$window.scroll(function(){
				if($j(this).scrollTop()>=player_header.top) {
					$j(".jp-type-playlist").css({'position':'fixed','top':0});
				}
				else {
					$j(".jp-type-playlist").css({'position':'absolute','top':'auto'});
				}
			});
		}
		
		$j('.product_bubble_link').hover(
			function() {
				var product=$j(this).attr('id');
				$j("."+product+".product_bubble").fadeIn( "fast" );
			}, function() {
				var product=$j(this).attr('id');
				$j("."+product+".product_bubble").fadeOut( "fast" );
			}
		);
		
		var home_message=$j(".messages").html();
		if(home_message){
			$j('.messages').html('<div class="message-container">'+home_message+'</div>');
		}
		
				
			

    });
	
	function change_select(current){
		$j('.tabs.secondary li.display_receipts').hide();
		$j('.tabs.secondary li.display_receipts[data-year="' + current + '"]').show();
	};
</script>


  
