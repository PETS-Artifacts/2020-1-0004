<script type="text/javascript">

$j(document).ready(function() {
    $j.reject({  
        reject: { 
			all: false, 
            safari: 5, // Apple Safari  
			safari4: false,
			firefox:20,//20
            chrome: 27, // Google Chrome  
            msie: 8, // Microsoft Internet Explorer  
			msie9: true,
			msie10: false,  
            opera: true, // Opera 
			android:false 
		},
		closeCookie: true,
		browserInfo: {  
			opera: { 
				text: '', 
				allow: { all: false}, //hide this. all others are by default
			},
			firefox:{
				text: 'firefox',
			},
			chrome:{
				text: 'chrome',
			},
			safari:{
				text: 'safari',
			},
			msie:{
				text: 'msie',
			}
    	},  
		header: 'Sorry but your browser is not supported',  
		paragraph1: '',
		paragraph2: 'Please select your browser to update it:' ,
		closeMessage: '',  
		imagePath: '/themes/soundsnap3/assets/', 
    }); 
    return false;  
}); 

	function createCookie(name,value,days) {
		if (days) {
			var date = new Date();
			date.setTime(date.getTime()+(days*24*60*60*1000));
			var expires = "; expires="+date.toGMTString();
		}
		else var expires = "";
		document.cookie = name+"="+value+expires+"; path=/";
	}
	
	function readCookie(name) {
		var nameEQ = name + "=";
		var ca = document.cookie.split(';');
		for(var i=0;i < ca.length;i++) {
			var c = ca[i];
			while (c.charAt(0)==' ') c = c.substring(1,c.length);
			if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
		}
		return null;
	}
	
	function findHeight(element){
		var h=0; 
		var h_desc; 
		$j(element).each(function () {
			$this = $j(this);
			if ( $this.height() > h ) {
				h_desc=this;
				h=$this.height();
			}
		}); 
		$j(element).height(h);
	} 

    $j(document).ready(function() {

		var $window = $j(window);
		var w_height=$window.height();
		
		//fix footer to bottom#maincontent
		var content_height=$j("#container").height();
		
		if(content_height<w_height){
			var newheight=w_height-$j("#footer").height()-92;
			$j("#maincontent").css({'min-height':newheight});
		}
		
		//faq collapses
		$j('.ojoo-answer').hide();
		$j('.collapsible').click(function() {			
			if($j(this).hasClass('collapsed')) { 	
				$j(this).children('.ojoo-answer').fadeOut('slow', function() {
					$j(this).hide();
				});
			} else {
				$j(this).children('.ojoo-answer').fadeIn('slow', function() {
				});
			}
		}); 
		//audio teasers
		$j('.more-info').click(function(e) {	
			e.preventDefault();	
			var row=$j(this).closest('div.audio-row').next('.audio-info');
			row.slideToggle('300');
			$j(this).toggleClass('active');
			
			$j(this).text(function(i, text){
          		return text = ($j(this).hasClass("active")) ? "- less info" : "+ more info";
      		})
		});
		
		$window.resize(function() {
			if($j(this).width()>768 ){
				$j(".logo, nav, .responsive-menu-button, .search, .responsive-search-button").removeAttr('style');
			}
		});
		
		function switch_sticky(status){
			if (status==='dark'){
				$j("#header-wrapper").removeClass('sticky-header').addClass('dark-wrapper');	
				$j(".logo img").attr('src','/themes/soundsnap3/assets/svg/logo-white.svg');
			} else {
				$j("#header-wrapper").addClass('sticky-header').removeClass('dark-wrapper');
				$j(".logo img").attr('src','/themes/soundsnap3/assets/svg/logo.svg');
			}
		}
		
		$j('.responsive-menu-button').click(function() {
			$j('nav').stop().animate({ width: "toggle"},100);
		});	
		
		$j('.menu-close').click(function(e) {
			$j('nav').animate({ width: "toggle"},100); 
		});
		
		$j('.responsive-search-button').click(function() {
			$j(".search").toggle();
			$j(this).toggleClass('search-close');
			var player_header=$j(".jp-type-playlist").offset();
			if(player_header){
				if( $j(".search").is( ":visible" ) )
					$j(".jp-type-playlist").addClass("shove");
				else
					$j(".jp-type-playlist").removeClass("shove");
			}
					});		
		
		//sticky menu
		var menu_header=$j(".maincontent").offset();
		
		if(menu_header){
			
			if($window.scrollTop()>=72) {
								}
			
			$window.scroll(function(){
				if($j(this).scrollTop()>=72) {
									}
			});
		}
		
		$j('.product_bubble_link').hover(
			function() {
				var product=$j(this).attr('id');
				$j("."+product+".product_bubble").fadeIn( "fast" );
			}, function() {
				var product=$j(this).attr('id');
				$j("."+product+".product_bubble").fadeOut( "fast" );
			}
		);
		
				
			

    });
	
	function change_select(current){
		$j('.tabs.secondary li.display_receipts').hide();
		$j('.tabs.secondary li.display_receipts[data-year="' + current + '"]').show();
	};
</script>


  
