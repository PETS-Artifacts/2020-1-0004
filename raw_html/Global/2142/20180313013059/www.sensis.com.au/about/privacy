            var $sasbu = {
                placeholder: ".sas-body-header",
                onShowCallback: function (browser) {
                    var buHeight = $("#sas-bu").outerHeight();
                    var bodyPaddingTop = $("body").css("padding-top");
                    $("body").css("padding-top", parseInt(bodyPaddingTop) + buHeight);
                },
                onHideCallback: function (b) {
                    var buHeight = $("#sas-bu").outerHeight();
                    var bodyPaddingTop = $("body").css("padding-top");
                    $("body").css("padding-top", parseInt(bodyPaddingTop) - buHeight);
                }
            };

            // Document Ready
            $(function () {
                SAS.BU.checkBrowser();
            });
