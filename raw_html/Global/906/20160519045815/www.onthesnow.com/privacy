    }
    else{ 
      var wallpaperison = "no"; 
    } 


    slots.push( googletag.defineSlot('/7730/OTS_COM/other', [[728, 90], [2, 1], [1600, 1000], [970, 66], [970, 250], [800, 250], [980, 240], [980, 150]], 'adpos_Top')
      .addService(googletag.pubads())
        .setTargeting("p",["Top"])
          .setTargeting("wpon",wallpaperison) );

    slots.push( googletag.defineSlot('/7730/OTS_COM/other', [[160, 600], [240, 600], [300, 600], [4, 1]], 'adpos_Skyscraper')
      .addService(googletag.pubads())
        .setTargeting("p",["Sky"]) );

