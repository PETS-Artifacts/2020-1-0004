  if($.browser.msie) {
    $(".mark").each(function() {
      var parentWidth = $(this).parent().css('width');
      parentWidth = parseInt(parentWidth.replace('px',''),10);
      var parentPaddingRight = $(this).parent().css('padding-right');
      parentPaddingRight = parseInt(parentPaddingRight.replace('px',''),10);
      var parentPaddingLeft = $(this).parent().css('padding-left');
      parentPaddingLeft = parseInt(parentPaddingLeft.replace('px',''),10);
      var newWidth = parentWidth + parentPaddingRight + parentPaddingLeft + 'px';
      $(this).css('width',newWidth);
    });
  }
