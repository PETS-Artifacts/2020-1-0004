{"bidder":"rubicon","params":{"accountId":"11424","siteId":"32214","zoneId":"130712","sizes":[2]}},
{"bidder":"appnexus","params":{"placementId":"10093356"}},
{"bidder":"triplelift","params":{"inventoryCode":"onthesnow_leaderboard"}},
{"bidder":"sovrn","params":{"tagid":"431686"}},
        ]
