<script type="text/javascript">
$(function() {
  $("img.lazy").lazyload({
    threshold : 400
  });
  $("div.lazy").lazyload({
    appear : function() {
      var $self = $(this);
      var adPos = $self.attr("data-adpos");
      if(adPos) {
        if( adPos == "adpos_Top" || adPos == "adpos_Right1" || adPos == "adpos_Skyscraper") {
          return;
        }
        forceRefreashAdPos(adPos);
        recordEventValue('LazyAdLoad', 'Load_'+adPos, 'OnTheSnow', 1, true);
      }
    }
  });
});
</script>


<!--this stops the flash ads from burning through the nav-->
<script language="javascript">
$(window).load(function(){
  OTS.dfp.makeAds();
  OTS.Search.init();
  // add onSeek handler for every scroller on the page.
  $(".scrollable, .weather_data_scroll").each( function() {
    var api = $(this).data("scrollable");
    if( api == null) return;
    api.firstScroll = true;
    api.onSeek( function() {
      if(! this.firstScroll ) OTS.dfp.reload();
      this.firstScroll = false;
    });
  });
});
FlashHeed.heed();

</script>





