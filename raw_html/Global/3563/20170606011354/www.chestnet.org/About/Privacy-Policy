<script type="text/javascript">

    //This part can`t be changed
    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];
    (function () {
        var gads = document.createElement('script');
        gads.async = true; gads.type = 'text/javascript';
        var useSSL = 'https:' == document.location.protocol;
        gads.src = (useSSL ? 'https:' : 'http:') + '//www.googletagservices.com/tag/js/gpt.js';
        var node = document.getElementsByTagName('script')[0];
        node.parentNode.insertBefore(gads, node);
    })();
    //end

    googletag.cmd.push(function () {

        //   Custom Targeting Format
        //   googletag.pubads().setTargeting('{key}', '{value}') ;  

        //   {key} = ChestZone
        //   Paid Ads: {value} = chestCommercial
        //   House Ads: {value} = chestHouse
        //   HomePage RoadBlock: {value} = chestHomePage

        googletag.pubads().setTargeting('ChestZone', 'chestCommercial');  // zone {value} will be pulled dynamically.

        //Following code creates the banner to display on body section for 160x600
        googletag.defineSlot('/30017927/ChestNET/Chest_Adslot_160x600', [160, 600], 'chestAdslot-0').addService(googletag.pubads());
        // googletag.defineSlot('/{network_id}/{Ad_Unit_Name}', {[banner size]}, '{Ad _Display_Name}').addService(googletag.pubads());

        // Following code creates the banner to display on body section for 728x90
        googletag.defineSlot('/30017927/ChestNET/Chest_Adslot_728x90', [728, 90], 'chestAdslot-1').addService(googletag.pubads());
        // googletag.defineSlot('/{network_id}/{Ad_Unit_Name}', {[banner size]}, '{Ad _Display_Name}').addService(googletag.pubads());

        // Following code creates the banner to display on body section for 300x250
        googletag.defineSlot('/30017927/ChestNET/Chest_Adslot_300x250', [300, 250], 'chestAdslot-2').addService(googletag.pubads());

        // Following code creates the banner to display on body section for 320x50 (mobile)
        googletag.defineSlot('/30017927/ChestNET/Chest_Adslot_320x50', [320, 50], 'chestAdslot-3').addService(googletag.pubads());

        // This Part can`t be changed
        googletag.pubads().enableSingleRequest();
        googletag.enableServices();
    });
    //end

</script>

