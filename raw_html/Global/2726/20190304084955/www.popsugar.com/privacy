<script type="text/javascript">
(function (id) {
function append(scriptid, url, async) {
var d = document, sn = 'script', f = d.getElementsByTagName(sn)[0];
if (!f) f = d.head;
var s = d.createElement(sn);
s.async = true;
s.id = scriptid;
s.src = url;
s.onerror = renderAdBlockWarning;
f.parentNode.insertBefore(s, f);
}
function getRootDomain() {
var parts = window.location.hostname.split('.');
if (parts.length === 2) rootDomain = parts[0];
else if (parts.length > 2) {
// see if the next to last value is a common tld
var part = parts[parts.length - 2];
if (part === 'com' || part === 'co') {
rootDomain = parts[parts.length - 3]; // go back one more
}
else {
rootDomain = part;
}
}
return rootDomain;
}
function renderAdBlockWarning() {
if (warningThrown) return;
warningThrown = true;
var outerContainer = document.createElement('div');
var xmlhttp = new XMLHttpRequest();
xmlhttp.onreadystatechange = function() {
if (xmlhttp.readyState == XMLHttpRequest.DONE) {   // XMLHttpRequest.DONE == 4
if (xmlhttp.status == 200) {
outerContainer.innerHTML = xmlhttp.responseText;
}
}
};
xmlhttp.open("GET", "/gdpr-failed", true);
xmlhttp.send();
document.body.appendChild(outerContainer);
}
window.evidon = {};
window.evidon.id = id;
var cdn = '//c.evidon.com/', rootDomain = getRootDomain(), noticecdn = cdn + 'sitenotice/';
var warningThrown = false;
append('evidon-notice', noticecdn + 'evidon-sitenotice-tag.js', false);
append('evidon-location', cdn + 'geo/country.js', true);
append('evidon-themes', noticecdn + id + '/snthemes.js', true);
if (rootDomain) append('evidon-settings', noticecdn + id + '/' + rootDomain + '/settings.js', true);
window.evidon.priorConsentCallback = function () {
function getUrlParameter(name) {
name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
var results = regex.exec(location.search);
return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
};
if (location.search.indexOf("amp=1") != -1) {
window.parent.postMessage({
type: 'consent-response',
action: 'accept'
}, '*');
}
else {
var destination = getUrlParameter('destination');
var popsugarHostName = "www.popsugar.co.uk";
if (destination) {
var getLocation = function(href) {
var l = document.createElement("a");
l.href = href;
return l;
};
var l = getLocation(destination);
if (l.hostname == popsugarHostName) {
window.location.href = "https://" + popsugarHostName + l.pathname + l.search + l.hash;
}
else {
window.location.href = "https://" + popsugarHostName;
}
}
else if (window.location.pathname === '/gdpr-consent') {
window.location.href = "https://" + popsugarHostName;
}
}
}
})(4134);
</script>
