    <script>
    (function() {
        var importantNoticeElem = document.getElementById('important-notice');
        var button = importantNoticeElem.querySelector('button');

        function getCookie(name) {
            var value = '; ' + document.cookie;
            var parts = value.split('; ' + name + '=');
            if (parts.length >= 2) {
                return parts.pop().split(';').shift();
            }
        }

        if (!getCookie('gdprNotification')) {
            importantNoticeElem.removeAttribute('hidden');
        }

        button.onclick = function() {
            var expirationDate = new Date();
            expirationDate.setTime(expirationDate.getTime() + (10*365*24*60*60*1000));
            document.cookie = 'gdprNotification=1;path=/;expires=' + expirationDate.toUTCString();

            importantNoticeElem.setAttribute('hidden', 'hidden');
        }
    })();
    </script>
</div>

