    $create(ExtenderBehaviors.OpenErrorValidationBehavior, {"errorMessage":null,"invalidCssClass":null,"isDependingOnInnerValidatedControl":false,"isPostBack":false,"isSuggestion":false,"openOnInitialLoad":false,"selfManagedErrorBox":false,"targetErrorBox":"tbLoginId_validationErrorBox","validCssClass":null}, null, null, $get("tbLoginId_textboxValidityValidator"));
});
Sys.Application.add_init(function() {
    $create(ExtenderBehaviors.CloseErrorValidationBehaviour, {"invalidCssClass":"s-hiddenImportant","targetErrorBox":"tbLoginId_validationErrorBox"}, null, null, $get("tbLoginId_txtBox"));
});
