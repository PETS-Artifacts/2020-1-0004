    $create(ExtenderBehaviors.CheckValidationBehavior, {"invalidCssClass":"i-checkMarkGray14x14","targetControl":"tbLoginId_validationCheck","validCssClass":"i-checkMark14x14"}, null, null, $get("tbLoginId_txtBox"));
});

