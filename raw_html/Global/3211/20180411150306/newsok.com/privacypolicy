  function refreshBid(adDivId, adSlot) {
    pbjs.que.push(function () {
      pbjs.requestBids({
        timeout: PREBID_TIMEOUT,
        adUnitCodes: [adDivId],
        bidsBackHandler: function () {
          pbjs.setTargetingForGPTAsync([adDivId]);
          googletag.pubads().refresh([adSlot]);
        }
      });
    });
  }
</script>

