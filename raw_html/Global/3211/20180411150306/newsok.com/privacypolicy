            <script>
                var hideSub;
                var showSub;
                var thisSub = "home_main";
                var mouseover = false;
                
                $(document).ready(function() {
                    $('.openSubNav').mouseenter(function(){
                        mouseover = true;
                    }).mouseleave(function(){
                        mouseover = false;
                    });

                    $('.openSubNav').mouseover(function() {
                        if ($('.subSubnavWrapper').css('height') == '65px') {
                            $('.subSubnav').hide();
                            $('#subSubnav_'+this.id).show();
                        } else {
                            var navId = this.id;
                            setTimeout(function () {
                                if (mouseover == true) {
                                    $('.subSubnav').hide();

                                    $('.subSubnavWrapper').css('height', '65px');
                                    $('#subSubnav_'+navId).show();
                                }
                            }, 500);
                            setTimeout("$('.subSubnavWrapper').css('overflow', 'visible');", 1500);
                        }
                    });

                    $('#subNav').bind("mouseleave",function() {
                        hideSub=setTimeout("hideSubNav()", 1000);
                        showSub=setTimeout("$('#subSubnav_'+thisSub).show()", 1000);
                    });
                    $('#subNav').bind("mouseenter",function() {
                        clearTimeout(hideSub);
                        clearTimeout(showSub);
                    });

                    // search box
                    var searchImg = $('.search-icon img').attr('src');
                    $('.search-icon').click(function(e) {
                        e.preventDefault();
                        if ($(this).find('img').attr('src') == searchImg) {
                            $(this).toggleClass('active').find('img').attr('src', '//s3.amazonaws.com/content.newsok.com/newsok/refactor/icon_search_close.png').parent().next().toggleClass('hide');
                        } else {
                            $(this).toggleClass('active').find('img').attr('src', searchImg).parent().next().toggleClass('hide');
                        }
                    });
                });
                function hideSubNav() {
                    if(document.getElementById('subSubnav_'+thisSub) == null) {
                        $('.subSubnavWrapper').css('overflow', 'hidden');
                        $('#mainBody').removeClass('subnav');
                        $('.subSubnavWrapper').css('height', '0px');
                    }
                }
                if (document.getElementById('subSubnav_'+thisSub) != null) {
                    $('.subSubnav').css('height', '0px');
                    $('.subSubnavWrapper').css('height', '65px');
                    $('#subSubnav_'+thisSub).css('height', '65px');
                    $('#mainBody').addClass('subnav');
                }
                
            </script>
            <script>
var timeout;

$('.popovers').popover({
    html: true,
    trigger: 'manual',
    container: $(this).attr('id'),
    animation: false,
    placement: 'right'
}).on("mouseenter", function () {
    clearTimeout(timeout);
    var _this = this;
    $(this).popover("show");
    $(this).siblings(".popover").on("mouseleave", function () {
        $(_this).popover('hide');
    });
}).on("mouseleave", function () {
    var _this = this;
    timeout = setTimeout(function () {
        if (!$(".popover:hover").length) {
            $(_this).popover("hide")
        }
    }, 100);
});

</script>


