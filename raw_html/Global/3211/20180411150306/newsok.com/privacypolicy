            <script>
                $(document).ready(function() {
                    $('[data-href][data-toggle=modal]').click(function() {
                        var theHref = $(this).attr('data-href');
                        var bannerZindex = $('.banner-wrapper').css('z-index'), subNavZindex = $('#subNav').css('z-index'), galleryZindex = $('.galleryZ').css('z-index'), newsokNavZindex = $('.newsokNav').css('z-index');
                    	$('#NewsOKUploadModal').on('shown.bs.modal', function(e) {
                    		$(this).before($('.modal-backdrop'));
                    		$('.banner-wrapper, #subNav, .galleryZ, .newsokNav').css('z-index', 1);
                            $(this).find('a[data-action=continue]').attr('href', theHref);
                    	}).on('hidden.bs.modal', function (e) {
                    		$('.banner-wrapper').css('z-index', bannerZindex);
                    		$('#subNav').css('z-index', subNavZindex);
                    		$('.galleryZ').css('z-index', galleryZindex);
                    		$('.newsokNav').css('z-index', newsokNavZindex);
                    	});
                    })
                });
            </script>

        
