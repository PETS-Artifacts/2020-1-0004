// Compare segments
function compareSegments(a, b, c){
  if (a.length == 0 || b.length == 0 || c.length > 0) return false;
  a.sort();
  b.sort();
  if (a === b) return true;
  if (a == null || b == null) return false;
  if (a.length != b.length) return false;
  for (var i = 0; i < a.length; ++i) {
    if (a[i] !== b[i]) return false;
  }
  return true;
}

