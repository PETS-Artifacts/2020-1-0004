<script type="text/javascript">
    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];
    googletag.cmd.push(function() {
       googletag.pubads().setTargeting("pubState", "OK");
        googletag.pubads().setTargeting("pgtype", "section");
        googletag.pubads().setTargeting("platform", "web");
        googletag.pubads().setTargeting("SOV", "base ros");
      googletag.pubads().disableInitialLoad();
    });


  pbjs.que.push(function() {
      const customConfigObject = {
   /*Setup in GH DFP
  $0.01 increments from $0.10 to $5.00
  $0.05 increments from $5.05 to $20.00
  $0.50 increments from $20.50 to $70.00
  */

  "buckets": [
    {
      "precision": 2,  //default is 2 if omitted - means 2.1234 rounded to 2 decimal places = 2.12
      "min"      : 0.10,
      "max"      : 5.00,
      "increment": 0.01
    },
    {
       "precision": 2,
       "min" : 5.05,
       "max" : 20.00,
       "increment" : 0.05
    },
    {
      "precision": 2,
      "min"      : 20.50,
      "max"      : 70.00,
      "increment": 0.50
    }
          ]
      };
      pbjs.setPriceGranularity(customConfigObject);
      
      pbjs.enableAnalytics([{
          provider: 'ga',
          options: {
              enableDistribution: false,
                            sampling: 0.50,
                            account: 'UA-109047579-1',
              trackerName:'hbAnalytics'
          }
      }]);
      pbjs.setConfig({
        //"priceGranularity" : "high",
        //"enableSendAllBids" : true,
        "publisherDomain" : "https://newsok.com",
        "bidderTimeout" : PREBID_TIMEOUT
       });
      pbjs.addAdUnits(adUnits);
      pbjs.requestBids({
          bidsBackHandler: sendAdserverRequest
      });
  });

  function sendAdserverRequest() {
      if (pbjs.adserverRequestSent) return;
      pbjs.adserverRequestSent = true;
      googletag.cmd.push(function() {
          pbjs.que.push(function() {
              pbjs.setTargetingForGPTAsync();
              //googletag.pubads().setForceSafeFrame(true);
              googletag.pubads().refresh();
          });
      });
  }

  setTimeout(function() {
      sendAdserverRequest();
  }, PREBID_TIMEOUT);

</script>
<!-- Prebid Boilerplate Section END -->
