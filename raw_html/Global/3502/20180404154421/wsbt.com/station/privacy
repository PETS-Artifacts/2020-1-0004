    function fetchWeather(zip) {
        var fetch = $.ajax({
            url: '/api/rest/audiencero/weather/zipcode/' + zip
        })
        return fetch;
    }

    function renderWeather(weatherZipCode) {
        fetchWeather(weatherZipCode).done(function(data){
            try {
                var obj = {
                    weather: data.data[0]
                }
                $('.sd-weather-current-icon').removeClass('wi-stk-66').addClass('wi-stk-'+obj.weather.forecast.current.iconCode);
                $('.sd-weather-current-temp').text(obj.weather.forecast.current.tempF);
                $('.weather-current >.no-margin .wx-temp').text(obj.weather.forecast.current.tempF).append('<i class="wi wi-degrees"/>');
                $('.wx-icon-text >.wx-icon >.wi').removeClass('wi-66').addClass('wi-'+obj.weather.forecast.current.iconCode);
                $('.wx-icon-text >.wx-text').text(obj.weather.forecast.current.skyText);
                $('.wx-container').css('opacity',1)
            } catch (e){
                console.log(e)
            }
        })
    }

    $('#sd-desktop-search').on('keydown',function(){
        if($(this).val().length >= 3) {
            $(this).siblings('button').addClass('search-active');
        } else {
            $(this).siblings('button').removeClass('search-active');
        }
    })

    renderWeather('46545')

},true)</script></div></div></div></div></div><div class='sd-nav-only'><div class='component-nav-default-v1'><div class="sd-livestream-notifier hidden touch mobile"><a href="/watch" class="watch-live watch-now">Watch<div class="sd-play-btn"></div></a></div><div class="all-nav-container"><div id="searchbar-mobile"><form action="/search" class="sd-mobile-search"><button type="submit"><i class="ion-ios-search"></i></button><input type="search" name="find" placeholder="Search WSBT"/></form></div><!--DESKTOP and MOBILE--><nav class="navbar sd-header-nav desktop"><div class="sd-page-width"><div id="navbar"><div class="sd-overflow-less"><a id="nav-less" href="#" class="sd-link"><i class="fa fa-angle-left"></i></a></div><div class="sd-overflow-more"><a id="nav-more" href="#" class="sd-link">more...</a></div><div class="sd-livestream-notifier hidden desktop"><a id="nav-watch-live" href="/watch" class="watch-live watch-now">Watch<div class="sd-play-btn"></div></a></div><div class="main-nav-container"><div class="swiper-nav-container"><div id="sd-nav" class="swiper-wrapper sd-nav clearfix"><div class="sd-nav-item swiper-slide"><a id="News-6bb47d2e-84b7-495f-83c9-4e6f9e697f93" href="/news" data-name="News" data-id="6bb47d2e-84b7-495f-83c9-4e6f9e697f93" data-index="0" class="sd-link has-children desktop-top-nav-a">News</a><div data-name="News" data-id="6bb47d2e-84b7-495f-83c9-4e6f9e697f93" class="sd-subnav"><ul class="clearfix"><li><a id="Local-f024c414-aabd-43ef-bcd5-0d8ae0949d68" href="/news/local" data-name="Local" class="sd-link desktop-sub-nav-a">Local</a></li><li><a id="Regional-664c56bd-bb71-4c16-ac67-171247bbb93f" href="/news/regional" data-name="Regional" class="sd-link desktop-sub-nav-a">Regional</a></li><li><a id="Nation &amp; World-4a32d351-2311-4188-a12e-55d35eb8c634" href="/news/nation-world" data-name="Nation &amp; World" class="sd-link desktop-sub-nav-a">Nation &amp; World</a></li><li><a id="Election-7d057dc0-da2e-48ca-b59f-a53a82fdf1fc" href="/news/election" dat