      "@yours."
    ];

    form.onValidate(function(){
      var email = form.vals().Email.toLowerCase();
      var emailElem = form.getFormElem().find("#Email");
      var errorMessage = "Please use a company email address.";
      if(email){
        // Check if email is valid and;
        // Check if form is main Contact Us form (mktoForm_3213)
        if(!isEmailGood(email) && mktoFormID != "3213") {
          form.submitable(false);
          form.showErrorMessage(errorMessage, emailElem);
        }else if(!isEmailGood(email) && mktoFormID == "3213" && $("#Contact_Us_Inquiry__c").val().toLowerCase() == "sales") {
          $("#Email").focus();
          form.submitable(false);
          form.showErrorMessage(errorMessage, emailElem);
        }else if(!isEmailGood(email) && mktoFormID == "3213" && $("#Contact_Us_Inquiry__c").val().toLowerCase() == "agency") {
            $("#Email").focus();
            form.submitable(false);
            form.showErrorMessage(errorMessage, emailElem);
        }else{
          form.submitable(true);
        }
      }
    });

    function isEmailGood(email) {
      for(var i=0; i < invalidDomains.length; i++) {
        var domain = invalidDomains[i].toLowerCase();
        if (email.indexOf(domain) != -1) {
          return false;
        }
      }
      return true;
    }

    // Add an onSuccess handler
    form.onSuccess(function(values, followUpUrl) {

        // // Get submitted values and set as variables for passing into querystring for Request Trial signup.
        var firstName = form.vals().FirstName;
        var lastName = form.vals().LastName;
        var email = form.vals().Email;
        var numberOfEmployees = form.vals().Number_of_Employees_Company_Size__c;
        var useCase = form.vals().Business_Use_Case__c;
        var redirectStringPAYGO = "/users/sign_up?client=true&first_name=" + firstName + "&last_name=" + lastName + "&email=" + email + "&free_trial=true";

        // Take the lead to a different page on successful submit, ignoring the form's configured followUpUrl
        if(useCase === "One time project" && numberOfEmployees === "1 - 10") {
          location.href = redirectStringPAYGO;
        } else if (useCase === "One time project" && numberOfEmployees === "11 - 50") {
          location.href = redirectStringPAYGO;
        } else {
          // Get the form's jQuery element and hide it
          form.getFormElem().hide();
          $('.modal-content .success-confirmation').show();
          $('#modal_title, #call_info, .terms-notice').hide();
        }

        // Return false to prevent the submission handler from taking the lead to the follow up url.
        return false;
    });

    $(document).on("change", "select[id^='Business_Use_Case__c']", function() {
      var trialBusinessUseCaseSelect = $("#Business_Use_Case__c").val().toLowerCase();
      var msgAgencyHTML = "<p>To learn more about the UserTesting Partner Program for agencies/consultants, please <a href=\"/partners\">click here</a>.</p>";
      var msgTesterHTML = "<p>To learn more about getting paid to test websites, apps, or prototypes, <a href=\"/be-a-user-tester\">click here</a> to sign up.</p>";
      if(mktoFormID == "3901") {
        if(trialBusinessUseCaseSelect == "agency/consultant on behalf of clients") {
          $("#mktoForm_3901 .mktoButtonRow").attr('style','display: none !important');
          $(".selection-notice-trial").html(msgAgencyHTML).show();
          $(".terms-notice").hide();
        }else if(trialBusinessUseCaseSelect == "i want to be a paid tester") {
          $("#mktoForm_3901 .mktoButtonRow").attr('style','display: none !important');
          $(".selection-notice-trial").html(msgTesterHTML).show();
          $(".terms-notice").hide();
        }else{
          $("#mktoForm_3901 .mktoButtonRow").attr('style','display: block !important');
          $(".selection-notice-trial").hide();
          $(".terms-notice").show();
        }
      }
    });
});

</script>



    

  </div>

    

</body>
</html>
