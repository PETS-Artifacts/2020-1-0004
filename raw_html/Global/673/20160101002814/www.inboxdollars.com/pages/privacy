<script>

var url = location.href;
var baseURL = url.substring(0, url.indexOf('/', 14));
var baseUrlLength = baseURL.length;

var anchors = document.getElementsByTagName("A");
for(var i=0;i<anchors.length;i++)
{
	var offerListingId = '';
	var joinIt = false;
	if (anchors[i].href.substring(baseUrlLength, (baseUrlLength + 24)) == "/members/survey/display/") {
		joinIt = true;
	}
	var pos = anchors[i].href.indexOf("/offers/go/", baseUrlLength);
	if (pos != -1) {
		//PARSE ANCHOR URL TO GET OFFER LISTING ID, AND PASS TO joinNow FUNCTION
		var urlParts = anchors[i].href.substring(pos + 11).split("/");
		offerListingId = urlParts[0];
		joinIt = true;
	}
	// slight tweak for my new promo_go function
	pos = anchors[i].href.indexOf("/offers/promo_go/", baseUrlLength);
	if (pos != -1) {
		var urlParts = anchors[i].href.substring(pos + 17).split("/");
		offerListingId = urlParts[1]; // 2nd param
		joinIt = true;
	}
	// adding surveys go function - 'off' type is the only type that show up for guests
	pos = anchors[i].href.indexOf("/surveys/go/off/", baseUrlLength);
	if (pos != -1) {
		var urlParts = anchors[i].href.substring(pos + 16).split("/");
		offerListingId = urlParts[0]; // 1st param
		joinIt = true;
	}
	if (joinIt) {
		anchors[i].onclick = function() {
			joinNow(offerListingId);
			return false;
		};
	}
}
</script>


