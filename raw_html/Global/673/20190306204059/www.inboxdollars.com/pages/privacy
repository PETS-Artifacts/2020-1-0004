<script type="text/javascript">
    jQuery(document).ready(function() {
        //update overlay sizing...
        function resizeOverlay()
        {
            var pageHeight = jQuery('body').height();
            if(parseInt(pageHeight) == 0) {
                pageHeight = Math.round(jQuery('#body-wrapper').height() + jQuery('#signupFloatingContainer').height());
            }
            jQuery('#guestSignupFormWrapper .overlay').css('height', pageHeight + 'px');
        }
        resizeOverlay();
        jQuery(window).resize(function() {
        	resizeOverlay();
        });

        //open/close functions...
        function closeSignupPopup() {
            jQuery('#guestSignupFormWrapper').hide();
        }
        function openSignupPopup() {
            resizeOverlay();
            jQuery('#guestSignupFormWrapper').show();
            jQuery('#guestSignupEmail').focus();
        }
        function closeLoginBlock() {
            jQuery('#guestLoginFormWrapper').hide();
        }
        function openLoginBlock() {
            jQuery('#guestLoginFormWrapper').show();
            jQuery('#guestLoginEmail').focus();
        }

        //overlay click functionality...
        var popupClicked = false;
        jQuery('#guestSignupFormWrapper .overlay').click(function() { //click overlay...
        	if (!popupClicked) {
        		closeSignupPopup();
        	}
        	popupClicked = false;
        });
        jQuery('#guestSignupFormWrapper .innerWrapper').click(function() { //click popup...
        	popupClicked = true;
        });

        //button functionality...
        jQuery('#guestSignupButton').click(function(e) {
            e.preventDefault();
            if (jQuery('#guestSignupFormWrapper').css('display') == 'none') {
                closeLoginBlock();
                openSignupPopup();
            } else {
                closeSignupPopup();
            }
        });
        jQuery('#guestSignupFormCloseButton').click(function() {
            closeSignupPopup();
        });
        jQuery('#guestSignupFormAlreadyAMemberLink').click(function(e) {
            e.preventDefault();
            closeSignupPopup();
            openLoginBlock();
        });
        jQuery('#guestLoginButton').click(function(e) {
            e.preventDefault();
            if (jQuery('#guestLoginFormWrapper').css('display') == 'none') {
                closeSignupPopup();
                openLoginBlock();
            } else {
                closeLoginBlock();
            }
        });

        //signup errors...
        if (jQuery('#guestSignupButton').css('display') != 'none') {
                    }
    });
</script>
