<script>
    jQuery(document).ready(function() {
        
        setupDeferredImages();
        
        setupFacetLessMoreButtons(8, '.search-facet');
        
        // For each query input (header navbar and search page main)
        var $query = jQuery('#query, #main-query');
        $query.each(function(){
            var $this = $(this);
            // Find collection name from form
            var collection = $this.parents('form').find('input[name=collection]').val();
            var fbUrl, datasets;
            
            // Collection-specific Config
            if (collection == 'monash-main-search') {
                fbUrl = 'https://mon3-search.clients.squiz.net',
                datasets = {
                    organic: {
                        name: 'Suggestions',
                        collection: 'monash-main-search',
                        profile: '_default',
                        show: '10'
                    },
                    courses: {
                        name: 'Courses',
                        collection: 'study-monash-courses-web',
                        profile: 'autocompletion',
                        show: '5',
                        program: fbUrl + '/s/search.html',
                        template: {
                            suggestion: jQuery('#auto-completion-courses').text()
                        }
                    },
                    research: {
                        name: 'Research',
                        collection: 'monash-research',
                        profile: 'autocompletion',
                        show: '5',
                        program: fbUrl + '/s/search.html',
                        template: {
                            suggestion: jQuery('#auto-completion-research').text()
                        }
                    }
                };
            } else if (collection && collection == '') {
                fbUrl = 'https://mon3-search.clients.squiz.net',
                datasets = {
                    customSearch: {
                        name: 'Suggestions',
                        collection: collection,
                        profile: '_default',
                        show: '10'
                    }
                };
            }
            
            console.log('qc init', this.id, collection, fbUrl);
            // Initialise auto completion
            if (fbUrl) {
                $this.qc({
                    program: fbUrl + '/s/suggest.json',
                    alpha: '0.5',
                    show: '10',
                    sort: '0',
                    length: '3',
                    horizontal: true,
                    datasets: datasets
                });
            }
        });
    });
</script>
    
