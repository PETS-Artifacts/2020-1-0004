 */
function cleanDFPParams(paramString) {
    var cleaned = paramString.replace(/\&/g, "-");
    cleaned = cleaned.replace(/\=/g, "-");
    cleaned = cleaned.replace(/\"/g, "");
    cleaned = cleaned.replace(/\'/g, "");
    cleaned = cleaned.replace(/\!/g, "");
    cleaned = cleaned.replace(/\+/g, "");
    cleaned = cleaned.replace(/\#/g, "");
    cleaned = cleaned.replace(/\*/g, "");
    cleaned = cleaned.replace(/\~/g, "");
    cleaned = cleaned.replace(/\;/g, "");
    cleaned = cleaned.replace(/\^/g, "");
    cleaned = cleaned.replace(/\(/g, "");
    cleaned = cleaned.replace(/\)/g, "");
    cleaned = cleaned.replace(/\</g, "");
    cleaned = cleaned.replace(/\>/g, "");
    cleaned = cleaned.replace(/\[/g, "");
    cleaned = cleaned.replace(/\]/g, "");
    cleaned = cleaned.replace(/\,/g, "");
    return(cleaned);
}
