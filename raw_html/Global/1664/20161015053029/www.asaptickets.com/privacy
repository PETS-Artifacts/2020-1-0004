<script>
    window.SmarterTravelNetwork_ready = window.SmarterTravelNetwork_ready || [];
    window.SmarterTravelNetwork_ready.push(function (STNetwork) {
        var $ = STNetwork.$;
        $(document).ready(function(){
            var	apiKey = 'qNEu1QZNQnaS6ebpazdKyw',
                mainSearchFormPlacementId = 'SnIBK6WJTpSF-OdNw8i1uQ',
                footerSearchFormPlacementId = 'J23Ts3EERPCZnaSZ8G88jw',
                adVertical = 'air',
                searchData = {},
                options = {},
                frontDoor;


            STNetwork.init(apiKey, searchData, options);
            // Create frontDoor based on which search form is using
            frontDoor = document.getElementById('searchForm') ?
                    createMainFrontDoor(frontDoor) : createFooterFrontDoor(frontDoor);

            STNetwork.loadPlacements([frontDoor]);

            function createMainFrontDoor(frontDoor) {
                frontDoor = STNetwork.createFrontDoor({
                    placementId: mainSearchFormPlacementId,
                    adVertical: adVertical,
                    containerSelector: '#stnFrontDoorContainer',
                    formSelector: '#searchForm',
                    redirectPageUrl: '/stn/redirect',
                    prePopOnJQueryDatePicker: true
                });

                frontDoor.setDateInputFormat('MM/DD/YYYY');
                frontDoor.setFormInput('origin_name', '#search_words_from1');
                frontDoor.setFormInput('destination_name', '#search_words_to1');
                frontDoor.setFormInput('date1', '#date1');
                frontDoor.setFormInput('date2', '#date2');
                frontDoor.setFormInput('num_travelers', '#adult');

                frontDoor.setFlightTypeInputMapping({
                    'roundtrip': 'roundtrip',
                    'oneway': 'oneway'
                });

                frontDoor.setFormInput('flight_type', '[name="tripType"]');

                frontDoor.setFormInput('flight_service_class', '#flight-class', function () {
                    var flightValue = $('#flight-class').val();
                    var flightClass;

                    if(flightValue === 'business') {
                        flightClass = 'business';
                    } else if (flightValue === 'first') {
                        flightClass = 'first_class';
                    } else {
                        flightClass = 'economy_coach';
                    }
                    return flightClass;
                });

                $('#perform-search').click(frontDoor.handleFormSubmitted);

                return frontDoor;
            }

            function createFooterFrontDoor(frontDoor) {
                frontDoor = STNetwork.createFrontDoor({
                    placementId: footerSearchFormPlacementId,
                    adVertical: adVertical,
                    containerSelector: '#stnFrontDoorContainer',
                    formSelector: '#big_search_form_2',
                    redirectPageUrl: '/stn/redirect',
                    prePopOnJQueryDatePicker: true
                });

                frontDoor.setDateInputFormat('MM/DD/YYYY');
                frontDoor.setFormInput('origin_name', '#search_words_from2');
                frontDoor.setFormInput('destination_name', '#search_words_to2');
                frontDoor.setFormInput('date1', '#date3');
                frontDoor.setFormInput('date2', '#date4');
                frontDoor.setFormInput('num_travelers', '#adult');

                frontDoor.setFormInput('flight_type', '#ftr-rt', function () {
                    return $('#oneway', '#big_search_form_2').prop('checked') ? 'oneway' : 'roundtrip';
                });

                frontDoor.setFormInput('flight_service_class', '#cabin_class', function () {
                    var flightValue = $('#cabin_class').val();
                    var flightClass = null;

                    if(flightValue === 'business') {
                        flightClass = 'business';
                    } else if (flightValue === 'first') {
                        flightClass = 'first_class';
                    } else {
                        flightClass = 'economy_coach';
                    }
                    return flightClass;
                });

                $('input[type="button"], .js-inner-ftr-srch').click(function() {
                    if($('#from2').val() !== '' && $('#to2').val() !== '') {
                        $('#big_search_form_2').removeClass('no-stnFrontDoor');
                    }
                }).click(frontDoor.handleFormSubmitted);

                return frontDoor;
            }
        });
    });

    // load the library
    (function () {
        var js = document.createElement('script');
        js.src = '//p.smartertravel.com/ext/lib/partner-ad-sdk.min.js';
        js.async = true;
        var entry = document.getElementsByTagName('script')[0];
        entry.parentNode.insertBefore(js, entry);
    })();
</script>    
