/* -------- Fix for strong tags in new galleries. nd-7844 ----- AD  */
.pg .box figcaption p strong {position:inherit;right:0;left:0;}
.pg .box figcaption span strong {position:inherit;right:0;left:0;}

/* ND-7840 ND - Top Carousel Spacing ------ AD 17.4 */
#topCarousel .main.dbfront { width: 462px;}
#topCarousel .main.dbfront p a.lead {font-size: 24px; padding-top: 8px;}
#topCarousel .main.dbfront p span {font-size: 14px;padding: 8px 0 0;}
#topCarousel .main.dbfront img{width:95%; margin:0;}

/* ND-7855 Paywall tweaks/bugs ---------- MC */
@media (min-width:980px) {
     #fullGall #gateOpt {min-height:428px;}
    #primary.quiz #gateOpt, #primary.pollContent #gateOpt, #primary.vcard #gateOpt, .map #primary #gateOpt, .listing #primary #gateOpt #primary.quiz #gateOpt  {width: 100%;}
}

#primary #gateOpt .inner + .inner p {margin: 0 0 20px 0;}
#primary #gateOpt p {font-size: 20px;font-weight: 300;line-height: 1.4em;color: #1a1a1a;letter-spacing: -1px;width: auto;margin: 20px 0;padding: 0;}

/* ND-7891 - Top sports video hack - Bug report - MC ---*/
#tImg.video {position: relative;padding-bottom: 56.25%; /* 16:9 */padding-top: 25px;height: 0;border:0;}
#tImg.video > div {position: absolute;top: 0;left: 0;width: 100%;height: 100%;}

/* ND-7909 -Sticky ads on columns fix -MC */
#header, #bodyContainer.column {overflow:inherit;}

/* NEED TICKET - HS Sports topper styles will need to modify CSS to fit - MC -----*/
#utility .hss {display: block;padding: 13px;text-transform: uppercase;font-size: 15px;}
#utility.sports {padding:0;border-width: 1px 1px 0;}
#utility.sports .module {float:none;border-top: 0;}

/* ND-7965 Weather leader bug -----*/
#main .fullBanner {min-width:728px;width:100%;}
</style>
