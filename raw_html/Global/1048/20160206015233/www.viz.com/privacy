<script language="javascript">
  $(".feedback").click(function() {
    Recaptcha.create("6Lc70sMSAAAAANndlXJjW8ujqOyuwlPB4OEgFByI", 
      "feedback_captcha", {
      theme: "blackglass"
    });
    $("#feedback_form_error").hide();
    $("#feedback_form").modal({
      minHeight : 450,
      onShow: function() { $(".modal_form_field label").inFieldLabels({fadeOpacity:"0.3"}); },
      onClose: function(){
        $("#feedback_form_comments").show(); 
        $("#feedback_form_result").hide();
        $("#feedback_form_error").hide();
        Recaptcha.destroy();
        $.modal.close();
      }
    });
  });

  $(document).on("click", "#show_feedback_from_header", function() {
    $(".feedback").click();
  });

  $("#send_feedback").click(function() {
    if ($("#feedback_comments").val().length > 0) {
      //Show loading animation...
      $("#feedback_sending").show();
      $("#feedback_form_comments").hide();
  
      var is_viz_manga = 0;
      var is_viz = 1;
      var is_wsj = 0;

      $.post("/mailer/feedback_send", { email: $("#feedback_email").val(), comments: $("#feedback_comments").val(), recaptcha_challenge_field: Recaptcha.get_challenge(), recaptcha_response_field: Recaptcha.get_response(), is_viz_manga: is_viz_manga, is_viz: is_viz, is_wsj: is_wsj },
      function(data, textStatus) {
        if (data.error == 1) {
          Recaptcha.reload();
          $("#feedback_form_error").show();
          $("#feedback_form_comments").show(); 
          $("#feedback_sending").hide(); 
        } else {
          $("#feedback_form_error").hide();
          $("#feedback_form_comments").hide(); 
          $("#feedback_sending").hide(); 
          $("#feedback_form_result").show();
        }
        return false;
      }, "json")  
    } else {
      alert("Please enter a message to send");
      return false;
    }
  });
</script>
	

    
