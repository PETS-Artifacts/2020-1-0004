<script language="javascript">

  var loginURL = "https://www.viz.com/account/try_login.json"; 

  function showSignUpForm() {
    Recaptcha.create("6Lc70sMSAAAAANndlXJjW8ujqOyuwlPB4OEgFByI", 
      "recaptcha_div", {
      theme: "blackglass"
    });
    $("#create_account_form").modal({
      minHeight: 640,
      minWidth: 435,
      overflow: "auto",
      autoPosition: (!isSmartphone()),
      onOpen: function(d) {
        if (isSmartphone()) {
          $(".simplemodal-container").css({position:"absolute", left:window.pageXOffset+"px", top:(window.pageYOffset+15)+"px"});
          $("#simplemodal-overlay").css({width:"980px !important", height:"5000px !important"});
        }
        self.container = d.container[0];
        d.overlay.fadeIn('fast');
        d.container.fadeIn('fast', function() {
          $("#create_account_form").show();
          $("#new_email").focus();
        });
      },
      onShow: function() { $(".modal_form_field label").inFieldLabels({fadeOpacity:"0.3"}); },
      onClose: function() {
        Recaptcha.destroy(); 
        $.modal.close();
      }
    });
  }
</script>


