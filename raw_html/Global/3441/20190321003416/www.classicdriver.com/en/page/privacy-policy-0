            JQbigSliderContainer.css({
              'min-height': ((JQbigSliderContainer.outerWidth() * 504 / 940) > 332 ? 332 : (JQbigSliderContainer.outerWidth() * 504 / 940)) + 'px'
            });
          });
        });
      }
    </script>
