<script type="text/javascript">
Downloads = {};
function trackDownloads(e) {
    var type;
    if (this.href.match(
        /\.(pdf|doc|xls|docx|rtf|xlsx|jpg|jpeg|gif|png|svg|ppt|pptx)$/i)
    ) { type='File'; }
    else if (this.href.match(/\/download$/)) { type='Rok'; }
    else { return; }

    if (Downloads[this.href]) { return; }
    Downloads[this.href] = 1;
    pageTracker._trackEvent('Download',type,this.href);
}
try {
    var pageTracker = _gat._getTracker("UA-8797305-1");
    pageTracker._trackPageview();
    jQuery('body').delegate("a","mousedown",trackDownloads);
} catch (err) {
    
}
</script>


