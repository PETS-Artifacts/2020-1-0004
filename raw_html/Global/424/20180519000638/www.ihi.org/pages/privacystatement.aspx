<script type="text/javascript">
    var addthis_config = {
        data_ga_property: 'UA-5146503-1',
        data_ga_social: true
    };
    
    //Enterprise keywords that this item is tagged with
    //we are tracking the keywords ONLY for individual blog items for now
    var taxKeywords = '';
    
    function logGoogleAnalyticsData(userGUID) {
        try {
            ihi.Analytics.AnalyticsId = 'UA-5146503-1';
            ihi.Analytics.pageTracker = ga;
            var currentUserGUID = '';
            if (currentUserGUID != '') {
                ga('create', 'UA-5146503-1', 'ihi.org', { 'userId': currentUserGUID});
            }
            else {
                ga('create', 'UA-5146503-1', 'ihi.org');
            }
            ga('set', 'dimension1', currentUserGUID);

            //taxonomy keywords are available only for blog items
            if (taxKeywords != '') {
                ga('set', 'dimension2', taxKeywords);
            }
            //ihi.Analytics.pageTracker = _gat._getTracker("UA-5146503-1");
            //ga('set', 'referrer', getReferrer());

            //current file path name
            var pathName = window.location.pathname.toLowerCase();

            //this is to set the domain name to a single name to enable tracking app.ihi.org visits also with the same domain name
            //ihi.Analytics.pageTracker._setDomainName('ihi.org');

            //downloadfile.aspx is a special file to handle document set file downloads after a user logs in using the modal popup login screen
            //this file does not get tracked since the file being downloaded gets tracked via a custom function in downloadfile.aspx
            //function "logFileDownloadPath" in "downloadfile.aspx" will be called once the page tracker object has been initialized
            var downloadFilePath = '/_layouts/15/ihi/downloadfile.aspx';
            var indexOfDowloadUrl = pathName.indexOf(downloadFilePath);
            if (indexOfDowloadUrl == -1) {
                //track the page as normal, if the page is not for downloading a specific file
                //ihi.Analytics.pageTracker._trackPageview();
                ga('send', 'pageview', {
                    useBeacon: false
                });
            }

            //Calls all the functions queued up to be called after the initialization of the pageTracker object
            ihi.Analytics.callAfterInitFunctions();
        }
        catch (err) { }

        //the userGUID value will be registered from the code-behind file
        //Uses a function from the analytics.js file
        ihi.Analytics.setGoogleUser(userGUID);
    }

    function bindAnalyticsTrackerEventToLinks() {
        var servername = window.location.hostname; 	//DEVWEB or TESTWEB or WWW.IHI.ORG
        var allValidLinksInPage = $("a[href]").not("a[onclick]").not("a[href^='/'][href$='.aspx']").not("a[href='#']").not("a[href^='\\.\\.']").not("a[href^='javascript']");

        var regExStart = new RegExp();
        var regExEnd = new RegExp();
        regExStart.compile('^(http|https|ftp)://' + servername + '|' + '^(http|https|ftp)://app.ihi.org');

        //list of file extensions to log the clicks within our internal server
        //files ending with all the below extensions will be tracked on the Google Analytics
        regExEnd.compile('(.doc|.docx|.xls|.xlsx|.ppt|.pptx|.pdf|.zip|.wma|.mp3|.wmv|.avi|.mp4|.arf|.asx|.jpg|.gif|.bmp)$');
        
        //filters invalid links to exclude from adding the onclick event to log to Analytics
        var filteredLinksInPage = $.grep(allValidLinksInPage, function (value) {
            //foreach of the <a href.../> elements, return TRUE to log clicking or FALSE to avoid logging the click events
            var linkToBeLogged = true;
            var url = $(value).attr('href').toLowerCase();

            //figure out whether the url goes through the "download.aspx" file, if so grab the "SourceURL" param value and use that as the logging URL
            var pathToMatch = '/_layouts/download.aspx?sourceurl=';
            if (url.indexOf(pathToMatch) > -1) {
                url = url.substring(url.indexOf(pathToMatch) + pathToMatch.length, url.length);
                url = unescape(url);
            }

            //remove any query parameters in the url
            if (url.indexOf('?') > -1) {
                url = url.substring(0, url.indexOf('?'));
            }

            if (regExStart.test(url) || url.substring(0, 1) == "/") {  //if the url starts with any protocol + current server name OR any relative links
                if (!regExEnd.test(url)) {  //and the url does not end with an extension which is not in the list of extensions
                    //URL is withthin our server itself and the resource does not end in any of the listed extensions above
                    linkToBeLogged = false;
                }
                else {
                    //inside URL but ends with list of extensions listed above, so track it
                    linkToBeLogged = true; //url ends in one of the allowed extensions
                }
            }
            else {
                linkToBeLogged = true; //url is to an external site, so track it
            }
            return linkToBeLogged;
        });

        //ADD the javascript click event to log to analytics
        $(filteredLinksInPage).click(function () {
            try {
                ihi.Analytics.logFileDownload(this); 
            }
            catch (ex) { }
        });
	}

    ///Function to initialize the Analytics objects and assign click events and log the current page visit
	function InitAndLogGoogleAnalytics(userGUID) {
        //Process as normal for "http://" requests
        try {
            //logs the current page visit and logs the current user (if any)
	        logGoogleAnalyticsData(userGUID);
	    }
	    catch (ex)
        {  /* What todo when error happens???  */ }


        try {
            //binds the click event to all the valid links (External urls, internal resources (pdf, wmv, ...)
            bindAnalyticsTrackerEventToLinks();
        }
        catch (ex) { /* What to do when error happens??? */ }
	}
</script>

        


