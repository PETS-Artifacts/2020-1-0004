                            unifiedData.metaInfo = unifiedData.metaInfo || {};
                            unifiedData.metaInfo.imagePresent = unifiedData.imagePresent;
                            delete unifiedData.imagePresent;
                        }

                        $.extend(appPayload, unifiedData);

                        
