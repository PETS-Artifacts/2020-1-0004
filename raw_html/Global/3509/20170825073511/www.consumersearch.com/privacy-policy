<script>
    'use strict';
    if (App) {
        (function () {
            App.events = App.events || [];
            App.events.push(
                    {
                        //open / close the search bar on mobile
                        partial: "PartialSearchBox",
                        contextSelector: ".PartialSearchBox",
                        elementSelector: ".js-PartialSearchBox-button",
                        type: "click",
                        handler: function (e) {
                            var navBar = $(".l-navigation-container, .l-header-container, .l-content-container");
                            //if search bar is closed, open it
                            if (!navBar.hasClass("search-focus")) {
                                navBar.addClass("search-focus");
                            }
                            //otherwise do nothing and the form will be submitted
                        }
                    },
                    {
                        partial: "PartialSearchBox",
                        contextSelector: ".PartialSearchBox",
                        elementSelector: ".js-PartialSearchBox-form",
                        type: "submit",
                        handler: function (e) {
                            var $searchTextInputBox = $(".js-PartialSearchBox-input", this);

                            if (!$searchTextInputBox.val()) {
                                return false;
                            }
                        }
                    },
                    {
                        partial: "PartialSearchBox",
                        contextSelector: ".PartialSearchBox",
                        elementSelector: ".PartialSearchBox-container",
                        type: "submit",
                        handler: function (e) {
                            var topForm = $(".js-PartialSearchBox-form", this),
                                    searchPayload = topForm.data("unified"),
                                    ask = typeof(ask) != undefined ? ask : {};

                            if (!searchPayload) {
                                return;
                            }
                            searchPayload.actionType = "click";
                            searchPayload.assetType = "searchBox";
                            topForm.unbind(e);
                            e.preventDefault();

                            if(ask && ask.unified !== undefined) {
                                ask.unified.sendAction(searchPayload, function () {
                                    topForm.submit();
                                });
                            } else {
                                topForm.submit();
                            }
                        }
                    },
                    {
                        partial: "PartialSearchBox",
                        contextSelector: ".PartialSearchBox",
                        elementSelector: null,
                        type: "ready",
                        handler: function () {

                            var navBar = $(".l-navigation-container, .l-header-container, .l-content-container");
                            //if search bar is closed, open it
                            var $searchTextInputBox = $(".js-PartialSearchBox-input", this);

                            if ($searchTextInputBox.val()) {
                                navBar.addClass("search-focus");
                            }

                            var input = $(".js-PartialSearchBox-input", this),
                                clear = $(".js-PartialSearchBox-clear", this);
                            if (clear) {
                                _searchUtil.searchBoxClear(input, clear);
                            }
                        }
                    },
                    {
                        partial: "PartialSearchBox",
                        contextSelector: ".PartialSearchBox",
                        elementSelector: ".js-PartialSearchBox-input",
                        type: "focus",
                        handler: function (e) {
                            
}
                    },
                    {
                        partial: "PartialSearchBox",
                        contextSelector: ".PartialSearchBox",
                        elementSelector: ".js-PartialSearchBox-input",
                        type: "blur",
                        handler: function (e) {
                            
}
                    },
                    {
                        partial: "PartialSearchBox",
                        contextSelector: ".PartialSearchBox",
                        elementSelector: ".js-PartialSearchBox-input",
                        type: "keydown",
                        handler: function (e) {
                            e.stopPropagation();
                        }
                    }
            );
        })();
    }
</script>




</div>
