<script>
    'use strict';
    if (App) {
        (function () {
            var open = false;
            App.events = App.events || [];
            App.events.push(
                {
                    //open / close the menu via button
                    partial: "PartialTopNavigation",
                    contextSelector: ".PartialTopNavigation",
                    elementSelector: "#menu-icon",
                    type: "click",
                    handler: function (e) {
                        if(open){
                            $(".nav-menu-open").removeClass("nav-menu-open");
                        }else{
                            $(this).parent().children(".links").addClass("nav-menu-open");
                            $(".l-content-container, .l-footer-container").addClass("nav-menu-open");
                        }
                        open = !open;
                    }
                }
            );
        })();
    }
</script>



