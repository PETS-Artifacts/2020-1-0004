    $('#menusearchbtn, #sidr-id-menusearchbtn').click(function () {
        var search_text = $('#menusearchinput').val();
        var search_text2 = $('#sidr-id-menusearchinput').val();
        if (search_text2.length > 0) search_text = search_text2;
