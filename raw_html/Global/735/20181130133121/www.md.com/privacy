                $('.social_share .facebook').on("click", function(event){
            event.preventDefault();
            event.stopPropagation();
            shareOnFacebook();
        });

        $('.social_share .googleplus').on("click", function(event){
            event.preventDefault();
            event.stopPropagation();
            mdTrackSocialShare( 'googleplus',  'https://www.md.com/privacy');
            $.popupWindow($(this).attr("href"), {
                height: 480,
                width: 480,
                onUnload: function() { // callback when window closes
                }
            });
        });

        $('.social_share .linkedin').on("click", function(event){
            event.preventDefault();
            event.stopPropagation();
            mdTrackSocialShare( 'linkedin',  'https://www.md.com/privacy');
            $.popupWindow($(this).attr("href"), {
                height: 570,
                width: 520,
                onUnload: function() { // callback when window closes
                }
            });
        });

        $('.social_share .email').on('click', function(event){
            event.preventDefault();
            event.stopPropagation();
            mdTrackSocialShare( 'email',  'https://www.md.com/privacy');
            window.setTimeout('document.location = "' + $(this).attr("href") + '"', 100);
        });

        $('.social_share .pinterest').on("click", function(event){
            event.preventDefault();
            event.stopPropagation();
            var pin_url = $(this).data("url");
            var pin_media = $(this).data("media");
            var pin_description = $(this).data("description");
            var pin_destination = $(this).attr("href") + '?url=' + pin_url + '&media=' + pin_media + '&description=' + pin_description;
            pin_url = decodeURIComponent(pin_url);
            mdTrackSocialShare( 'pinterest',  pin_url);
            $.popupWindow(pin_destination, {
                height: 333,
                width: 767,
                onUnload: function() { // callback when window closes
                }
            });
        });

