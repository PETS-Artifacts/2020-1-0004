        (function( $ ){

            $('input, textarea').placeholder();

            function popitup(title, url) {
                newwindow=window.open(url,title,'height=600,width=800');
                if (window.focus) {newwindow.focus()}
                return false;
            }

            $.fn.extend( {
                share_is_email: function() {
                    return /^((([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+(\.([a-z]|\d|[!#\$%&'\*\+\-\/=\?\^_`{\|}~]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])+)*)|((\x22)((((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(([\x01-\x08\x0b\x0c\x0e-\x1f\x7f]|\x21|[\x23-\x5b]|[\x5d-\x7e]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(\\([\x01-\x09\x0b\x0c\x0d-\x7f]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF]))))*(((\x20|\x09)*(\x0d\x0a))?(\x20|\x09)+)?(\x22)))@((([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|\d|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.)+(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])|(([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])([a-z]|\d|-|\.|_|~|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])*([a-z]|[\u00A0-\uD7FF\uF900-\uFDCF\uFDF0-\uFFEF])))\.?$/i.test( this.val() );
                }
            } );

            $('.popup_dialog').on('click', function (e) {
                e.preventDefault();
                e.stopPropagation();
                var $link = $(this);
                popitup($link.html(),$link.attr('href'))
            });

            // Submit validation
            $( '#send_invite_popup input[type=submit]' ).unbind( 'click' ).click( function() {
                var form = $( this ).parents( 'form' );
                var error = [];

                // Disable buttons + enable loading icon
                $( this ).prop( 'disabled', true );
                form.find( 'img.loading' ).show();

                $( '#send_invite_popup .errors' ).hide();
                $( '#send_invite_popup .error' ).removeClass( 'error' );

                if ( ! $( '#send_invite_popup input[name=patient_type]' ).is(':checked') ) {
                    error.push(' patient type');
                    $( '#send_invite_popup input[name=patient_type]' ).addClass( 'error' );
                }
                if ( ! $( '#send_invite_popup input[name=source_name]' ).val().length ) {
                    error.push(' name');
                    $( '#send_invite_popup input[name=source_name]' ).addClass( 'error' );
                }

                if ( ! $( '#send_invite_popup input[name=target_email]' ).share_is_email() ) {
                    error.push(' email');
                    $( '#send_invite_popup input[name=target_email]' ).addClass( 'error' );
                }

                if ( $( '#send_invite_popup .error' ).length === 0 ) {
                    // AJAX send the form
                    $.ajax( {
                        url: form.attr('action'),
                        type: 'POST',
                        data: form.serialize(),
                        success: function( response ) {
                            form.find( 'img.loading' ).hide();
                            if ( response === '1' || response === '2' ) {
                                $( '#send_invite_popup .errors-1 p' ).html('Invite was not sent - check if all the fields are correct!').show();
                                form.find( 'input[type=submit]' ).removeAttr( 'disabled' );
                                form.find( 'a.sharing_cancel' ).show();

                                if ( 'object' === typeof Recaptcha && 'function' === typeof Recaptcha.reload ) {
                                    Recaptcha.reload();
                                }
                            }
                            else if ( response === '3' ) {
                                $( '#send_invite_popup .errors-1 p' ).html('Invite was not sent - you have already sent one!').show();
                            }
                            else {
                                $send_invite_popup.append( response );
                            }
                        }
                    } );

                    return false;
                }

                form.find( 'img.loading' ).hide();
                form.find( 'input[type=submit]' ).removeAttr( 'disabled' );
                $( '#send_invite_popup .errors-1 p' ).html('Invite was not sent - check'+error.join()+'!');
                $( '#send_invite_popup .errors-1' ).show();

                return false;
            } );
            $( '#send_invite_popup input').on('focus', function(){
                $(this).removeClass('error');
            })
        // $('.see_another_doctor').on('click', function(e){
        //     e.preventDefault();
