    $(function(){
        // bind change event to select
        $('body').on('change', '#state_located', function () {
            var url = $(this).val(), // get selected value
                baseUrl = "http://www.md.com/doctors//";

            if (url) { // require a URL
                window.location = baseUrl+url.toLowerCase()+'/~/~/telemedicine'; // redirect
            }
            return false;
        });
    });
