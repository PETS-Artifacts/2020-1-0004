<script type="text/javascript">
    (function( $ ){

        
        
        
        
        
        
        
        
        
                // PLUGIN TO PULSE A CLASS ON AND OFF AN ELEMENT
        $.fn.pulseClass = function(className, pulseTime, waitBefore, waitAfter) {
            $(this).each(function(){
                $that = $(this);
                (function doPulse() {
                    // DETERMINE HOW LONG FOR EACH HALF
                    toggleTime = Math.round(pulseTime/4);
                    // WAIT AS NEEDED, THEN TOGGLE CLASS OFF AND ON
                    $that.delay(waitBefore).
                        toggleClass(className, toggleTime).
                        toggleClass(className, toggleTime).
                        toggleClass(className, toggleTime).
                        toggleClass(className, toggleTime);
                    // CALL SELF AFTER APPROPRIATE TIME
                    setTimeout(doPulse, pulseTime + waitAfter);
                })();
            });
            return this;
        };
        // DO THE PULSE ON ALREADY A MEMBER
        $('.mainsite .already_member').pulseClass('already_member_pulse', 2000, 2500, 2500);
            })( jQuery );
</script>

