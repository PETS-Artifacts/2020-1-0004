	<script type="text/javascript">
		(function() {
			if (document.querySelector('html').getAttribute('class').indexOf('no-touch') === -1) {
				var topNav = document.querySelectorAll('.nav__tier--1 .js-dropdown-trigger > .lnk');
				var search = document.querySelector('#global-search-submit');
				for (var x = 0; x < topNav.length; x++) {
					topNav[x].ontouchend = function(event) {
						event.preventDefault();
						var scope = document.querySelector('#' + event.currentTarget.id).parentNode;
						var link = document.querySelector('#' + event.currentTarget.id);
						if (scope.getAttribute('class').indexOf('open') > -1) {
							window.location.href = link.getAttribute('href');
						} else {
							var openTopNav = document.querySelector('.js-dropdown-trigger.open');
							if (openTopNav) {
								openTopNav.setAttribute('class', 'js-dropdown-trigger');
							}
							scope.setAttribute('class', 'js-dropdown-trigger open');
							return false
						}
					}
				}
			}
		})();
	</script>
