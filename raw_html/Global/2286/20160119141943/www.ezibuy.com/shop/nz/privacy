dataLayer=[];


getCustomerInfoForAnalytics = function(userId,customerState,email){
	
			dataLayer.push({
				"customerState" : customerState,
				"userId" : userId,
			 });	 
			getNanigansInfoForAnalytics(userId,email);
		
}

getNanigansInfoForAnalytics = function(userId,email){
	
	function eventData()
	{
		if(email!=null && email!='' && userId!='' && userId!=null)
		{ 
			this.ut1 = CryptoJS.SHA256(email).toString();
	    } 
		
		if(userId!='' && userId!=null)
			{
					this.userId = userId;
			}
	}	
	data = new eventData();
	dataLayer.push(data);
}

pushViewProductDetailsToNanigans =function(productCode){
	data.event = 'viewProduct';
	data.sku = productCode;
}
	
pushQuickViewProductDetailsToNanigans =function(productName,productCode,categoryName){
	dataLayer.push({
        'pageType' : 'product-quick-view',
        'name' : productName,
        'stylenumber' : productCode,
        'category' : categoryName,
  });

}

pushQuickOrderDataInfoToNanigans = function(quickOrderProductsArray){
	data.event = 'addToCart';
	data.addedProducts = quickOrderProductsArray;
	dataLayer.push(data);
}

pushRegistrationDetailToNanigans = function(){
	data.event = 'newEmail';
	dataLayer.push(data);
}

pushAddToCartInfoToAnalytics = function(sku,qty,price,productname,brand,variant,category){
	
	var price=qty * price;
	dataLayer.push({
        'event': 'addToCart',
        'sku' : sku,
        'value' : price.toFixed(2),               
        'qty' : qty,
        'ecommerce':{
  	      'add':{
  	         'products':[
  	            {
  	               'name':productname,
  	               'id':sku,
  	               'price':price.toFixed(2),
  	               'brand':brand,
  	               'category':category,
  	               'variant':variant,
  	               'quantity':qty
  	            }
  	         ]
  	      }
  	   }
  });

}

pushRemoveFromCartInfoToAnalytics = function(sku,qty,price,productname,brand,variant,category){
	dataLayer.push({ 
		'event': 'removeFromCart',
		'ecommerce':{ 
			'remove': {  
				'products': [{ 
					'name': productname, 
					'id': sku, 
					'price': price, 
					'brand': brand, 
					'category': category, 
					'variant': variant, 
					'quantity': qty 
				}] 
			} 
		} 
	});
	
}



pushCheckoutInfoToAnalytics = function(step,productArray,impressionsArray,eventCallback){
	var virtualUrl = "";
	if(undefined != step && eventCallback =='https://www.ezibuy.com/shop/nz/ezicheckout'){
		if(step ==1){
			virtualUrl = "/customerstate/acdn_shopping_bag";
		}else if(step ==2){
			virtualUrl = "/customerstate/acdn_about_you";
		}else if(step ==3){
			virtualUrl = "/customerstate/acdn_delivery";
		}else if(step ==4){
			virtualUrl = "/customerstate/acdn_payment";
		}else if(step ==5){
			virtualUrl = "/customerstate/acdn_review";
		}
	}
	if(undefined != impressionsArray && impressionsArray.length >0){
		dataLayer.push({
			'event': 'checkout',
			'virtualUrl':virtualUrl,
			'ecommerce':{ 
				'checkout':{
					'actionField':{'step':step},
					'products' :productArray,
					'impressions':impressionsArray
				}
			}/* ,
			'eventCallback':function(){
				document.location = eventCallback;
			} */
		});
	}else{
		dataLayer.push({
			'event': 'checkout',
			'virtualUrl':virtualUrl,
			'ecommerce':{ 
				'checkout':{
					'actionField':{'step':step},
					'products' :productArray
				}
			}/* ,
			'eventCallback':function(){
				document.location = eventCallback;
			} */
		});
	}
	
}

pushUpdateToCartInfoToAnalytics = function(productName,categoryName,sku,qty,price){
	var price=qty * price;
	dataLayer.push({
        'event': 'addToCart',
        'name' : productName,
        'category' : categoryName,
        'sku' : sku,
        'value' : price.toFixed(2),               
        'qty' : qty                     
  });

}

pushSearchResultsDataToAnalytics = function(resultString){
	dataLayer.push({
	       'event': 'searchResults',
	       'searchEventCategory': resultString
	});
}

pushAddToWishlistDataToAnalytics = function(code){
	dataLayer.push({
	       'event': 'AddToWishlist',
	       'styleNumber': code
	});
}

pushRemoveFromWishlistDataToAnalytics = function(code){
	dataLayer.push({
	       'event': 'RemoveFromWishlist',
	       'styleNumber': code
	});
}

pushCategoryDetailsToAnalytics = function(categoryProductArray){
	dataLayer.push({
	      'catProduct': categoryProductArray
	});
}  

pushCartDataInfoForAnalytics = function(cartEntryData,criteoCartEntryData){
	dataLayer.push({
		"cartProducts" : cartEntryData,
		"criteoItems": criteoCartEntryData
	 });	
}

pushCheckoutEventToAnalytics = function(stepName){	
	if(typeof customerDetails != "undefined" && customerDetails != null && customerDetails != 'undefined' && customerDetails != '')
	{	
	var virtualURL = "/"+customerDetails["customerState"]+"/"+stepName;
	dataLayer.push({
		"event" : "virtualPageview",
		"virtualURL" : virtualURL,
	 });
	}
}


