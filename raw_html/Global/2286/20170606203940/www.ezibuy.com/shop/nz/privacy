pushCheckoutEventToAnalytics = function(stepName,cartProductsArray){
	
	if(typeof customerDetails != "undefined" && customerDetails != null && customerDetails != 'undefined' && customerDetails != '')
	{	
	var virtualURL = "/"+customerDetails["customerState"]+"/"+stepName;
	if(cartProductsArray != null){
	dataLayer.push({
		"event" : "virtualPageview",
		"virtualURL" : virtualURL,
		'addedProducts':cartProductsArray
	 });
	}
	else{
		dataLayer.push({
			"event" : "virtualPageview",
			"virtualURL" : virtualURL,
	});
	}
	}
}

pushInternalPromotionsForAnalytics = function(id,name,creative,position,url,type){
	if(id != '' || name != ''){
		dataLayer.push({
			'event': 'promotionClick', 
			'ecommerce': {
				'promoClick': {
					'promotions': [ {
						'id': id, // Name or ID is required.
						'name': type+'-'+name, 
						'creative': creative, 
						'position': 'slot'+position 
						}] } 
		}, 
		'eventCallback': function() {
			document.location = url; 
			} 
		});
	}
}

pushProductImpressionsForAnalytics = function(impressions,eventName){
	try
	{	
		if(event == null || event == '')
		{	
			dataLayer.push({
				'event':'productImpressions',
				'ecommerce': {
				'impressions': impressions,
			}});
		}
		else
		{
			dataLayer.push({
				'event' : eventName,
				'ecommerce': {
				'impressions': impressions,
			}});
		}
	}catch(e)
	{
		console.log("Error while pushing product impressions to analytics")
	}
}

pushProductImpressionsForAnalyticsSelectedTab = function(impressions,eventName,selectedTab){
	try
	{	
		if(event == null || event == '')
		{	
			
			dataLayer.push({
				'event':'productRecosTab',
				'selectedTab' : selectedTab,
				'ecommerce': {
				'impressions': impressions,
			}});
		}
		else
		{
			
			dataLayer.push({
				'event' : eventName,
				'selectedTab' : selectedTab,
				'ecommerce': {
				'impressions': impressions,
			}});
		}
	}catch(e)
	{
		console.log("Error while pushing product impressions to analytics")
	}
}

flushAnalyticsDataLayer = function()
{
	dataLayer = [];	
}


