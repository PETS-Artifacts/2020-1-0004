<script>

dataLayer=[];

pushPromoDetailsForAnalytics = function(name,position,url){
	dataLayer.push({
					'event': 'promotionClick', 
					'ecommerce': {
						'promoClick': {
							'promotions': [ {
								'name': name, 
								'position': 'slot'+position 
								}] } 
				} ,
				'eventCallback': function() {
					document.location = url;
					} 
				});	
	}
	
pushProductsListForAnalytics = function(list){

		dataLayer.push({
			'ecommerce': {
			'promoView': {
			'promotions': list 
			}
			}
			});	
}
pushFirstThreeCatProductsListForAnalytics = function(list){
	dataLayer.push({
		'catProductList': list 
	});
	
}
getCustomerInfoForAnalytics = function(userId,customerState,email){
	
			dataLayer.push({
				"customerState" : customerState,
				"userId" : userId,
			 });	 
			getNanigansInfoForAnalytics(userId,email);
		
}

getNanigansInfoForAnalytics = function(userId,email){
	
	function eventData()
	{
		if(email!=null && email!='' && userId!='' && userId!=null)
		{ 
			this.ut1 = CryptoJS.SHA256(email).toString();
	    } 
		
		if(userId!='' && userId!=null)
			{
					this.userId = userId;
			}
	}	
	data = new eventData();
	dataLayer.push(data);
}

pushViewProductDetailsToNanigans =function(productCode){
	data.event = 'viewProduct';
	data.sku = productCode;
}
	
pushQuickViewProductDetailsToNanigans =function(productName,productCode,categoryName){
	dataLayer.push({
        'pageType' : 'product-quick-view',
        'name' : productName,
        'stylenumber' : productCode,
        'category' : categoryName,
  });

}

pushQuickOrderDataInfoToNanigans = function(quickOrderProductsArray,productsArray){
	dataLayer.push({
		'event': 'addToCart',
		'addedProducts':quickOrderProductsArray,
		'ecommerce': {
		'add' :{
			'products':productsArray
			}
		}
	});
}

pushRegistrationDetailToNanigans = function(){
	data.event = 'newEmail';
	dataLayer.push(data);
}

pushAddToCartInfoToAnalytics = function(sku,qty,price,productname,brand,variant,category,standalone){
	var price = parseFloat(price);
	var eventname = 'addToCart';
	if(standalone == true)
	{
		eventname = 'catalogue_quickview_addToCart';
	}
	dataLayer.push({
        'event': eventname,
        'sku' : sku,
        'value' : price.toFixed(2),               
        'qty' : qty,
        'ecommerce':{
  	      'add':{
  	         'products':[
  	            {
  	               'name':productname,
  	               'id':sku,
  	               'price':price.toFixed(2),
  	               'brand':brand,
  	               'category':category,
  	               'variant':variant,
  	               'quantity':qty
  	            }
  	         ]
  	      }
  	   }
  });

}

pushRemoveFromCartInfoToAnalytics = function(sku,qty,price,productname,brand,variant,category){
	var price = parseFloat(price);
	dataLayer.push({ 
		'event': 'removeFromCart',
		'ecommerce':{ 
			'remove': {  
				'products': [{ 
					'name': productname, 
					'id': sku, 
					'price': price, 
					'brand': brand, 
					'category': category, 
					'variant': variant, 
					'quantity': qty 
				}] 
			} 
		} 
	});
}



pushCheckoutInfoToAnalytics = function(step,productArray,impressionsArray,eventCallback){
	if(undefined != impressionsArray && impressionsArray.length >0){
		dataLayer.push({
			'event': 'checkout',
			/* 'virtualUrl':virtualUrl, */
			'ecommerce':{ 
				'checkout':{
					'actionField':{'step':step},
					'products' :productArray,
					'impressions':impressionsArray
				}
			}/* ,
			'eventCallback':function(){
				document.location = eventCallback;
			} */
		});
	}else{
		dataLayer.push({
			'event': 'checkout',
			'ecommerce':{ 
				'checkout':{
					'actionField':{'step':step},
					'products' :productArray
				}
			}/* ,
			'eventCallback':function(){
				document.location = eventCallback;
			} */
		});
	}
}

pushUpdateToCartInfoToAnalytics = function(productName,categoryName,sku,qty,price,brand,variant){
	var price = parseFloat(price);
	dataLayer.push({
        'event': 'addToCart',
        'name' : productName,
        'category' : categoryName,
        'sku' : sku,
        'value' : price.toFixed(2),               
        'qty' : qty,
        'ecommerce':{
    	      'add':{
    	         'products':[
    	            {
    	               'name':productName,
    	               'id':sku,
    	               'price':price.toFixed(2),
    	               'brand':brand,
    	               'category':categoryName,
    	               'variant':variant,
    	               'quantity':qty
    	            }
    	         ]
    	      }
    	   }                     
  });
}

