<script type="text/javascript">//<![CDATA[
  $(function() {
    var cookie = $.cookie('_ticketinfo');
    cookie = $.parseJSON(cookie);
    var is_updating_cookie = false;
    var has_updated_cookie = false;
    
              var in_purchase_process = false;
          
        var display_header = function() {

      if (cookie && cookie.nb_tickets > 0) {
        $('#pageAlertWrapper .jsNbTickets').html(__('{nb_tickets} items', { 'nb_tickets': cookie.nb_tickets}));
        $('#pageAlertWrapper .jsValue').html(__('${value}', { 'value': _format_currency(cookie.value)}));
        $('#pageAlertWrapper .jsPaymentUrl').attr('href', cookie.payment_url);

        if (!$('#pageAlertWrapper.ticketingTimer').is(':visible')) {
          $('body').animate({ paddingTop: 50 }, 300, null, function() {
            $('#pageAlertWrapper.ticketingTimer').show();
          });
        }
      }
    };

    var hide_header = function() {
      
      if ($('#pageAlertWrapper.ticketingTimer').is(':visible')) {
        $('body').animate({ paddingTop: 0 }, 300, null, function() {
          $('#pageAlertWrapper.ticketingTimer').hide();
          $('.jsTimeRemaining').hide();
          $('.jsCartHeader').show();
        });
      }
    };


    var show_timer = 300000;
    var show_notice = Math.floor(show_timer / 2);
    var show_warning = Math.floor(show_timer / 3);

        var update_cookie = function(cookie) {

      cookie = $.cookie('_ticketinfo');
      cookie = $.parseJSON(cookie);

      if (cookie && typeof(cookie.nb_tickets) !== 'undefined') {
        
        cookie.expire_time = cookie.expire_time + cookie.client_clock_delta;
      } else {
        cookie = false;
      }
      
      return cookie;
    };
    
    var update_cookie_from_server = function(cookie) {
      if (!is_updating_cookie && typeof(cookie.info_url) !== 'undefined') {
        is_updating_cookie = true;
        $.get(cookie.info_url + '?client_time=' + Math.floor(new Date().getTime()/1000)); 
      }
    }

    var ticketingTimer = function() {

      cookie = update_cookie(cookie);
      var re_run = 10000;
      if (cookie) {
        
        var time_remaining = (cookie.expire_time * 1000) - new Date().getTime();
        
        re_run = time_remaining > 300000 ? 10000 : 1000;

        var minutes = new Number(time_remaining/60000 - 0.5).toFixed(0);
        var seconds = new Number((time_remaining/1000)%60 - 0.5).toFixed(0);

        if (seconds.length == 1) seconds = '0' + seconds;
        if (seconds <= 0) seconds = '00';
        if (minutes <= 0) minutes = '0';

        $('#countDown').text(minutes + ':' + seconds);

        if (time_remaining > show_timer) {
          if (in_purchase_process) {
            hide_header();
          }
        } else {
          
          update_cookie_from_server(cookie);
          $('.jsTimeRemaining').show();
          $('.jsCartHeader').hide();

          display_header();

          if (time_remaining < 0) {
            $('#pageAlertInner').css('background-color', 'red');
            $('#countDownText').html(__('Sorry, your purchase has timed out. <a href="{url}">Start again?</a>', { 'url': cookie.reset_url}));

            $.removeCookie('_ticketinfo', { path: '/' });
            //window.location = cookie.reset_url;

            return;
          } else if (time_remaining < show_warning) {
            $('#pageAlertInner').css('background-color', '#f26522');
            $('#countDownText').text(__('Quick! Time is running out to complete your purchase.', {}));
          } else if (time_remaining < show_notice) {
            $('#countDownText').text(__('Please complete your purchase soon.', {}));
          }

        }
      } else {
        hide_header();
      }

      setTimeout(ticketingTimer, re_run);
    }

    ticketingTimer();
    if (!in_purchase_process) {
      display_header();
    }
    
  });
//]]></script>
