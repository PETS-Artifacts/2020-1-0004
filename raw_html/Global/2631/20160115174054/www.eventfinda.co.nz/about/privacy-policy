//<![CDATA[
  $(document).ready(
    function () {

      $('#email_status').parents('.formRow, .form-row, .form-field').addClass('');
      
      $('#email').bind('blur', 
        function () {
          $.ajax( {
                    type: 'POST', 
                    dataType: 'html', 
                    data: 'value=' + escape($('#email').val()).replace('+', '%2B') 
                      + '&field=email'
                                                                ,
                    url: '/register/check',
                    success: function (i, textStatus, jqXHR) { 
                      var field = $('#email_status').parents('.formRow:first, .form-row:first, .form-field:first');

                      if (i.indexOf("validation-incorrect") >= 0) {
                        $('#email_status').html(i);
                        $(field).addClass("error");
                        $(field).removeClass("success");
                      } else if (i.indexOf("validation-correct") >= 0) {   
                        $(field).removeClass("error");
                        $(field).addClass("success");
                      }

                      if (jqXHR.getResponseHeader('X-Eventfinder-Activated')) {
                        $login_form = $('#jsModalRegistration form');

                        if ($login_form.length > 0 && typeof(jsModalRegistrationLoginModeFunc) == 'function') {
                          if (jqXHR.getResponseHeader('X-Eventfinder-Registered-Fb')) {
                                                        return true;
                          } else {
                            if (jsModalRegistrationLoginModeFunc($login_form)) {
                              return true;
                            }
                          }
                        }
                      }

                    }
                  }
          ) 
        }
      );   
    }
  );
