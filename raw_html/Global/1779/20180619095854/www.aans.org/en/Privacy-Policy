<script type="text/javascript">
function processForm(e) {
    if (e.preventDefault) e.preventDefault();

    var textinput = document.getElementById("userQuery");
    var searchterm = textinput.value;
    window.location.assign("http://www.aans.org/Search?userQuery=" + searchterm)

    return false; // Block form
}
$('#userQuery').keypress(function(e){
	if(e.which == 13){
		processForm();
	}
});


$('#submit').click(processForm);
</script>
