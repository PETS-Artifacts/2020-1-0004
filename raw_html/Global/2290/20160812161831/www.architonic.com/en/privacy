<script type="text/javascript">/*privacy*/window.addEventListener("load", function(event) { 
  jQuery('a.subscribe-modal-link').click(function(e) {
    e.preventDefault();
    e.stopPropagation();
    
    jQuery('div#subscribe-modal').foundation('reveal', 'open', {
        method: 'GET',
        url: jQuery(this).data('reveal-ajax'),
        data: {
          Newsletter : {
            email: jQuery('input#subscription-email-field').val()
          }
        }
    });
    
  });
  
  jQuery('form#subscribe-newsletter-form').submit(function(e) {
    e.preventDefault();
    e.stopPropagation();
    
    jQuery('div#subscribe-modal').foundation('reveal', 'open', {
        method: 'GET',
        url: jQuery(this).data('reveal-ajax'),
        data: {
          Newsletter : {
            email: jQuery('input#subscription-email-field').val()
          }
        }
    });
    
  });
        
});</script>
<script type="text/javascript">/*privacy*/window.addEventListener("load", function(event) { jQuery(document).ready(function () {
jQuery('#search-input').autocomplete({"source":"https:\/\/www.architonic.com\/en\/search-autocomplete","autoFill":true,"select":function( event, ui ) {
                  if(ui.item.href) {
                    window.location.href = ui.item.href;
                  }
                 },"open":function( event, ui ) {
                  //the following line resolves problem with single tap in iOS Safari  
                  $('.ui-autocomplete').off('menufocus hover mouseover mouseenter'); 
                  
                  $('.search-menu').css('display', 'none');
                  $('.ui-autocomplete-category').removeClass('ui-menu-item').removeAttr('id');
                 },"close":function( event, ui ) {
                  $('.search-menu').css('display', 'none');
                 },"response":function( event, ui ) {
                  jQuery('input#search-result-ready').attr('data-init', '1');
                 }});

        jQuery('#search-input').data('ui-autocomplete')._renderItem = function( ul, item ) {            
          var el = jQuery('<li>');
          if(item.category) {
            el.addClass('ui-autocomplete-category ' + item.css_li)
              .append('<i></i> '  + item.label);
          } else {
            el.addClass(item.css_li);
            // create item link
            var link = jQuery('<a>'); 
            
              // add class to trigger prediction log request
              link.addClass('prediction-item');
              // add prediction related data attributes
              link.attr({
              'data-item-id': item.itemId, 
              'data-item-type': item.type, 
              'data-navi-search-phrase': item.search, 
              'data-log-type': item.logType, 
              'data-area-id': item.areaId
              });
             
            link.attr({'href': item.href, 'data-value': item.value});
            if (item.image && item.image.length > 1) {
              link.append('<img src="' + item.image + '"/>');
            } 
            link.append('<span>' + item.label + '</span>');
            
            el.append(link); 
          }
          return el.appendTo( ul );
        }
});
});</script>  <div id="isSmallDevice" class="show-for-small-only"><!-- This div is visible only for small screens --></div>

  
