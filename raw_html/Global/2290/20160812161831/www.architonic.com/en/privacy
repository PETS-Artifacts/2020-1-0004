    <li class="current unavailable" property="itemListElement" typeof="ListItem">
    <a property="item" typeof="WebPage" href="https://www.architonic.com/en/privacy" >
      <span property="name">
        Privacy      </span>
    </a> 
    <meta property="position" content="0" />
  </li>
</ul>
