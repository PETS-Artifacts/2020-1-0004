        var currentPage = window.location.href;
        var addthis_share = {
            url: currentPage,
            templates: {
                           twitter: '{{title}} {{url}} via @USGSA'
                       }
        }
        var addthis_config = {
            services_compact:'facebook, twitter, favorites, email'
        }

        jQuery(document).ready(function() {
            var removeClass = false;

            if ( (jQuery('.table').length > 0 && (jQuery('.table').hasClass('dataTable') || jQuery('.table').hasClass('plainTable')))){

                removeClass = true;

                //jQuery('.table').addClass('stripe');
                jQuery('.table').removeClass('table');
            }

            if (jQuery("div.feature-block").length > 0 && jQuery(".feature-block").css("background-image").length > 0) {
                jQuery('div.feature-block > h1').remove();
                if (jQuery('div.feature-block:has(div.no-pad-container)')){
                    jQuery('div.feature-block > div.no-pad-container > h1').remove();
                    jQuery('h1').appendTo(jQuery('div.feature-block > div.no-pad-container'));
                }
                else {
                    // move h1 inside the block
                    jQuery('h1').appendTo(jQuery('div.feature-block'));
                }
                removeClass = true;
            }

            if (jQuery('h1').text() == "Explore by Timeline" || jQuery('.timeline-year').length > 0){
                removeClass = true;
            }

            if (removeClass){

                jQuery('.text-asset-container').removeClass('text-asset-container'); // it is breaking pages for these kind of pages
            }
            jQuery('.stripedTable').addClass('stripe');
            
        });

    </script>
