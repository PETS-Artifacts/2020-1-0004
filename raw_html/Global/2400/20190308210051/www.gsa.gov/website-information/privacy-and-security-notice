<!--//--><![CDATA[//><!--

                /* DevNote; This script comes from AjaxCache-Expry.php */
                function clearLocalStorageCacheIfNessesary() {

                    var css_js_query_string = 'pnyswj';

                    // Bail if this browser dosnt support local-storage
                    if ( typeof localStorage == 'undefined' ) return;

                    // Bail if there is no previously seen css_js_query_string
                    var last_css_js_query_string = localStorage.getItem('last_css_js_query_string');
                    localStorage.setItem('last_css_js_query_string', css_js_query_string);
                    if ( last_css_js_query_string == null ) return;

                    // If the css_js_query_string is the same as last time, no need to flush the cache
                    if ( last_css_js_query_string == css_js_query_string ) return;

                    // If we have not returned by this line, then it is time to flush the local-storage cache
                    localStorage.removeItem('featuredblocks');
                    localStorage.removeItem('gsanavigation');
                    console.log('Since the css_js_query_string, the local-storage cache has been cleared');

                };

                clearLocalStorageCacheIfNessesary();
            
//--><!]]>
</script>
