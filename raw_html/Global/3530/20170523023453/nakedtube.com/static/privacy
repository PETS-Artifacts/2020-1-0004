        var $sideBar = $('.sideBar');
        var $topBar = $('.topBar');

        $window.scroll(function(e) {

            if ($window.scrollTop() > 25) {
                $sideBar.addClass("fix-side-bar");
                $topBar.addClass("top-bar-fixed");
            } else {
                $sideBar.removeClass("fix-side-bar");
                $topBar.removeClass("top-bar-fixed");
            }

        });

