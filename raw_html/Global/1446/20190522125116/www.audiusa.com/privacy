          var s = document.getElementsByTagName("script")[0];
          tagjs.async = true;
          tagjs.src = "//s.btstatic.com/tag.js#site=" + site + "&referrer=" + encodeURIComponent(document.location.href) + "";
          s.parentNode.insertBefore(tagjs, s);
        }
      }());
</script>
</body>
</html>
