      (function () {
          var isHybridAppPage = window.location.pathname.indexOf('/myaudi-app') >= 0;
          // load script tag if not a myaudi-app page or if they have enabled tracking
          if ( !isHybridAppPage || '' != '1') {
          var site = "a525KK2";
          if(isHybridAppPage) {
            site = "EDJwMnN";
          }
