<script type="text/javascript">
	function getQuery(key, default_)
	{
	  if (default_==null) default_="";
	  key = key.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
	  var regex = new RegExp("[\\?&]"+key+"=([^&#]*)");
	  var qs = regex.exec(window.location.href);
	  if(qs == null)
	    return default_;
	  else
	    return qs[1];
	}	

	var pageProp11="";
	var regex = new RegExp("/x/");
	var qs = regex.exec(window.location.href);
	if(qs != null) {
		pageProp11 = "ShortURL:"+"Privacy Policy";
	}
	
	var srcVal = getQuery("source");
	if ( srcVal != "" )
	{
		pageProp11=srcVal + ":" + (pageProp11 == "" ? "Privacy Policy" : pageProp11 );
	}
</script>



