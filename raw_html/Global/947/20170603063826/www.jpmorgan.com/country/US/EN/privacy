// This function uses to track the traffic of client login box links 
function TrackClientLogonLinks(clientLoginName, currentPageLink, reportSuite)
{
	
	if(!(clientLoginName=="Institutional Asset Management" && reportSuite=="jpmassetmgmtIA"))
	{
		trackLinks(clientLoginName,currentPageLink,reportSuite);
	}
}

