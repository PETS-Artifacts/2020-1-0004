<script type="text/javascript">
	function getQuery(key, default_)
	{
	  if (default_==null) default_="";
	  key = key.replace(/[\[]/,"\\\[").replace(/[\]]/,"\\\]");
	  var regex = new RegExp("[\\?&]"+key+"=([^&#]*)");
	  var qs = regex.exec(window.location.href);
	  if(qs == null)
	    return default_;
	  else
	    return qs[1];
	}	

	var pageProp11="";
	var regex = new RegExp("/x/");
	var qs = regex.exec(window.location.href);
	if(qs != null) {
		pageProp11 = "ShortURL:"+"Privacy Policy";
	}
	
	var srcVal = getQuery("source");
	if ( srcVal != "" )
	{
		pageProp11=srcVal + ":" + (pageProp11 == "" ? "Privacy Policy" : pageProp11 );
	}
</script>
<script language="JavaScript">

	function jsTrack(whatKind, whatSpot)
	{
		jpmTrackMedia(trackVideoName, whatKind, whatSpot);
	}

	function fixTime(curTime)
	{
		return Math.round( curTime );
	}

	function jpmTrackMedia(curName, whatKind, whatSpot, playerName)
	{
		if ( playerName == null )
		{
		    playerName = "unknown player";
		}
		if ( (whatSpot == null) || (whatSpot == "") )
		{
		    whatSpot = "180";
		}
		whatSpot = fixTime( whatSpot );
		switch (whatKind) 
		{
			case "open" :
			case "opened" :
				s.Media.open(curName,whatSpot, playerName);
				s.Media.track(curName);
			break;

			case "started" :
				s.Media.play(curName,whatSpot);
				s.Media.track(curName);
			break;

			case "paused" :
				s.Media.stop(curName,whatSpot);
				s.Media.track(curName);
			break;

			case "stopped" :
				s.Media.stop(curName,whatSpot);
				s.Media.close(curName);
				s.Media.track(curName);
			break;

			default:
			break;
		}
	}

</script>

