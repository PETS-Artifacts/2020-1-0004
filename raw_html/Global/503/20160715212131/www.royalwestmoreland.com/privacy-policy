<script type="text/javascript">
	$(window).load(function(){
		$(".datepicker").datepicker({dateFormat:"dd-mm-yy"});

		var today = new Date();

		$(".datepickerStart").datepicker({
		    dateFormat: "dd-mm-yy",
		    minDate: today,
		    onSelect: function (dateText, inst) {
		        $(".datepickerEnd").datepicker("option", "minDate", new Date(inst.selectedYear, inst.selectedMonth, parseInt(inst.selectedDay) + 5));
		    }
		});

		$(".datepickerEnd").datepicker({
		    dateFormat: "dd-mm-yy",
		    minDate: new Date(today.getFullYear(), today.getMonth(), today.getDate() + 5)
		});

		//hide gallery button when there's only one slide
		if ($('#content-gallery .bread-holder').length > 0) { 
		    $('#fs-button').addClass('active');
		}
		else {
			$('#fs-button').addClass('disabled');
		}

		//golf map stuff
		$('#golfMapSlide .slide').hide();
		$('#golfMapSlide .slide:first').show();
		$('#controls a:first').addClass('active');
		 
		$('#controls a').click(function(){

			$('#controls a').removeClass('active');
			$(this).addClass('active');
			var currentTab = $(this).attr('href');
			$('#golfMapSlide .slide').hide("fast");
			$(currentTab).show("slow");

	 	return false;

		});

	});
</script>





		
		
