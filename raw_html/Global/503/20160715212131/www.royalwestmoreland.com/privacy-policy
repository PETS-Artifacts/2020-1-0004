    <script>
        ('placeholder' in document.createElement('input')) || (function (inputs) {
            function blur(placeholder) {
                if (this.value === '') {
                    this.value = placeholder;
                }
            }
            function focus(placeholder) {
                if (this.value === placeholder) {
                    this.value = '';
                }
            }
            function bind() {
                var placeholder = this.getAttribute('placeholder');
                if (placeholder) {
                    this.onfocus = function () {
                        focus.call(this, placeholder);
                    }
                    this.onblur = function () {
                        blur.call(this, placeholder);
                    }
                    this.onblur();
                }
            }
            for (var i = 0, l = inputs.length; i < l; i++) {
                bind.call(inputs[i]);
            }
        }(document.getElementsByTagName('input')));
</script>

