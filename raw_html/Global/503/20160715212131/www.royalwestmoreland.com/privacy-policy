<script type="text/javascript">
        function customLightboxe() {
        
            Fat.bind(document.querySelector('.callBackCta'), 'click', function(e) {
                if (Fat.hasClass(document.querySelector('.lbinst-custom'), 'open')) {
                    Fat.removeClass(document.querySelector('.lbinst-custom'), 'open');
                } else {    
                    Fat.addClass(document.querySelector('.lbinst-custom'), 'open');
                };

                if (Fat.hasClass(document.querySelector('.lightbox-cover-custom'), 'open')) {
                    Fat.removeClass(document.querySelector('.lightbox-cover-custom'), 'open');
                } else {    
                    Fat.addClass(document.querySelector('.lightbox-cover-custom'), 'open');
                };
            });

            Fat.bind(document.querySelector('.lightbox-cover-custom'), 'click', function(e) {
                if (Fat.hasClass(document.querySelector('.lbinst-custom'), 'open')) {
                    Fat.removeClass(document.querySelector('.lbinst-custom'), 'open');
                    Fat.removeClass(document.querySelector('.lightbox-cover-custom'), 'open');
                };
            });

            Fat.bind(document.querySelector('.close-btn-custom'), 'click', function(e) {
        
                if (Fat.hasClass(document.querySelector('.lbinst-custom'), 'open')) {
                    Fat.removeClass(document.querySelector('.lbinst-custom'), 'open');
                } else {    
                    Fat.addClass(document.querySelector('.lbinst-custom'), 'open');
                }

                if (Fat.hasClass(document.querySelector('.lightbox-cover-custom'), 'open')) {
                    Fat.removeClass(document.querySelector('.lightbox-cover-custom'), 'open');
                } else {    
                    Fat.addClass(document.querySelector('.lightbox-cover-custom'), 'open');
                };
            });
        }

        (this.Fat || (this.Fat = [])).push(function () {
            Fat.bind(window, 'load', customLightboxe);
        });
</script>

                    

