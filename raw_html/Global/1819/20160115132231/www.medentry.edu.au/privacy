	function displayTopBarBookmarkGuide() {
		if(document.getElementById("topbookmarkpopup").style.display == "")
			document.getElementById("topbookmarkpopup").style.display = "none";
		else
			document.getElementById("topbookmarkpopup").style.display = "";
	}
