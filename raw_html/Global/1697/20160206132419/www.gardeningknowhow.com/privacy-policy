					jQuery.post(ajaxurl, dito, function(data) {
						if(data.retmobi){
							retmobi = data.retmobi;
							if(retmobi == "apple"){
								isapple = true; ismobi = true;
								if(isapple){
									jQuery("#topsocks .apple-icon, #footer .apple-icon").css({"display":"inline-block"});
									jQuery(".appleget").show();
								}
							}
							else if(retmobi == "all"){
								isapple = false;
								ismobi = true;
							}
							if(retmobi == "all" || retmobi == "apple"){
								/*window._mNHandle = window._mNHandle || {};
								window._mNHandle.queue = window._mNHandle.queue || [];
								medianet_versionId = "151199"; 
								(function() {
									var sct = document.createElement("script"),
									winObj = window.top || window,
									sctHl = winObj.document.getElementsByTagName("script")[0],
									isSSL = 'https:' == document.location.protocol;
									sct.type = "text/javascript";
									sct.src = (isSSL ? 'https:' : 'http:') + '//contextual.media.net/inslmedianet.js?cid=8CUV186OG' + (isSSL ? '&https=1' : '')+'&crid=935572598&size=249x300';
									sct.async = "async";
									sctHl.parentNode.insertBefore(sct, sctHl);
								})();*/							
							}
							else{
								/*window._mNHandle = window._mNHandle || {};
								window._mNHandle.queue = window._mNHandle.queue || [];
