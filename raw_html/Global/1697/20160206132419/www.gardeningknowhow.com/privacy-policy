							}
						}
						else{retmobi = "no"; retmobi = "no";}
						},"json"
					);

					//var retmobi = "no";
					//isapple = retmobi =="apple"?true:false;
					
					
					var qtop = jQuery("#askq").position();
					var qhite = jQuery("#askq").outerHeight();
					var ahite = qhite;
					jQuery(window).scroll(function(){
						var classtoad = "stickynab";						
												if(jQuery(this).scrollTop() > qtop.top){
							jQuery("#askq").addClass(classtoad);
							if(isapple){
							jQuery(".appleget").addClass("stickyapple");
							jQuery(".appleget").css({"top": ahite + "px"});
							jQuery("#noqnomo").css({"height": (qhite + 35) + "px"});
							}else{
								jQuery("#noqnomo").css({"height": qhite + "px"});
							}							
						}else{
							jQuery("#askq").removeClass(classtoad);
							if(isapple){
								jQuery(".appleget").removeClass("stickyapple");
								//jQuery(".appleget").removeAttr("style");
							}
							jQuery("#noqnomo").removeAttr("style");
						}
						
					});
				});
