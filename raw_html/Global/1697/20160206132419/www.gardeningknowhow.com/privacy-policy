<script>
var tmoot;
jQuery(document).ready(function(){
	tmoot = setTimeout(changeSource, 2000);	
});
function changeSource(){
	clearTimeout(tmoot);
	jQuery("input.gsc-search-button").attr("src", "http://www.gardeningknowhow.com/wp-content/themes/yourgarden/imgs/blankicon.png");
}
