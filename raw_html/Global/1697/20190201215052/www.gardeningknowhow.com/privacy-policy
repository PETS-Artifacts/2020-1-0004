function doMobileStuff(){
	md = new MobileDetect(window.navigator.userAgent);
	if(md.mobile()){
		ismobi = true;
		var opSys = md.os();
		if(opSys == "iOS"){
			isapple = true;
			isandroid = false;
			//jQuery("#topsocks .apple-icon, #footer .apple-icon").css({"display":"inline-block"});
			//jQuery(".appleget").show();
			//jQuery(".androdget").hide();
		}/*AndroidOS, iOS*/
		if(opSys == "AndroidOS"){
			isapple = false;
			isandroid = true;
			//jQuery("#topsocks .android-icon, #footer .android-icon").css({"display":"inline-block"});
			//jQuery(".appleget").hide();
			//jQuery(".androdget").show();
		}/*AndroidOS, iOS*/
		/*doSydicate(false);*/
		//jQuery("#menu-main-menu").prepend(jQuery("#askq"));
			}else{
		//doSydicate(true);
	}
	
	var qtop = jQuery("#stickfromhere").position();
	var qhite = jQuery("#askq").outerHeight();
	var ahite = qhite;
	jQuery(window).scroll(function(){
		if(ismobi || isapple || isandroid){return;}
		var classtoad = "stickynab";						
				var sT = jQuery(document).scrollTop();
		if(sT > qtop.top){
			if(jQuery(".iseighthundred").is(":visible")){
				jQuery("#askq").addClass(classtoad);
			}
			jQuery("#holdergcsearch").show();
			//jQuery("#askq").show();
			if(isapple){
				//jQuery(".appleget").addClass("stickyapple");
				//jQuery(".appleget").css({"top": ahite + "px"});
				jQuery("#noqnomo").css({"height": (qhite + 35) + "px"});
			}else if(isandroid){
				//jQuery(".androidget").addClass("stickyapple");
				//jQuery(".androidget").css({"top": ahite + "px"});
				jQuery("#noqnomo").css({"height": (qhite + 35) + "px"});	
			}else{
				jQuery("#noqnomo").css({"height": qhite + "px"});
			}	
			jQuery(document).find("table.gstl_52.gssb_c").css({"top": (sT + qhite + 50) + "px"});/*gstl_50*/
		}else{
			if(jQuery(".iseighthundred").is(":visible")){
				jQuery("#askq").removeClass(classtoad);
			}
			jQuery("#holdergcsearch").hide();
			//jQuery("#askq").hide();
			jQuery(document).find(".gsc-input #gsc-iw-id1").removeClass("gsc-input-box-focus");
			jQuery(document).find("table.gstl_52.gssb_c").hide();/*gstl_50*/
			if(isapple){
				//jQuery(".appleget").removeClass("stickyapple");
				/*jQuery(".appleget").removeAttr("style");*/
			}else if(isandroid){
				//jQuery(".androidget").removeClass("stickyapple");
			}
			jQuery("#noqnomo").removeAttr("style");
		}
	
	});
	jQuery("#askq .within").keyup(function(){
		if(!jQuery(".iseighthundred").is(":visible")){
			var sTi = jQuery(document).scrollTop();
			var qht = jQuery("#askq").outerHeight();
			var sttop = jQuery("#stickfromhere").position();
			if(sTi == 0){
				jQuery(document).find("table.gstl_52.gssb_c").css({"top": (sttop.top + 60) + "px"});/*gstl_50*/
			}else{
				jQuery(document).find("table.gstl_52.gssb_c").css({"top": (sTi + qht) + "px"});/*gstl_50*/
			}
			//console.log(sTi + " :: " + sttop.top + "::" + qht);
		}
	});
}

function respondSidebarAd(){
	if(jQuery(".div980").is(":visible")){
		if(jQuery("#homernextoside > #homersidebar").length == 0){
			jQuery("#homersidebar").insertAfter("#homemidtisement");
		}
	}else{
		if(jQuery("#homernextoside > #homersidebar").length != 0){
			jQuery("#homersidebar").insertAfter("#homernextoside");
		}
	}
	
	if(jQuery(".issixfifty").is(":visible")){
		jQuery("body").addClass("js");
	}else{
		jQuery("body").removeClass("js");
	}
	
	
	if(!jQuery("#sibedarjq").is(":visible")){
		adtextwidg.appendTo("#widgets");
	}else{adtextwidg.prependTo("#widgets");}	
}

function addLink() {
    if(window.getSelection().containsNode(document.getElementsByClassName('entry-content')[0], true)){
		var body_element = document.getElementsByTagName('body')[0];
		var selection;
		selection = window.getSelection();
		var ln = selection.toString().split(' ').length;
		if(ln < 5){ 
			return;
		}
		var oldselection = selection
		
		var pagelink = "<br /><br /> Read more at Gardening Know How: Privacy Policy <a href='https://www.gardeningknowhow.com/privacy-policy'>https://www.gardeningknowhow.com/privacy-policy</a>"; 
		
		var copy_text = selection + pagelink;
		var new_div = document.createElement('div');
		new_div.style.left='-99999px';
		new_div.style.position='absolute';
		 body_element.appendChild(new_div );
		new_div.innerHTML = copy_text ;
		selection.selectAllChildren(new_div );
		window.setTimeout(function() {
			body_element.removeChild(new_div );
		},0);
	}
}
</script>
