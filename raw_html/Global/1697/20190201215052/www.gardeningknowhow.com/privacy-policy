<script type="text/javascript">
/*window.onload = function(){
	document.getElementById('gsc-i-id1').placeholder = 'Search...';
}*/
var inputSearchTimer = 0;
var inputSignupTimer = 0;
jQuery(document).ready(function(){
	jQuery("#newcheeseburger").click(function(){
		jQuery(".main-nav #menu-main-menu").slideToggle("fast");
	});
	jQuery(".topsearch").click(function(){
		jQuery("#holdergcsearch").slideToggle("fast").promise().done(function(){
			if(jQuery(this).is(":visible")){
				if(inputSearchTimer === 0){
					startSearchInputTimer();
				}else{
					resetSearchInputTimer();
				}
			}else{
				stopSearchInputTimer();
			}
		});
	});
	jQuery(".topupdates").click(function(){
		jQuery("#madmimiblock").slideToggle("fast").promise().done(function(){
			if(jQuery(this).is(":visible")){
				if(inputSignupTimer === 0){
					startSignupInputTimer();
				}else{
					resetSignupInputTimer();
				}
			}else{
				stopSignupInputTimer();
			}
		});
	});
	jQuery("#madmimiblock input").keydown(function(evt){
		resetSignupInputTimer();
	});
	
	/*jQuery("#pseudosearchbtn").click(function(){
		jQuery(".thesform button.gsc-search-button").click();
	});*/
	jQuery("#askq").keypress(function(){
		//setTimeout(changeSearchHints, 500);
		exitSearchInputTimer();
	});
	/*jQuery(".adnoshowsignup .clss").click(function(){
		hidePopup();
	});*/
});
function footSearchPage(){
	var q = jQuery("#footsearch").val();
	window.location.href = "https://www.gardeningknowhow.com/search/q=" + q;
}
var curelm;
function dosgnup(elm){
	var axrel = "https://www.gardeningknowhow.com/wp-admin/admin-ajax.php";
	curelm = elm.parents(".adnoshowsignup");
	var vl = curelm.find(".theirmmljoin").val();
	var lto = {action: 'doregemail', mml: vl};
	var mldoer = jQuery.ajax({
		type: "POST",
		url: axrel,
		data: lto,
		dataType: "json"
	});	
	mldoer.done(function (data, status, jqXHR){
		var elmhite = curelm.outerHeight();
		if(!data.respns){
			curelm.find(".responsemsg").html("Something went wrong. We are unable to process your subscription at this time. *noresponse").show();
			//curelm.css("height": elmhite + "px");
		}else{
			curelm.find(".responsemsg").html(data.respns).show();
			if(data.haserror){
				curelm.find(".responsemsg").click(function(){
					jQuery(this).html("");
					jQuery(this).hide();
				});
			}
			//curelm.css("height": elmhite + "px");
		}
		
	});
}
function changeSearchHints(){
	jQuery(document).find("table.gstl_52").css({"top": (jQuery(window).scrollTop() + jQuery("#askq").outerHeight()) + "px"});/*gstl_50*/
}

function startSignupInputTimer(){
	inputSignupTimer = window.setTimeout(stopSignupInputTimer, 10000);
}

function stopSignupInputTimer(){
	if(jQuery("#madmimiblock").is(":visible")){
		jQuery("#madmimiblock").slideUp("fast");
	}
	window.clearTimeout(inputSignupTimer);
	inputSignupTimer = 0;
}

function resetSignupInputTimer(){
	window.clearTimeout(inputSignupTimer);
	inputSignupTimer = window.setTimeout(stopSignupInputTimer, 10000);
}
function startSearchInputTimer(){
	inputSearchTimer = window.setTimeout(stopSearchInputTimer, 10000);
}

function stopSearchInputTimer(){
	if(jQuery("#holdergcsearch").is(":visible")){
		jQuery("#holdergcsearch").slideUp("fast");
	}
	window.clearTimeout(inputSearchTimer);
	inputSearchTimer = 0;
}
function exitSearchInputTimer(){
	window.clearTimeout(inputSearchTimer);
	inputSearchTimer = 0;
}
function resetSearchInputTimer(){
	window.clearTimeout(inputSearchTimer);
	inputSearchTimer = window.setTimeout(stopSearchInputTimer, 10000);
}

/*!
 * jQuery Cookie Plugin v1.4.1
 * https://github.com/carhartl/jquery-cookie
 *
 * Copyright 2013 Klaus Hartl
 * Released under the MIT license
 */
(function (factory) {
	if (typeof define === 'function' && define.amd) {
		// AMD
		define(['jquery'], factory);
	} else if (typeof exports === 'object') {
		// CommonJS
		factory(require('jquery'));
	} else {
		// Browser globals
		factory(jQuery);
	}
}(function ($) {

	var pluses = /\+/g;

	function encode(s) {
		return config.raw ? s : encodeURIComponent(s);
	}

	function decode(s) {
		return config.raw ? s : decodeURIComponent(s);
	}

	function stringifyCookieValue(value) {
		return encode(config.json ? JSON.stringify(value) : String(value));
	}

	function parseCookieValue(s) {
		if (s.indexOf('"') === 0) {
			// This is a quoted cookie as according to RFC2068, unescape...
			s = s.slice(1, -1).replace(/\\"/g, '"').replace(/\\\\/g, '\\');
		}

		try {
			// Replace server-side written pluses with spaces.
			// If we can't decode the cookie, ignore it, it's unusable.
			// If we can't parse the cookie, ignore it, it's unusable.
			s = decodeURIComponent(s.replace(pluses, ' '));
			return config.json ? JSON.parse(s) : s;
		} catch(e) {}
	}

	function read(s, converter) {
		var value = config.raw ? s : parseCookieValue(s);
		return $.isFunction(converter) ? converter(value) : value;
	}

	var config = $.cookie = function (key, value, options) {

		// Write

		if (value !== undefined && !$.isFunction(value)) {
			options = $.extend({}, config.defaults, options);

			if (typeof options.expires === 'number') {
				var days = options.expires, t = options.expires = new Date();
				t.setTime(+t + days * 864e+5);
			}

			return (document.cookie = [
				encode(key), '=', stringifyCookieValue(value),
				options.expires ? '; expires=' + options.expires.toUTCString() : '', // use expires attribute, max-age is not supported by IE
				options.path    ? '; path=' + options.path : '',
				options.domain  ? '; domain=' + options.domain : '',
				options.secure  ? '; secure' : ''
			].join(''));
		}

		// Read

		var result = key ? undefined : {};

		// To prevent the for loop in the first place assign an empty array
		// in case there are no cookies at all. Also prevents odd result when
		// calling $.cookie().
		var cookies = document.cookie ? document.cookie.split('; ') : [];

		for (var i = 0, l = cookies.length; i < l; i++) {
			var parts = cookies[i].split('=');
			var name = decode(parts.shift());
			var cookie = parts.join('=');

			if (key && key === name) {
				// If second argument (value) is a function it's a converter...
				result = read(cookie, value);
				break;
			}

			// Prevent storing a cookie that we couldn't decode.
			if (!key && (cookie = read(cookie)) !== undefined) {
				result[name] = cookie;
			}
		}

		return result;
	};

	config.defaults = {};

	$.removeCookie = function (key, options) {
		if ($.cookie(key) === undefined) {
			return false;
		}

		// Must not alter options, thus extending a fresh object...
		$.cookie(key, '', $.extend({}, options, { expires: -1 }));
		return !$.cookie(key);
	};

}));
window.addEventListener("load", function(){
window.cookieconsent.initialise({
  "palette": {
    "popup": {
      "background": "#efefef",
      "text": "#404040"
    },
    "button": {
      "background": "transparent",
      "text": "#79a642",
      "border": "#79a642"
    }
  },
  //"position": "bottom-left",
  "content": {
    "message": 'This website occasionally uses cookies to ensure you get the best experience on our website. By continuing to use our site you agree to our <a href="https://www.gardeningknowhow.com/terms-and-conditions/" target="_blank">Terms of Service</a> and our <a href="https://www.gardeningknowhow.com/privacy-policy/" target="_blank">privacy policy</a>',
	"link": '',
    "href": ""
  }
})});
</script>
