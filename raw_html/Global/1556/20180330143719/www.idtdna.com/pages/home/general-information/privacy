<script type="text/javascript">
$(document).ready(function() {
    $('.navbar-nav a').on('click', function() {
        $('#menu_sq').removeClass("in");
        $('#menu_sq').attr("aria-expanded", "false");
        $('.menuWrap .squashy a').addClass("collapsed");
        $('.menuWrap .squashy a').attr("aria-expanded", "false");
    });
    
    var hamburgerBtn = $('.megaWrap .navbar-toggle');
    $(hamburgerBtn).on('click', function() {
        var hmbClasses = $(hamburgerBtn).attr('class');
        
        if (hmbClasses.search('active') < 0) {
            $(this).addClass('active');
        } else {
            $(this).removeClass('active');
        }
    });
});
</script>




