        <script>
            $(function () {
                var headerLink = $("#profilenavanchor");
                headerLink.after("<img id=\"welcomeback-loader\" src=\"/_img/ico-geolocation-loading.gif\"/>");
                $.ajax({
                    type: "GET",
                    url: "/myaccount/rl/Login/GetSavedUsername",
                    success: function (data) {
                        $("#welcomeback-loader").hide();
                        if (data && 0 !== data.length && data.trim() && data.indexOf("Error 404") === -1) {
                            headerLink.text('Welcome Back!');
                            $('#previousloginname').text(data);
                            $('#auth-user-name').val(data);
                            $('#header-nav-profile .accordian-item').hide();
                            $('#header-nav-profile .block-link').hide();
                            $('#header-nav-profile .recentvisitorlink').show();
                        }
                    },
                    error: function (data) {
                        $("#welcomeback-loader").hide();
                    }
                });

                $("#resetlogin")
                    .click(function (event) {
                        headerLink.text('Login / Register');
                        $('#header-nav-profile .accordian-item').show();
                        $('#header-nav-profile .block-link').show();
                        $('#header-nav-profile .recentvisitorlink').hide();
                    });

                $("#login-button").click(function () { $(this).parents('form:first').submit(); });

                // funky because of requireJS
                if (window.location.href.indexOf("ERROR_CODE=0x132120c8") > -1) {
                    $(window)
                        .load(function () {
                            setTimeout(function () {
                                $('#login-error-box').text('You entered invalid credentials.');
                                $('#profilenavanchor').click();
                            },
                                500);
                        });
                }

            });
        </script>


    <input id="baseUrl" name="baseUrl" type="hidden" value="/myaccount/" /> 
    

