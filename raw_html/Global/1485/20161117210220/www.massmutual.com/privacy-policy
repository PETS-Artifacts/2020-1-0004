    <section class="main-page-header-outer">
        <div class="main-page-header-image">
            <img alt="" data-src="/-/media/upload/bg-default-article-hero-3200-600.jpg?w=1600" data-src_medium="/-/media/upload/bg-generic-hero-2000-600.jpg?w=1000" data-src_medium_retina="/-/media/upload/bg-generic-hero-2000-600.jpg?w=2000" data-src_small="/-/media/upload/bg-generic-hero-2000-600.jpg?w=500" data-src_small_retina="/-/media/upload/bg-generic-hero-2000-600.jpg?w=1000" data-src_standard="/-/media/upload/bg-default-article-hero-3200-600.jpg?w=1600" data-src_standard_retina="/-/media/upload/bg-default-article-hero-3200-600.jpg?w=3200" />
