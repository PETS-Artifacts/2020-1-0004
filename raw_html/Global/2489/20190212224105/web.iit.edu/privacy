  <div class="block-content content"><script type="text/javascript">
(function(){

    var iFrames = null;

    function addEvent(element, evnt, funct){
        if (element.attachEvent) {
            return element.attachEvent('on'+evnt, funct);
        }
        else {
            return element.addEventListener(evnt, funct, false);
        }
    }

    function init(){
        iFrames = document.getElementsByTagName("iframe");
        checkFrameSizes();
        addEvent(window, "resize", checkFrameSizes);
    }

    function receiveMessage(event){
        // Check to make sure sender is a iit domain
        if (event.origin.indexOf("iit.edu") !== -1) {
            if (typeof event.data === 'string') {
                if (event.data === 'calcLoad') {
                    //console.log("frame load");
                    checkFrameSizes();
                }
            }
            var message = null;
            try {
                message = JSON.parse(event.data);
            } catch (e) {
                // error handling
            }
            if (typeof message === 'object' && message !== null) {
                    if (message.hasOwnProperty("frameSize")) {
                        //console.log("iframe " + message.fid + " height is: " + message.frameSize.frameHeight);
                        iFrames[message.fid].style.width = "100%";
                        iFrames[message.fid].style.height = (message.frameSize.frameHeight + 50) + "px";
                    }
            }
        }
    }

    function askForFrameSize(aFrame, frameOrigin, fid){
        var message = JSON.stringify({"q":"frameSize?", "fid":fid});
        aFrame.contentWindow.postMessage(message, frameOrigin);
    }

    function checkFrameSizes(){
        for (var i = 0; i < iFrames.length; i++) {
            if (iFrames[i].getAttribute("data-height") === "auto") {
                var frameSrc = iFrames[i].getAttribute("src");
                var protocol = window.location.protocol;
                var frameOrigin = "";

                if (frameSrc.indexOf("//") === 0) {
                    frameSrc = protocol + frameSrc;
                    frameOrigin = frameSrc.substring(0, frameSrc.indexOf("/", 8));
                }
                else {
                    frameOrigin = frameSrc.substring(0, frameSrc.indexOf("/", 8));
                }
                askForFrameSize(iFrames[i], frameOrigin, i);
            }
        }
    }

    if (window.postMessage && JSON.stringify) {
        addEvent(window, "message", receiveMessage);
        addEvent(window, "load", init);
    }

})();
</script>
</div>
</div></div>
