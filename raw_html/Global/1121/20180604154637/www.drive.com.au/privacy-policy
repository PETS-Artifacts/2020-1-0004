<script>
$('.smm select:not(.formModel)').change(function(){
	var theElement = $('#smmSearch');
	var thePage = '/inc/ajax/ajax-select-smm.php';
	var paramString = 'formType=new&category='+$(this).closest('form').find('.formCategory').val();
	if($(this).hasClass('formCategory')){
		$(this).closest('form').action = $(this).val();
	}else{
		paramString += '&make='+$(this).closest('form').find('.formMake').val();
	}
	
	fireAjax(theElement,thePage,paramString, false, "GET");
});
</script>
