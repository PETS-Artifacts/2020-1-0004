                if (document.getElementById('ctl00_ctl00_zhHeaderControl_ZHeaderMenu_HeaderHomeQuickSearch_txtKeyword')) {
                    var searchText = document.getElementById('ctl00_ctl00_zhHeaderControl_ZHeaderMenu_HeaderHomeQuickSearch_txtKeyword').value;
