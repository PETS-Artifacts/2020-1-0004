var getDeviceType = function() {
    if (jQuery('#nav > li.show-for-small-only').is(':visible')) {
        return 'm';
    } else {
        var userAgent = navigator.userAgent;
        if (userAgent) {
            userAgent = userAgent.toLowerCase();
            if (/mobi/i.test(userAgent) || /tablet/i.test(userAgent) || /ipad/i.test(userAgent) || /playbook/i.test(userAgent)) {
                return 't';
            }
        }
        
        return 'd';
    }
};

jQuery(document).ready(function(){
    dataLayer[0]['device'] = getDeviceType();
});

//]]>
