<script>
$(document).ready(function() {
    $("#fg").validate({
        rules: {
            username: {
                required: true,
                email: true
            }
        },
        messages: {
            username: {
                required: "Please Enter Email Address",
                email: "Please Enter a Valid Email Address"
            }
        }
    });

     $(".hubsub").on("change.bfhselectbox", function () { 
        var parentForm = $(this).closest("form");
        if (parentForm && parentForm.length > 0)
       parentForm.submit();
    });
    $(".cursel").on("change", function () { 
        var parentForm = $(this).closest("form");
        if (parentForm && parentForm.length > 0)
       parentForm.submit();
    });
    $('.facebookdiv .bfh-languages').on('change.bfhselectbox', function () {
               var addurl = "/privacy-policy";
       var dataLang = $(this).val();
       var twoChar=dataLang.substring(0, 2);
       var origin = window.location.origin
       var currentHref = window.location.href
     
        var url = origin + '/account/api/setlang/' + twoChar

        $.ajax({
          url: url,
          success: function (re) {
            // console.log(re)
             if (-1 !== currentHref.indexOf('/account')) {
              window.location.reload()
             } else {
              console.log(twoChar, origin + addurl)
              if(twoChar==="en")
              {
                window.location.replace(origin + addurl);
              } else {
                window.location.replace(origin + "/" + twoChar + addurl);
              }
           }
          },
          error: function (re) {
            alert(re.responseJSON.error);
          }
        });
   });
});
</script>


