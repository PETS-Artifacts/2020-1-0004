  $(document).ready(function() {
    var owl = $("#critical-information-alerts-carousel");
    owl.owlCarousel({
        singleItem: true,
        autoPlay: 10000
    });
    // Custom Navigation Events
    $(".next").click(function(){
      owl.trigger('owl.next');
    })
    $(".prev").click(function(){
      owl.trigger('owl.prev');
    })
