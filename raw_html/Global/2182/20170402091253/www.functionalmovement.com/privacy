<script type="text/javascript">
    $(document).ready(function () {
        if (window.location.href.indexOf('new=') > -1) {
            $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486 > #result').removeClass();
            $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486 > #result').addClass("alert alert-info alert-dismissable fade in");
            $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486 > #result').html("Registration was successful. Please login to continue.  An email has been sent with login instructions.");
            $('#loginSubmit-4433ec6a-8b18-443e-99dd-beb9506eb486').show();
        }
    });

    $('#loginCancel-4433ec6a-8b18-443e-99dd-beb9506eb486').click(function () {
        $(this).resetValidation();
        $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486')[0].reset();
        $('#loginSubmit-4433ec6a-8b18-443e-99dd-beb9506eb486').show();
    });
    $('#login-password').on('keyup', function (e) {
        if (e.keyCode == 13) {
            login('loginform-4433ec6a-8b18-443e-99dd-beb9506eb486');
        }
    });
    $('#loginSubmit-4433ec6a-8b18-443e-99dd-beb9506eb486').on('keyup', function (e) {
        login('loginform-4433ec6a-8b18-443e-99dd-beb9506eb486');
    });
    $('#loginSubmit-4433ec6a-8b18-443e-99dd-beb9506eb486').click(function () {
        login('loginform-4433ec6a-8b18-443e-99dd-beb9506eb486');
    });

    var login = function (formId) {
        if ($('#' + formId).valid()) {
            $('#loginSubmit-4433ec6a-8b18-443e-99dd-beb9506eb486').hide();
            $('#' + formId + ' > #result').removeClass();
            $('#' + formId + ' > #result').addClass("alert alert-info alert-dismissable fade in")
            $('#' + formId + ' > #result').html("Signing in...");

            var request = {}
            request["Username"] = $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486 #login-username').val();
            request["Password"] = $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486 #login-password').val();
            request["clientID"] = $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486 #clientID').val();
            request["proUserID"] = $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486 #proUserID').val();
            request["code"] = $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486 #code').val();
            request["RememberMe"] = $('#loginform-4433ec6a-8b18-443e-99dd-beb9506eb486 #login-remember')[0].checked;
            request["Return"] = (location.pathname + location.search).substr(1);
            var token = $('#' + formId + ' > input[name="__RequestVerificationToken"]').val();
            request["__RequestVerificationToken"] = token;

            $.ajax({
                url: '/account/login',
                async: false,
                data: request,
                type: "post",
                success: function (response) {
                    if (response.hasOwnProperty("Status")) {
                        if (response["Status"] == "SUCCESS") {
                            window.location.href = response["Url"];
                        } else {

                            $('#' + formId + ' > #result').removeClass();
                            $('#' + formId + ' > #result').addClass("alert alert-danger alert-dismissable fade in");
                            $('#' + formId + ' > #result').html(response.Status);
                            $('#loginSubmit-4433ec6a-8b18-443e-99dd-beb9506eb486').show();
                        }
                    } else {
                        if (location.href.toLowerCase().indexOf('account/login') > -1 && location.href.indexOf('?return=') == -1) {
                            location.href = '/';
                        } else if (location.href.indexOf('?return=') > -1) {
                            location.href = urldecode(location.href.substring(location.href.indexOf('?return=')).replace('?return=', ''))
                        } else {
                            location.reload();
                        }
                    }
                },
                error: function () {
                    $('#' + formId).show();
                    $('#loginSubmit-4433ec6a-8b18-443e-99dd-beb9506eb486').show();
                    console.log(arguments);
                }
            });
        }
    }
</script>

