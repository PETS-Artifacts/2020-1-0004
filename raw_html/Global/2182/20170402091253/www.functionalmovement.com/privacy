<script type="text/javascript">
    $('#btnSearchMembersFooter').on('click', function () {

        window.location = '/members/?postal=' + $('#txtPostalCodeFooter').val();
    });
    $("#txtPostalCodeFooter").on('keyup', function (e) {
        if (e.keyCode == 13) {
            // Do something
            if ($('#txtPostalCodeFooter').val().length > 0) {
                window.location = '/members/?postal=' + $('#txtPostalCodeFooter').val();
            }
        }
    });

