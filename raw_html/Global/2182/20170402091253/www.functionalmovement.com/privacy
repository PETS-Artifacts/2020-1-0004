    function searchsite() {
        var siteSearchVal = $('#sitesearch').val();
        if (siteSearchVal)
        {
            $('#gsc-i-id1').focus(); 
            $('#gsc-i-id1').val(siteSearchVal); 
            $(".gsc-search-button").trigger('click');
        }
    }
