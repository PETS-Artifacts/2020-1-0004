<script type="text/javascript">
/*** This is example code only ***/
//if session visit count === 1, it's a new session
var isNewSession = false;

try {
  // This is a reference to the cookie we are currently using for session tracking
  // On every page, we set a global variable on the window to make referencing the cookie easier
  // On each pageview, we increment the cookie value by 1 to capture visit count
  var visitCount = window.clearhead.cookies.session_minutes_30;
  isNewSession = visitCount === 1;
} catch(e) {}

// check if items are in cart
// if items are in cart, we know that the visitor is a return visitor
// so we do not have to check return visitor status separately
var itemsCnt = ($('.navQty').text().match(/\d+/) || [])[0] || 0;
var hasItemsInCart = parseInt(itemsCnt) > 0;

// Suppress exp4 if new visitor modal or promotional modal set to pop
// CPWM sets a global variable to true if 3rd party modals should be suppressed
var suppressExp4 = window._cpwm_block_ups_stlhs;

function showExp4MiniCart() {
  // add class to apply exp4 styles
  $('.globalCartLayer').addClass('exp4');
 
  	
  // open the mini-cart basket
  $('.globalCartLayer').css('display', 'block').css('width', '323px').css('z-index', '2999');
 
}

function closeExp4MiniCart() {
  // hide the mini-cart basket
  $('.globalCartLayer').css('display', 'none');
  // remove class
  $('.globalCartLayer').removeClass('exp4');
  // remove the banner entirely so it doesn't appear again
  $('.ch-welcome-banner').remove();
 
};
$('.ml-basket-close-btn').click(function(){
  $('.globalCartLayer').hide();
})
// Activate if targeting conditions met
if (isNewSession && hasItemsInCart && !suppressExp4) {
  $(document).ready(function() {
    showExp4MiniCart();
    
    // close mini-cart after 8 seconds if no visitor interaction
    var timeout = setTimeout(closeExp4MiniCart, 8000);
	
    var $cart = $('.globalCartLayer');
    
    // clear timeout if visitor interacts with cart
    $cart.on('mouseenter', function() {
     clearTimeout(timeout);
    });

    // close mini-cart when user stops interacting
    $cart.on('mouseleave', closeExp4MiniCart);

    // close mini-cart on click of x
    $cart.on('click', '.ch-close', closeExp4MiniCart);
  });
}
</script>



