<script type="text/javascript">

var icpForm11802 = document.getElementById('icpsignup11802');

if (document.location.protocol === "https:")

	icpForm11802.action = "https://app.icontact.com/icp/signup.php";

function verifyRequired11802() {
	if (icpForm11802["fields_email"].value == "") {
		icpForm11802["fields_email"].focus();
		alert("The Email field is required.");
		return false;
	}

	if (icpForm11802["fields_email"].value && icpForm11802["fields_email"].value !== "") {
		icpForm11802["fields_email"].value = icpForm11802["fields_email"].value.trim();
		alert("Thank you");
	}



	return true;
}

function validDate11802(value) {

	if (value == "") {
		return true;
	}

	var dateSections = value.split('/');
	if (dateSections.length != 3) {
		return false;
	}

	var month = dateSections[0];
	if (month && (month.length > 2 || month < 1 || month > 12)) {
		return false;
	}

	var day = dateSections[1];
	if (day && (day.length > 2 || day < 1 || day > 31)) {
		return false;
	}

	var year = dateSections[2];
	if (!year || (year && year.length != 4)) {
		return false;
	}

	var dateValue = new Date(value);
	if (month != (dateValue.getMonth()+1) || day != dateValue.getDate() || year != dateValue.getFullYear()) {
		return false;
	}

	return true;
}
</script>
