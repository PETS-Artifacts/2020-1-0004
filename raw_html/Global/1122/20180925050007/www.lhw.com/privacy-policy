<script type="text/javascript">

    function essentialTabs() {

        function hideAll() {
            jQuery('#divReservations').hide();
            jQuery('#divLeaders').hide();
            jQuery('#divSales').hide();
            jQuery('#divGroup').hide();
        }

        // hide all tab sections
        hideAll();

        // show reservations by default
        jQuery('#divReservations').show();

        // setup tab options
        jQuery('.essential-tabs a').click(function () {
            jQuery('.essential-tabs a').removeClass('active-tab');
            jQuery(this).addClass('active-tab');
        });

        // Hides all other tabs except reservations
        jQuery('a[id$=hlReservationAssistance]').live('click', function () {
            hideAll();
            jQuery('#divReservations').show();
        });

        // Hides all other tabs except leaders club
        jQuery('a[id$=hlLeadersClubAssistance]').live('click', function () {
            hideAll();
            jQuery('#divLeaders').show();
        });

        // Hides all other tabs except sales office
        jQuery('a[id$=hlSalesOffices]').live('click', function () {
            hideAll();
            jQuery('#divSales').show();
        });

        // Hides all other tabs except Group sales
        jQuery('a[id$=hlGroupSales]').live('click', function () {
            hideAll();
            jQuery('#divGroup').show();
        });



        jQuery('.customer-care .sectionheader').live('click', function () {
            jQuery(this).toggleClass('open');
            jQuery(this).next('.section-wrap').slideToggle();
        });

        jQuery('#divReservationAccordion .sectionheader').eq('').addClass('open').next('.section-wrap').slideDown();

    }
    
    jQuery(document).ready(function () {

        // load tabs
        essentialTabs();

    });

</script>
