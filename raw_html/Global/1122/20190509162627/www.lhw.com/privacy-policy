        window.showLanguageModal = function() {
            jQuery(".language-modal, .globalsearchoverlay").show();
            setShowHomeCarouselsBCookie("0", -1);
        }

        window.hideLanguageModal = function(val) {
            jQuery(".language-modal, .globalsearchoverlay").hide();
            setShowLanguageModalCookie(val);
        }

        window.setShowLanguageModalCookie = function(val) {
            var numDays = 1;
            setCookieMain(cookieNameShowLanguageModal, val, numDays, 0);
        }

        window.setLangPrefCookie = function (val) {
            setCookieMain(cookieNameLangPref, val, 3650, 0);
        }

