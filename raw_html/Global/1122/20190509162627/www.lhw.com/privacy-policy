            </div>

            <div class="col col-children">
                <label for="children">Children</label>
                <input class="form-control custom-select" name="children" type="text" readonly="true" v-model.number="totalChildren" v-on:click="showRoomList">
            </div>

            <div class="col col-num-guests">
                <label for="children">No. of Guests</label>
                <input class="form-control custom-select" name="numguests" type="text" readonly="true" v-model.number="totalGuests" v-on:click="showRoomList">
            </div>


            <div class="col col-button">
                <a tabindex="0" class="btn btn-sm btn-block btn-primary btn-submit" v-bind:class="{'btn-active': autocomplete.hasError==false && (autocomplete.location.length >= autocomplete.minLocationChar)}" v-on:click="submitSearch">Search</a>
            </div>
        </div>

        <transition v-on:enter="transitionEnter" v-on:leave="transitionLeave" :duration="transitionTime">
            <div class="room-list" v-show="roomListVisible">
                <hr />
                <transition-group name="roomList" v-on:enter="transitionEnter" v-on:leave="transitionLeave" :duration="transitionTime">
                    <div class="form-group" v-for="(room, rIndex) in rooms" v-bind:key="rIndex">
                        <div class="row">
                            <div class="col col-room-name">
                                <label>Room: {{rIndex+1}}</label>
                            </div>
                            <div class="col col-adults-drop">
                                <label for="adultsDrop">Adults</label>
                                <select class="form-control custom-select" name="adultsDrop" v-model.number="room.adults">
                                    <option v-for="n in 9">{{n}}</option>
                                </select>
                            </div>
                            <div class="col col-children-drop">
                                <label for="childrenDrop">Children</label>
                                <select class="form-control custom-select" name="childrenDrop" v-model.number="room.totalChildren" v-on:change="changeNumberOfChildren(room)">
                                    <option v-for="n in 11">{{n-1}}</option>
                                </select>
                            </div>
                            <div class="col col-child-ages" v-show="room.totalChildren>0">
                                <label>Age Of Child</label>
                                <div class="row">
                                    <div class="col col-child-age-drop" v-for="(child, cIndex) in room.children">
                                        <div class="form-group">
                                            <select class="form-control custom-select" v-model.number="child.age">
                                                <option v-for="n in 17">{{n}}</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </transition-group>
                <div class="row">
                    <div class="col col-button">
                        <a class="btn btn-sm close-room-list" v-on:click="hideRoomList">Close</a>
                    </div>
                </div>
            </div>
        </transition>
        <div class="row mobile-submit">
            <div class="col">
                <a tabindex="0" class="btn btn-sm btn-block btn-primary btn-submit" v-bind:class="{'btn-active': autocomplete.hasError==false && autocomplete.location.length}" v-on:click="submitSearch">Search</a>
            </div>
        </div>
    </div>
</script>
    
    

