    <script type="text/javascript">

        var cookieDomain = '.lhw.com';
        var cookieNameLangPref = 'langpref';
        var cookieNameVisited = "visited";
        var cookieNameShowHomeCarouselsB = "showhomecarb";
        var cookieNameShowLanguageModal = "showlanguagemodal";
        //var cookieNameShowEmailOptInModal = "showemailoptinmodal";

        // all images with class img-lazy-load will show only when within (100) pixels of window
        //
        // to show images on "show more", just trigger scroll 
        // ex: jQuery("img.img-lazy-load:visible").trigger("scroll");
        //
        // for crawlers, they don't use JS, so initially, we need to hide the lazy-load images
        // via CSS or display: none on the image, and then place the real image via <noscript>
        // under the real image (i.e: 
        // <img class="lazy" src="img/grey.gif" data-original="img/example.jpg"  width="640" heigh="480">
        // <noscript><img src="img/example.jpg" width="640" heigh="480"></noscript>)
        // This is so that browsers that don't have js will have the real image show (via noscript) and the main img hidden
        //
        // In document.ready because the images that are lazy-loaded are not within initial view, so after page is done loading, lazy load images.
        jQuery(document).ready(function () {
            // set visited cookie
            if (getCookieMain(cookieNameVisited) != "1") {
                setCookieMain(cookieNameVisited, "1", 3650, 0);
                setCookieMain(cookieNameShowHomeCarouselsB, "1", 0, 1);
            }

            // set showemailoptinmodal cookie to not show for 30 days
            if (window.location.search.toLowerCase().indexOf("utm_medium=email") > -1) {
                //setShowEmailOptInModalCookie("0", 30);
                setCookieMain(cookieNameShowHomeCarouselsB, "0", 0, -1);
            }

            // set showemailoptinmodal cookie to not show and set ut cookie for 90 days
            if (window.location.search.toLowerCase().indexOf("ut=clubh") > -1) {
                //setShowEmailOptInModalCookie("0", 90);
                setCookieMain("ut", "clubh", 90, 0);
            }

            // append HGM to these hidden values
            if (window.location.search.toLowerCase().indexOf("ut=clubh") > -1 || getCookieMain("ut") == "clubh") {
                if (jQuery("#hfLoggedInStatus").val().toLowerCase().indexOf("clubh") == -1) {
                    jQuery("#hfLoggedInStatus").val(jQuery("#hfLoggedInStatus").val() + "_CLUBH");
                }

                if (jQuery("#hfLCMembership").val().toLowerCase().indexOf("clubh") == -1) {
                    jQuery("#hfLCMembership").val(jQuery("#hfLCMembership").val() + "_CLUBH");
                }
            }

            // all images
            jQuery("img.img-lazy-load").show().lazyload({ 
                threshold: 100,
                effect: "fadeIn"
            });

            // homepage theme section background image
            jQuery("div.carouselwrap > div.home-main.img-lazy-load-background").lazyload({
                event: "reveal"
                //effect: "fadeIn"
            });

            // for carousel images
            // each container for images is set
            jQuery(".page-carousel-inner").each(function () {

                var thisContainer = jQuery(this);

                // Don't need show because carousel is JS
                // so crawlers wont see it anyways
                // event = custom event reveal
                // must be manually triggered
                jQuery(this).find("div.img-lazy-load-carousel").lazyload({
                    effect: "fadeIn",
                    event: "reveal",
                    container: thisContainer
                });

            });

            // for press center detail carousel images
            // each container for images is set
            jQuery(".pc-image-car .carousel-inner").each(function () {

                var thisContainer = jQuery(this);

                // Don't need show because carousel is JS
                // so crawlers wont see it anyways
                // event = custom event reveal
                // must be manually triggered
                jQuery(this).find("img.img-lazy-load-carousel").lazyload({
                    effect: "fadeIn",
                    event: "reveal",
                    container: thisContainer
                });

            });

            // for homepage theme carousel images
            // each container for images is set
            jQuery(".theme-car-inner").each(function () {

                var thisContainer = jQuery(this);

                // Don't need show because carousel is JS
                // so crawlers wont see it anyways
                // event = custom event reveal
                // must be manually triggered
                jQuery(this).find("div.img-lazy-load-carousel").lazyload({
                    event: "reveal",
                    effect: "fadeIn",
                    container: thisContainer
                });

            });

            // for homepage carousel images
            // each container for images is set
            jQuery(".home-big-car-inner").each(function () {

                var thisContainer = jQuery(this);

                // Don't need show because carousel is JS
                // so crawlers wont see it anyways
                // event = custom event reveal
                // must be manually triggered
                jQuery(this).find("div.img-lazy-load-carousel").lazyload({
                    event: "reveal",
                    effect: "fadeIn",
                    container: thisContainer
                });

            });

            // lazy load images in navs -- these are always hidden so new class
            // each container for images is set
            jQuery(".promos").each(function () {

                var thisContainer = jQuery(this);

                jQuery(this).find("img.img-lazy-load-main").show().lazyload({
                    threshold: 100,
                    effect: "fadeIn",
                    container: thisContainer
                });

            });

            // jQuery(".innerlabel label").inFieldLabels() fix
            setTimeout(function () {
                jQuery(".innerlabel label").css({ "position": "absolute", "top": "0px", "left": "0px" });
            }, 500);

            // load language modal
            setTimeout(function () {
                if (jQuery(".language-modal").length > 0 && getCookieMain(cookieNameShowLanguageModal) != "0") {
                    jQuery(".language-modal iframe").attr("src", "/widgets/language-modal");
                }
                else if (getCookieMain(cookieNameShowLanguageModal) != "0") {
                    setShowLanguageModalCookie(0);
                }
            }, 500);

            // load email optin modal
            //setTimeout(function () {
            //    if (jQuery(".optin-modal").length > 0 && getCookieMain(cookieNameShowEmailOptInModal) != "0")
            //        jQuery(".optin-modal iframe").attr("src", "/widgets/email-optin-modal?pgurl=https%3a%2f%2fwww.lhw.com%2fprivacy-policy");
            //}, 500);
        });

