  $.fn.dlaSanitize = function(input) {
    var output = input.replace(/<script[^>]*?>.*?<\/script>/gi, '').
    replace(/<[\/\!]*?[^<>]*?>/gi, '').
    replace(/<style[^>]*?>.*?<\/style>/gi, '').
    replace(/<![\s\S]*?--[ \t\n\r]*>/gi, '');
    return output;
  };

  $.fn.dlaBuildEvent = function () {
    // Check for segment library.
    // if (!window.analytics) {
    //   throw 'Segment analytics library not loaded';
    // }
    var props = {};
    // Get data attributes and add to prop.
    $.each($(this).data(), function (item, value) {
      props[item] = value;
    });
    // Get non-data attributes with custom dlaAttrs function and add to prop.
    $.each($(this).dlaAttrs(), function (item, value) {
      props[item] = value;
    });
    // Add more info to prop.
    props['dlaAnalyticsElement'] = $(this).prop("tagName");
    props['dlaAnalyticsHtml'] = $(this).html();
    // Check to see if it has an img tag as a child, if so load in those attributes to prop.
    if ($(this).has("img").length > 0) {
      $.each($(this).find("img").dlaAttrsImg(), function (item, value) {
        props[item] = value;
      });
    }
    return props;
  }

  // Prototype holds methods (to save memory space).
  DLAA.prototype = {

    dlaTrack: function (attribute) {
      if (!$) {
        throw 'jQuery not loaded';
      }
      $(this.selector).once('dla-analytics', function () {
        var props = $(this).dlaBuildEvent();
        // Grab readertype from cookie STYXKEYdla_readertype to send along with this event
        if ($.cookie("STYXKEYdla_readertype")) {
          props['dlaReaderType'] = $.cookie("STYXKEYdla_readertype");
        }
        var event_name = props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'];
        if (props['dlaAnalyticsSpecific'] == '1') {
          event_name = props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'];
        }
        switch ($(this).attr(attribute)) {
          case "rollover":
              $(this).mouseover(function () {
                if (window.analytics) {
                  analytics.track(event_name, props);
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track(event_name, props);
                }
                // check for GA flag
                if (props['dlaAnalyticsGa'] == '1') {
                  ga('GA2.send', 'event', { eventCategory: props['dlaAnalyticsObject'], eventAction: event_name, eventLabel: props['dlaAnalyticsDescription'], eventValue: 0});
                }
              });
            break;
          case "click":
              $(this).click(function () {
                if (window.analytics) {
                  analytics.track(event_name, props);
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track(event_name, props);
                }
                // check for GA flag
                if (props['dlaAnalyticsGa'] == '1') {
                  ga('GA2.send', 'event', { eventCategory: props['dlaAnalyticsObject'], eventAction: event_name, eventLabel: props['dlaAnalyticsDescription'], eventValue: 0});
                }
              });
            break;
          case "track-link":
            if (window.analytics) {
              analytics.trackLink($(this), event_name, props);
            }
            if (window.analytics === undefined && window.mixpanel) {
              mixpanel.track_links($(this), event_name, props);
            }
            // check for GA flag
            if (props['dlaAnalyticsGa'] == '1') {
              ga('GA2.send', 'event', { eventCategory: props['dlaAnalyticsObject'], eventAction: event_name, eventLabel: props['dlaAnalyticsDescription'], eventValue: 0});
            }
            break;
          case "dropdown":
            $(this).change(function() {
              props['selected'] = $(this).val();
              if (window.analytics) {
                analytics.track(event_name, props);
              }
              if (window.analytics === undefined && window.mixpanel) {
                mixpanel.track(event_name, props);
              }
              // check for GA flag
              if (props['dlaAnalyticsGa'] == '1') {
                ga('GA2.send', 'event', { eventCategory: props['dlaAnalyticsObject'], eventAction: event_name, eventLabel: props['dlaAnalyticsDescription'], eventValue: 0});
              }
            });
            break;
          case "bs-dropdown-item":
            $(this).on("click", function(){
              props['selected'] = $(this).context.text;
              if (window.analytics) {
                analytics.track(event_name, props);
              }
              if (window.analytics === undefined && window.mixpanel) {
                mixpanel.track(event_name, props);
              }
              // check for GA flag
              if (props['dlaAnalyticsGa'] == '1') {
                ga('GA2.send', 'event', { eventCategory: props['dlaAnalyticsObject'], eventAction: event_name, eventLabel: props['dlaAnalyticsDescription'], eventValue: 0});
              }
            })
            break;
          case "textfield":
            $(this).focus(function () {
              if (window.analytics) {
                analytics.track(event_name, props);
              }
              if (window.analytics === undefined && window.mixpanel) {
                mixpanel.track(event_name, props);
              }
              // check for GA flag
              if (props['dlaAnalyticsGa'] == '1') {
                ga('GA2.send', 'event', { eventCategory: props['dlaAnalyticsObject'], eventAction: event_name, eventLabel: props['dlaAnalyticsDescription'], eventValue: 0});
              }
            });
            break;
          case "textfield_focusout":
            $(this).focusout(function () {
              // Only send if user entered a value in the textfield
              if ($(this)["0"].value !== '') {
                if (props['dlaAnalyticsSendInputValue'] == '1') {
                  //props['dlaAnalyticsInputValue'] = $(this)["0"].value;
                  props['dlaAnalyticsInputValue'] = $(this).dlaSanitize($(this)["0"].value);
                }
                if (window.analytics) {
                  analytics.track(event_name, props);
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track(event_name, props);
                }
                // check for GA flag
                if (props['dlaAnalyticsGa'] == '1') {
                  ga('GA2.send', 'event', { eventCategory: props['dlaAnalyticsObject'], eventAction: event_name, eventLabel: props['dlaAnalyticsDescription'], eventValue: 0});
                }
              }
            });
            break;
          case "form":
              // Setup a handler to run when the form is submitted
              $(this).on('submit', function(e) {
                // If some client-side validation kicked in and wants to prevent
                // the form from submitting, bail out now without calling track or identify
                if (e.isDefaultPrevented()) {
                  return;
                }

                // needs a half second delay in order to get these events to fire in Firefox PC
                setTimeout(function() {
                  if (window.analytics) {
                    analytics.track(event_name, props);
                  }
                  if (window.analytics === undefined && window.mixpanel) {
                    mixpanel.track(event_name, props);
                  }
                  // check for GA flag
                  if (props['dlaAnalyticsGa'] == '1') {
                    ga('GA2.send', 'event', { eventCategory: props['dlaAnalyticsObject'], eventAction: event_name, eventLabel: props['dlaAnalyticsDescription'], eventValue: 0});
                  }
                }, 500);

                // If we got here, it's okay to fire our events and submit the form
                // Stop the form from submitting...for now
                e.preventDefault();

                // Submit the form now that all our analytics stuff is done
                $(e.target).unbind('submit').trigger('submit');
              });
            break;
        }
      });

      // Make chainable.
      return this;
    }

  };

  // The actual object is created here, allowing us to 'new' an object without calling 'new'.
  DLAA.init = function (selector) {
    var self = this;
    self.selector = selector || '';
  }

  // Trick borrowed from jQuery so we don't have to use the 'new' keyword.
  DLAA.init.prototype = DLAA.prototype;

  // Attach our DLAA to the global object, and provide a shorthand '$DLAA' for ease our poor fingers.
  global.DLAA = global.$DLAA = DLAA;

}(window, jQuery));
//--><!]]>
</script>
