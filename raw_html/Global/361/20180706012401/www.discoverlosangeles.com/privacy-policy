              $('#meme-gen__dummy-input').focusout(function () {
                $('[data-dla-analytics-memegen-txt]').attr('data-dla-analytics-memegen-txt', $(this).dlaSanitize($(this)["0"].value));
              });

              $('#meme-gen__color-toggle').click(function () {
                var color = 'white';
