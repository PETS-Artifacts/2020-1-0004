        if (meme_gen_container.length != 0) {
          setTimeout(function waitMemeGenContainer() {
            if (meme_gen_container.length != 0) {
              // Populate data attributes with meme info based on info entered
              $('#meme-gen__image-selection-container .meme-gen__default-image').click(function () {
                $('[data-dla-analytics-memegen-image-src]').attr('data-dla-analytics-memegen-image-src', $(this).find("img").attr('src'));
