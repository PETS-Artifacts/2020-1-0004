                  color = 'black';
                }
                $('[data-dla-analytics-memegen-txtcolor]').attr('data-dla-analytics-memegen-txtcolor', color);
              });

              $('#meme-gen__photo-button').focusout(function () {
                $('[data-dla-analytics-memegen-image-src]').attr('data-dla-analytics-memegen-image-src', 'uploaded photo');
              });

              $('[data-dla-analytics-memegen-image-src]').click(function () {
                var props = $(this).dlaBuildEvent();
                props['dlaAnalyticsMemegenImageSrc'] = $('.meme-gen__data').data('dla-analytics-memegen-image-src');
                props['dlaAnalyticsMemegenTxt'] = $('.meme-gen__data').data('dla-analytics-memegen-txt');
                props['dlaAnalyticsMemegenTxtcolor'] = $('.meme-gen__data').data('dla-analytics-memegen-txtcolor');
                var event_name = props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'];
                if (window.analytics) {
                  analytics.track(event_name, props);
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track(event_name, props);
                }
                // check for GA flag
                if (props['dlaAnalyticsGa'] == '1') {
                  ga('GA2.send', 'event', { eventCategory: props['dlaAnalyticsObject'], eventAction: event_name, eventLabel: props['dlaAnalyticsDescription'], eventValue: 0});
                }
              });


            } else {
              setTimeout(waitMemeGenContainer, 100);
            }
          }, 100);
        }

      }
    };

  }
  // Run when not bootstrapping Drupal
  else {
    $(document).ready(function() {
      // Set name of element's attribute to track
      var attribute = 'data-dla-analytics-track';
      if ($('[' + attribute + ']').length) {
        var dla = $DLAA(attribute);
        dla.dlaTrack(attribute);
      }
    })
  }
}(jQuery));
