<script type="text/javascript">
<!--//--><![CDATA[//><!--
;(function (global, $) {
  // A tracking library that track actions on DOM elements
  // Example on how to use.
  // This targets all elements with the data attribute "data-dla-analytics-track"
  // var dla = $DLAA('[data-dla-analytics-track]');
  // dla.dlaTrack();
  // 'new' an object.
  var DLAA = function (attribute) {
    var selector = '[' + attribute +']';
    return new DLAA.init(selector);
  }

  $.fn.dlaAttrs = function () {
    var attributes = {};
    if (this.length) {
      $.each(this[0].attributes, function (index, attr) {
        // Only return non data attributes
        if (attr.name.indexOf('data-') !== 0) {
          attributes[attr.name] = attr.value;
        }
      });
    }
    return attributes;
  }

  $.fn.dlaBuildEvent = function () {
    // Check for segment library.
    // if (!window.analytics) {
    //   throw 'Segment analytics library not loaded';
    // }
    var props = {};
    // Get data attributes and add to prop.
    $.each($(this).data(), function (item, value) {
      props[item] = value;
    });
    // Get non-data attributes with custom dlaAttrs function and add to prop.
    $.each($(this).dlaAttrs(), function (item, value) {
      props[item] = value;
    });
    // Add more info to prop.
    props['dlaAnalyticsElement'] = $(this).prop("tagName");
    props['dlaAnalyticsHtml'] = $(this).html();
    // Check to see if it has an img tag as a child, if so load in those attributes to prop.
    if ($(this).has("img").length > 0) {
      $.each($(this).find("img").dlaAttrs(), function (item, value) {
        props[item] = value;
      });
    }
    return props;
  }

  // Prototype holds methods (to save memory space).
  DLAA.prototype = {

    dlaTrack: function (attribute) {
      if (!$) {
        throw 'jQuery not loaded';
      }
      $(this.selector).once('dla-analytics', function () {
        var props = $(this).dlaBuildEvent();
        switch ($(this).attr(attribute)) {
          case "rollover":
              $(this).mouseover(function () {
                if (window.analytics) {
                  analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                  if (props['dlaAnalyticsSpecific'] == '1') {
                    analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                  }
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                  if (props['dlaAnalyticsSpecific'] == '1') {
                    mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                  }
                }
              });
            break;
          case "click":
              $(this).click(function () {
                if (window.analytics) {
                  analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                  if (props['dlaAnalyticsSpecific'] == '1') {
                    analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                  }
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                  if (props['dlaAnalyticsSpecific'] == '1') {
                    mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                  }
                }
              });
            break;
          case "track-link":
            if (window.analytics) {
              analytics.trackLink($(this), props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
              if (props['dlaAnalyticsSpecific'] == '1') {
                analytics.trackLink($(this), props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
              }
            }
            if (window.analytics === undefined && window.mixpanel) {
              mixpanel.track_links($(this), props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
              if (props['dlaAnalyticsSpecific'] == '1') {
                mixpanel.track_links($(this), props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
              }
            }
            break;
          case "dropdown":
            $(this).change(function() {
              props['selected'] = $(this).val();
              if (window.analytics) {
                analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                if (props['dlaAnalyticsSpecific'] == '1') {
                  analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                }
              }
              if (window.analytics === undefined && window.mixpanel) {
                mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                if (props['dlaAnalyticsSpecific'] == '1') {
                  mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                }
              }              
            });
            break;
          case "bs-dropdown-item":
            $(this).on("click", function(){
              props['selected'] = $(this).context.text;
              if (window.analytics) {
                analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                if (props['dlaAnalyticsSpecific'] == '1') {
                  analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                }
              }
              if (window.analytics === undefined && window.mixpanel) {
                mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                if (props['dlaAnalyticsSpecific'] == '1') {
                  mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                }
              }   
            })
            break;
          case "textfield":
            $(this).focus(function () {
              if (window.analytics) {
                analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                if (props['dlaAnalyticsSpecific'] == '1') {
                  analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                }
              }
              if (window.analytics === undefined && window.mixpanel) {
                mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                if (props['dlaAnalyticsSpecific'] == '1') {
                  mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                }
              }
            });
            break;
          case "form":
              // Setup a handler to run when the form is submitted
              $(this).on('submit', function(e) {
                // If some client-side validation kicked in and wants to prevent
                // the form from submitting, bail out now without calling track or identify
                if (e.isDefaultPrevented()) {
                  return;
                }

                // needs a half second delay in order to get these events to fire in Firefox PC
                setTimeout(function() {
                  if (window.analytics) {
                    analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                    if (props['dlaAnalyticsSpecific'] == '1') {
                      analytics.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                    }
                  }
                  if (window.analytics === undefined && window.mixpanel) {
                    mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'], props);
                    if (props['dlaAnalyticsSpecific'] == '1') {
                      mixpanel.track(props['dlaAnalyticsObject'] + ' ' + props['dlaAnalyticsAction'] + ' - ' + props['dlaAnalyticsDescription'], props);
                    }
                  }
                }, 500);

                // If we got here, it's okay to fire our events and submit the form
                // Stop the form from submitting...for now
                e.preventDefault();

                // Submit the form now that all our analytics stuff is done
                $(e.target).unbind('submit').trigger('submit');
              });
            break;
        }
      });

      // Make chainable.
      return this;
    }

  };

  // The actual object is created here, allowing us to 'new' an object without calling 'new'.
  DLAA.init = function (selector) {
    var self = this;
    self.selector = selector || '';
  }

  // Trick borrowed from jQuery so we don't have to use the 'new' keyword.
  DLAA.init.prototype = DLAA.prototype;

  // Attach our DLAA to the global object, and provide a shorthand '$DLAA' for ease our poor fingers.
  global.DLAA = global.$DLAA = DLAA;

}(window, jQuery));
//--><!]]>
</script>
<script type="text/javascript">
<!--//--><![CDATA[//><!--
(function($) {
  // Run when bootstrapping Drupal
  if (typeof Drupal !== 'undefined') {
    Drupal.behaviors.dlaAnalyticsExec = {
      attach: function(context, settings) {
        // Set name of element's attribute to track
        var attribute = 'data-dla-analytics-track';
        if ($('[' + attribute + ']').length) {
          var dla = $DLAA(attribute);
          dla.dlaTrack(attribute);
        }

        // Track Open Table Widgets
        var opentable_wrapper = $('.detail-page-opentable-widget, .opentable-widget');
        if (opentable_wrapper.length != 0) {
          setTimeout(function waitOpenTableWidget() {
              var open_table_link = $('#OT_Find_a_Table, form.ot-dtp-picker-form input');
              if (open_table_link.length != 0) {
                var open_table_props = {};
                open_table_props['dlaAnalyticsElement'] = open_table_link.prop("tagName");
                open_table_props['dlaAnalyticsHtml'] = open_table_link.html();
                open_table_props['dlaAnalyticsPath'] = window.location.pathname;
                open_table_props['dlaAnalyticsTitle'] = document.title;
                open_table_props['dlaAnalyticsDescription'] = 'Book Now';
                open_table_props['dlaAnalyticsSlot'] = 'Open Table Widget';
                // Desktop Widget
                if ($("[id='RestaurantID']").length != 0) {
                  open_table_props['dlaAnalyticsRestaurantID'] = $("[name='RestaurantID']").val();
                }
                // Mobile Widget (which is a button)
                if ($("[id='OT_Find_a_Table']").attr('href').indexOf("mobile.opentable.com/opentable/?ref=2666") >= 0) {
                  function dlaGetUrlParameter(name, string) {
                    name = name.replace(/[\[]/, '\\[').replace(/[\]]/, '\\]');
                    var regex = new RegExp('[\\?&]' + name + '=([^&#]*)');
                    var results = regex.exec(string);
                    return results === null ? '' : decodeURIComponent(results[1].replace(/\+/g, ' '));
                  };
                  open_table_props['dlaAnalyticsRestaurantID'] = dlaGetUrlParameter('restId', $("[id='OT_Find_a_Table']").attr('href'));
                }
                var dineLA_prefix = '';
                if (window.location.pathname.indexOf('dinela-los-angeles-restaurant-week') === 1) {
                  dineLA_prefix = 'dineL.A. ';
                }
                if (window.analytics) {
                  analytics.trackLink(open_table_link, dineLA_prefix + 'Link Clicked', open_table_props);
                  analytics.trackLink(open_table_link, dineLA_prefix + 'Link Clicked - Book Now - Open Table', open_table_props);
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track_links(open_table_link, dineLA_prefix + 'Link Clicked', open_table_props);
                  mixpanel.track_links(open_table_link, dineLA_prefix + 'Link Clicked - Book Now - Open Table', open_table_props);
                }
                // Send events to Google Analytics
                if (window.location.host === 'www.discoverlosangeles.com') {
                  ga('create', 'UA-5027411-1', 'auto');
                  ga('create', 'UA-5027411-17', 'auto', {'name' : 'GA2'});
                } else {
                  ga('create', 'UA-5027411-18', 'auto', {'name' : 'GA2'});
                }
                // Fire event to GA Properties when button is clicked
                $('#OT_Find_a_Table').bind('click', function(e) {
                  ga('send', 'event', { eventCategory: 'Affiliate Link', eventAction: 'Open Table', eventLabel: 'Book Now: ' + open_table_props['dlaAnalyticsRestaurantID'], eventValue: 0});
                  ga('GA2.send', 'event', { eventCategory: 'Affiliate Link', eventAction: 'Open Table', eventLabel: 'Book Now: ' + open_table_props['dlaAnalyticsRestaurantID'], eventValue: 0});
                  ga('send', 'event', { eventCategory: 'Affiliate Link', eventAction: open_table_props['dlaAnalyticsSlot'], eventLabel: 'Book Now: ' + open_table_props['dlaAnalyticsRestaurantID'], eventValue: 0});
                  ga('GA2.send', 'event', { eventCategory: 'Affiliate Link', eventAction: open_table_props['dlaAnalyticsSlot'], eventLabel: 'Book Now: ' + open_table_props['dlaAnalyticsRestaurantID'], eventValue: 0});
                });
              } else {
                setTimeout(waitOpenTableWidget, 100);
              }
          }, 100);
        }

        // Track aRES Widgets in Main Nav
        var ares_wrapper_main_nav = $('.megamenu-search');
        if (ares_wrapper_main_nav.length != 0) {
          setTimeout(function waitAresWidgetMainNav() {
              var ares_link_main_nav = $('.megamenu-search #aresWidget .bookNowBtn input');
              if (ares_link_main_nav.length != 0) {
                var ares_props_main_nav = {};
                ares_props_main_nav['dlaAnalyticsElement'] = ares_link_main_nav.prop("tagName");
                ares_props_main_nav['dlaAnalyticsHtml'] = ares_link_main_nav.html();
                ares_props_main_nav['dlaAnalyticsPath'] = window.location.pathname;
                ares_props_main_nav['dlaAnalyticsTitle'] = document.title;
                ares_props_main_nav['dlaAnalyticsDescription'] = 'aRES Widget';
                ares_props_main_nav['dlaAnalyticsSlot'] = 'Main Nav';
                var dineLA_prefix = '';
                if (window.analytics) {
                  analytics.trackLink(ares_link_main_nav, 'Link Clicked', ares_props_main_nav);
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track_forms('#hotelSearchForm', 'Link Clicked', ares_props_main_nav);
                }
              } else {
                setTimeout(waitAresWidgetMainNav, 100);
              }
          }, 100);
        }

        // Track aRES Widgets in Category Pages
        var ares_wrapper_cat_page = $('.category-page-intro-image-inner');
        if (ares_wrapper_cat_page.length != 0) {
          setTimeout(function waitAresWidgetCatPage() {
              var ares_link_cat_page = $('.category-page-intro-image-inner #aresWidgetCat .bookNowBtn input');
              if (ares_link_cat_page.length != 0) {
                var ares_props_cat_page = {};
                ares_props_cat_page['dlaAnalyticsElement'] = ares_link_cat_page.prop("tagName");
                ares_props_cat_page['dlaAnalyticsHtml'] = ares_link_cat_page.html();
                ares_props_cat_page['dlaAnalyticsPath'] = window.location.pathname;
                ares_props_cat_page['dlaAnalyticsTitle'] = document.title;
                ares_props_cat_page['dlaAnalyticsDescription'] = 'aRES Widget';
                ares_props_cat_page['dlaAnalyticsSlot'] = 'Category Page';
                if (window.analytics) {
                  analytics.trackLink(ares_link_cat_page, 'Link Clicked', ares_props_cat_page);
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track_forms('#hotelSearchForm', 'Link Clicked', ares_props_cat_page);
                }
              } else {
                setTimeout(waitAresWidgetCatPage, 100);
              }
          }, 100);
        }

        // Track aRES Widgets in Detail Pages
        var ares_wrapper_detail_page = $('.detail-page-ares-widget');
        if (ares_wrapper_detail_page.length != 0) {
          setTimeout(function waitAresWidgetDetailPage() {
              var ares_link_detail_page = $('.detail-page-ares-widget #aresWidgetSingle .bookNowBtn input');
              if (ares_link_detail_page.length != 0) {
                var ares_props_detail_page = {};
                ares_props_detail_page['dlaAnalyticsElement'] = ares_link_detail_page.prop("tagName");
                ares_props_detail_page['dlaAnalyticsHtml'] = ares_link_detail_page.html();
                ares_props_detail_page['dlaAnalyticsPath'] = window.location.pathname;
                ares_props_detail_page['dlaAnalyticsTitle'] = document.title;
                ares_props_detail_page['dlaAnalyticsDescription'] = 'aRES Widget';
                ares_props_detail_page['dlaAnalyticsSlot'] = 'Detail Page';
                if (window.analytics) {
                  analytics.trackLink(ares_link_detail_page, 'Link Clicked', ares_props_detail_page);
                }
                if (window.analytics === undefined && window.mixpanel) {
                  mixpanel.track_forms('#hotelSearchForm', 'Link Clicked', ares_props_detail_page);
                }
              } else {
                setTimeout(waitAresWidgetDetailPage, 100);
              }
          }, 100);
        }

      }
    };

  }
  // Run when not bootstrapping Drupal
  else {
    $(document).ready(function() {
      // Set name of element's attribute to track
      var attribute = 'data-dla-analytics-track';
      if ($('[' + attribute + ']').length) {
        var dla = $DLAA(attribute);
        dla.dlaTrack(attribute);
      }
    })
  }
}(jQuery));
//--><!]]>
</script>
