<script>
	$(".sfsearchBox").ready(function () {
		$("#FromDatePicker").kendoDatePicker({ format: "MM/dd/yyyy", max: new Date(), value: new Date(new Date().setDate(new Date().getDate() - 30)) });
		$("#UntilDatePicker").kendoDatePicker({ format: "MM/dd/yyyy", max: new Date(), value: new Date() });
		$("#DdCategory").kendoDropDownList();

		$("#Nav_T12F1B5D5010_ctl00_ctl00_textToSearch").change(function (e) {
			$("#searchTextBox").val($("#Nav_T12F1B5D5010_ctl00_ctl00_textToSearch").val() + ", " + $("#DdCategory").val() + ", " + $("#FromDatePicker").val() + ", " + $("#UntilDatePicker").val());
			return true;
		});

		$("#DdCategory").change(function (e) {
			$("#searchTextBox").val($("#Nav_T12F1B5D5010_ctl00_ctl00_textToSearch").val() + ", " + $("#DdCategory").val() + ", " + $("#FromDatePicker").val() + ", " + $("#UntilDatePicker").val());
				return true;
		});

		$("#FromDatePicker").change(function (e) {
			$("#searchTextBox").val($("#Nav_T12F1B5D5010_ctl00_ctl00_textToSearch").val() + ", " + $("#DdCategory").val() + ", " + $("#FromDatePicker").val() + ", " + $("#UntilDatePicker").val());
			return true;
		});

		$("#UntilDatePicker").change(function (e) {
			$("#searchTextBox").val($("#Nav_T12F1B5D5010_ctl00_ctl00_textToSearch").val() + ", " + $("#DdCategory").val() + ", " + $("#FromDatePicker").val() + ", " + $("#UntilDatePicker").val());
			return true;
		});
	});
</script>
</div>
  
