<script type="text/javascript">
    if (checkCookie()) {

        $.ajax({
            url: '/json/WeatherRail/GetRailModelJson/',
            data: {
                Zip: getCookie("WeatherCookie")
,Css:'http://www.keloland.com/css/ae-wx-widgets.css'                ,NoAlertsText:'No Active Alerts'                ,Days:'5'                ,BannerGUID:'00000000-0000-0000-0000-000000000000'            },
            success: function (Model) {
                if (Model.City != undefined) {
                    var locationTitle = '<span>' + Model.City.Name + ', ' + Model.City.StateAbbr + '</span> - <a href="#location-modal" class="change-location">Change</a></div>';
                    $('.weather.module-rail div.location div.location-title').html(locationTitle);
                    var current = '<wx:widget type="current-conditions" template="simple-box" location="' + Model.City.Id + '" header="false" id="ae-rail-current-conditions" css="' + Model.Css + '"></wx:widget>';
                    $('.weather.module-rail div.current').html(current);
                    var forcastList = '<wx:widget type="daily" template="simple-full-width" location="' + Model.City.Id + '" id="ae-rail-forecast" group="true" header="false" days="' + Model.Days + '" css="' + Model.Css + '"></wx:widget>';
                    $('.weather.module-rail div.forcast-list').html(forcastList);

                }
            },
            dataType: "json"
        });
    }
</script>


