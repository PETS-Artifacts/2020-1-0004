<script type="text/javascript">
var _siteMask = $$('#site_mask');
var _siteDialog = $$('#site_dialog');
var _siteDialogContent = $$('#site_dialog_content',_siteDialog);
var _modalSpeed =  260;
// Init Widgets
{
	_siteMask.fadeTo(0,0).hide();
	_siteDialog.hide();
}

function showMask() {
	_siteMask.show().fadeTo(_modalSpeed, 0.8);	
}

function hideMask() {
	_siteMask.fadeTo(_modalSpeed/2, 0, function() {_siteMask.hide(); });	
}

function openHtmlModalDialog(d_width, d_height, htmlContent, element) {
	var centerX = Math.round($$(window).width()/2);
	var centerY = Math.round($$(window).height()/2);
	if (element != null) {
		var offset = $$(element).offset();
		_siteDialog.css({ width: 0, height: 0, top:offset.top, left:offset.left });
	} else {
		_siteDialog.css({ width: 0, height: 0, top:centerY - $(window).scrollTop(), left:centerX - $(window).scrollLeft() });
	}
	
	showMask();
	_siteDialog.show().animate({
		width:  d_width,
		height: d_height,
		top: centerY - (d_height/2),
		left: centerX - (d_width/2)
	},_modalSpeed, function() {
		_siteDialogContent.html(htmlContent);
	});
}

function openIframeModalDialog(d_width, d_height, iframeUrl, element) {
	var centerX = Math.round($$(window).width()/2);
	var centerY = Math.round($$(window).height()/2);
	if (element != null) {
		var offset = $$(element).offset();
		_siteDialog.css({ width: 0, height: 0, top:offset.top, left:offset.left });
	} else {
		_siteDialog.css({ width: 0, height: 0, top:centerY - $(window).scrollTop(), left:centerX - $(window).scrollLeft()});
	}
	
	showMask();
	_siteDialog.show().animate({
		width:  d_width,
		height: d_height,
		top: centerY - (d_height/2),
		left: centerX - (d_width/2)
	},_modalSpeed, function() {
		_siteDialogContent.html("<iframe src='"+iframeUrl+"' width='"+d_width+"' height='"+d_height+"' frameborder='0' webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>");
	});
}

function openYoutubeModalDialog(d_width, d_height, youtubeUrl, element) {
	var centerX = Math.round($$(window).width()/2);
	var centerY = Math.round($$(window).height()/2);
	var videoId = "";
	if (youtubeUrl.lastIndexOf("=") != -1)
		videoId = youtubeUrl.substr(youtubeUrl.lastIndexOf("=")+1);
	else if (youtubeUrl.lastIndexOf("/") != -1) {
		videoId = youtubeUrl.substr(youtubeUrl.lastIndexOf("/")+1);
	}
	if (element != null) {
		var offset = $$(element).offset();
		_siteDialog.css({ width: 0, height: 0, top:offset.top, left:offset.left });
	} else {
		_siteDialog.css({ width: 0, height: 0, top:centerY - $(window).scrollTop(), left:centerX - $(window).scrollLeft() });
	}
	
	showMask();
	_siteDialog.show().animate({
		width:  d_width,
		height: d_height,
		top: centerY - (d_height/2),
		left: centerX - (d_width/2)
	},_modalSpeed, function() {
		_siteDialogContent.html("<iframe src='https://www.youtube.com/embed/"+videoId+"' width='"+d_width+"' height='"+d_height+"' frameborder='0' webkitallowfullscreen mozallowfullscreen allowfullscreen></iframe>");
	});
}

function openImageModalDialog(element) {
	var centerX = Math.round($$(window).width()/2);
	var centerY = Math.round($$(window).height()/2);
	var defaultWidth = 300; var defaultHeight = 300;
	if (element != null) {
		var offset = $$(element).offset();
		var href = $$(element).attr('href'); 
		_siteDialog.css({ width: $$(element).width(), height: $$(element).height(), top:offset.top - $(window).scrollTop(), left:offset.left - $(window).scrollLeft() });
		var img = new Image();
		showMask();
		_siteDialog.show().animate({
			width:  defaultWidth,
			height: defaultHeight,
			top: centerY - (defaultHeight/2),
			left: centerX - (defaultWidth/2)
		},_modalSpeed, function() {
			_siteDialogContent.html("<div style='background: url(/main/corporate/img/modal/loading.gif) no-repeat center center; position: absolute; top: 0; left: 0; right: 0; bottom: 0;'></div>");
			img.onload = function () {
				var imgWidth = this.width;
				var imgHeight = this.height;
				resizeModalDialog(imgWidth,imgHeight,_modalSpeed/2, function () {
					_siteDialogContent.html("<img src='"+href+"'/>");
				});
			};
			img.src = href;
		});
	}
}

function openVideoModalDialog(d_width, d_height, element) {
	var centerX = Math.round($$(window).width()/2);
	var centerY = Math.round($$(window).height()/2);
	if (element != null) {
		var offset = $$(element).offset();
		var href = $$(element).attr('href'); 
		_siteDialog.css({ width: 0, height: 0, top:offset.top - $(window).scrollTop(), left:offset.left - $(window).scrollLeft() });
		_siteDialogContent.html("<div id='modal-player' style='display: none; background: #333; width:"+d_width+"; height:"+d_height+";'></div>");
		var img = new Image();
		showMask();
		_siteDialog.show().animate({
			width:  d_width,
			height: d_height,
			top: centerY - (d_height/2),
			left: centerX - (d_width/2)
		},_modalSpeed, function() {
			$$("#modal-player",_siteDialogContent).show();
			$f("modal-player", {src: "http://ww1.jeppesen.com"+jeppPlayer.paths.v3_2_7, wmode: 'transparent'}, {
			key: jeppPlayer.keys.v3_2,
				plugins: {
					controls: jeppPlayer.config.controls
				},
				clip: {
					url: href,
					scaling: 'fit',
					autoPlay: true,
					autoBuffering: true
				},
				canvas: {
					backgroundColor: '#000',
					backgroundGradient: 'none'
				}
			}).ipad();
		});
	}
}

function openDialogWindow(URL, width, height) {
    window.open(URL, "jeppDialogWindow", "location=0,menubar=0,status=0,toolbar=0,scollbars=1,width="+width+",height="+height);
}

function resizeModalDialog(newWidth, newHeight, speed, callback) {
	var centerX = Math.round($$(window).width()/2);
	var centerY = Math.round($$(window).height()/2);
	_siteDialog.animate({
		width:  newWidth,
		height: newHeight,
		top: centerY - (newHeight/2),
		left: centerX - (newWidth/2)
	}, speed, callback);
}

function closeModalDialog() {
	if ($f() != undefined) {
		$f().stop();
	}
	hideMask();
	_siteDialog.hide();
	_siteDialogContent.empty();
}

// Initiate Auto Modal
$$(document).ready(function() {
    $$("#site_mask").height($$(document).height());
	$$("a.modal-image").click(function() {
		openImageModalDialog($$(this));
		return false;
	});
	
	$$("a.modal-video").click(function() {
		openVideoModalDialog(640, 480, $$(this));
		return false;
	});
	
	$$(".modal-image-gallery").each(function() {
	});
    
    $$("a.dialog-window").click(function() {
        var URL = $$(this).attr("href");
        openDialogWindow(URL, 640, 480);
        return false;
    });
	
	$$("a.dialog-iframe").click(function() {
		var width = 640;
		var height = 480;
        var URL = $$(this).attr("href");
		if ($$(this).attr("data-width")) width = $$(this).attr("data-width");
		if ($$(this).attr("data-height")) height = $$(this).attr("data-height");
        openIframeModalDialog(width, height, URL, $$(this));
        return false;
    });
	
	$$("a.dialog-youtube").click(function() {
		var width = 640;
		var height = 480;
        var URL = $$(this).attr("href");
		if ($$(this).attr("data-width")) width = $$(this).attr("data-width");
		if ($$(this).attr("data-height")) height = $$(this).attr("data-height");
        openYoutubeModalDialog(width, height, URL, $$(this));
        return false;
    });
	
	_siteMask.click(closeModalDialog);
});
</script>
		
