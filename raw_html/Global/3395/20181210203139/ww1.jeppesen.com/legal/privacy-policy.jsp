		handler.click(function (e) {
			if (!isLink) {
				if (!event) var event = e;
				if (event.preventDefault) { event.preventDefault(); } else { event.returnValue = false; }
			}
			if (block.hasClass('active') && !isLink) {
				util.removeClass('selected-util');
				block.removeClass('active');
				dialog.hide();
			} else if (!isLink) {
				$$('#site-utils .util.active .util-dialog').hide();
				$$('#site-utils .active').removeClass('active');
				util.addClass('selected-util');
				block.addClass('active');
				dialog.show();
			}
		});
		link.click(function () {
			isLink = true;
		});
	});
	$$('#site-utils .util').hover(function() {
		jeppUtilHover = true;
	},function() {
		jeppUtilHover = false;
	});
	$$().click(function() {
		if (!jeppUtilHover) hideUtils();
	});
}

function hideUtils() {
	$$('ul#site-utils li.active .util-dialog').hide();
	$$('ul#site-utils .active').removeClass('active');
}
