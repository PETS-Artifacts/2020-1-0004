    jQuery(function ($) {

        if (Cookies.get('williamsf1#accept-consent') !== 'accept') {
            $('.cookie-consent').addClass('show');
        }
        else {
            $('.cookie-consent').hide();
        }

        $('.cookie-consent').on('closed.bs.alert', function () {
            Cookies.set('williamsf1#accept-consent', 'accept', { expires: 365 });
        })

    });

