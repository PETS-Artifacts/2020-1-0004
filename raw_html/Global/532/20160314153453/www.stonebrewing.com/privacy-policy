    <section id="block-views-beers-for-search-block" class="block block-views clearfix">

        <h2 class="block-title">Available Beers</h2>
    
  <div class="view view-beers-for-search view-id-beers_for_search view-display-id-block view-dom-id-f729c5ed863ad8c7e0125dd2ee84e4d8" >
        
  
  
      <div class="view-content">
        <div class="views-row views-row-1 views-row-odd views-row-first">
      
  <div class="views-field views-field-nothing">        <span class="field-content"><a href="/beer/year-round-releases/stone-ruination-double-ipa-20" alt="Stone Ruination Double IPA 2.0"> 
