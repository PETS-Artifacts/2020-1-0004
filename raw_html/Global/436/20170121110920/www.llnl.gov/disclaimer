  <script>
    var siteName = document.domain;
    var siteID;

    //builds a function that will provide a cached copy of the tracker for a given ID
    function cached_tracker_generator(id) {
      return function() {
        var tracker = null;
        return function(){
          if (tracker === null) {
            tracker = Piwik.getTracker("https://analytics.llnl.gov/piwik.php", id);
          }
          return tracker;
        }
      }();
    }

    jQuery.getJSON(
      "https://analytics.llnl.gov" + "/get_id.php?name=" + siteName + "&callback=?",
      function (data){
        if (!isNaN(data.id)) {
          siteID = data.id;
          var get_local_tracker = cached_tracker_generator(siteID);
        }
      }
    );

    var get_global_tracker = cached_tracker_generator(1);

    function initial_tracking(error_message) {
      var current_path = location.pathname;
      var search = false;
      current_path = current_path.split("/");
      if (current_path.length > 1) {
        if (current_path[1] == "llnl_search") {
          search = true;
        }
      }
      try {
        var llnl = get_global_tracker();
        llnl.setCustomVariable(1, "drupal", window.location.hostname, "visit");
        llnl.enableLinkTracking();
        if (!search) {
          llnl.trackPageView();
        }
        if (error_message !== undefined) {
          llnl.setCustomVariable(1, "error", error_message, "page");
        }
      } catch(err){}

      try {
        var site = get_local_tracker();
        site.enableLinkTracking();
        if (!search) {
          site.trackPageView();
        }

        if (error_message !== undefined) {
          site.setCustomVariable(1, "error", error_message, "page");
        }
      } catch(err){}
    } 

            initial_tracking();
        </script>

