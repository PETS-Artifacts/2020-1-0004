    <script type='text/javascript'>var mmaUrl = 'https://secure.nelnet.net/Account/Login/';</script>
    <script type="text/javascript">
        function btnLogin_Click() {
            var txtUserName = document.getElementById('txtUserName');
            var userNameEnterred = "" + txtUserName.value;
            if (userNameEnterred.length > 0)
            {
                var r = new RegExp("^[A-Za-z0-9-_ ]+$");

                if (userNameEnterred.match(r))
                {
                    document.location.href = mmaUrl + userNameEnterred;
                }
                else{
                    document.getElementById('usernameError').style.display = "block";
                }            
            }
        }
    </script>

    
