    <div id="modal-background" ng-click="vm.tempStore.remove('currentTab')">
    </div>
    <div id="sign-in-up-modal" name="sign-in-up-form" ng-init="vm.checkQueryParams()" class="fullOverlay">
        <div id="modal-main" class="fullOverlay">
            <div>
                <div id="sign-in-tab" class="top-tabs" ng-class="{ activeTab: vm.tempStore.expect('currentTab', gc.constants.LOGIN_ACTIONS.signIn) }" ng-click="vm.loginModalService.showLoginModal(gc.constants.LOGIN_ACTIONS.signIn)">
                    <span>Sign In</span>
                </div>
                <div id="sign-up-tab" class="top-tabs" ng-class="{ activeTab: vm.tempStore.expect('currentTab', gc.constants.LOGIN_ACTIONS.signUp) || vm.tempStore.expect('currentTab', gc.constants.LOGIN_ACTIONS.profileId) }" ng-click="vm.loginModalService.showLoginModal(gc.constants.LOGIN_ACTIONS.signUp)">
                    <span>Sign Up</span>
                </div>
            </div>
            <div class="padding20">
                <div id="modal-header">
                    <span class="close-modal" ng-click="vm.tempStore.remove('currentTab'); vm.tempStore.remove(currentTab.customMessage)" ng-bind="vm.closeLinkText"></span>
                    <h3 ng-bind-html="vm.currentTabValues.headerText"></h3>
                </div>
                <div ng-show="vm.errors.length > 0">
                    <div class="error-message" ng-repeat="error in vm.errors">
                        <p ng-bind-html="error"></p>
                    </div>
                </div>
                <div class="modal-description">
                    <p ng-bind-html="vm.loginModalService.getCustomMessage() ? vm.loginModalService.getCustomMessage() : vm.currentTabValues.descriptionText"></p>
                </div>
                <form name="modal-form">
                    <div class="submit-overlay" ng-if="vm.submitting">
                        <div class="overlay-spinner-background-div overlay-centering">
                            <img alt="" src="https://d5y2y5rl4e57i.cloudfront.net/ajax-loader.gif" class="overlay-spinner">
                            <p class="overlay-text" ng-bind="vm.currentTabValues.spinnerText"></p>
                        </div>
                    </div>
                    <div class="form-item-modal">
                        <label for="modal-email-address" class="clear gileft">Email address</label>
                        <input id="modal-email-address" class="clear left required modal-input" name="email" type="email" maxlength="254" placeholder="Email Address" ng-show="vm.currentTabValues.requiredFields.email" ng-model="vm.user.email" ng-required="vm.currentTabValues.requiredFields.email">
                    </div>
                    <div class="form-item-modal">
                        <label for="modal-password" class="clear left">Password</label>
                        <input id="modal-password" class="clear left required modal-input" name="password" value="" type="password" maxlength="128" placeholder="Password" ng-show="vm.currentTabValues.requiredFields.password && !vm.tempStore.has('profileMismatch')" ng-model="vm.user.password" ng-required="vm.currentTabValues.requiredFields.password">
                    </div>
                    <div class="form-item-modal">
                        <label for="modal-confirm-password" class="clear left">Confirm Password</label>
                        <input id="modal-confirm-password" class="clear left required modal-input" name="confirm_password" type="password" maxlength="128" placeholder="Confirm Password" ng-show="vm.currentTabValues.requiredFields.confirmPassword && !vm.tempStore.has('profileMismatch')" ng-model="vm.user.confirm_password" ng-required="vm.currentTabValues.requiredFields.confirmPassword">
                    </div>
                    <div class="form-item-modal">
                        <label for="modal-first-name" class="clear left">First Name</label>
                        <input id="modal-first-name" class="clear left required modal-input" name="first_name" type="text" maxlength="255" placeholder="First Name" ng-show="vm.currentTabValues.requiredFields.firstName" ng-model="vm.user.first_name" ng-required="vm.currentTabValues.requiredFields.firstName">
                    </div>
                    <div class="form-item-modal">
                        <label for="modal-last-name" class="clear left">Last Name</label>
                        <input id="modal-last-name" class="clear left required modal-input" name="last_name" name="last_name" type="text" maxlength="255" placeholder="Last Name" ng-show="vm.currentTabValues.requiredFields.lastName" ng-model="vm.user.last_name" ng-required="vm.currentTabValues.requiredFields.lastName">
                    </div>
                    <div class="form-item-modal">
                        <label for="modal-postal-code" class="clear left">Postal Code</label>
                        <input id="modal-postal-code" class="clear left required modal-input" name="postal_code" type="text" maxlength="255" placeholder="Postal Code" ng-show="vm.currentTabValues.requiredFields.postalCode" ng-model="vm.user.postal_code" ng-change="vm.checkCaslOptIn()" ng-required="vm.currentTabValues.requiredFields.postalCode">
                    </div>
                    <div class="form-item-modal" ng-show="vm.showCaslOptIn">
                        <input id="modal-casl-opt-in" class="option left casl-checkbox" name="casl_opt_in" type="checkbox" ng-model="vm.user.casl_opt_in">
                        <label for="modal-casl-opt-in" class="show form-checkbox casl-label">Sign up for deals and exclusive offers, delivered right to your inbox!</label>
                    </div>
                    <div class="form-item-modal">
                        <input name="create-account-button" id="create-account-button" type="submit" value="Create New Account" class="form-submit modal-submit" ng-if="vm.tempStore.expect('currentTab', gc.constants.LOGIN_ACTIONS.signUp)" ng-click="vm.signUp()">
                        <input name="sign-in-button" id="sign-in-button" type="submit" value="Sign In" class="form-submit modal-submit" ng-if="vm.tempStore.expect('currentTab', gc.constants.LOGIN_ACTIONS.signIn)" ng-click="vm.authenticate(vm.user.email, vm.user.password)">
                        <input name="forgot-password-button" id="forgot-password-button" type="submit" value="Email New Password" class="form-submit modal-submit" ng-if="vm.tempStore.expect('currentTab', gc.constants.LOGIN_ACTIONS.forgotPassword)" ng-click="vm.sendForgotPasswordEmail(vm.user.email)">
                        <input name="reset-password-button" id="reset-password-button" type="submit" value="Reset Password" class="form-submit modal-submit" ng-if="vm.tempStore.expect('currentTab', gc.constants.LOGIN_ACTIONS.resetPassword)" ng-click="vm.resetPassword(vm.user.password, vm.user.confirm_password)">
                        <input name="creamysqlte-account-profile-id-button" id="create-account-profile-id-button" type="submit" value="Create New Account" class="form-submit modal-submit" ng-if="vm.tempStore.expect('currentTab', gc.constants.LOGIN_ACTIONS.profileId)" ng-click="vm.signUpWithProfileId(vm.user.email, vm.user.password, vm.user.confirm_password)">
                        <a id="forgot-password" href="#" ng-show="vm.tempStore.expect('currentTab', gc.constants.LOGIN_ACTIONS.signIn)" ng-click="vm.loginModalService.showLoginModal(gc.constants.LOGIN_ACTIONS.forgotPassword)">Forgot password?</a>
                    </div>
                </form>
                <form ng-if="vm.tempStore.has('profileMismatch')">
                    <hr class="login-hr">
                    <div class="modal-description">
                        <p>Or create a new account with this email address:</p>
                        <p class="strong" ng-bind="vm.user.enteredEmail"></p>
                    </div>
                    <input id="create-fresh-account-button" type="button" value="Create Account" class="form-submit modal-submit" ng-click="vm.tabSwitchStatusClear()">
                </form>
                <hr class="login-hr">
                <div class="footer-address">
                    <p>Great Wolf Resorts &bull; 525 Junction Rd Ste 6000 &bull; Madison, WI 53717</p>
                </div>
            </div>
        </div>
    </div>
</div>
  <div>
    <div id="mobile-datepicker" ng-controller="MobileDatepickerController as vm">
        <calendar ng-if="vm.tempstore.expect('showMobileDatepicker', true)"
                  data-title="'Select Stay Dates'"
                  data-submittext="'Done'"
                  data-hide="vm.tempstore.set('showMobileDatepicker', false)"
                  data-notifysubmit="vm.onCalendarModalSubmit">
        </calendar>
    </div>
