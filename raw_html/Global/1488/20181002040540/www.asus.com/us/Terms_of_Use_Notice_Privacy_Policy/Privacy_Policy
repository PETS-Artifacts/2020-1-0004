                add_cookie('ProductGroup_RelationId', ProductGroup_RelationId);
            }
            else {
                document.cookie = "ProductGroup_RelationId = '' ;path=/;domain=.asus.com;";
                add_cookie('ProductGroup_RelationId', ProductGroup_RelationId);
            }
        }
        
        function setBusinessCookie(type) {
