<script>
    
    $(document).ready(function () {
        if (getWebsite() == "us") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/us/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }
        if (getWebsite() == "th") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/th/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }
        if (getWebsite() == "tw") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/tw/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }
        if (getWebsite() == "ph") {
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("data-link", "http://store.asus.com/ph/");
            $("ul.aai-nav").find("li[data-name=store-sub-nav]").attr("target", "_blank");
        }

        if ('' == "Y") {
            //針對首頁 loading 效果 by Suzy 
            var screenHeight = $(window).innerHeight();
            if (window.siteConfig.route == 'Index' || window.siteConfig.route == "PDLinePage") {
                $('#af-react').css('min-height', screenHeight);
            }
        }
        $("#liBuy > a").click(function() {
            window.dataLayer = window.dataLayer || [];
            window.dataLayer.push({ 
                'event': 'product-page-buybtn',
                'ecommerce': {
                'add': {                                      
                    'products': [{                             
                        'name': $(".page-title > span").text(), 
                        'category': window.siteConfig.breadcrumb.L2.LevelName
                    }]
                }
                }
            });
        });
        
        $("#liwheretobuy > a").click(function() {
            window.dataLayer = window.dataLayer || [];
            window.dataLayer.push({ 
                'event': 'product-page-wheretobuy',
                'ecommerce': {
                'add': {                                      
                    'products': [{                             
                        'name': $(".page-title > span").text(), 
                        'category': window.siteConfig.breadcrumb.L2.LevelName
                    }]
                }
                }
            });
        });
        
    });

