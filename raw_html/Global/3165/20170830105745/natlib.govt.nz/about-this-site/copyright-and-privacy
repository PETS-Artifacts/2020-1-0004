    <script type="text/javascript">
      function setSearchBoxFocus() {
        var v = document.getElementById('search-box'),
            hash_tag = window.location.hash.replace('#', '');

        if (v && (hash_tag.length == 0 || document.getElementById(hash_tag) == null)) {
          v.focus();
        }
      }
    </script>
