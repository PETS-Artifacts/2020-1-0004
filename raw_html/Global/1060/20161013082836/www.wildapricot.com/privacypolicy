    {
        $(this).attr('data-clipboard-text', $(this).find('.copyBoxText').text());
		
        new ZeroClipboard(this).on('aftercopy', function()
        {
            $(this).css({ backgroundColor: '#f5f5f5' })
                .find('.copyBoxDone')
                .show();
        });
    });

    if (location.hash.indexOf('#/') == 0 && (hashElement = $('#' + location.hash.replace('#/', ''))))
    {
        hashElement.smoothScroll();
    }
</script>


