        helpers: { overlay: { closeClick: true, showEarly: true, locked: false } }
    });

    $('.fancybox.iframe').each(function(i, link) {
        $(link).fancybox({
            scrolling: 'no',
            autoResize: false,
            type: 'iframe',
            width: $(link).attr('data-width'),
            height: $(link).attr('data-height'),
            helpers: { overlay: { closeClick: false, showEarly: true, locked: false } }
        });
    });

    $('.sfAnchor').anchorContainer({ instantScroll: false });
    $('.sfInstantAnchor').anchorContainer({ instantScroll: true });
    $('.sfAnchorBlock a').anchorContainer({ instantScroll: false });
    $('.sfInstantAnchorBlock a').anchorContainer({ instantScroll: true });

