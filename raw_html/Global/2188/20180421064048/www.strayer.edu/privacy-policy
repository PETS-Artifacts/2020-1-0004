    <script type="text/javascript">
    var checkChat = setInterval(function(){
        if($(".LPMoverlay").length)  {
          clearInterval(checkChat);
            setTimeout(function(){ $(".chat-popup-container").show('10000'); }, '6000');
        }else {

        //  $('.chat-tooltip').hide();

               }
            }, 1000);



        $(document).ready(function(){
        //Get current time
        var currentTime = new Date().getTime();
        //Add hours function
        Date.prototype.addHours = function(h) {
           this.setTime(this.getTime() + (h*60*60*1000));
           return this;
        }
        //Get time after 24 hours
        var after24 = new Date().addHours(10).getTime();
        //Hide div click
        $('.chat-tooltip-close, .chat-link').click(function(){
            //Hide div
            $('.chat-tooltip').hide();
            //Set desired time till you want to hide that div
            localStorage.setItem('desiredTime', after24);
        });
        //If desired time >= currentTime, based on that HIDE / SHOW
        if(localStorage.getItem('desiredTime') >= currentTime)
        {
            $('.chat-tooltip').hide();
        }
        else
        {
            $('.chat-tooltip').show();
        }
      });



      ( function( $ ) {
        var Component, ComponentInterior, ComponentFloating;
        function CheckValid() {
          if( !Component.length ) return false;
          ComponentInterior = Component.find( '.interior' );
          if( !ComponentInterior.length ) return false;
          ComponentFloating = ComponentInterior.find( '.btmbar-floating' , 'header'  );
          if( !ComponentFloating.length ) return false;
          return true;
        }
        function UpdateAnchor() {
          if( !CheckValid() ) return;
          ComponentInterior.css( { 'height' : ComponentFloating.outerHeight() } );
        }
        function CheckAnchor(){
          if( !CheckValid() ) return;
          if( $( window ).scrollTop() > 0 && !ComponentFloating.hasClass( 'scrolled' ) ) {
            ComponentFloating.addClass( 'scrolled' );
          }
          if( $( window ).scrollTop() < 1 && ComponentFloating.hasClass( 'scrolled' ) ) {
            ComponentFloating.removeClass( 'scrolled' );
          }
          ComponentFloating.css( { 'position' : 'fixed' } );
          if( ComponentFloating.offset().top - $( window ).scrollTop() > ComponentInterior.offset().top - $( window ).scrollTop() ) {
            ComponentFloating.css( { 'position' : 'relative' } );
          } else {
            return;
          }
        }
        $( document ).on( 'ready', function() {
          Component = $( '#component-3' );
          $( window ).on( 'resize', function() {
            CheckAnchor();
            UpdateAnchor();
          } );
          $( window ).on( 'scroll', function() {
            CheckAnchor();
            UpdateAnchor();
          } );
        });
        //Interaction event to scroll to bottom rfi
        // var $bottomRfi     = $("#btmbar-scroll-brfi");

        // $("#btmbar-scroll-brfi").click(function() {
        //   $('html, body').animate({
        //     scrollTop: $(".component-rficont").offset().top - 64
        //   }, 2000);

        //           //   $('#bottom-homepage-edit-submitted-firstname').focus();

        // });

        $('#btmbar-scroll-brfi a').foundation('scrollToLoc','#rficont');


        $("#topbar-scroll-trfi").click(function() {
          $('html, body').animate({
            scrollTop: $(".component-rficont").offset().top - 64
          }, 2000);
        });
        //Enter key event for live chat (508)
        $( "div#LP_DIV_1493825586559" ).keypress(function(e) {
          if(e.which == 13) {
            $( "div#LP_DIV_1493825586559 > div > span" ).trigger( "click" );
          }
        });

        //Enter Key Event for RFI (508)
        $("#btmbar-scroll-brfi").keypress(function(e) {
          if(e.which == 13) {
            $( "#btmbar-scroll-brfi > a" ).trigger( "click" );
          }
        });
      } )( jQuery );
    </script>
