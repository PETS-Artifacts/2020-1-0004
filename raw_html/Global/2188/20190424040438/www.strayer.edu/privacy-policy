    <script type="text/javascript">

    /*var checkChat = setInterval(function(){
        if($(".LPMoverlay").length && $.cookie("tooltip-status") !== "hide")  {
          clearInterval(checkChat);
            setTimeout(function(){ $(".chat-popup-container").show('10000'); }, ' //echo render($content['field_set_chat_timmer']['#items']['0']['value'] ); ?>');
