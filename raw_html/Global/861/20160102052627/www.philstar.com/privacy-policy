<script>
	(function ($){
		$(document).ready(function(){

			var latestNewsRecommended = $(".latest-news-trending");

			latestNewsRecommended.find('.nav.latest-news').click(function(){
				var getIndex = $(this).index();
				latestNewsRecommended.find(".nav-tab-content").removeClass("selected").eq(getIndex+2).addClass("selected");
				$(this).addClass("selected");	
			});
			
			latestNewsRecommended.find('.nav.trending').click(function(){
				var getIndex = $(this).index();
				latestNewsRecommended.find(".nav-tab-content").removeClass("selected").eq(getIndex+2).addClass("selected");
				$(this).addClass("selected");
			});
			
		});
	})(jQuery);
</script>

