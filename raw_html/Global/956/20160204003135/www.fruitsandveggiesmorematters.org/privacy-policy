  <script type="text/javascript">

  function ate_clear_form() {
    document.getElementById("ate_form1_title").value = "";
   // document.getElementById("ate_form1_content").value = "";
   
    var e2 = document.getElementById("ate_form1_email");
    if(e2 != null) { e2.value = ""; }
    var e3 = document.getElementById("ate_form1_name");
    if(e3 != null) { e3.value = ""; }
    document.getElementById("ate_form1_msg_div").innerHTML = "";
    document.getElementById("ate_form1_div").style.display = 'block';
  }
  function ate_send_form_cb(message) {
    document.getElementById("ate_form1_name").value = '';
	document.getElementById("ate_form1_email").value = '';
	document.getElementById("ate_form1_title").value = '';
    document.getElementById("ate_form1_msg_div").innerHTML = message;
	document.getElementById('ate_frm_tbl').style.display="block";
    document.getElementById('ate_frm_loader').style.display="none";
	//document.getElementById('ate_submit_btn').style.display="block";
  }
  function ate_send_form(divpos) { //alert(divpos);
  document.getElementById("divpostrack").value=divpos;
  document.getElementById('ate_frm_tbl').style.display="none";
  document.getElementById('ate_frm_loader').style.display="block";
  
  	
  	//alert();
    var message = document.getElementById("ate_form1_msg_div");
    
    var title = document.getElementById("ate_form1_title").value;
    var content = "";
	var category =5;//document.getElementById("categories_mf").value;
    var error = "";
    if(title.length == 0){ error += "You must specify a question.<br/>"; }
   // if(content.length == 0){ error += "You must add some text.<br/>"; }
    
    var name = "";
    var e1 = document.getElementById("ate_form1_name");
    if(e1 != null){ 
      name = e1.value; 
      if(name.length == 0){ 
        error += "You must specify a name so we know who it comes from.<br/>"; 
      }
    }
    var email = "";
    var e2 = document.getElementById("ate_form1_email");
    if(e2 != null){ 
      email = e2.value; 
      if(echeck(email) == false){ 
        error += "The email you specified is invalid.<br/>"; 
      }
    }
    
    var web = "ate";
   /* var e3 = document.getElementById("ate_form1_web");
    if(e3 != null){
     // web = e3.value;
     web="ate";
    }*/
