						type:"POST",
						url:"http://www.fruitsandveggiesmorematters.org/wp-content/themes/pbh/show_rightmenu_read.php",
						data:"parent_id="+parent_id,
						success: function(data){        //alert(data);
						 if(data==1)
						 { //alert("fdgfd");
						 jQuery('.main-sub-menu').width("295px");//.css("width", "+=455px");//.css('width:','555px');
						 data='';
						 jQuery('.menu_right_show').html(data);
						 
						 }
						 else
						 {//alert("fdgfd111");
						  jQuery('.main-sub-menu').width("590px");
						  jQuery('.menu_right_show').html(data);
						  }
						},
						complete: function(){
						
						}
					});
			}
	</script>
    




