  function getXMLHttp()
{
  var xmlHttp

  try
  {
    //Firefox, Opera 8.0+, Safari
    xmlHttp = new XMLHttpRequest();
  }
  catch(e)
  {
    //Internet Explorer
    try
    {
      xmlHttp = new ActiveXObject("Msxml2.XMLHTTP");
    }
    catch(e)
    {
      try
      {
        xmlHttp = new ActiveXObject("Microsoft.XMLHTTP");
      }
      catch(e)
      {
        alert("Your browser does not support AJAX!")
        return false;
      }
    }
  }
  return xmlHttp;
}

function MakeRequest(URLS)
{
//alert(URLS);
  var xmlHttp = getXMLHttp();
  xmlHttp.open("GET", URLS, true); 
  xmlHttp.onreadystatechange = function() {
  if(xmlHttp.readyState == 4) {
    //alert(xmlHttp.responseText);
	if(document.getElementById("divpostrack").value==1)
	{
	 ate_send_form_cb(xmlHttp.responseText);
	 }
	 else
	 {
	 document.getElementById("ate_form1_msg_div_widget").innerHTML = xmlHttp.responseText;
	 document.getElementById("ate_form1_msg_div_widget").style.display="block";
	//  document.getElementById('ate_submit_btn').style.display="block";
	 document.getElementById('ate_frm_tbl').style.display="block";
     document.getElementById('ate_frm_loader').style.display="none";
	 }
  }
}
 xmlHttp.send(null);
}

 function x_ate_ajax_send_post(title, content, notify, email, name, web,category,userID,tdomf_send_form_cb,send_mail){
 //alert(email);
  	  //alert(userID);
  	var randomnumber=Math.floor(Math.random()*11111)
	var SendURLS;
	SendURLS	=	"http://www.fruitsandveggiesmorematters.org/doAjax.php?WTOAuthorID="+userID+"&WTDOTitle="+title+'&WTDOContent='+content+'&WTDONotify='+notify+'&WTDOEmail='+email+'&WTDOName='+name+'&WTDOWeb='+web+'&WTDOCategory='+category+"&s="+randomnumber+'&send_mail='+send_mail;
	MakeRequest(SendURLS);
    }
