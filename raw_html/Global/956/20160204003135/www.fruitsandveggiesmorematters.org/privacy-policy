<script type="text/javascript">
	jQuery( document ).ready( function($){
		
		 $('.cv-jmml-widget-wrap').css('background-color','#c5c5c5');
		 $('.chrome .cv-jmml-widget-wrap iframe').css('border-right','1px solid #c5c5c5');
		 $('.safari .cv-jmml-widget-wrap iframe').css('background','#FFFFFF');
		
		setMaxWidth();
		$( window ).bind( "resize", setMaxWidth ); //Remove this if it's not needed. It will react when window changes size.
		function setMaxWidth() 
		{
			if( $( window ).width() < 992)
			{
				
				$( ".reminderlink, .healthy_menu_ideas_con" ).find('img').each(function(){
					//console.log($(this).attr('src'));
					if($(this).parent( "td" ).attr('colspan') == 5)
					{
						$(this).css( "width", $(this).closest( "div" ).width() );
						$(this).parent( "table" ).css( "width", $(this).closest( "div" ).width() );
					}
					else
					{
						var variable_here = $(this).parent( "a" ).attr('href');
						if(typeof variable_here === 'undefined'){
							$(this).css( "maxWidth", "100%" ).css( "width", "auto" );
						}
					}
				});
				
				$( "#shlistbkg" ).find('img').each(function(){
					if($(this).parent( "td" ).attr('colspan') == 3)
					{
						$(this).css( "width", $(this).closest( "div" ).width() );
						$(this).parent( "table" ).css( "width", $(this).closest( "div" ).width() );
					}
					else
					{
						var variable_here = $(this).parent( "a" ).attr('href');
						if(typeof variable_here === 'undefined'){
							$(this).css( "maxWidth", "100%" ).css( "width", "auto" );
						}
					}
				});
				
				$( ".middle_contain_top" ).find('img').each(function(){
					if($(this).parent( "td" ).attr('colspan') == 5)
					{
						$(this).css( "width", $(this).closest( "div" ).width() );
						$(this).css( "height", 1);
						$(this).parent( "table" ).css( "width", $(this).closest( "div" ).width() );
					}
				});
				$( ".middle_contain_top table" ).each(function() {		
					var w = $(this).width();
					var pDivWidth = $(this).closest( ".middle_contain_top" ).width();
					if(pDivWidth < w){
						$(this).css( "width", pDivWidth );
					}
				});
				$( ".middle_contain_top img" ).each(function() {		
						var imgW = $(this).width();
						var imgH = $(this).height();
						if(imgW >= 500 && imgH == 1){
							$(this).css( "width", $(this).closest( ".middle_contain_top" ).width() );
							//console.log(imgW);
							$(this).css( "height", 1);
						}
				});
			}
		}
	});
</script>
