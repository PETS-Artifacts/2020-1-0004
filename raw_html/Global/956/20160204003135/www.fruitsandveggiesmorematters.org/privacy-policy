function check_feedback(f)































































{































































    var name = f.name.value;































































    var email1 = f.email.value;































































	var comment = f.comment.value;































































	var issue_type ='';































































	var radios = document.getElementsByName("issue_type");































































	































































    var found = 1;































































    for (var i = 0; i < radios.length; i++) {       































































    if (radios[i].checked) {  issue_type = radios[i].value;   }































































   }































































	if(name=='')































































	{































































		document.getElementById('feedback_error').innerHTML=" Please enter name!";































































