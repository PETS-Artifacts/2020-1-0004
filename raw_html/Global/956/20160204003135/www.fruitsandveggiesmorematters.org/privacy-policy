      var theform = document.getElementById("ate_form1_div");
     // theform.style.display = 'none';
	 theform.style.display = 'block';
      var links = document.getElementById("ateLinks");
      links.style.display='none';
      //message.innerHTML = '<center><img src="http://www.fruitsandveggiesmorematters.org/wp-content/plugins/Widgets/ajax-loader.gif" /></center>';
      web="ate";
