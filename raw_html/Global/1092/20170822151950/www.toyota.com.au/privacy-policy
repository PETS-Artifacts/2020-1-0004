<script type="text/javascript">
$(document).ready(function(){
var gtmSnippetHTML = "";
window.location.hostname.indexOf("toyota.com.au") > -1 ? gtmSnippetHTML = "gtm-analytics.html" : gtmSnippetHTML =
"gtm-analytics-staging.html";
$(".footer-script").load("/assets/javascripts/vendor/analytics/"+gtmSnippetHTML);
});
</script>
<script type="text/javascript">
    $(document).ready(function(){
        var rightSidebar = $('.policy-menu-column > div');
        $(window).on("scroll load", function() {
            var headerHeight = $('nav.navbar-default').height();
            if ($('body').scrollTop() > headerHeight ) {
                rightSidebar.addClass('sidebar-fixed');
            }
            else{
                rightSidebar.removeClass('sidebar-fixed');
            }
        });
    });
</script>
