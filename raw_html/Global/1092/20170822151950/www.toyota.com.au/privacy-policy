<script id="tmpl-pick-a-dealer" type="text/template">
  <label class="dealer-address">

    <input type="radio" name="preferredDealer" id="dealer-<%= branchId %>" class="my-dealer-input" value="<%= branchId %>"
      data-dealer-name="<%= name %>"
      data-dealer-address="<%= address %>"
      data-dealer-id="<%= dealerId %>"
      data-dealer-branch-id="<%= branchId %>"
      data-dealer-state="<%= state %>"
      data-dealer-email="<%= email %>"
      data-dealer-telephone="<%= telephone %>"
      data-dealer-suburb="<%= suburb %>"
      data-dealer-postcode="<%= postcode %>"
      data-dealer-fax="<%= fax %>"
      data-dealer-region-name="<%= regionName %>"
      data-dealer-region-type="<%= regionType %>">

    <address>
      <h3><%= name %></h3>
      <small>
        <span class="street"><%= address %>, </span>
        <span class="suburb"><%= suburb %> <%= state %>, <%= postcode %></span>
      </small>
    </address>
  </label>


</script>



    
