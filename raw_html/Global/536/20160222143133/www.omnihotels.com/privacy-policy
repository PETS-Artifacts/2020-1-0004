<script type="text/javascript">
    window.updateSelectedHotel = function (hotel) {
        var hotelText = $j("#hotelSelect option:selected").text().trim();
        $j("#hotelSelect option:selected").text(hotelText);
        $j("input[data-id='selectedHotel']").val(hotel.value);
    }
</script>
