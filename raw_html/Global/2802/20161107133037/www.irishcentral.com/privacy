<script type="text/javascript">
  $(function(){

    if ( !jQuery.cookie("signup_opt_in") && !jQuery.cookie("signup_opt_out") ) {
      $('#newsletter-popup').modal();
    }



    $('form[action="/newsletter/competition"]').submit(function(e) {
      jQuery.cookie("signup_opt_in", "1", { expires: 360, path: '/' });
      return true;
    });

    $('#newsletter-popup').on('hidden.bs.modal', function (e) {
      jQuery.cookie("signup_opt_out", "1", { expires: 360, path: '/' });
      return true;
    });
    $("#signup-opt-out, #popup-close-button").on("click", function() {
      jQuery.cookie("signup_opt_out", "1", { expires: 360, path: '/' });
      $('#newsletter-popup').modal('hide');
      return true;
    });
  });
</script>

    <script type="text/javascript">
    $(function(){
      if ( $(".takeover-side").length && !mobile_user ) {
        var takeoverTimeout = false;
        $(window).resize(function(){
          if(takeoverTimeout !== false)
            clearTimeout(takeoverTimeout);
          takeoverTimeout = setTimeout(resetTakeoverPosition, 500);
        });

        $('.takeover-side').affix({
          offset: {
            top: function() {
              return (this.top = $("#right_to_bank").offset().top)
            },
            bottom: function() {
              return (this.bottom = $('footer').outerHeight(true) - 50)
            }
          }
        });

        $(".takeover-side").on('affixed.bs.affix', function(){ computeTakeoverPosition(); });
        $(".takeover-side").on('affix-top.bs.affix', function(){ resetTakeoverPosition(); });
      }

    });
    function computeTakeoverPosition() {
      $('.takeover-side').css({ 'position': 'fixed', 'top': $("nav.navbar-fixed-top").height() });
      $('#right_to_bank').css({
        'width': $('#right_to_bank img').outerWidth(),
        'left':($(".container.main-container").offset().left + $(".container.main-container").outerWidth()),
        'right': '', 'margin-right': ''
      });
      $('#left_to_bank').css({
        'width': $('#left_to_bank img').outerWidth(),
        'right':($(".container.main-container").offset().left + $(".container.main-container").outerWidth()),
        'left': '', 'margin-left': ''
      });
    }
    function resetTakeoverPosition() {
      $('.takeover-side').css({ 'position': 'absolute', 'top': 0 });
      $('#right_to_bank').css({
        'left': '',
        'right': -$('#right_to_bank img').outerWidth()
      });
      $('#left_to_bank').css({
        'right': '',
        'left': -$('#left_to_bank img').outerWidth()
      });
    }
  </script>


