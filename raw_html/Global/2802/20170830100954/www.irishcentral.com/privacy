  <script async="async" src="https://www.irishcentral.com/assets/icprebid-52797793bdb56229c12426e86cefba6e.js" type="text/javascript"></script>
  <script>
    var googletag = googletag || {};
    googletag.cmd = googletag.cmd || [];

    pbjs.que.push(function() {
      pbjs.addAdUnits(adUnits);
    });

    // ex: getBidAndDisplayAd('ic_top_leaderboard', top_leaderboard_slot)
    function getBidAndDisplayAd(unit_code, unit_mapping_slot) {
      console.log("Calling getBidAndDisplayAd on " + unit_code + "/" + unit_mapping_slot);
      pbjs.que.push(function() {
        pbjs.requestBids({
          timeout: PREBID_TIMEOUT,
          adUnitCodes: [unit_code],
          bidsBackHandler: function() {
            pbjs.setTargetingForGPTAsync([unit_code]);
            googletag.pubads().refresh([unit_mapping_slot]);
          }
        });
      });
    }

  </script>
