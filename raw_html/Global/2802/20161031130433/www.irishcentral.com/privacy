  function loadBGTakeover() {
    var left_bg_takeover = $("<a/>", { 'style': "position: absolute; left: 0; width: 2000px; margin-left: -2000px; height: 100%; background: url('" + OAS_LEFT_BKG_IMG + "') no-repeat right top;", 'href': OAS_LEFT_BKG_URL, 'target': "_blank" } );
    var right_bg_takeover = $("<a/>", { 'style': "position: absolute; right: 0; width: 2000px; margin-right: -2000px; height: 100%; background: url('" + OAS_RIGHT_BKG_IMG + "') no-repeat left top;", 'href': OAS_RIGHT_BKG_URL, 'target': "_blank" } );
    $("body").css("background", OAS_BKG_COLOR);
    $(".container.main-container").prepend(left_bg_takeover, right_bg_takeover);
  }
</script>
