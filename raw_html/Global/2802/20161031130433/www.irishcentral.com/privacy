<script type="text/javascript">
  $(function(){

    if ( !jQuery.cookie("signup_opt_in") && !jQuery.cookie("signup_opt_out") ) {
      $('#newsletter-popup').modal();
    }



    $('form[action="/newsletter/competition"]').submit(function(e) {
      jQuery.cookie("signup_opt_in", "1", { expires: 360, path: '/' });
      return true;
    });

    $('#newsletter-popup').on('hidden.bs.modal', function (e) {
      jQuery.cookie("signup_opt_out", "1", { expires: 360, path: '/' });
      return true;
    });
    $("#signup-opt-out, #popup-close-button").on("click", function() {
      jQuery.cookie("signup_opt_out", "1", { expires: 360, path: '/' });
      $('#newsletter-popup').modal('hide');
      return true;
    });
  });
</script>

  

