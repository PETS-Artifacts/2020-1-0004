    //mobile
    oas_device = "mobile";
    sitepage_domain = sitepage_domain.replace("www", "m");
  }

  OAS_sitepage = sitepage_env + sitepage_domain + "/privacy";

  //check device and change if necessary
  var clean_listpos = "Top,x20,x21,Bottom,Frame1";
