    clean_listpos = clean_listpos.replace("x20", "x26");
    clean_listpos = clean_listpos.replace("x21", "x27");
    clean_listpos = clean_listpos.replace("x22", "x28");
    clean_listpos = clean_listpos.replace("Frame1,", "");
