    clean_listpos = clean_listpos.replace("Frame1,", "");
  } else if ( oas_device == "desktop" ) {}

  OAS_listpos = clean_listpos;
  OAS_query = "";
  OAS_target = '';
  OAS_version = 10;
  OAS_rn = new String (Math.random()); OAS_rns = OAS_rn.substring (2, 11);
  function OAS_NORMAL(pos) {
  document.write('<A HREF="' + OAS_url + 'click_nx.ads/' + OAS_sitepage + '/1' + OAS_rns + '@' + OAS_listpos + '!' + pos + '?' + OAS_query + '" TARGET=' +
