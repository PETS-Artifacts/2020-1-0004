else{
   var themes = ['red', 'orange', 'gold', 'green', 'blue','purple'];
   var x = Math.floor(Math.random() * 6);
   if(!($('body').hasClass("red") || $('body').hasClass("orange") || $('body').hasClass("gold") || $('body').hasClass("green") || $('body').hasClass("blue") || $('body').hasClass("purple"))) {
       $("body").addClass(themes[x]);
    }
   $('input[name="sitetheme"]').val(themes[x]);
   $.cookie('theme', themes[x], { path: '/' });
}

if($('#quickIcons a#order').length > 0){
    var orderLink = $('#quickIcons a#order').attr('href');
    $('#quickIcons a#order').attr('href', orderLink+'&c='+$.cookie("theme"));
}

if($.urlParam('utm_campaign')){
  var utm_campaign = $.urlParam('utm_campaign');
  var utm_source = $.urlParam('utm_source');
  var utm_medium = $.urlParam('utm_medium');
  $.cookie(utm_campaign, utm_source+';'+utm_medium+';'+utm_campaign, { path: '/' });
}
