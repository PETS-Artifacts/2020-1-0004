        <input placeholder="Search..." id="newsearch" type="text" name="newsearch" />        </div>

            <button type="submit" class="search-button"><img src="https://media1-us.shopto.net/ShopToMedia/images/theme-images/searchIcon.png" title="Search" alt="Search"/></button>
        
