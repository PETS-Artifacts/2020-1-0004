<script type="text/javascript">
    
    $('#w-basket').mouseenter(function(evt){
        evt.stopPropagation();
        headerBar.hideAll('basket');
        //$('#basket-content div.basket-items').finish();
        $('#basket-content .basket-items').slideDown();
    });
    
    $('#w-basket').mouseleave(function(evt){
        evt.stopPropagation();
        //$('#basket-content div.basket-items').finish();
        $('#basket-content .basket-items').slideUp();
    });
    basket.setHeaderValues();
    
    $('#basket-content .basket-items').hide();
</script>
        
