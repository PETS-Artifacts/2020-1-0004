<script>
    
    var filterWidth = $('.search-filter').width();
    var inputWidth = 300;
    
    $(function(){
        
        $("#newsearch").autocomplete("destroy");
        $("#newsearch").autocomplete({
            minLength : 1,
            source : '/search/getAutocompleteList?platform='+$("#platform option:selected").val(),
            position : {
                my : "left top",
                at : "left bottom",
                of : "#search-box-wrapper",
            },
            select : function(evt, ui) {
                window.location.href = ui.item.href;
            },
        }).data("autocomplete")._renderItem = function(ul, item)         
        {
            // change css for search if microsite result
            if(item.microsite)
            {
                return $("<li></li>")
                .data("item.autocomplete", item)
                .append('<a><span class="search-auto seach-microsite"><img src="'+item.icon+'" width="8" height="8"/></span>'+item.label+'</a>')
                .appendTo(ul);
            } else {
                return $("<li></li>")
                .data("item.autocomplete", item)
                .append('<a><span class="search-auto"><img src="'+item.icon+'" width="8" height="8"/></span>'+item.label+'</a>')
                .appendTo(ul);
            }        
        };
        
        $( "#newsearch" ).bind( "autocompleteopen", function(event, ui) {
            
            var search = $('#newsearch').val();
            
                    });
        
        $('#newsearch').width(inputWidth);
        
        $('#platform').bind("change", function(event) {
            
            $('#search-filter-text').text($("#platform option:selected").text());
            setSearchWidth();
            
            //This is to line the search function with platform prefix.
            $("#newsearch").autocomplete("destroy");
            $("#newsearch").autocomplete({
                minLength : 1,
                source : '/search/getAutocompleteList?platform='+$("#platform option:selected").val(),
                position : {
                    my : "left top",
                    at : "left bottom",
                    of : "#search-box-wrapper",
                },
                select : function(evt, ui) {evt.preventDefault();
                    window.location.href = ui.find('a').attr('href');
                },
            }).data("autocomplete")._renderItem = function(ul, item) {
                return $("<li></li>")
                .data("item.autocomplete", item)
                .append('<a><span class="search-auto"><img src="'+item.icon+'" width="8" height="8"/></span>'+item.label+'</a>')
                .appendTo(ul);
            };
        });
        
        $('#newsearch').focus(function(event){
            $(this).attr('placeholder', '');
        });
        
        
        $('#newsearch').bind("keyup", function(event) {
            if($(this).val()){
                $(this).css('color', '#333333');
            }else{
                $(this).css('color', 'silver');
            }
        });
        
        /*$('#newsearch').on('autocompleteselect', function(evt, ui){
            window.location.href = ui.item.href;
        });*/
        
        setSearchWidth();
    });
    
    function setSearchWidth()
    {
        $('#newsearch').width(50);
        $('#platform').width(80);
        if($('.search-filter').width() >= filterWidth) {
            $('#newsearch').width((inputWidth-($('.search-filter').width()-filterWidth)));
        }else{
            $('#newsearch').width((inputWidth+(filterWidth - $('.search-filter').width())));
        }
        $('#platform').width($('.search-filter').width());
    }
</script>
            </div>                
        </div>
        
    </div>
</header>

<script>

    // Clear default text on focus and replace with title text on loss of focus
    $('#mobile-header-login-content > form:nth-child(2) > div:nth-child(1) > div:nth-child(2)').find('input').focus(function () {
            if ($(this).val() == $(this).attr("title")) {
                    $(this).val("");
            }
    }).blur(function () {
            if ($(this).val() == "") {
                    //$(this).val($(this).attr("title"));
            }
    });


    $(document).on('click', function(event) {

        if (!$(event.target).closest('#menu-icon-img').length) 
        {
            $('.mobileDropDownMenu').hide();
            $("#menu-icon-img").attr("src","/themes/shopto/images/menu_grey.png");
        }

        if (!$(event.target).closest('.mobile-header-logged-out').length) 
        {
            $('#mobile-header-login-content').hide();
        }

        if (!$(event.target).closest('.mobile-header-logged-in').length) 
        {
            $('.account-mobile').hide();
        }

    });

    function mobileAccountLoggedOut()
    {
        $('#mobile-header-login-content').toggle();    
    }

    function mobileAccountLoggedIn()
    {
        $('.account-mobile').toggle();    
    }

    function mobileMenu()
    {
        var img = $("#menu-icon-img").attr("src");

        if (img.indexOf('menu_grey')!=-1) {

            $('.mobileDropDownMenu').show();

            $("#menu-icon-img").attr("src","/themes/shopto/images/menu_red.png");

        } else {

            $('.mobileDropDownMenu').hide();

            $("#menu-icon-img").attr("src","/themes/shopto/images/menu_grey.png");
        }

    };

    $('#mobile-header-login-close').click(function(){
            $('#mobile-header-login-link').next('#mobile-header-login-content').slideToggle();
    });

</script>    
                
