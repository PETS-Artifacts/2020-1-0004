    $('.w-currency-choice').click(function(){
        var newCurrency = $(this).attr('id');
        $('.w-currency-dropdown').hide();
        var newUrl = '$baseUrl/currency?currency='+newCurrency;
        window.location.href = '/currency?currency='+newCurrency;
    });
    
    $('#w-currency-selected').click(function(){
        headerBar.hideAll('currency');
        $('.w-currency-dropdown').slideToggle();
    });
</script>
