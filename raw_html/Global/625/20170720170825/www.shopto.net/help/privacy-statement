            <div class="search unit-m-width">
                <form action="/search/newSearchPage" id="search_box" method="GET" autocomplete="off">
                <div id="search-box-wrapper">
                    <div class="search-filter">
                        <div class="wrapper only_mobile">
                        <select id="dropdown_dept" name="Filter_department" class="dropdown-departments">
<option value="">All Departments</option>
<option value="CYCLING">CYCLING</option>
<option value="ELECTRONICS">ELECTRONICS</option>
