<script>
    
    var filterWidth = $('.search-filter').width();
    var inputWidth = 300;
   
    $(function(){

        $("#newsearch").autocomplete("destroy");
        $('#newsearch').width(inputWidth);

        $("#newsearch").autocomplete({
            minLength : 2,
            source : '/search/getAutocompleteListNew',
            position : {
                my : "left top",
                at : "left bottom",
                of : "#search-box-wrapper",
            },
            select : function(evt, ui) {
                window.location.href = ui.item.href;
            }
        })
        //This option display the autocomplete list
        .data("autocomplete")._renderItem = function(ul, item)         
        {         
            ul.css("z-index", '1000');
            if(item.departments)
            {
               return $("<li>")
                .data("item.autocomplete", item)
                .append('<a href="' + item.href + '">'+ item.label + '</a>')
                .appendTo(ul);      
            }
            else
            {
                return $("<li>")
                .data("item.autocomplete", item)
                .append('<a href="' + item.href + '"><span class="search-auto"><img src="' + item.icon + '" width="8" height="8"/></span>' + item.label + '</a>')
                .appendTo(ul);      
            }   
        }
        
        $('#newsearch').bind("keyup", function(event) {
            if($(this).val()){
                $(this).css('color', '#333333');
            }else{
                $(this).css('color', 'silver');
            }
        });
        
        $('#dropdown_dept').bind("change", function(event) {

            $('#search-filter-text').text($("#dropdown_dept option:selected").text());
            setSearchWidth();
            
        });
        setSearchWidth();
});

function setSearchWidth()
{
    $('#newsearch').width(50);
    $('#dropdown_dept').width(140);
    if($('.search-filter').width() >= filterWidth) {
        $('#newsearch').width((inputWidth-($('.search-filter').width()-filterWidth)));
    }else{
        $('#newsearch').width((inputWidth+(filterWidth - $('.search-filter').width())));
    }
    $('#dropdown_dept').width($('.search-filter').width());
}
    
</script>
            </div>
        </div>
    </div>
    
