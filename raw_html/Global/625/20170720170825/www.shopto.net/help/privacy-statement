<script>

    // functionally equivalent for click on the main button. 
    // show all main categories after first clicked and hides 
    // everything after the second clicked.
    var showTop = false;
    var buttonT = document.querySelector(".topcat");
    buttonT.addEventListener("click", function() {
        showTop = buttonsManageT(showTop);
    });
    
    function buttonsManageT(showTop)
