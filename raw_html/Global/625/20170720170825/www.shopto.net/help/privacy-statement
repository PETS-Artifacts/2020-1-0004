    //This is for the dinamic width of the supermenu in the home page based on the number of columns added in CP
    $( document ).ready(function(e) {       
                    var img = document.getElementById("img_Sony");//the image width            
            
            //check if image exist
            if(img === null)
            {
                var img = 100;
            }else{
                var img = img.width;
            }
            
            var panelSony = $(".panel.panel_Sony");	//get the panels
            var subHolderSony = $(".panel_Sony .subHolder");//get the columns in the panel
            var width_subHolder_Sony = subHolderSony.width();//get the width of each column in the panel
            var ajust_width_Sony = width_subHolder_Sony * subHolderSony.length + img;//get the width of all columns + the width of the image
            $(panelSony).width(ajust_width_Sony);//add the total width to the panel
                    var img = document.getElementById("img_Microsoft");//the image width            
            
