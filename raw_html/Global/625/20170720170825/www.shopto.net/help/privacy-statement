<script>
function closepop()
{
    $("#popup").css("display", "none");  
}

$(document).ready(function(){
    $("#super_menu").hover(function(){
        $("#popup").css("display", "none");
        }, function(){
        $("#popup").css("display", "none");
    });
});
</script>
