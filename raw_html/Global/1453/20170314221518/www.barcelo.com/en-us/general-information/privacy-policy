    if (track_home_hotel == "true"){
        var obj = JSON.parse(JSON.stringify(utag_data));
        obj.ecommerce_action = 'detail';
        obj.product_name = [""];
        obj.product_id = [""];
        obj.product_brand = [""];
        obj.product_category = [""];
        utag_data = obj;
    }

    var paso_reserva = "";

    if (paso_reserva != ""){
        var obj = JSON.parse(JSON.stringify(utag_data));

        if (paso_reserva == "4"){
            obj.ecommerce_action = 'purchase';
            obj.id = "";
            obj.revenue = "";
            obj.affiliation = "";
            obj.tax = "";
            obj.coupon = ""
        }else{
            obj.ecommerce_action = 'checkout';
            obj.ecommerce_actionField_step = "";
        }

        obj.product_id = [""];
        obj.product_name = [""];
        obj.product_brand = [""];
        obj.product_category = [""];
        obj.product_quantity = ['1'];

        if (paso_reserva != "1"){
            obj.product_price = [""];
        }
        utag_data = obj;

    }

    var include_search = 0;
    if (include_search == "1"){

