                    <input type="button" class="btndivactividadWhite" id="btnInactChat" value="Booking chat">
                <input type="button" class="btndivactividadWhitePlus" id="btnInactFAQ" value="FAQs">
                <input type="button" class="btndivactividad" id="btnInactSig" value="Continue">
            </div>
        </div>
    </div>
</div>

<script>
(function (Barcelo, $, undefined) {
    'use strict';
    jQuery.fn.exists = function () { return this.length > 0; }

    Barcelo.InactivityWidget = (function (BarceloGlobalVars) {
        function InactivityWidget() {
            var idleTime = 0;
            var enabled = true;
            var self = this;
            var $esMobil = false;
            var live = true;

            function doRefreshSession(close) {
                $.ajax({
                    type: 'POST',
                    url: web_path + "/BookingDatosPersonales/RefreshBookingSession",
                    dataType: "json",
                    data: {},
                    success: function (data) {
                        if (data.success) {

                        } else {

                        }
                        console.log("refresh success");
                    },
                    error: function (data) {
                        console.log("refresh error");
                    },
                    complete: function () {
                        if (close) {
                            $("#modal-actividad").modal("hide");
                        }
                    }
                });
            }

            $("#btnInactChat").on("click", function (e) {
                e.preventDefault();
                window.open('http://barcelo.custhelp.com/app/chat/chat_launch2'.replace(/&amp;/g, '&'), '_blank');
                $("#modal-actividad").modal("hide");
            });
            $("#btnInactFAQ").on("click", function (e) {
                e.preventDefault();
                window.open('https://barcelo.custhelp.com/'.replace(/&amp;/g, '&'), '_blank');
                $("#modal-actividad").modal("hide");
            });
            $("#btnInactSig").on("click", function (e) {
                e.preventDefault();
                doRefreshSession(true);
            });
            this.setIdleTime = function (value) {
                idleTime = value;
            };
            this.getIdleTime = function () {
                return idleTime;
            };

            this.tick = function () {
                if (idleTime > 15) { // 15 minutes      
                    if (!$("#modal-actividad").is(":visible") && enabled && !$esMobil && live) {
                        $("#modal-actividad").modal("show");
                        doRefreshSession(false);
                    }
                }
            };

            //PBC1P00009091
            function timerIncrement() {
                idleTime = idleTime + 1;
                self.tick();
            };

            function startInactivityWidget() {
                var idleInterval = setInterval(timerIncrement, 60000); // 1 minute
                //Zero the idle timer on mouse movement.
                $(window).mousemove(function (e) {
                    idleTime = 0;
                });
                $(window).keypress(function (e) {
                    idleTime = 0;
                });
                var d = Device.device;
                if(d.isMobile() && !d.isTablet() && !d.isDesktop()){
                    $esMobil = true;
                }else{
                    $esMobil = false;
                }
            };
            this.enable = function () {
                enabled = true;
            };
            this.disable = function () {
                enabled = false;
            }
            this.isEnabled = function () {
                return enabled;
            }
            this.isMobile = function () {
                return $esMobil;
            }
            this.isLive = function () {
                return live;
            }
            this.init = function () {
                startInactivityWidget();
                self.registerEnquire();
            }

            this.registerEnquire = function () {
                var that = this;

                //MOVILE XS
                //enquire.register('only screen and (max-width : 767px)', {
                //    match: function () {
                //        that.disable();
                //        $esMobil = true;
                //    },
                //    unmatch: function () {
                //        that.setIdleTime(0);
                //        that.enable();
                //        $esMobil = false;
                //    }
                //});

                Device.register({
                        id:"inactivitywidget", 
                        callback:function(d){
                            if(d.isMobile() && !d.isTablet() && !d.isDesktop()){
                        $esMobil = true;
                            }else{
                        that.setIdleTime(0);
                        that.enable();
                        $esMobil = false;
                    }
                        }
                });
                
            };

            return this.init();
        }

        return new InactivityWidget();
    })(window.BarceloGlobalVars = window.BarceloGlobalVars || {});
    
})(window.Barcelo = window.Barcelo || {}, jQuery, enquire);
</script>



