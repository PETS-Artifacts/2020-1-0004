    <script>
        function generalError(title, description, urlToGo) {
            $("#general-error-modal-title").html(title);
            $("#general-error-modal-description").html(description);
            $(".general-error-modal-close").off("click");
            $(".general-error-modal-close").on("click", function (e) {
                $("#general-error-modal").modal("hide");
                window.location = urlToGo;
            });
            $("#general-error-modal").modal("show");
        };

        $(document).ready(function (e) {
            if (window.BarceloGeneralError) {
                var generalErrorVar = window.BarceloGeneralError;
                BarceloGeneralError = null;
                generalError(generalErrorVar.title, generalErrorVar.description, generalErrorVar.urlToGo);
            }
        });
    </script>

    
