      <div class="panel panel-default">
        <div v-for="item in chunkedList" class="panel-item">
          <div id="{{issue.key+_uid+$index}}-heading" role="tab" class="panel-heading">
            <div class="panel-title"><a role="button" data-toggle="collapse" data-parent="#recalls-issues{{_uid}}" href="#{{issue.key+_uid+$index}}" aria-expanded="false" aria-controls="{{issue.key+_uid+$index}}" class="collapsed"><i class="faq-Icon"></i>
                <div>{{item.reportReceivedDate | date}} <span>NHTSA Campaign Number: {{item.nhtsaCampaignNumber}}</span></div><span class="panel-title-caption">{{item.subject}}
                  <p>{{item.consequence}}</p></span></a></div>
          </div>
          <div id="{{issue.key+_uid+$index}}" role="tabpanel" aria-labelledby="{{issue.key+_uid+$index}}-heading" class="panel-collapse collapse">
            <div class="panel-body">
              <p><b>NHTSA Campaign Number:&nbsp;</b><span>{{item.nhtsaCampaignNumber}}</span></p>
              <p><b>Manufacturer&nbsp;</b><span>{{item.manufacturer}}</span></p>
              <p><b>Components&nbsp;</b><span>{{item.components | componentName}}</span></p>
              <p v-if="item.potentialNumberOfUnitsAffected"><b>Potential Number of Units Affected&nbsp;</b><span>{{item.potentialNumberOfUnitsAffected | numberWithCommas}}</span></p>
              <div v-show="item.summary">
                <p class="vehicle-detail--issue-summary"><b>Summary</b></p>
                <p v-html="item.summary | returnsToBreaks"></p>
              </div>
              <div v-show="item.correctiveAction">
                <p><b>Remedy</b></p>
                <p v-html="item.correctiveAction | returnsToBreaks"></p>
              </div>
              <div v-show="item.notes">
                <p><b>Notes</b></p>
                <p v-html="item.notes | returnsToBreaks"></p>
              </div>
              <associated-products :products="item.associatedProducts" title="Affected Product"></associated-products>
              <associated-documents :documents="item.associatedDocuments" title="Associated Document"></associated-documents>
              <associated-investigations :investigations="item.investigations" title="Associated Investigation"></associated-investigations>
              <associated-recalls :recalls="item.recalls" title="Associated Recall"></associated-recalls>
            </div>
          </div>
        </div>
      </div>
    </div>
    <memory-pagination :list="filteredIssues" @mp-update="updateList" :offset="5" v-show="filteredIssues.length &gt;= 5"></memory-pagination>
  </div>
</script>
