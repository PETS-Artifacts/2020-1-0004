            <div class="pull-left">
                <a class="btn-round btn btn-xs" href="javascript:void(0)" title="Previous Theatres" data-bind="click: theatres.previous, css:{ disabled: !theatres.hasPrevious()}">
                    <span class="glyphicon glyphicon-chevron-left"></span><span class="sr-only" aria-hidden="true">Previous</span>
                </a>
            </div>
            <div class="pull-right">
                <a class="btn-round btn btn-xs" href="javascript:void(0)" title="More Theatres" data-bind="click: theatres.next, css:{ disabled: !theatres.hasNext()}">
                    <span class="sr-only" aria-hidden="true">Next</span><span class="glyphicon glyphicon-chevron-right"></span>
                </a>
            </div>
        </nav>
    </div>
</div>
</script>
    <script type="text/javascript">
        amc = amc || {};
        amc.currentContext = amc.currentContext || {};
        amc.require.onReady(function () {
            if (amc.currentContext['mobile-account-navigation-bound'])
                return;

            amc.currentContext['mobile-account-navigation-bound'] = true;
            ko.applyBindings(new amc.views.MobileAccountNavigationViewModel(), $('.mobile-navbar').get(0));
        });
    </script>

    <script type="text/javascript">
        amc.require.onReady('low', function () {
            amc.views.helpers.renderForDesktop(function () {
                amc.events.primaryMovieMenuHover.one($(document), function (evt, data) {
                    $('#mainnav-movies-multi-5259').html($('#movie-menu-template').html());
                 
                    amc.events.myMoviesLoaded.bind($(document), function (evt, data) {
                        amc.myMovies.BindKo('#mainnav-movies-multi-5259 [data-bind-my-movie]');
                    });                  
                });
            });
        });
    </script>
