    <script>
        amc.require.onReady('high', function() {
            amc.events.userInfoInitialized.one(function() {
                $('.powerbar').addClass((amc.user.info.hasPointEarningRewards() || amc.user.info.hasLegacyAccount()) ? 'navbar-inverse' : 'navbar-default').css('visibility', 'visible');
                $(".account-navigation").addClass(amc.user.info.hasLegacyAccount() ? "legacy" : "");

                if (window.location.href.indexOf("/order/confirmation") < 0 && amc.user.info.location.getCurrentTheatre()) {
                    var profileMessage = amc.user.info.profileCompletionMessage();
                    if (profileMessage.length > 0 ) {
                        $('.profile-completion-alert').show();
                        $('#profile-completion-alert-message').html(profileMessage);
                    }
                }
            });
        });
    </script>

