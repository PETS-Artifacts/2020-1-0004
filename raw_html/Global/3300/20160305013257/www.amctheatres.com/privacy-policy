<script type="text/javascript">
    amc = amc || {};
    amc.currentContext = amc.currentContext || {};
    amc.require.onReady(function () {
        if (amc.currentContext['shopping-cart-navigation-bound'])
            return;

        amc.currentContext['shopping-cart-navigation-bound'] = true;

        var vm = new amc.views.ShoppingCartViewModel();
        ko.applyBindings(vm, $('#shopping-cart-navigation').get(0));
        vm.initShoppingCartNav({"ShoppingCartId":0,"AmcAccountId":null,"Total":0.0,"Subtotal":0.0,"Email":null,"CartItems":[],"SavedForLaterItems":[],"Messages":[],"Warnings":[],"CanRemoveItems":false,"Quantity":0},{ bindingContainerSelector: '#shopping-cart-navigation'} );
    });
</script>

