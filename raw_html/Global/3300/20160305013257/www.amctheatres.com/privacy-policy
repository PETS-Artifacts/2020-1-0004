<script>
amc.require.onReady(function () {
    var IETest = (function () {
        "use strict";

        var isIE = false;
        var version = null;

        var jsMap = {
            "5.5": 5.5,
            "5.6": 6,
            "5.7": 7,
            "5.8": 8,
            "9": 9,
            "10": 10
        };
        var jsVersion = new Function("/*@cc_on return @_jscript_version; @*/")();

        if (jsVersion !== undefined) {
            isIE = true;
            version = jsMap[jsVersion];
        }

        return {
            isIE: isIE,
            version: version
        };
    }());

    if (!IETest.isIE)
        return;

    if (!IETest.version)
        return;

    if ($.cookie("ieSSLWarningAcknowledged"))
        return;
		
	var showModal = function(){
		var closeClick = function () {
			$.cookie("ieSSLWarningAcknowledged", true);
			$.fancybox.close();
		};

		var contents = $('<div><h2 class="h2 zing" style="margin-top: 0">Security Warning!</h2><h3>browser update suggested</h3><p> Unfortunately your browser does not support the latest technology required to securely purchase tickets on the AMC website. For your security, we recommend you upgrade to a supported browser in order to make a ticket purchase. Or, if you are just looking for showtimes and theatre information, you may continue to browse.</p></div>');
		var controls = $('<p style="text-align: right"><a href="http://browsehappy.com/"><span class="btn">Upgrade Your Browser</span></a></p>');
		var button = $('<button class="btn" style="margin-left: 10px">Continue to Browse Only</button>');

		button.one('click', closeClick);
		controls.append(button);
		contents.append(controls);

		$.fancybox.open({
			maxWidth: 600,
			maxHeight: 600,
			closeClick: false,
			modal: true,
			content: contents
		});
	};
	
	$('<img />').on('error', showModal).attr('src', 'https://payment.amctheatres.com/img/amc-logo-circle.png').appendTo($(document));
});
</script>
