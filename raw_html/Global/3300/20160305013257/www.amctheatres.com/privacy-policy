    <script type="text/javascript">
        amc.require.onReady('low', function () {
            amc.views.helpers.renderForDesktop(function () {
                amc.events.primaryTheatreMenuHover.one($(document), function (evt, data) {
                    var el = $('#theatre-list-menu-1693');
                    el.html($('#theatre-list-menu-template').html());
                    ko.applyBindings(new amc.views.TheatreListMenuViewModel(), el.get(0));
                });
            });
        });
    </script>
    <script type="text/html" id="theatre-template">
        <li class="col-sm-4">
            <div class="media-left">
                <img class="media-object theatre-icon" data-bind="attr: { src: TheatreImageIcon }">
            </div>
            <div class="media-body">
                <h4 class="margin-top-none margin-bottom-none"><a href="#" data-bind="attr: { href: WebsiteUrl}, text: Name"></a></h4>
                <p>
                    <span data-bind="text: AddressLine1"></span><br />
                    <span data-bind="visible: AddressLine2(), html: AddressLine2() + '<br />'"></span>
                    <span data-bind="text: City() + ', ' + State() + ' ' + FormattedPostalCode()"></span><br />

                    <!-- ko if: $parent.currentTheatreId() && $parent.currentTheatreId() == TheatreId() -->
                    <span data-bind="visible: $parent.showMakeCurrentTheatre"><strong>Your current theatre</strong></span>
                    <!-- /ko -->

                    <!-- ko if: !$parent.currentTheatreId() || $parent.currentTheatreId() != TheatreId() -->
                    <a href="#" data-bind="visible: $parent.showMakeCurrentTheatre, click: $parent.setTheatreAsCurrent">Make current theatre</a>
                    <!-- /ko -->
                </p>
            </div>
            <!-- /end .body -->
        </li>
        <li class="clearfix" data-bind="visible: ($index() + 1) % 3 == 0"></li>
    </script>
