<script type="text/javascript">
    (function (amc) {
        amc.require.now(function () {
            amc.user.info.init({ inlineDataId: 'user-info-inline-data' });

            var status = amc.user.info.loginStatus();

            window.dataLayer = window.dataLayer || [];
            dataLayer.push({ 'loginStatus': status });

            var authenticatedWithLoyaltyStyle = "";
            switch (status) {
                case "Free":
                    authenticatedWithLoyaltyStyle = " authenticated-with-free-loyalty";
                    break;
                case "Premiere":
                case "Legacy":
                    authenticatedWithLoyaltyStyle = " authenticated-with-loyalty";
                    break;
            }

            if (status != 'Anonymous')
                document.getElementsByTagName('html')[0].className += ' authenticated' + authenticatedWithLoyaltyStyle;
        });
    })(amc);
</script>

