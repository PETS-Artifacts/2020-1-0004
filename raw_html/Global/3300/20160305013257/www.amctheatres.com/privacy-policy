    <script>
        amc.require.onReady('high', function() {
            amc.events.userInfoInitialized.one(function() {
                $('.powerbar').addClass((amc.user.info.isAssociate() || amc.user.info.hasPremiereAccount() || amc.user.info.hasFreeAccount() || amc.user.info.hasLegacyAccount()) ? 'navbar-inverse' : 'navbar-default').css('visibility', 'visible');

                $('.powerbar').addClass((amc.user.info.isLoggedIn() && amc.user.info.hasFreeAccount()) ? 'navbar-free-tier' : '');

                $('.account-navigation').addClass((amc.user.info.isLoggedIn() && amc.user.info.hasPremiereAccount()) ? 'premiere' : '');

                $(".account-navigation").addClass(amc.user.info.hasLegacyAccount() || amc.user.info.isAssociate() ? "legacy" : "");

                if (amc.user.info.location.getCurrentTheatre()) {
                    var loyaltyVersion = parseInt(amc.user.info.location.getCurrentTheatre().LoyaltyVersion);

                    if (amc.user.info.isLoggedIn() && (!amc.user.info.isProfileComplete() && (loyaltyVersion === amc.user.info.loyaltyVersion.V2))) {
                        var profileCompletionMsg = "";

                        if (window.location.href.indexOf("/settings") > -1) {
                            profileCompletionMsg = "Get all the benefits of the AMC Stubs Program by completing your profile.";
                        }
                        else {
                            profileCompletionMsg = "Get all the benefits of the AMC Stubs Program by completing your profile. <a href='/settings'>Click here to complete your profile!</a>";
                        }

                        $('.profile-completion-alert').show();
                        $('#profile-completion-alert-message').html(profileCompletionMsg);
                    }
                }
            });
        });
    </script>

