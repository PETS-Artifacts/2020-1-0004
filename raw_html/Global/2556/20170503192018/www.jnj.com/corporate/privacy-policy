<script type="text/javascript">$(' .Byline .Byline-author-name [itemprop="name"] a, .Byline .Byline-author-name [itemprop="name"]').each(function(){
var $this = $(this);
if( /,$/.test($this.html()) ){
	$this.html($this.html().replace(/,$/,""));
}
})</script>
<script type="text/javascript">(function($hline){
   if($hline.length){
   	$hline.html($hline.html().replace('J&amp;J','J<small style="font-size: 57%;position: relative;top: -8px;display: inline-block;left: 1px;">&amp;</small>J '));
   }
})($('[data-jnj-article-options*=7-things-you-may-not-know-about-band-aid-brand-adhesive-bandages] .ArticleFullBleedLede-headline'));

(function($relSlide){
	$relSlide.each(function(iter){
       var $iter = $($relSlide[iter]);
       $iter.html($iter.html().replace('J&amp;J','J<span style="font-size: 57%;position: relative; top: -6px;">&</span>J')); 
    });
})($('[data-jnj-lede-carousel] a[href*=7-things-you-may-not-know-about-band-aid-brand-adhesive-bandages]'));

(function($galleryArticle){
   if($galleryArticle.length){
       $galleryArticle.addClass('_narrowGallery')
   }
})($('[data-jnj-article-options*=8-innovative-office-perks-designed-to-make-you-happier-at-work] .Gallery-introText'));</script>
<script type="text/javascript">/*RTE is leaving an empty P tag at the end which is not enabling selection of the last item... This pretty much removes that empty P tag and then cleans up the bottom margin for the item that should be the last child.  

REASON:  In pages with above, and below section modules the empty p tag plus the default margins was adding too much space.*/
(function(){
  var p = $('[data-jnj-article-options*=a-perfect-match-how-a-bone-marrow-transplant-saved-a-johnson-and-johnson-employees-life] .ArticleFullBleedLede-body p:empty:last-child');
  if(p.length){
     p.remove();
     $('head').append('<style>@media only screen and (min-width:768px){.CallToActionModule-inner{margin-top:0}}.Separator:last-child{margin-bottom:0}</style>');
  } 
})();</script>
<script type="text/javascript">$('.ArticleFullBleedLede-articleModulesAboveTags .RichTextModule-description .ArticleEnhancement-align-middle').css('margin', '0 auto');</script>

