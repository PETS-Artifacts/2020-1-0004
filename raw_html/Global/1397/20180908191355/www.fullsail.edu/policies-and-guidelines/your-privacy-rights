<script>
var fmf = Object();
fmf.get_cookie_obj = function()
{
var cookie_obj = Object();
cookie_obj['affiliatecode'] = fmf.get_cookie('AFFILIATECODE');
cookie_obj['subid']         = fmf.get_cookie('SUBID');
cookie_obj['formid']        = fmf.get_cookie('FORMID');
cookie_obj['landingpage']   = fmf.get_cookie('LANDINGPAGE');
cookie_obj['utm_content']   = fmf.get_cookie('UTM_CONTENT');
cookie_obj['mncid']         = fmf.get_cookie('MNCID');
cookie_obj['mnc']           = fmf.get_cookie('MNC');
cookie_obj['mnckeyword']    = fmf.get_cookie('MNCKEYWORD');
cookie_obj['kw']            = fmf.get_cookie('KW');
cookie_obj['programid']     = fmf.get_cookie('PROGRAMID');
cookie_obj['groupid']       = fmf.get_cookie('GROUPID');
cookie_obj['isinlb']        = fmf.get_cookie('ISINLB');
cookie_obj['gclid']         = fmf.get_cookie('CLICKID');
cookie_obj['msclkid']       = fmf.get_cookie('CLICKID');
cookie_obj['nan_pid']       = fmf.get_cookie('CLICKID');
return cookie_obj;
};
fmf.get_cookie = function(cname)
{
var name = cname + "=";
var ca = document.cookie.split(';');
for(var i=0; i<ca.length; i++)
{
var c = ca[i];
while (c.charAt(0)==' ') c = c.substring(1);
if (c.indexOf(name) == 0) return c.substring(name.length,c.length);
}
return "";
}
fmf.set_cookie = function(cname, cvalue, exdays)
{
var cstr = '';
var d = new Date();
d.setTime(d.getTime() + (exdays*24*60*60*1000));
var cdomain = '.fullsail.edu';
cstr = cstr + cname + "=" + cvalue + ";";
cstr = cstr + "expires=" + d.toUTCString() + ";";
cstr = cstr + "domain=" + cdomain + ";";
cstr = cstr + "path=/";
document.cookie = cstr;
}
fmf.get_querystring_obj = function()
{
var querystring_obj = Object();
var urlquery = unescape(location.search);
if(!urlquery)
{
return querystring_obj;
}
urlquery = urlquery.substring(1);
var kv_stack = urlquery.split('&');
for( var x = 0; x < kv_stack.length; x++)
{
var kv_item = kv_stack[x].split("=");
var kv_key = kv_item[0];
var kv_val = kv_item[1];
kv_key = kv_key.toLowerCase();
querystring_obj[kv_key] = kv_val;
}
return querystring_obj;
}
fmf.get_qstring_obj = function()
{
var qs = fmf.get_querystring_obj();
var qstring_obj = Object();
qstring_obj['affiliatecode'] = (qs['affiliatecode'] ? qs['affiliatecode'] : '');
qstring_obj['subid']         = (qs['subid']         ? qs['subid']         : '');
qstring_obj['formid']        = (qs['formid']        ? qs['formid']        : '');
qstring_obj['landingpage']   = (qs['landingpage']   ? qs['landingpage']   : '');
qstring_obj['utm_content']   = (qs['utm_content']   ? qs['utm_content']   : '');
qstring_obj['mncid']         = (qs['mncid']         ? qs['mncid']         : '');
qstring_obj['mnc']           = (qs['mnc']           ? qs['mnc']           : '');
qstring_obj['mnckeyword']    = (qs['mnckeyword']    ? qs['mnckeyword']    : '');
qstring_obj['kw']            = (qs['kw']            ? qs['kw']            : '');
qstring_obj['programid']     = (qs['programid']     ? qs['programid']     : '');
qstring_obj['step']          = (qs['step']          ? qs['step']          : '');
qstring_obj['groupid']       = (qs['groupid']       ? qs['groupid']       : '');
qstring_obj['isinlb']        = (qs['isinlb']        ? qs['isinlb']        : '');
qstring_obj['gclid']         = (qs['gclid']         ? qs['gclid']         : '');
qstring_obj['msclkid']       = (qs['msclkid']       ? qs['msclkid']       : '');
qstring_obj['nan_pid']       = (qs['nan_pid']       ? qs['nan_pid']       : '');
if(qstring_obj['affiliatecode'])
{
qstring_obj['affiliatecode'] = qstring_obj['affiliatecode'];
}
else if(qstring_obj['subid'])
{
qstring_obj['affiliatecode'] = qstring_obj['subid'];
}
else
{
qstring_obj['affiliatecode'] = '';
}
if(qstring_obj['landingpage'])
{
qstring_obj['landingpage'] = qstring_obj['landingpage'];
}
else if(qstring_obj['utm_content'])
{
qstring_obj['landingpage'] = qstring_obj['utm_content'];
}
else
{
qstring_obj['landingpage'] = '';
}
if(qstring_obj['mncid'])
{
qstring_obj['mncid'] = qstring_obj['mncid'];
}
else if(qstring_obj['mnc'])
{
qstring_obj['mncid'] = qstring_obj['mnc'];
}
else
{
qstring_obj['mncid'] = '';
}
if(qstring_obj['mnckeyword'])
{
qstring_obj['mnckeyword'] = qstring_obj['mnckeyword'];
}
else if(qstring_obj['kw'])
{
qstring_obj['mnckeyword'] = qstring_obj['kw'];
}
else
{
qstring_obj['mnckeyword'] = '';
}
if(qstring_obj['gclid'])
{
qstring_obj['gclid'] = qstring_obj['gclid'];
}
else if(qstring_obj['msclkid'])
{
qstring_obj['msclkid'] = qstring_obj['msclkid'];
}
else if(qstring_obj['nan_pid'])
{
qstring_obj['nan_pid'] = qstring_obj['nan_pid'];
}
else
{
}
return qstring_obj;
}
fmf.get_combo_obj = function()
{
var combo_obj = Object();
var qo = fmf.get_qstring_obj();
var co = fmf.get_cookie_obj();
combo_obj['affiliatecode'] = (qo['affiliatecode'] ? qo['affiliatecode'] : co['affiliatecode']);
combo_obj['subid']         = (qo['subid']         ? qo['subid']         : co['subid']);
combo_obj['formid']        = (qo['formid']        ? qo['formid']        : co['formid']);
combo_obj['landingpage']   = (qo['landingpage']   ? qo['landingpage']   : co['landingpage']);
combo_obj['utm_content']   = (qo['utm_content']   ? qo['utm_content']   : co['utm_content']);
combo_obj['mncid']         = (qo['mncid']         ? qo['mncid']         : co['mncid']);
combo_obj['mnc']           = (qo['mnc']           ? qo['mnc']           : co['mnc']);
combo_obj['mnckeyword']    = (qo['mnckeyword']    ? qo['mnckeyword']    : co['mnckeyword']);
combo_obj['kw']            = (qo['kw']            ? qo['kw']            : co['kw']);
combo_obj['programid']     = (qo['programid']     ? qo['programid']     : co['programid']);
combo_obj['step']          = (qo['step']          ? qo['step']          : '');
combo_obj['groupid']       = (qo['groupid']       ? qo['groupid']       : co['groupid']);
combo_obj['isinlb']        = (qo['isinlb']        ? qo['isinlb']        : co['isinlb']);
combo_obj['gclid']         = (qo['gclid']         ? qo['gclid']         : co['gclid']);
combo_obj['msclkid']       = (qo['msclkid']       ? qo['msclkid']       : co['msclkid']);
combo_obj['nan_pid']       = (qo['nan_pid']       ? qo['nan_pid']       : co['nan_pid']);
return combo_obj;
}
fmf.get_monster_obj = function()
{
var combo_obj = fmf.get_combo_obj();
var monster_obj = Object();
if( combo_obj['affiliatecode'] )
{
monster_obj['affiliatecode'] = combo_obj['affiliatecode'];
}
else if( combo_obj['subid'] )
{
monster_obj['affiliatecode'] = combo_obj['subid'];
}
else
{
monster_obj['affiliatecode'] = '';
}
if( combo_obj['formid'] )
{
monster_obj['formid'] = combo_obj['formid'];
}
else
{
monster_obj['formid'] = '';
}
if( combo_obj['landingpage'] )
{
monster_obj['landingpage'] = combo_obj['landingpage'];
}
else if( combo_obj['utm_content'] )
{
monster_obj['landingpage'] = combo_obj['utm_content'];
}
else
{
monster_obj['landingpage'] = '';
}
if( combo_obj['mncid'] )
{
monster_obj['mncid'] = combo_obj['mncid'];
}
else if( combo_obj['mnc'] )
{
monster_obj['mncid'] = combo_obj['mnc'];
}
else
{
monster_obj['mncid'] = '1348';
}
if( combo_obj['mnckeyword'] )
{
monster_obj['mnckeyword'] = combo_obj['mnckeyword'];
}
else if( combo_obj['kw'] )
{
monster_obj['mnckeyword'] = combo_obj['kw'];
}
else
{
monster_obj['mnckeyword'] = '';
}
if( combo_obj['programid'] )
{
monster_obj['programid'] = combo_obj['programid'];
}
else
{
monster_obj['programid'] = '';
}
if( combo_obj['step'] )
{
monster_obj['step'] = combo_obj['step'];
}
else
{
monster_obj['step'] = '';
}
if( combo_obj['step'] == 'input' && !combo_obj['landingpage'].length && !combo_obj['utm_content'].length )
{
if (window.location.pathname == "/")
{
var path = "home";
}
else
{
var path = window.location.pathname;
}
monster_obj['landingpage'] = path;
}
if( combo_obj['groupid'] )
{
monster_obj['groupid'] = combo_obj['groupid'];
}
else
{
monster_obj['groupid'] = '';
}
if( combo_obj['isinlb'] )
{
monster_obj['isinlb'] = combo_obj['isinlb'];
}
else
{
monster_obj['isinlb'] = '';
}
if( combo_obj['gclid'] )
{
monster_obj['gclid'] = combo_obj['gclid'];
}
if( combo_obj['msclkid'] )
{
monster_obj['msclkid'] = combo_obj['msclkid'];
}
if( combo_obj['nan_pid'] )
{
monster_obj['nan_pid'] = combo_obj['msclkid'];
}
return monster_obj;
}
fmf.checkc = function(cofv, mofv)
{
if(mofv)
{
if(mofv != cofv)
{
return true;
}
}
return false;
}
fmf.monster_to_cookie = function()
{
var mo = fmf.get_monster_obj();
var co = fmf.get_cookie_obj();
if(fmf.checkc(co['subid'],       mo['affiliatecode']) ) { fmf.set_cookie('SUBID',       mo['affiliatecode'], 14); }
if(fmf.checkc(co['formid'],      mo['formid'])        ) { fmf.set_cookie('FORMID',      mo['formid'],        14); }
if(fmf.checkc(co['landingpage'], mo['landingpage'])   ) { fmf.set_cookie('LANDINGPAGE', mo['landingpage'],   14); }
if(fmf.checkc(co['mnc'],         mo['mncid'])         ) { fmf.set_cookie('MNC',         mo['mncid'],         14); }
if(fmf.checkc(co['mnckeyword'],  mo['mnckeyword'])    ) { fmf.set_cookie('MNCKEYWORD',  mo['mnckeyword'],    14); }
if(fmf.checkc(co['programid'],   mo['programid'])     ) { fmf.set_cookie('PROGRAMID',   mo['programid'],     14); }
if(fmf.checkc(co['groupid'],     mo['groupid'])       ) { fmf.set_cookie('GROUPID',     mo['groupid'],       14); }
if(fmf.checkc(co['isinlb'],      mo['isinlb'])        ) { fmf.set_cookie('ISINLB',      mo['isinlb'],        14); }
if(fmf.checkc(co['gclid'],       mo['gclid'])         ) { fmf.set_cookie('CLICKID',     mo['gclid'],         14); }
if(fmf.checkc(co['msclkid'],     mo['msclkid'])       ) { fmf.set_cookie('CLICKID',     mo['msclkid'],       14); }
if(fmf.checkc(co['nan_pid'],     mo['nan_pid'])       ) { fmf.set_cookie('CLICKID',     mo['nan_pid'],       14); }
}
fmf.monster_to_dlayer = function()
{
var mo = fmf.get_monster_obj();
dataLayer = [];
dataLayer.push(mo);
}
fmf.monster_to_cookie();
fmf.monster_to_dlayer();
</script>
