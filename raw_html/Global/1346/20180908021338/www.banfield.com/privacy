<script type="text/javascript">
    $(function () {
        $('#btnSubmitNotificationEnrollment').click(function () {
            var info = {
                Email: $('#enrollment_email').val(),
                Birthday: $('#enrollment_month').val() + '/' + $('#enrollment_day').val() + '/' + $('#enrollment_year').val()
            };
                
            $('#enrollment_birthday_error').text('');
            $('#enrollment_email_error').text('');

            $.ajax({
                url: "/api/userregistrationapi/NotificationEnrollment",
                type: "POST",
                headers: { 'RequestVerificationToken': $('#__AntiXsrfToken').val() },
                dataType: "json",
                data: info,
                success: function (response) {
                    if (response.errors) {
                        var canApplyAgeCheck = false;
                        //alert(JSON.stringify(response.errors));
                        for (var x in response.errors) {
                            var errorMsg = response.errors[x];

                            if (x == 'request.Email') {
                                $('#enrollment_email_error').text(errorMsg);
                                canApplyAgeCheck = true;
                            }
                            else if (errorMsg.indexOf("Birthday") > -1) {
                                $('#enrollment_birthday_error').text(errorMsg);
                                canApplyAgeCheck = true;
                            }
                        }
                          
                        if (!canApplyAgeCheck){
                            for (var x in response.errors) {
                                if (response.errors[x] == "You are not eligible for this offer.") {
                                    $('#newsletter-completed-header').html("We're sorry!");
                                    $('#newsletter-completed-body').html("Based on the information you've entered, you're not elegible at this time.");
                                    $('#newsletter-completed-body').addClass('email-response-2');
                                    parent.$.fancybox.close();
                                    $('#newsletter-completed-trigger').trigger('click');
                                }
                            }
                        }
                          
                          
                    } else {
                        _satellite.track("emailsuccessdirect");
                        $('#newsletter-completed-header').html("Thank you for signing up!");
                        $('#newsletter-completed-body').html("Look for a Welcome Email from your friends at Banfield coming soon!");
                        parent.$.fancybox.close();
                        $('#newsletter-completed-body').addClass('email-response-1');
                        $('#newsletter-completed-trigger').trigger('click');
                    }
                },
                error: function (jqXHR, textStatus, errorThrown) {
                    alert("Error: " + errorThrown);
                }
            });

            return (false);
        });         
    });
</script><!--<script src="https://scripts.banfield.com/CMSScripts/Custom/jquery.truncate.min.js?BanfieldBuildReleaseTag=20180830"></script>
