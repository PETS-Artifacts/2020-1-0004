    <script type="text/javascript">
        jQuery(document).ready(function ($) {
            $('#hospitalLocationSearch').bind('focus', function (e) {
                _gas.push(['_setCustomVar', 4, 'Hospital Locator Widget', 'Start', 3]);
            });
            $('#hospitalLocationSearch').bind('keypress', function (e) {
                var ENTER_KEY_CODE = 13;
                var code = (e.keyCode ? e.keyCode : e.which);
                if (code == ENTER_KEY_CODE) {
                    e.preventDefault();
                    var addr = document.getElementById('hospitalLocationSearch').value;
                    if (addr != null && addr != undefined) {
                        hospitalLocator.geocodeAddessRedirect(document.getElementById('hospitalLocationSearch').value, '/our-hospitals/hospital-locations');
                    }
                    return false;
                }
            });

            $('#btnSubmit').click(function (e) {
                e.preventDefault();
                var addr = document.getElementById('hospitalLocationSearch').value;
                if (addr != null && addr != undefined) {
                    _gas.push(['_setCustomVar', 4, 'Hospital Locator Widget', 'Consideration: Complete', 3]);
                    _gas.push(['_trackEvent', 'Widget', 'Hospital Locator', 'Locator Search', 2, false]);
                    hospitalLocator.geocodeAddessRedirect(addr, '/our-hospitals/hospital-locations');
                }
                return false;
            });
        });
    </script>

