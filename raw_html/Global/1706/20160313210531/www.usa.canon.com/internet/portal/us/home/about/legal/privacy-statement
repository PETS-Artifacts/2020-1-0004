	//Check for browser compatibility
	if ($('#devicetype-desktop').is(':visible')) { 
		$.reject();
	}else{
		//mobile device so check versions
		if(checkMobileVersion()){
			// if true version is old
			$.reject({
				reject:{all:true},
				browserShow: false,
				header: "Your browser is currently not supported.",
				paragraph1: "Your mobile device's operating system and/or browser are out of date and not supported. ",
				paragraph2: 'Please consider updating your phone.',   
			});
		}
	}
	
	//Check to show theme baner
	var showBanner = 'N';
	if(showBanner == 'Y') $('#themeBanner').show();
	
	
	
	createMMCThemeLinks();
</script>

