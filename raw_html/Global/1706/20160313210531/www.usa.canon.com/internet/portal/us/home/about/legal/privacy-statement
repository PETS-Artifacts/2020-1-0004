	<script>
		var category = "[ObjectIDImpl 'Z6_MQH8HIC0LOEBC0AEJN3H7A1OM1', CONTENT_NODE, VP: -2493, [Domain: rel], DB: 43F6-564714250315BBC58072F38E7854C036]".split("'")[1];
		var parentCat = "[ObjectIDImpl 'Z6_MQH8HIC0L0R970A6D6H9NB0DC0', CONTENT_NODE, VP: -2493, [Domain: rel], DB: 43F6-564714250315EC748032CDC47417680C]".split("'")[1]; 
		var catTitle = "Privacy Statement";
		if(catTitle == 'Home'){
			digitalData.page.category.primaryCategory = category;		
		}else{
			digitalData.page.category.primaryCategory = parentCat;
			digitalData.page.category.parentCategory = parentCat;		
		}	

	</script>

