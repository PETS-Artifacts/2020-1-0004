var securityUrl = baseurl+"home/getSecuritySettings";

$( document ).ajaxSuccess(function( event, xhr, settings ) {
 var str = settings.url;
 var n = str.indexOf("getSecuritySettings");
 
if(n<0){
		$.ajax({
		  type: 'GET',
		  async:false,
		  url: securityUrl,
		  data: '',
		  success: function(html) {
		  var obj = JSON.parse(html);
				csfrData[obj.varTokenName] = obj.varTokenValue;
				$("form").each(function(){
					$(this).find('input[name="'+obj.varTokenName+'"]').val(obj.varTokenValue);
				});
		  }
		}); 
		}
});
