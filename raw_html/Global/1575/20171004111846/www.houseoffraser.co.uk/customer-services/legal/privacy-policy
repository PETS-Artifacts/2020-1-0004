<script>
    var NewsLetterClicked = function() {
        if (!$('#js-email-signup').valid()) {
            hof.analytics.pushFormJsErrorsToGTM($("#js-email-signup").validate().errorList, "client", "Email sign up");
            return false;
        }
        else {
            hof.analytics.formSubmitEventPushToGTM("Email sign up");
            return true;
        }
    }
</script>

