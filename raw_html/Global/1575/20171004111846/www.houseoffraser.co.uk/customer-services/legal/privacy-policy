                                        <button type="button" class="js-remove-item remove-prod" aria-label="Remove item " data-bind="event: { click: $parent.removeBasketItem } "><span aria-hidden="true"><i class="hof-md hof-delete"></i></span></button>
                                        <!-- /ko -->
                                        <!-- ko if: !BasketItemDetail.Updateable()-->
                                        <span class="col-md-3 pull-right">FREE</span>
