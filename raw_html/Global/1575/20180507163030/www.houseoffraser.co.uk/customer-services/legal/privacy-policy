<script>
    var NewsLetterClicked = function() {
        if (!$('#js-email-signup').valid()) {
            hof.analytics.pushFormJsErrorsToGTM($("#js-email-signup").validate().errorList, "client", "Email sign up");
            $(".footer__sgnup__input").parent().addClass("hof-invalid-email");
            $(".js-email-signup-error").removeClass("hide");
            return false;
        }
        else {
            hof.analytics.formSubmitEventPushToGTM("Email sign up");
            $(".footer__sgnup__input").parent().removeClass("hof-invalid-email");
            $(".js-email-signup-error").addClass("hide");
            return true;
        }       
    }    
</script>

