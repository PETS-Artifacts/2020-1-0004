                                                $(document).ready(function () {
                                                    //var pre = function () {
                                                    //    $('.signupEmailProcessing').css({ 'display': 'block', 'opacity': '0' });
                                                    //    $('.signupEmailSuccess').css({ 'display': 'block', 'opacity': '0' });
