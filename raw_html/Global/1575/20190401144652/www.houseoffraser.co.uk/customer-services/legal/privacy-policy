                                                    var error = function () {
                                                        $('.signupEmailError').css('display', 'block');
                                                        $('.signupEmailProcessing').css('display', 'none');
                                                    }
                                                    window.SetupMailSubscriptionInline('#menNewsLetter', 'male', '3', '1', 'HOF', '', processing, success, error);
                                                    window.SetupMailSubscriptionInline('#womenNewsLetter', 'female', '3', '1', 'HOF', '', processing, success, error);
