function valide() {
	var url="https://gc.synxis.com/";
	var Hotel="?Hotel=58302";
	var Chain="&Chain=14463";
	var Dest="&Dest=SUN";
	var varStart="&start=availresults&childages=13|13|13";

	var jour=document.getElementById('jour').value;
	var mois=document.getElementById('mois').value;
	var annee=document.getElementById('annee').value;

	var nuits="&Nights="+document.getElementById('Nights').value;
	var adult="&Adult="+document.getElementById('Adult').value;
	var child="";
	
    
      
		var locale="&locale=en-US";
		var datearrive='&arrive='+mois+'/'+jour+'/'+annee;
	

	//var urltogo=url+Hotel+Chain+Dest+locale+adult+child+datearrive+nuits;
	//window.open(urltogo, '_blank');	
	
	try {
		ga(function() {
		var eventlabel = datearrive+nuits+adult+child+Hotel;
		ga('send', 'event', 'booking-form', 'submit', eventlabel);
		var tracker = ga.getByName('t0');
		var clientId = tracker.get('clientId');
		var urltogo=url+Hotel+Chain+Dest+locale+adult+child+datearrive+nuits+varStart+"#_ga="+clientId;
		window.open(urltogo, '_blank');	
	});
	var trackertest = ga.getByName('t0');
	} 
	catch(err) {
		var urltogo=url+Hotel+Chain+Dest+locale+adult+child+datearrive+nuits+varStart;
		window.open(urltogo, '_blank');
	}	
	
	
}

function setTodayDate() {
	var date = new Date;
	//document.getElementById('jour').selectedIndex = date.getDate();
