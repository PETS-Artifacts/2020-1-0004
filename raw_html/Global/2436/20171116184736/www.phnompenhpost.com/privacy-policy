<script type="text/javascript">

	$(window).on('beforeunload', function ()
    {
    		$.removeCookie('splash_time');


  	});

	var counter = 10;
	var pause = false;
	
	$('#splash-image').show(function() {
	$('#close').hide();
	$('#pause-ads').hide();
			
		var timer = setInterval(function() {
			if (!pause) {
				if (counter < 1) {
					clearInterval(timer);
					hide_ads();
					return;
				}
			counter--;
			
			if(counter <= 5){
				$('#splash-progress').html(counter + " second(s)");
				$('#close').show();
				$('#pause-ads').show();
				
				}
			}
		}, 1000);
	});

	
	$('#splash').click(function() {
		hide_ads();
	});
	

	function hide_ads() 
	{
		$('#splash').hide();
		$('#splash-ads').hide();
	}
	
	function pause_ads(button)
	{
		if (pause) {
			$(button).attr("src", "http://www.phnompenhpost.com/sites/all/themes/ppp/images/Pause-splash.png");
			pause = false;
		} else {
			$(button).attr("src", "http://www.phnompenhpost.com/sites/all/themes/ppp/images/Play-splash.png");
			pause = true;
		}
	}

