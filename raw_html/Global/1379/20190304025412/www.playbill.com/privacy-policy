<script> 

var gptAdSlots = []; 

googletag.cmd.push(function() { 
var REFRESH_KEY = 'refresh';
var REFRESH_VALUE = 'true';


    var mappingHorizontal = googletag.sizeMapping(). 

      addSize([1345, 0],[[970, 250],[970, 90]]). 

      addSize([1024, 0],[[728, 90]]). 

      addSize([729, 0],[[728, 90]]). 

      addSize([0, 0],[300, 250]). 

      build(); 

  

    var mappingMobile = googletag.sizeMapping(). 

      addSize([767, 0],[]). 

      addSize([0, 0],[[320, 50]]). 

      build(); 

  

    var mappingRightRail = googletag.sizeMapping(). 

        addSize([1261, 0],[[300, 250]]). 

        addSize([0, 0],[]). 

        build(); 

    gptAdSlots[0] = googletag.defineSlot('/21727259002/NEWS_TOP_970x90', [[300, 250], [728,90], [970, 90], [970, 250]], 'div-gpt-ad-1539795686678-0').defineSizeMapping(mappingHorizontal).setCollapseEmptyDiv(true).setTargeting('content_title', contentTitle).addService(googletag.pubads()); 

    gptAdSlots[1] = googletag.defineSlot('/21727259002/NEWS_RAIL_300x250', [300, 250], 'div-gpt-ad-1539795759549-0').defineSizeMapping(mappingRightRail).setCollapseEmptyDiv(true).setTargeting('content_title', contentTitle).addService(googletag.pubads()); 

    gptAdSlots[2] = googletag.defineSlot('/21727259002/NEWS_LOWER_970x90', [[300, 250], [728,90], [970, 250], [970, 90]], 'div-gpt-ad-1539795891223-0').defineSizeMapping(mappingHorizontal).setCollapseEmptyDiv(true).setTargeting('content_title', contentTitle).addService(googletag.pubads()); 

    gptAdSlots[3] = googletag.defineSlot('/21727259002/NEWS_320x50', [320, 50], 'div-gpt-ad-1539795990271-0').defineSizeMapping(mappingMobile).setCollapseEmptyDiv(true).setTargeting('content_title', contentTitle).addService(googletag.pubads());googletag.pubads().setCentering(true); 

gptAdSlots[4] = googletag.defineSlot('/21727259002/PHOTO_Interstitial_300x250', [300, 250], 'div-gpt-ad-1544812886243-0').setTargeting(REFRESH_KEY, REFRESH_VALUE).addService(googletag.pubads()); 

var SECONDS_TO_WAIT_AFTER_VIEWABILITY = 10 

googletag.pubads().addEventListener('impressionViewable', function(event) { 

var slot = event.slot; 

if (slot.getTargeting(REFRESH_KEY).indexOf(REFRESH_VALUE) > -1) { 

setTimeout(function() { 

googletag.pubads().refresh([slot]); 

}, SECONDS_TO_WAIT_AFTER_VIEWABILITY * 1000); 

} 

}); 

googletag.pubads().setTargeting('content_title', contentTitle).enableSingleRequest(); 

googletag.enableServices(); 



  }); 

</script> 


