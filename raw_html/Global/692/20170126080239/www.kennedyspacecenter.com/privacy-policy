<script>
    // get favorites from local storage or empty array
   // var KSCtp = Number(localStorage.getItem('todo-counter')) + 1,
    var KSCtp = JSON.parse(localStorage.getItem('TRIP')) || [];

    KSCtp.forEach(function(KSCtp) {
        document.getElementById(KSCtp).className='fav';
    });
    document.querySelector('.list').addEventListener('click', function(e) {
        var id = e.target.id,
                item = e.target,
                index = KSCtp.indexOf(id);
        // return if target doesn't have an id (shouldn't happen)
        if (!id) return;
        // item is not favorite
        if (index == -1) {
            KSCtp.push(id);
            item.className = 'fav';
            // item is already tp
        } else {
            KSCtp.splice(index, 1);
            item.className = 'fav-removed';
        }
        // store array in local storage
// Object

        localStorage.setItem('todo-orders', JSON.stringify(KSCtp).replace(/]|[[]/g, '').replace(/"/g, ''));
        localStorage.setItem('TRIP', JSON.stringify(KSCtp));
    });

</script>

