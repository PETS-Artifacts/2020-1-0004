<script>
    $(document).ready(function(){
     if( /Android|webOS|iPhone|iPad|iPod|BlackBerry/i.test(navigator.userAgent) ) {
        $(".tp-iframe").colorbox({iframe:true, width:"95%", height:"79%"});
        $(".tp-inline").colorbox({inline:true, width:"70%"});
    } else {
         $(".tp-iframe").colorbox({iframe:true, width:"853", height:"90%"});
        $(".tp-inline").colorbox({inline:true, width:"70%"});
    }
    });

$(document).bind('cbox_open', function() {
    var position = $('body').scrollTop();
        $('body').attr("data-position", position);
        $('body').css({position : 'fixed', top: '-' + position + 'px'});
    }).bind('cbox_closed', function() {
        $('body').css({position : 'relative', top: '0'}); 
        var position = $('body').attr("data-position");
        $('body').scrollTop(position);
    });


$("#scrollTop").on("click",function(e){
  $('html,body').animate({scrollTop: 0}, 'slow');
});

$('#tp-iframe').animate({scrollTop: 1234}, 100);

</script>

