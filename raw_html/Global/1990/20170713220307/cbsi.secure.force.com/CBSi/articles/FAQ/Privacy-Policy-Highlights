    <script>


        function togglePanel(object,showLabel,hideLabel) {

            var hiding = (object.dom.style.display != "none");

            var animationCallback = function() {
                if (hiding) {
                    document.getElementById(showLabel).style.display = "block";
                    document.getElementById(hideLabel).style.display = "none";
                } else {
                    document.getElementById(showLabel).style.display = "none";
                    document.getElementById(hideLabel).style.display = "block";
                }
             };

            var animationSettings = {useDisplay:true, duration:0.5, easing:"easeBoth", callback:animationCallback};

            if(hiding) {
                object.slideOut('t', animationSettings);
            } else {
                object.slideIn('t', animationSettings);
            }
        }         

        function attachDetachArticleToCaseBackToCaseStart(b) {
            if (b.isAttach) {
                attachArticleAndGoBackToCase();
            } else {
                detachArticleAndGoBackToCase();
            }
        }

        function goBackToCaseUrl() {
            if (false) {
                window.opener.location.reload(false);
                window.close();
            } else {
                window.location = '';
            }
        }

        function attachDetachArticleToCaseStart(b) {
            if (b.isAttach) {
                attachArticleToCase();
                b.isAttach = false;
                b.setText("Detach from case");
                if(true) {
                    b.backToCaseButton.isAttach = false;
                    b.backToCaseButton.setText("Detach and Return to case");
                }
            } else {
                detachArticleToCase();
                b.isAttach = true;
                b.setText("Attach to case");
                if(true) {
                    b.backToCaseButton.isAttach = true;
                    b.backToCaseButton.setText("Attach and Return to case");
                }
            }
        }

        Ext.onReady(function() {
            //the toolbar
            var theTopicCP;
            var toolbar = new Ext.Toolbar({
                renderTo: 'j_id0:j_id2:j_id5:j_id6:toolbar',
                cls: 'articleRendererToolbar'
            });
            
            if(false) {
                if(true) {
                    if(false) {
                        var attachButton = new Ext.Button({
                            id: 'j_id0:j_id2:j_id5:j_id6:toolbar:attachButton',
                            handler: attachDetachArticleToCaseStart,
                            text: 'Attach to case',
                            isAttach: true
                        });
                        toolbar.addButton(attachButton);
                    } else {
                        var attachBackToCaseButton = new Ext.Button({
                            id: 'j_id0:j_id2:j_id5:j_id6:toolbar:attachBackToCaseButton',
                            handler: attachDetachArticleToCaseBackToCaseStart,
                            text: 'Attach and Return to case',
                            isAttach: true
                        });
                        var attachButton = new Ext.Button({
                            id: 'j_id0:j_id2:j_id5:j_id6:toolbar:attachButton',
                            handler: attachDetachArticleToCaseStart,
                            text: 'Attach to case',
                            isAttach: true,
                            backToCaseButton: attachBackToCaseButton
                        });
                        toolbar.addButton(attachButton);
                        toolbar.addButton(attachBackToCaseButton);
                    }
                } else {
                    if(false) {
                        var detachButton = new Ext.Button({
                            id: 'j_id0:j_id2:j_id5:j_id6:toolbar:detachButton',
                            handler: attachDetachArticleToCaseStart,
                            text: 'Detach from case',
                            isAttach: false
                        });
                        toolbar.addButton(detachButton);
                    } else {
                        var detachBackToCaseButton = new Ext.Button({
                            id: 'j_id0:j_id2:j_id5:j_id6:toolbar:detachBackToCaseButton',
                            handler: attachDetachArticleToCaseBackToCaseStart,
                            text: 'Detach and Return to case',
                            isAttach: false
                        });
                        var detachButton = new Ext.Button({
                            id: 'j_id0:j_id2:j_id5:j_id6:toolbar:detachButton',
                            handler: attachDetachArticleToCaseStart,
                            text: 'Detach from case',
                            isAttach: false,
                            backToCaseButton: detachBackToCaseButton
                        });
                        toolbar.addButton(detachButton);
                        toolbar.addButton(detachBackToCaseButton);
                    }
                }
            }

            if(false && true && false) {
             if(false) {
              toolbar.add('<div class="toolbarPlaceHolder"><p>&#160;</p></div>');
             } else {
              return;
             }
            }
                toolbar.add(Ext.fly('j_id0:j_id2:j_id5:j_id6:votingComponent').dom.innerHTML);

            if(false) {
             theTopicCP = toolbar.add('');
            }
           
            toolbar.addFill();

            if(true){
             toolbar.add(Ext.fly('j_id0:j_id2:j_id5:j_id6:propertiesPanelLabel').dom.innerHTML);
            }
            toolbar.doLayout();
            
           if(false) {
             var topicInToolBar = document.getElementById(theTopicCP.id);
             topicInToolBar.appendChild(document.getElementById('knowledgeTopic'));
      }
        });

