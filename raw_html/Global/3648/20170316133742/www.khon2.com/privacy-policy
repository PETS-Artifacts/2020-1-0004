			var func = function() {
				var iframe_form = document.getElementById('wpcom-iframe-form-675f6996ec5a1846e8577c9f89f58ead-58ca9532bf2aa');
				var iframe = document.getElementById('wpcom-iframe-675f6996ec5a1846e8577c9f89f58ead-58ca9532bf2aa');
