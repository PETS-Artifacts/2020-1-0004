	// dataLayer
	dataLayer = [];
	
	function rC(name) { 
		name += '='; 
		for (var ca = document.cookie.split(/;\s*/), i = ca.length - 1; i >= 0; i--)
		if (!ca[i].indexOf(name)) 
			return ca[i].replace(name, ''); 
	}
	
	var gaUc = rC("_ga"); 
	if (gaUc != undefined)  { 
		var cV = gaUc.split('.');
		if (cV.length > 2 )  { 
			var uid1 = cV[2]; 
			dataLayer.push({'element-classes-custom':'uid_c','event':'gaEvent', 'uid1': uid1}); 
		}
	}
	
</script>
