// dataLayer

(function($, Drupal){

	$(function (){
	
	
		// Get language on load (document ready)
		dataLayer.push({ 'element-classes-custom':'language','event': 'gaEvent','selected-language':$('#edit-language option:selected').text().trim() });
		
		// User ID (is logged in)
		Drupal.settings.fbconnect.user.uid === 0 || dataLayer.push({ 'element-classes-custom':'user_login_details','event': 'gaEvent','user-name': Drupal.settings.name, 'uid': Drupal.settings.fbconnect.user.uid, 'fbuid': Drupal.settings.fbconnect.user.fbuid });
			
		// Search form submit...
		$('.search').on('submit', function(){ dataLayer.push({'element-classes-custom':'search-keywords','event': 'gaEvent','search-keywords':$('.form-text').val() }) });

		// get email address on click
		$('.field-item').find('a[href^="mailto:"]').on('click', function() {
			dataLayer.push({ 'element-classes-custom':'field_item_email','event': 'gaEvent','email-address':$(this).text().trim() }); 
		});
		
		// get web address on click
		$('.field-item').find('a[target="_blank"]').on('click', function() {
			dataLayer.push({ 'element-classes-custom':'field_item_web','event': 'gaEvent','website-address':$(this).text().trim() });
		});
		
		// Trip Planner Links
		$('.trip-planner-nav').find('a').on('click', function() {
			dataLayer.push({ 'element-classes-custom':'trip_planner_navigation','event': 'gaEvent','trip-planner-nav': $(this).text().trim() });
		});
		
		// Travel Specialists filter state
		$('#edit-field-agent-state-value').on('change', function() {
			dataLayer.push({'element-classes-custom':'edit_submit_matai_agents_state','event': 'gaEvent', 'country': $('#edit-field-country-iso2 option:selected').text().trim(), 'city': $('#edit-field-agent-state-value option:selected').text().trim() });
		});
		
		// Travel Specialists filter country
		$('#edit-field-country-iso2').on('change', function() {
			dataLayer.push({'element-classes-custom':'edit_submit_matai_agents_country','event': 'gaEvent', 'country': $('#edit-field-country-iso2 option:selected').text().trim()});
		});	
		
		// Travel specialist engagement
		$('.view-matai-agents').find('tr.row').on('click', function(e){
			$(this).is('.opened') || 	
			dataLayer.push({ 
				'element-classes-custom':'view_matai_agents_click',
				'event': 'gaEvent',
				'agency':$(this).find('td:nth-child(1)').text().trim()
			}),
			dataLayer.push({ 
				'element-classes-custom':'view_matai_agents_click',
				'event': 'gaEvent',
				'agent-name':$(this).find('td:nth-child(2)').text().trim()
			}),
			dataLayer.push({ 
				'element-classes-custom':'view_matai_agents_click',
				'event': 'gaEvent',
				'agent-country':$(this).find('td:nth-child(3)').text().trim()
			}),
			dataLayer.push({ 
				'element-classes-custom':'view_matai_agents_click',
				'event': 'gaEvent',
				'agent-state':$(this).find('td:nth-child(4)').text().trim()
			}),
			dataLayer.push({ 
				'element-classes-custom':'view_matai_agents_click',
				'event': 'gaEvent',
				'agent-city':$(this).find('td:nth-child(5)').text().trim()
			});
		});
		
		// Travel specialist engagement click email
		$('.view-matai-agents').find('a[href^="mailto:"]').on('click', function(){
			dataLayer.push({'element-classes-custom':'view_matai_agents_email','event': 'gaEvent', 'agency': $(this).closest('tr').prev().find('td:nth-child(1)').text().trim(), 'agent-email': $(this).attr('href') });
		});
		
		// Travel specialist engagement click website
		$('.view-matai-agents').find('a[target="_blank"]').on('click', function() {
			dataLayer.push({'element-classes-custom':'view_matai_agents_website','event': 'gaEvent', 'agency': $(this).closest('tr').prev().find('td:nth-child(1)').text().trim(), 'agent-website': $(this).attr('href') });
		});
		
		// Log in button in header - On Click
		$('#user-buttons').find('a:nth-child(2)').on('click', function(e) {
			dataLayer.push({'element-classes-custom':'user_buttons','event': 'gaEvent','Login-Action': 'Log in button in header','Current-Page': $('body').find('h1').text().trim() });
		});
		
				
		// Video Play Buttons
		$('.rsPlayBtnIcon').on( 'click', function() {
			dataLayer.push({ 'element-classes-custom':'rsPlayBtnIcon','event': 'gaEvent','Video-Action':'Play', 'Video-Caption': $(this).closest('.rsfiji').find('.rsGCaption').text().trim() });
		});
		
		// Welcome to happiness tiles
		$('.welcome-to-happiness-categories').find('a').on('click', function() {
			dataLayer.push({'element-classes-custom':'welcome_to_happiness','event': 'gaEvent','event_category': $(this).closest('.views-row').find('.views-field-title').text().trim(),'event_url': $(this).attr('href')});
		});
		
		// Welcome to happiness Fiji deals
		$('.welcome-to-happiness-deals').find('a').on('click', function() {
			dataLayer.push({'element-classes-custom':'welcome_to_happiness_fiji_deals','event': 'gaEvent','event_label': $(this).closest('.views-row').find('.views-field-title').text().trim(),'event_url': $(this).attr('href')});
		});
		
		// Signup Button in header - On Click
		$('#user-buttons').find('a:first-child').on('click', function(e) {
			dataLayer.push({'element-classes-custom':'signup-header','event': 'gaEvent','Signup-Action': 'Signup button in header','Current-Page': $('body').find('h1').text()});
		});

		// Newsletter Subscribe Registration Form - On Click
		$('#edit-submit').on('click', function(e){
			!$('#edit-mailchimp-lists-mailchimp-our-email-newsletter-subscribe:checked').length || dataLayer.push({'element-classes-custom':'newsletter-subscribe-reg-form','event': 'gaEvent','Country':$('#edit-field-country-und option:selected').text(), 'Subscribe-Newsletter': 'yes'});
		});

		// Newsletter Subscribe Home page - On Click
		$('#mc-embedded-subscribe').on('click', function(e){
			dataLayer.push({'element-classes-custom':'newsletter-subscribe-home-page','event': 'gaEvent','Country': $('#mc-embedded-subscribe').closest('form').find('select option:selected').text(), 'Subscribe-Newsletter': 'yes'});
		});

		// Get Quote - On Click
		$('.get-a-quote').on('click', function(e) {
			$('#timeline').find('.sortable').each( function(e){
				dataLayer.push({'element-classes-custom':'get-a-quote','event': 'gaEvent','Date': $(this).find('.date-entry').text(), 'Operator-Business-Name': $(this).find('h3').text()});
			});
		});

	
		// Trip Planner Main Page - On Add To Planner (On Click)
		$('.filter-results').find('.flag-link-toggle').not('.unflag-action').on('click', function(e) { 
			dataLayer.push({'element-classes-custom':'flag-link-toggle','event': 'gaEvent','Parent-Section': $('body').find('h1').text().trim(), 'Operator-Business-Name': $(this).closest('.views-row').find('.views-field-title').text().trim()});
		});
		

		// Trip Planner Child page - On Add To Planner (On Click)
		$('.sidebar, .contact').find('.flag-link-toggle').not('.unflag-action').on('click', function(e) {
			dataLayer.push({'element-classes-custom':'.sidebar.flag-link-toggle','event': 'gaEvent','Operator-Business-Name': $('body').find('h1').text().trim()});
		});
		
	
		// Get email contact address - On Click
		$('a.icon-email').on('click', function(e){
			dataLayer.push({'element-classes-custom':'a.icon-email','event': 'gaEvent','Operator-Business-Name': $('body').find('h1').text().trim(), 'Email-Address': $(this).attr('href')});
			//dataLayer.push({"Operator-Business-Name": $('body').find('h1').text().trim(), "Email-Address": $(this).attr('href')});
		});

		// Get web address - On Click
		$('a.icon-web').on('click', function(e){
		dataLayer.push({'element-classes-custom':'a.icon-web','event': 'gaEvent','Operator-Business-Name': $('body').find('h1').text().trim(), 'Web-Address': $(this).attr('href')});
			//dataLayer.push({"Operator-Business-Name": $('body').find('h1').text().trim(), "Web-Address": $(this).attr('href')});
		});

		// Get phone number
		// Note: this icon already has an event listener which fires our listener multiple times, we cannot stop propagation as is might cause unwanted side effects...
		$('a.icon-phone').on('click', function(e){
			dataLayer.push({'element-classes-custom':'a.icon-phone','event': 'gaEvent',"Operator-Business-Name": $('body').find('h1').text().trim(), "Phone-Number": $(this).find('span').text().trim()});
		});
		
	});
})(jQuery, Drupal);
