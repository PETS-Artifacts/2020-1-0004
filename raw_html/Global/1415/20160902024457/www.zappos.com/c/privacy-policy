<script type="text/javascript">
  (function (w, z) {
    // Check if customer is VIP
    // This crazy regex from: https://developer.mozilla.org/en-US/docs/Web/API/Document/cookie
    // This explicitly grabs the value of the holmes cookie
    var holmes = document.cookie.replace(/(?:(?:^|.*;\s*)holmes\s*\=\s*([^;]*).*$)|^.*$/, "$1");
    if (holmes.indexOf('"vip":true') != -1) {
      z.isVip = true;
    } else {
      z.isVip = false;
    }
  })(window, window.z = window.z || {});
</script>

