    function upgradeBrowserPrompt() {
      var latestUnsupportedIE = 10;
      if (document.documentMode && document.documentMode <= latestUnsupportedIE) {
        var store = 'Zappos' || 'Zappos';
        var el = document.createElement('p');
        el.innerHTML = 'Sorry! Your browser is not currently supported. To shop '+store+', we recommend using the latest version of Chrome, Safari, Firefox or Edge.';
        el.style.background = '#EFEFAF';
        el.style.textAlign = 'center';
        el.style.fontSize = '18px';
        el.style.color = '#000';
        el.style.width = '100%';
        el.style.display = 'block';
        el.style.zIndex = '100';
        el.style.position = 'relative';
        document.body.insertBefore(el, document.body.childNodes[0]);
      }
    }
    upgradeBrowserPrompt();
