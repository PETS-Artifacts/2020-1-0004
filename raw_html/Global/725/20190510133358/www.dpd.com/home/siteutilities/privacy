    <script type="text/javascript">
        
        try {            var secondTracker = _gat._getTracker("UA-6989252-1");
            _gat._anonymizeIp();
            secondTracker._setDomainName(".dpd.com");
            secondTracker._trackPageview();
            } catch(err) {}    </script>

    


    
    
    <script type="text/javascript">
        try {            var firstTracker = _gat._getTracker("UA-11117105-1");
            _gat._anonymizeIp();
            firstTracker._setDomainName(".dpd.com");
            firstTracker._trackPageview();
            } catch(err) {}
        
        
        
        
        function tracking(category, action, label, value, page)
        {            try {                firstTracker._trackEvent(category, action, label);
                firstTracker._trackPageview(page);
                } catch(err) {}            }
        
        function trackingEvent( category, action, label, url, target )
        {            try
            {                firstTracker._trackEvent( category, action, label );
                }            catch( err ) {}
            if ( url !== undefined && url.length !== 0 )
            {                if ( target == '_blank' )
                {                    window.setTimeout( "dpdWindow( '" + url + "' )", 100 );
                }                else
                {                    window.setTimeout( "window.location.href = '" + url + "'", 100 );
                }            }        }
        
        function trackingBanner(category, action, label, value, page, target)
        {            try {                firstTracker._trackEvent(category, action, label);
                } catch(err) {}
            if (target == '_blank')
            {                window.setTimeout("dpdWindow('" + page + "')", 100);
            }            else
            {                window.setTimeout("window.location.href = '" + page + "'", 100);
            }        }
    </script>
    

<script type="text/javascript">

function dpdWindow(page){    myDodWindow = window.open(page, 'FormDPD', '');
    myDodWindow.focus();}</script>











