<script type="text/javascript">	
	$(document).ready(function() {

	var webid = $('#webid').val();
	$('#vote1').click(function() {
	$.post("http://www.csswinner.com/home/submitvote", { id: webid },function(data) {
	if(data>0){$('#votnos').text(data);}else{jQnotice('Oops, You voted already for this website!');}
	});
	});

$( ".dirvote" ).click(function() {
	var thisel = $( this );

var wproid = thisel.data( "value" );

	$.post("http://www.csswinner.com/home/submitvotepro", { id: wproid },function(data) {
	if(data>0){thisel.addClass( " voted" );thisel.unbind( "click" );thisel.text(data);}
	});
 
});

	});
</script>
<script type="text/javascript">
	function DoVoting(webid){
	var id = "#vot"+webid;
	$.post("http://www.csswinner.com/home/submitvote", { id: webid },function(datas) {
	if(datas>0){$(id).text(datas);}else{jQnotice('Oops, You voted already for this website!');}
	});
return false;
}
</script>
