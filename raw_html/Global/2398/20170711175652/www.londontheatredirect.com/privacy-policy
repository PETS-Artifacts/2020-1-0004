                <input type="button" name="ctl00$BookingBox$HeaderBookingBox$ctl01" value="Book Tickets" onclick="if (!bookingBoxCheck(event)) return false;__doPostBack(&#39;ctl00$BookingBox$HeaderBookingBox$ctl01&#39;,&#39;&#39;)" class="button header-booking-box__submit" />
            </div>
        </figure>
    
</div>
    <script>
        function bookingBoxCheck(e) {
            if (!document.getElementsByClassName) return true;
            var input = document.getElementsByClassName('header-booking-box__input')[0];
            if (!input) return true;

            var CLASS_EMPTY = 'header-booking-box__input--error';

            if (!input.value) {
                input.classList.add(CLASS_EMPTY);
                return false;
            }
            else {
                input.classList.remove(CLASS_EMPTY);
                return true;
            }
        }


        (function () {
            if (!document.getElementsByClassName) return true;

            var button = document.getElementsByClassName('header-booking-box__submit')[0];
            if (!button) return true;

            button.addEventListener('touchstart', bookingBoxCheck);

        }());
    </script>

                

                

