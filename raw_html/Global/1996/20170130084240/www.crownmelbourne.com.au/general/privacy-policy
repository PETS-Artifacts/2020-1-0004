  (function () {
    var checkLogin = function () {
      var rootPath = '/crown-rewards-hub';
      var crtLocation = window.location.pathname.toLowerCase();
      var needLogin = crtLocation.indexOf(rootPath) !== -1;
      if (!needLogin) {
        return;
      }
      var isLogin = 'False'.toLowerCase() == 'true';
      var LOGIN_PAGE = '/crown-rewards/log-in';
      var LOGIN_REDIRECT_PAGE = '/crown-rewards-hub/crown-rewards/my-account/points-summary';
      if (!isLogin) {
        window.location.href = LOGIN_PAGE;
        return;
      }
      if (crtLocation.indexOf(LOGIN_PAGE) !== -1) {
        if (isLogin) {
          window.location.href = LOGIN_REDIRECT_PAGE;
        }
      }
    };
    checkLogin();
    window.onpageshow = function (event) {
      checkLogin();
    };
  })();
</script>

