<script type="text/javascript">
  (function() {
    var checkLogin = function() {
      var lists = ['member-portal/club-news', 'member-portal/my-account-privileges', 'member-portal/my-profile-change-password'];
      var crtLocation = window.location.pathname.toLocaleLowerCase();
      var len = lists.length;
      var needLogin = false;
      var LOGIN_PAGE = '/member-portal/login';
      var LOGIN_REDIRECT_PAGE = '/member-portal/club-news';
      
      for (var i = 0; i < len; i++) {
        if (crtLocation.indexOf(lists[i]) !== -1) {
          needLogin = true;
        }
      }
	  
	  if(!needLogin) {
        return;
      }
      
      var getCookie = function(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i=0; i<ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0)==' ') c = c.substring(1);
          if (c.indexOf(name) == 0) return c.substring(name.length, c.length);
        }
        return "";
      };
      
      var isLogin = (getCookie('isLogin') === '1');
      console.log('needLogin: ', needLogin, " isLogin:", isLogin);
      if (needLogin && !isLogin) {
        window.location.href = LOGIN_PAGE;
        return;
      }
      
      if (crtLocation.indexOf(LOGIN_PAGE) !== -1) {
        if (isLogin) {
          window.location.href = LOGIN_REDIRECT_PAGE;
        }
      }
    };
    checkLogin();
    window.onpageshow = function(event) {
      checkLogin();
    };
  })();
</script>

