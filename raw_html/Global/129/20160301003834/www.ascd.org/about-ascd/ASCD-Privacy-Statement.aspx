	function siteChannel(aVal){
		
		// Grab Path
		var myUrlOriginal = window.location.pathname;
		
		// Remove everything after first '.'
		var oPath = myUrlOriginal.substr(0, myUrlOriginal.lastIndexOf("."));

		//document.writeln(oPath);
		
		// Split data into sections
		var nPath = oPath.split('/');
		var nPathName = "";
		
		// Assign sections in array
		for(i = 0; i < nPath.length; i++){
			nPathName += "/";
			nPathName += nPath[i];
		}

		//document.writeln(nPath[1]);
		
		// Assign output to variable
		var channelOutput = nPath[aVal];
		//console.log('Channel: ' + channelOutput);
		return channelOutput;
	}
	
	// Analytics: Grab Site Hier
	function siteHier(){
		
		// Grab Path
		var myUrlOriginal = window.location.pathname;
		
		// Remove everything after first '.'
		var oPath = myUrlOriginal.substr(0, myUrlOriginal.lastIndexOf("."));
		//console.log(oPath);
		
		// Split data into sections
		var nPath = oPath.split('/');
		//console.log('Hier: ' + nPath);
		
