	lin_slot_nxmwebadhesion = googletag.defineSlot('/5678/mg.KRON/privacy-policy', [320,50], "nx-mweb-adhesion").defineSizeMapping(lin_get_size_mapping("nx-mweb-adhesion")).addService(googletag.pubads()).setTargeting("WP-Position", "nx-mweb-adhesion").setTargeting("WP-Post", "95357");
} else {
	jQuery( '#nx-mweb-adhesion .advertisement-label' ).hide();
}
googletag.pubads().collapseEmptyDivs();
googletag.enableServices();
});
</script>
