	-ms-transform:scale(1.5);
	-o-transform:scale(1.5);
	transform:scale(1.5);
	-webkit-transition:all 500ms;
	-moz-transition:all 500ms;
	-o-transition:all 500ms;
	transition:all 500ms
}
#toTop:hover { background:#0065b8;}
#toTop.shown {opacity:1; -webkit-transform:scale(1); -moz-transform:scale(1); -ms-transform:scale(1); -o-transform:scale(1); transform:scale(1); }
.no-opacity #toTop {background:#333; text-align:center;}
.no-opacity #toTop .alt {font-size:1.2em; font-weight:bold;}
.no-opacity #toTop .icon {display:none;}
.no-opacity #toTop:hover {background-color:#0065b8;}
</style>
<script>
$LAB.queueWait(function () {
		var $body         = $('body');
		var $html         = $('html');
		var $scrollToTop  = $('#toTop');
	  
		var scrollTimeout;
		$(window).on('scroll', function(){
	  
		  var $this = $(this);
	  
		  if ( scrollTimeout )
			  clearTimeout(scrollTimeout);
	  
		  scrollTimeout = setTimeout(function(){
			if ( $this.scrollTop() > 200 ) {
			  $scrollToTop.addClass('shown');
			} else {
			  $scrollToTop.removeClass('shown');
			}
	  
		  }, 500)
		});
	  
		$scrollToTop.on('click', function(){
		  $body.add( $html ).animate({ scrollTop: 0 }, 1000);
		  $(this).removeClass('shown');
		})
	});
</script>
<div id="toTop"><span class="icon icon-toggle0" aria-hidden="true"></span><span class="alt">&circ;</span></div>		<div class="site-content-wrap">
