		var taxes = ""+(tc_vars["confirmation_total_amount_tax_incl_EUR"]-tc_vars["confirmation_total_amount_tax_excl_EUR"]).toFixed(2);
		if(taxes == 0){
			taxes = "";
		}
		tc_vars["confirmation_taxes_EUR"] = taxes;
		taxes = ""+(tc_vars["confirmation_total_amount_tax_incl_booked_currency"]-tc_vars["confirmation_total_amount_tax_excl_booked_currency"]).toFixed(2);
		if(taxes == 0){
			taxes = "";
		}
 		tc_vars["confirmation_taxes_booked_currency"] = taxes;
		
