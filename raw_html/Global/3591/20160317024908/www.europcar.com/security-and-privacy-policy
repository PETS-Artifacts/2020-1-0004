      var merge = function(a, b) {
        for (var key in b) {
          if (typeof b[key] === "object" && typeof a[key] === "object") {
            merge(a[key], b[key]);
          } else {
            a[key] = b[key];
          }
        }
      }

      if (eca.data) {
        merge(eca.data, data);
      } else {
        eca.data = data;
      }
      if (eca.data.HIDE_TOP_NAV) {
    	  document.body.className = document.body.className + ' hide-nav'
      }
      return eca;
    })(typeof eca === "undefined" ? {} : eca);
