		var promoCode;
		promoCode = "";
		if(promoCode == ""){
			promoCode = "";
		}

		var siteContractType;
		siteContractType = "null";
		if(siteContractType == ""){
			siteContractType = "";
		}

		var myImpressions = $('.impression');
		var sizeImpressions = myImpressions.size();
		var testImpression ="";
		var idAT="";
		var caroussel = 10;
		var info = 7;
		var currentUrl = window.location.href;
		var bookType = ""; 
		
		for(var i=0;i<sizeImpressions;i++){
			idAT = "";
			var myId = $(myImpressions[i]).attr("id");
			myId = myId.substring(myId.indexOf("_")+1,myId.length);
			
			if($(myImpressions[i]).hasClass('caroussel')){
				idAT = caroussel++;
			}
			testImpression = testImpression+"INT-"+idAT+myId+"-"+"["+$(myImpressions[i]).val().replace(' (BOOKING_HOME_PAGE)','')+"]";
			if(i<sizeImpressions-1){
				testImpression = testImpression+",";
			}
			
		}
		if($('#atUpgrade').length != 0){
			testImpression = testImpression + "INT-"+"33";
		}
		if($('#e-rental').length != 0){
			testImpression = testImpression + "INT-"+"35,";
		}
		if($('#atConfirmed').length != 0){
			testImpression = testImpression + "INT-"+"38";
		}
		if($('#atCaution').length != 0){
			testImpression = testImpression + "INT-"+"37";
		}
		
		if(currentUrl.indexOf("step2") != -1 && $('#filter > .loyalty').length != 0){
			testImpression = testImpression + "INT-40-["+""+"]";
		}
		if(currentUrl.indexOf("step3") != -1 && $('#enquiry .loyalty').length != 0){
			testImpression = testImpression + "INT-41-["+""+"]";
		}
		if(currentUrl.indexOf("step2") != -1 || currentUrl.indexOf("step3") != -1 || currentUrl.indexOf("step4") != -1){
			bookType = "1";
		}
		
		tc_vars["search_checkout_country"] = "";
		tc_vars["search_country_of_residence"] = "US";


		tc_vars["site_banners_impression"] = testImpression;
		tc_vars["site_contract_ID"] = promoCode;

		if (siteContractType == "B2B2C") {
			tc_vars["site_contract_ID_type"] = "1";
		} else {
			tc_vars["site_contract_ID_type"] = "";
		}

		tc_vars["lp_promo_name"] = "";
		tc_vars["lp_promo_id"] = "";
		
		tc_vars["bp_modified_booking"] = "0";
		
		tc_vars["select_checkout_country"] = "";
		tc_vars["select_checkout_station_code"] = "";
		tc_vars["select_checkout_date"] = "";
		tc_vars["select_checkout_time"] = "";
		tc_vars["select_checkin_country"] = "";
		tc_vars["select_checkin_station_code"] = "";
		tc_vars["select_checkin_date"] = "";
		tc_vars["select_checkin_time"] = "";
		tc_vars["select_roundtrip"] = "";
		tc_vars["select_country_of_residence"] = "US";
		tc_vars["select_duration"] = "";
		tc_vars["select_book_to_rent_window"] = "";
		
		tc_vars["select_vehicle_acriss"] = "";
		tc_vars["select_vehicle_acriss_ID"] = "";
		
		tc_vars["select_vehicle_type"] = "";
		tc_vars["pickup_location"] = "null";
		tc_vars["return_location"] = "null";
		
		tc_vars["bpp_change_checkout_date"] = "";
		tc_vars["bpp_change_checkout_time"] = "";
		tc_vars["bpp_change_checkin_date"] = "";
		tc_vars["bpp_change_checkin_time"] = "";
		tc_vars["bpp_vehicle_category"] = "";
		tc_vars["bpp_vehicle_category_ID"] = "";
		tc_vars["bpp_vehicle_segment"] = "No segment";
		tc_vars["bpp_vehicle_segment_ID"] = "";
		tc_vars["bpp_vehicle_product"] = "";
		tc_vars["bpp_vehicle_product_ID"] = "";

		
		tc_vars["bpp_vehicle_unitprice_tax_incl_EUR"] = '';
		tc_vars["bpp_vehicle_unitprice_tax_excl_EUR"] = '';
		tc_vars["bpp_vehicle_unitprice_tax_incl"] = '';
