					var fromHP = getFlatParameterByName('fromHP');
					var place = getFlatParameterByName('place');
				  	if (fromHP != null && fromHP == "true") {
				       tc_vars["site_banner_click"]=[];
				       tc_vars["site_banner_click"][0]=[];
				       tc_vars["site_banner_click"][0]["category"] = "homepage_teaser";
