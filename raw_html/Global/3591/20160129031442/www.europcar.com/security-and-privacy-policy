<script type="text/javascript">
				$(document).ready(function(){
					$("body").on("click", "#openableHeaderLink", function(){
						
						$('#helpLayer').toggle();
						$('#openableHeaderLink').toggleClass("opened");
						
						
						var openableHeaderLinkSize = parseInt($("#openableHeaderLink").css("width"), 10);
					    var helpLayerSize = parseInt($("#helpLayer").css("width"), 10);
					    var moveLayerSize = parseInt(openableHeaderLinkSize - helpLayerSize, 10)-1;
					    $("#helpLayer").css('margin-left', moveLayerSize + 'px');
					    
					    
						if($('body').find("div#shimOpenableHeaderMenu").size() == 0) {
							$('body').append("<div id='shimOpenableHeaderMenu'></div>");
						}
						$('#shimOpenableHeaderMenu').toggleClass("show");
					});
					
					$("body").on("click", "#shimOpenableHeaderMenu", function(){
						$('#helpLayer').hide();
						$('#openableHeaderLink').removeClass("opened");
						$('#shimOpenableHeaderMenu').removeClass("show");
					});
				});
			</script>
		
