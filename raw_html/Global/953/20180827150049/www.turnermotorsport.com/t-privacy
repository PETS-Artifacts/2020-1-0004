  window.atwlLoginRecaptchaCallback = function( token ) {
    $( "#atwlLogin .js-recaptcha-token" ).val(token);
    $( "#atwlLogin .js-recaptcha-token" ).click();
    for (i = 0; i < ___grecaptcha_cfg.count; i++) { 
        grecaptcha.reset(i);
    }
  };
</script>



