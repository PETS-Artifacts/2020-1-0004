<script type="text/javascript" language="javascript">
function refreshwebcam(camnum) {
	var uniq = new Date();
	uniq = uniq.getTime();
	if (camnum == 3) {
	    document.webcampic.src = "/webcam/video.jpg?" + uniq;
	}
	else {
	    document.webcampic.src = "/webcam/newcam" + camnum + ".jpg?" + uniq;
	}
	id =setTimeout("refreshwebcam(" + camnum + ")",10000);
};
</script>
