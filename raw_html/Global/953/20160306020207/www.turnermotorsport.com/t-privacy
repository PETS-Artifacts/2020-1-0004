  var registerScriptLoadCallback;
  (function() {
    /*
      Formerly "scriptsLoaded"... Do not bind to this directly, it
      will cause race conditions and drive you crazy. I'm specifically
      using a closure here to make it difficult to bind directly.
    */
    var scriptLoadEventName = "scriptsLoaded" + Math.random();
    var scriptsLoaded = false;
    registerScriptLoadCallback = function(callback) {
      if(scriptsLoaded) {
        callback();
      }
      else {
        $(document).bind(scriptLoadEventName, callback);
      }
    }
    registerScriptLoadCallback(function() {
      scriptsLoaded = true;
    });


