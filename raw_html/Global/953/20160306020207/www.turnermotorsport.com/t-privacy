<script type="text/javascript">

	

 registerScriptLoadCallback(function() {
 // if (getOnlineStatus(0,0)) 
 //     $('.LPMcontainer').css('visibility', 'visible');
	  
$(function()
	{
		$('[rel^="lightbox"]').each(function(i,myElem){
		var mySrc = $(myElem).attr('href');
			if(!mySrc.match(/^http/))
			{
				mySrc = "https://1c2a8a2161d644d95009-22d26b38e78c173d82b3a9a01c774ffa.ssl.cf1.rackcdn.com"+mySrc;
				$(myElem).attr('href',mySrc);
			}
			else
			{
				var lbRegEx = /(^(http|https)(:\/\/)(www.turnermotorsport.com))/gi;
				var myMatch = mySrc.match(lbRegEx);
				if(myMatch)
				{
					mySrc = mySrc.replace(myMatch[0],'https://1c2a8a2161d644d95009-22d26b38e78c173d82b3a9a01c774ffa.ssl.cf1.rackcdn.com');
					$(myElem).attr('href',mySrc);
				}
			}
		}).lightbox();
	});
	
	    		function setPreservedWidths(){
	$('.preserve').each(function(n,myElem){
		
		maxW=parseInt($(myElem).attr('width'));
		$(myElem).css('max-width',maxW+'px');
		
	});
	
	}
		
		function objToLazyYT(obj,ytID, vidWidth, vidHeight, cb){
			var $myObj = $(obj);
			// Don't go greater than 300 width and 200 height to keep containers responsive.
			var height = vidHeight < 200 ? vidHeight : '200';
			var width = vidWidth < 300 ? vidWidth : '300';
			$myObj.replaceWith('<div class="js-lazyYT" data-youtube-id="'+ytID+'" data-width="'+width+'" data-height="'+height+'" data-desc="true"></div>');
			
			if(typeof cb === "function")
				cb($myObj);
			
		}
		// Make youtube embeds part of product descriptions mobile friendly, convert to lazyYT format used elsewhere.
		checkEmbeds = function (){
			//check both known variations of embedded youtube videos in product descriptions (iframes and objects)
			// Check iframes first
			$('.ObjectDescPanel iframe, .TMSR_ProductContentCell iframe, .productlongdesc iframe').each(function(i,myElem){
				var ytCheck = /(?:youtube\.com\/embed\/)([^#\&\?]*)/;
				var ytMatch = $(myElem).attr('src').match(ytCheck);
				if(ytMatch)
				{
					var vidW = $(myElem).attr('width')< 240 ? $(myElem).attr('width') : '240';
					var vidH  = $(myElem).attr('height') < 160 ? $(myElem).attr('height') : '160';
					objToLazyYT(myElem,ytMatch[1],vidW,vidH,function(myObj){
						if($(myObj).parent().css('float','right')&&(!($(myObj).parent().is('.ObjectDescPanel, .TMSR_ProductContentCell, .productlongdesc')))) // html5 embeds with parent css having float:right gets hidden
							$(myObj).parent().css('float','none');
					$('.js-lazyYT').trigger('lazyTYLoaded');
					
					});
				}
			});
			//check objects next
			$('.ObjectDescPanel object, .TMSR_ProductContentCell object, .productlongdesc object').each(function(i,myElem){
				var ytCheck = /(?:youtube.com\/v\/)([^#\&\?]*)/;
				// todo, extend check to carry width/height
				//var ytCheck = /(?:youtube.com\/v\/)([^#\&\?]*)(?:.*)(?:width=?)(?:"|'?)([0-9+])(?:"|'?)(?:.*)(?:height=)(?:"|'?)([0-9]+)(?:"|'?)/;
				var ytMatch = $(myElem).find('param').attr('value').match(ytCheck);
				if(ytMatch)
				{
					objToLazyYT(myElem,ytMatch[1],ytMatch[2],ytMatch[3],function(myObj){
						if($(myObj).parent().css('float','right')&&(!($(myObj).parent().is('.ObjectDescPanel, .TMSR_ProductContentCell, .productlongdesc')))) // html5 embeds with parent css having float:right gets hidden
							$(myObj).parent().css('float','none');
						$('.js-lazyYT').trigger('lazyTYLoaded');
						
					});
				}
			});
		}
		checkEmbeds();
		
		setTimeout(function(){
		// Give checkEmbeds() a few seconds to prepare embeds before checking lazyYT elements for possible conversion
						$('.js-lazyYT').each(function(i,myElem){
						// pass prodDesc flag to force inherited width/height settings.
						if($(myElem).data('desc'))
						$(myElem).lazyYT({prodDesc:true});

						if($(myElem).parent().css('float','right')&&(!($(myElem).parent().is('.ObjectDescPanel, .TMSR_ProductContentCell, .productlongdesc'))))
							$(myElem).parent().css('float','none');
						
						});
					},2000);
		setTimeout(function(){
		$(window).trigger('lazyTYLoaded');
		
		$('.js-lazyYT').addClass('lazyYTLoaded');
		},3000)
	
        function slideToggleElem(myElem, mySpeed){
    
        if( $(myElem).hasClass('open'))
        {
           
            $(myElem).removeClass('open').addClass('close');
            $(myElem).animate({height:'toggle'},mySpeed);
            
        }
        else
        {
            if($(myElem).hasClass('close'))
            {
           
            $(myElem).removeClass('close').addClass('open');
            $(myElem).animate({height:'toggle'},mySpeed);
            }
                else  // For first time toggle, do a hidden minimize
            {
               
                $(myElem).animate({height:'toggle'},mySpeed);
                //$(myElem).animate({height:'toggle'},10000);
                $(myElem).addClass('open').show();
            }
            
        }
        
        
    }
    
    function toggleNestedAccordian($rowClicked) {
        var $togglee = $rowClicked.next('div');
        
        if (!$togglee.length) return;
        
        if ($togglee.is(':hidden'))
            $togglee.slideDown();
        else
            $togglee.slideUp();
        
    }
    
	var parentDivs = $('#nestedAccordion div'),
	childDivs = $('#nestedAccordion h3').siblings('div');
    childDivs2 = $('#nestedAccordion h4').siblings('div');
	childDivs3 = $('#nestedAccordion h5').siblings('div');
	$('#nestedAccordion h2').click(function(){
		parentDivs.slideUp();
        toggleNestedAccordian($(this));
	});
	$('#nestedAccordion h3').click(function(){
		childDivs.slideUp();
		toggleNestedAccordian($(this));
	});
    $('#nestedAccordion h4').click(function(){
		childDivs2.slideUp();
		toggleNestedAccordian($(this));
	});
    $('#nestedAccordion h5').click(function(){
		childDivs3.slideUp();
		toggleNestedAccordian($(this));
	});
    
    

	function initPositions(){
			
		density = window.devicePixelRatio;
		
		if((density>1)&&(window-innerWidth<550))
		{
			$('#categories li').css('font-size', density+'em').css('height', density+'em');
			$('#navHeader').css('font-size', (density*1.5)+'em');
		}
			
			
		//Header Icon placement v1
        //$('li.headerIcons').css('top',function(){
			//return (Math.floor(2+(window.innerWidth-480)/(window.innerWidth/40))+'px');
            // return (Math.floor(17+(window.innerWidth-550)/8)+'px');
      //  });
	
		var myWidth=window.innerWidth;
	  
	    if(window.innerWidth>1370)
	   {
	   $('#header_bg_x').css('width','562px').css('display','inline-block');
	   }
	   else if(window.innerWidth>1280)
	   {
	   $('#header_bg_x').css('width',(window.innerWidth-815)+'px').css('display','inline-block');
	   }
	   else if(window.innerWidth>1200)
		{
		$('#header_bg_x').css('width',(myWidth-810)+'px').css('display','inline-block');
		}
	   else if(window.innerWidth>788)
	   {
	   $('#header_bg_x').css('width',(myWidth-783-(( myWidth*myWidth / ( 788 * 788) )*7))+'px').css('display','inline-block');
	   }
	   else if(window.innerWidth<=788)
	   {
	    $('#header_bg_x').css('display','none');
	   }
	   
	   
			if(window.innerWidth<900)
			{
				if(window.innerWidth>550)
				{

					$('.TopMenuItem' ).css('font-size', function(){
		   
							var textRatio =  (window.innerWidth/900)*(900/700) ;
							return textRatio.toFixed(2)+'em';
							
					});
					$('#horizNav2 ul.tameHoriz li' ).css('font-size', function(){
		   
							var textRatio = window.innerWidth / 900;
							return textRatio.toFixed(2)+'em';
							
					});
					$('#horizNav2 ul.tameHoriz li span.pipe').css('padding',function(){
							var Ratio = window.innerWidth / 1700;
							return Ratio.toFixed(2)+'em';
					
					
					});
				}
				else
				 $('#horizNav2 ul.tameHoriz li' ).css('font-size', '.75em');
				 
				$('#QueryResultsPanel').css('max-width',$(window).innerWidth()-30+'px');
				
			}
	}
	
	function fixFBPost(){
			$(".fb-post iframe").attr({"width":'','height':''});
			
			
			if(window.innerWidth<550)
				{
				$(".fb-post span, .fb-post iframe").css({
					'width' : '100%',
					'max-width' : '550px',
					'height' : parseInt(window.innerWidth*1.35) +'px'}
					);
				}
			else
				{
					$(".fb-post span, .fb-post iframe").css({
					'width' : '100%',
					'max-width' : '550px',
					'height' : parseInt(window.innerWidth * .3) +'px'}
					);
				}
			}
	initPositions();
	if(window.innerWidth<932)
		fixFBPost();
	window.addEventListener('orientationchange', initPositions);
	
	
    $(window).resize(function(){
		initPositions();
		if(window.innerWidth<932)
			fixFBPost();
		
    });
    

	var mql = window.matchMedia("(min-width: 550px)");
	var mql2 = window.matchMedia("(min-width: 900px)");
	var mql3 = window.matchMedia("(max-width:550px)");
	
	var handleMediaChange = function (mediaQueryList) {
		if (mediaQueryList.matches) {
			// The browser window is at least 550px wide, close mobile nav and show search bar.
			if($('#nestedAccordion').hasClass('open'))
				{
				slideToggleElem($('#nestedAccordion'),0);
				}
				if($('#horizNav2').hasClass('close'))
				{
				slideToggleElem($('#horizNav2'),0);
				}
			
			
		}
		else {
			// The browser window is less than 550px wide, keep searchbar closed by default unless clicked.
			if($('#horizNav2').hasClass('open'))
				{
				slideToggleElem($('#horizNav2'),0);
				}
				
		}
	}


	var fixTextRatio = function (mediaQueryList) {

	 if (mediaQueryList.matches) {
			// The browser window is at least 900px wide.
		$('.TopMenuItem').css('font-size', '14px');
		$('#horizNav2 ul.tameHoriz li').css('font-size', '1em');

		$('#horizNav2 ul.tameHoriz li span.pipe').css('padding', '1em');
			
		}
		else {
			// The browser window is less than 900px wide.
		}
	}

	var fixHeaderBGX = function (mediaQueryList){

		if(mediaQueryList.matches)
		{
			$('#header_bg_x').css('display','none');
		}
		else
		{

		}

	}
		
	mql.addListener(handleMediaChange);
	mql2.addListener(fixTextRatio);
	mql3.addListener(fixHeaderBGX);
	fixTextRatio(mql2);
	handleMediaChange(mql);
	fixHeaderBGX(mql3);

	
    $('.subMenu').click(function(){
       
        if($('.indicatorT', this).hasClass('open'))
        {
            $('.indicatorT', this).removeClass('open');
        }
        else
        $('.indicatorT', this).addClass('open');
        
    });
    
    $('#navIcon').click(function(){
    
        var myElem = $('#nestedAccordion');
        slideToggleElem(myElem, 350);
        

    
    });
    
    $('#searchIcon').click(function(){
        var myElem = $('#horizNav2');
    slideToggleElem(myElem, 350);
    });
    $('#contactIcon').click(function(){
		window.location.href="/t-contact";
	});
	$('#accountIcon').click(function(){
		window.location.href="/MyAccount";
	});
	$('#wishlistIcon').click(function(){
		window.location.href="/MyAccount/WishList";
	});
	$("#cartIcon").click(function(){
		window.location.href="/Checkout/";
	});
	$('.LPMlabel').click( function() {
	 openClientInNewWindow('/Chat/?init_color=blue','495','750');
	});
	$('.LPMimage').click( function() {
	 openClientInNewWindow('/Chat/?init_color=blue','495','750');
	});
	$('.LPMcloseButton').click(function(){
	  $('.LPMcontainer').hide();
	});

	$('.leftNavMenu li:not(a)').on('click',function(){
			var newLoc = $(this).find('a').attr('href');
			window.location.href=newLoc;
	});

var $viewportMeta = $('meta[name="viewport"]');
$('input, select, textarea').bind('focus blur', function(event) {
$viewportMeta.attr('content', 'width=device-width,initial-scale=1,maximum-scale=' +        (event.type == 'blur' ? 10 : 1));
});
   //$('meta[name="viewport"]').prop('content', 'width=1440');
   

});





    
