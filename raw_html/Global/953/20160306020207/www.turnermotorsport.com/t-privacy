 $(document).ready(function() {
	$('.colcontent').parent().css({'width':'100%', 'max-width':'640px'});

	jQuery(".colcontent").hide();
	//toggle the componenet with class msg_body
	jQuery(".colheading").click(function(){
		jQuery(this).next(".colcontent").slideToggle(250);
	});

	$("#topsearchform").submit(function () {
		if ($.trim($("#searchBox").val()).length < 3) {
			alert("Search term is too short. You must search by a minimum of three characters.");
			$('#searchBox').val("");
			return false;
		}
	});
 });
});
