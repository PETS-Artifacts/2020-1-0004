<script>
    function onDetailImageError(element) {
        var placeholder = "https://1c2a8a2161d644d95009-22d26b38e78c173d82b3a9a01c774ffa.ssl.cf1.rackcdn.com/skins/Skin_1/images/turner_place_holder_image.jpg";
        var webp_600 = document.getElementsByClassName("imageURL600W")[0];
        //var jpg_600 = document.getElementsByClassName("imageURL600")[0];
        if (webp_600.srcset) {
            webp_600.srcset = '';
        } else {
            element.onerror = null;	
            element.src = placeholder;
        }
        return true;
    }
    function onProductImageError(element) {
        var placeholder = "https://1c2a8a2161d644d95009-22d26b38e78c173d82b3a9a01c774ffa.ssl.cf1.rackcdn.com/skins/Skin_1/images/turner_place_holder_image.jpg";
        var isWebp = element.currentSrc ? element.currentSrc.toLowerCase().endsWith('.webp') : false;
        var source;
        if (isWebp) {
            var children = Array.from(element.parentNode.childNodes);
            children.forEach(function(child) {
                if (child.nodeName == 'SOURCE') {
                    source = child;
                }
            });
        }
        if (source && source.srcset) {
            source.srcset = '';
        } else {
            element.onerror = null;
            element.src = placeholder;
        }
        return true;
    }
</script>
