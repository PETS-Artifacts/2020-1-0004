	<script type='text/javascript'>
                    var x=document.body.getElementsByTagName('a');
                    for (i=0;i<x.length;i++) {
                    var upperHref = x[i].href.toUpperCase();
                    if (upperHref.indexOf('.PDF') >= 0 ||
                    (upperHref.indexOf('/ASSETS/') >= 0 && 
                    upperHref.indexOf('-PDF') >= 0)) {
                    var url_split=x[i].href.match(/(\w+):\/\/([\w.]+)(\/\S*)/);
                    var wt_domain=url_split[2];
                    var wt_uri=url_split[3];
                    var wt_title=x[i].innerHTML
                    var elem = $(x[i]);
                    elem.data('wt_domain', wt_domain);
                    elem.data('wt_uri', wt_uri);
                    elem.data('wt_title', wt_title);
                    elem.click(function(){
                    var that = $(this);
                    dcsMultiTrack('dcssip', that.data('wt_domain'), 'dcsuri', that.data('wt_uri'), 'WT.ti', that.data('wt_title'));
                    });}}
