<script type="text/javascript">// <![CDATA[
var btn = document.getElementById("eyemdbtn"); var zip = document.getElementById("eyemdzip"); var goFindAnEyeMd = function() {var isValidZip = /(^\d{5}$)|(^\d{5}-\d{4}$)/.test(zip.value); if (!isValidZip) { alert("Please enter a valid ZIP code"); return false; } location.href = "https://secure.aao.org/aao/find-ophthalmologist?zip=" + zip.value; return true; }; btn.addEventListener("click", goFindAnEyeMd); zip.addEventListener("keyup", function(e) { if (e.keyCode === 13) { goFindAnEyeMd(); return false; } else { return true; } }); 
