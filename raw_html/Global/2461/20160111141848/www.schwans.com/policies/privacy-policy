        !window.jQuery && document.write('<script src="/client/schwans.combined.hdr.1.0.5850.27102.min.js" type="text/javascript"><\/script>');
        !window.jQuery && document.write('<link href="/client/schwans.combined.1.0.5850.27102.min.css" rel="stylesheet" type="text/css" />');
