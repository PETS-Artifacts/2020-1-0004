    <script>
        var szAutoCompleteQuery;
        $(function () {
            //***** This section is for search - START 
            //Initialize auto complete for search header
            $('#js-autocomplete').autocomplete({
                serviceUrl: '/products/search-ac.ashx',
                dataType: 'json',
                minChars: 3,
                maxHeight: 400,
                zIndex: 59, // must be less than the header z-index (60)
                tabDisabled: true,
                appendTo: '#js-autocomplete-container',
                forceFixPosition: true,
                onSearchStart: function (query) {
                    szAutoCompleteQuery = query.query;  //Save off what they had typed
                },
                onSelect: function (suggestion) {
                    //Specific analytics tracking
                    s.eVar4 = "";       //clear out
                    s.prop3 = "lucene_search";
                    s.eVar3 = "search";
                    s.products = suggestion.data;
                    s.prop10 = suggestion.value;
                    s.eVar1 = szAutoCompleteQuery;
                    s.prop2 = "predicted search term selected";
                    s.prop1 = szAutoCompleteQuery; //Textbox already has selected value so use saved
                    s.events = "event1";
                    var s_code = s.t();     //Submit the analytics

                    bWarnUserLeavingPage = false;

                    //Suggesion.data has 5 digit product id, redirect right to that page
                    window.location.href = "http://www.schwans.com/products/product?id=" + suggestion.data;
                }
            });

            //Get enter in search header
            $('#js-autocomplete').on('keypress', function (e) {
                if (e.which == 13) {
                    PerformSearch();
                    return false;
                }
            });
            //***** This section is for search - END 



        });
    </script>
    <script>
        //***** This section is for email sign up in footer - START
        //Sign up
        $(function () {
            $('#email-capture-form1').on('click', '.js-email-capture-submit1', function () {
                'use strict';

                var szEmail = $("#emailSignUp1").val();
                if (!/^[\w\.]+@([a-zA-Z0-9]+\.)*[a-zA-Z]{2,4}$/.test(szEmail)) {
                    var emailinput = $("#emailSignUp1");
                    var spanerrormessage = $("#spanemailsignup")
                    emailinput.addClass("unobtrusiveHasError");
                    spanerrormessage.removeClass("isVisuallyHidden");
                }
                else {
                    postEmailSignupToServer(szEmail, "www.schwans.com");
                    var emailsuccess = $("#email-signup-success");
                    var emailtextfield = $("#email-signup-textfield");
                    emailsuccess.removeClass("isVisuallyHidden");
                    emailtextfield.addClass("hidden");
                }
                });

            });
            //***** This section is for email sign up in footer - END
    </script>
    
    

    


    
