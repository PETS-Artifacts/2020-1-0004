		 (function($) {
		  $('#hidden').hide().click(function(e) {
			e.stopPropagation();
		  });
		  $('.toggle-button').click(function(e) {
			$('#hidden').animate({ opacity: 'toggle' });
			$(this).html(($('.toggle-button').text() == 'Display Detailed Data +') ? 'Hide Detailed Data -' : 'Display Detailed Data +');
			e.stopPropagation();
		  });
