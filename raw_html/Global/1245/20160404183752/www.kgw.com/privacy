	<!-- begin UI: SEARCH -->
	<div class="col-12" style="display:none">
		<form id="nav-search" action="http://www.kgw.com/search?p_p_id=searchresult_WAR_sitesearchportlet&p_p_lifecycle=1&p_p_state=normal&_searchresult_WAR_sitesearchportlet_javax.portlet.action=search" method="GET" name="sf" class="ui-search non-compact">
			<section class="mod-wrapper mod-search"> 
				
				<div class="search-length">
					<div class="column-6">
						<input type="text" placeholder="" tabindex="1" id="searchTerm_box" name="searchTerm_box" value=""/>
					</div>
					<div class="column-4 select-dropdown">
						<select name="scopeType" id="scopeType">
							<option value="0" >Site</option>
							
						</select>
						
						<select name="sortType" id="sortType" style="display: none;">
							<option value="0" selected>Relevance</option>
							<option value="1" >Date</option>
						</select>
					</div>
				</div>
				
				<button type="button" class="ui-btn" onclick="submit_searchbox_WAR_sitesearchportlet_Form()"><i class="ep-icon-search"></i> <span class="visuallyhidden">Search</span></button>
			</section>
			<span class="is-1140"></span>
		</form>
	</div>
	<!-- end UI: SEARCH -->
	
