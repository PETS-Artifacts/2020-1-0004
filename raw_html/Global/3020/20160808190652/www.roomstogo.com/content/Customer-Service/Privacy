		    	var promotions = [], attrFound = false; // reset

				if($(this).filterAttr("data-promotion")){
					//consoleLog(this);
					promotions.push({
						"name": $(this).attr("data-promotion-name"),
						"id": $(this).attr("data-promotion-id"),
						"creative": $(this).attr("data-promotion-creative"),
						"position": $(this).attr("data-promotion-position")
					});
					attrFound = true;
				}

		    	if($(this).find("area")) {
					$(this).find("area").each(function(){ // if the slide has image maps
						//consoleLog(this);
			    		if($(this).filterAttr("data-promotion")){
					        promotions.push({
								"name": $(this).attr("data-promotion-name"),
								"id": $(this).attr("data-promotion-id"),
								"creative": $(this).attr("data-promotion-creative"),
								"position": $(this).attr("data-promotion-position")
					        });
					        attrFound = true;
				    	}
			    	});
		    	}

		    	if(attrFound){
					dataLayer.push({
						"event": "promotionClick",
						"ecommerce": {
							"promoClick": {
								"promotions" : promotions
							}
						}
					});
				}
				consoleLog(promotions); debug(e);

		    });

