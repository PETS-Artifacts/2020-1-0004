		    }

	    	if($(".atg_store_pageProductDetail.mattressSet").length){
				if($("#gtmProductName").length){
					var chosenMattressProduct = $(".chosenMattressProduct:checked");
					if(chosenMattressProduct){
						detail.push({
							"products": [{
								"name": $(chosenMattressProduct).parent().parent().parent().parent().find("#gtmProductName").val().split("\n")[0],
								"id": $(chosenMattressProduct).parent().parent().parent().parent().find("#gtmProductId").val(),
								"price": $(chosenMattressProduct).parent().parent().parent().parent().find("#gtmProductPrice").val().split("$")[1],
								"brand": $(chosenMattressProduct).parent().parent().parent().parent().find("#gtmProductBrand").val(),
								"category": $(chosenMattressProduct).parent().parent().parent().parent().find("#gtmProductCategory").val(),
							}]
						});
						var detail = detail[0];
					}
				}

	    	}else if($(".atg_store_pageProductDetail").length){
				if($("#gtmProductName").length){
					detail.push({
						"products": [{
							"name": $("#gtmProductName").val().split("\n")[0],
