		    $(".atg_store_update").on("click", function(e){  // update cart. remove if quantity equals 0

		    	if($(this).closest("tr").find(".qty").val() == "0") {
					remove.push({
						"products": [{
							"name": $(this).closest("tr").find("#productName").val().split("\n")[0],
							"id": $(this).closest("tr").find("#productId").val(),
							"price": $(this).closest("tr").find("#productPrice").val().split("$")[1],
