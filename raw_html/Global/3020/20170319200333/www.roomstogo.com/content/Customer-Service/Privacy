	<script type="text/javascript">
		var utag_data = {};

		//common calls
		utag_data.cart_product_img = [];
		utag_data.cart_product_name = [];
		utag_data.cart_product_price = [];
		utag_data.cart_product_quantity = [];
		utag_data.cart_product_sku = [];
		utag_data.credit_provider = rtgSeo.creditProvider;
		utag_data.customer_city = rtgSeo.city;
		utag_data.customer_id = rtgSeo.sessionID;
		utag_data.customer_postal = rtgSeo.zip;
		utag_data.customer_state  = rtgSeo.state;
		utag_data.in_market = rtgSeo.inMarket;
		utag_data.market = rtgSeo.market;
		utag_data.page_name = $("title").text().trim();
		utag_data.page_type = rtgSeo.pagename;
		utag_data.profile_id = rtgSeo.profileId;
		utag_data.site_id = rtgSeo.siteId;

		if(rtgSeo.visitor !== undefined){
			utag_data.customer_email = rtgSeo.visitor.email;
			utag_data.customer_first_name = rtgSeo.visitor.firstName;
			utag_data.customer_last_name = rtgSeo.visitor.lastName;
		};

		if(rtgSeo.miniCart !== undefined && rtgSeo.miniCart !== null){
			utag_data.cart_total = rtgSeo.miniCart.subtotal;

			for(var i = 0; i < rtgSeo.miniCart.items.length; i++){
				utag_data.cart_product_img.push(rtgSeo.miniCart.items[i].imageUrl);
				utag_data.cart_product_name.push(rtgSeo.miniCart.items[i].name);
				utag_data.cart_product_price.push(rtgSeo.miniCart.items[i].prices[0].price);
				utag_data.cart_product_quantity.push(rtgSeo.miniCart.items[i].prices[0].quantity);
				utag_data.cart_product_sku.push(rtgSeo.miniCart.items[i].skuId);
			};
		};


