<script>
    var ML_img_ServerOnline = new Image();
    var MetrixLab_Protocol = document.location.protocol;

    ML_img_ServerOnline.src = MetrixLab_Protocol + '//invitation.opinionbar.com/popups/ServerOnline.gif';
