    /* LP Mobile JS Configuration */
    //var _LP_CFG_ = {
    //    app_id : 'Purina2-CSmobile-english',
    //    options :
    //    { skill : 'Purina2-CSmobile-english' }
    //};

    /* LP Mobile JS Configuration */
    var _LP_CFG_ = {
        app_id: "79915849",
        options: {
            skill: 'Purina2-CSmobile-english',
            //Custom Button uses selector to your Chat button
            triggerSelector: '.js-liveperson-mobile-chat',

            //Custom Button State
            // An event that fires when chat is disabled. It takes one parameter which is an array of your trigger selectors.
            onChatDisabled: function (els) {
                $('.lpchatpurina2mobileNavContact').addClass("isHidden");
                console.log('mobile LP disnabled');

                $('.js-liveperson-mobileonly-button .js-liveperson-mobile-message').html('Chat Offline<br /><span class="mix-iconList-item-text_normalCase">Check back soon</span>');
                $('.js-liveperson-mobileonly-button').removeClass('iconList-item_isActive').show();

                $('.js-liveperson-article-button .js-liveperson-mobile-message').html('Chat Offline');
                $('.js-liveperson-article-button .mediaLink-container-img').removeClass('mediaLink-container-img_chat');
                $('.js-liveperson-article-button .mediaLink-container-img').addClass('mediaLink-container-img_chat_offline');
                $('.js-liveperson-article-button').show();
            },
            // An event that fires when chat is enabled. It takes one parameter which is an array of your trigger selectors.
            onChatEnabled: function (els) {
                $('.lpchatpurina2mobileNavContact').removeClass("isHidden");
                console.log('mobile LP enabled');

                $('.js-liveperson-mobileonly-button .js-liveperson-mobile-message').html('Chat With A<br />Pet Advisor</span>');
                $('.js-liveperson-mobileonly-button').addClass('iconList-item_isActive').show();


                $('.js-liveperson-article-button .js-liveperson-mobile-message').html('Chat Live');
                $('.js-liveperson-article-button .mediaLink-container-img').addClass('mediaLink-container-img_chat');
                $('.js-liveperson-article-button .mediaLink-container-img').removeClass('mediaLink-container-img_chat_offline');
                $('.js-liveperson-article-button').show();
            },
        }
    };
</script>
