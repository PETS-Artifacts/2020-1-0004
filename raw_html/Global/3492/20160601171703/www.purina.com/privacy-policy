<script>var addthis_config = { "data_track_addressbar": false };</script>
<script id=addthis src="//s7.addthis.com/js/300/addthis_widget.js#pubid=ra-51798fa60b850da9&async=1"></script>
<script>
    // Since AddThis doesn't seem to count shares properly, we're getting the live counts dynamically and overriding
    // the default count values with the correct ones.
    (function ($, addthis, undefined) {
        // This is so articleAjax.js can re-initialize this behavior when it blows away AddThis and reloads it
        window.updateAddThisShareLinks = function(at) {
            // addEventListener is provided by addthis, not the browser, no need to use attachEvent
            at.addEventListener('addthis.ready', onReady, false);
        };

        updateAddThisShareLinks(addthis);

        function onReady() {
            $(function () {
                var $counters = $('.addthis_counter');
                var remaining = $counters.length - 1;
                // We are using mutual recursion here, since the call to getShareCounts
                // doesn't seem to work properly when executed in a loop (only a single request
                // is being executed)
                function getCount() {
                    // If we've gone past 0 on items remaining, we're done
                    if (remaining < 0)
                        return;

                    // Get the current element
                    var $el = $counters.eq(remaining);
                    // Get the current share url
                    var shareUrl = ensureUriAbsolute($el.attr('addthis:url'));
                    // Execute a request to AddThis to get the live share counts from Facebook and Twitter
                    if (shareUrl) {
                        addthis.sharecounters.getShareCounts({ service: ['facebook', 'twitter'], countUrl: shareUrl }, function (obj) {
                            // Sum the totals
                            var totalShares = 0;
                            for (var i = 0; i < obj.length; i++) {
                                if (typeof obj[i].count === 'number') {
                                    totalShares += obj[i].count;
                                }
                            }
                            // Reset the current count with the new one
                            setCount(totalShares);
                        });
                        // If no url was defined, then decrement remaining and recurse
                    } else {
                        remaining = remaining - 1;
                        getCount();
                    }
                }

                function setCount(count) {
                    // Get the current element
                    var $el = $counters.eq(remaining);
                    // Set the new value
                    $el.text(count > 999 ? (count / 1000).toString() + 'k' : count);
                    // Decrement remaining and recurse
                    remaining = remaining - 1;
                    getCount();
                }

                function ensureUriAbsolute(uri) {
                    // Given a null, empty or non-string value, return the origin
                    if (!uri || typeof uri !== 'string') {
                        return window.location.origin;
                    }

                    if (/^http[s]?:\/\/.*\.com\/.*/.test(uri)) {
                        // The uri is already good to go
                        return uri;
                    }
                    else {
                        // Return origin + uri, ensuring there is a path seperator between the two
                        return window.location.origin + (uri[0] === '/' ? uri : '/' + uri);
                    }
                }

                // Kick things off
                getCount();
            });
        }
    })(jQuery, addthis);
</script>
<script id=purinasignin-jscontroller-init>
    window.purinaSignInEventInit = function () {
        PURINASINGIN.init({
            client_id: '143ba2b2b4e99b28b830020074f57735',// Client ID
            reg_path: '/registration',// registration path
            api_url: 'https://profiles.purina.com',
            signed_out_template : '<a id="signin-link"></a> <a id="register-link">Sign Up for Purina News</a>', // Optional
            signed_in_template : 'Hello, {user}. <a id="signout-link">Sign Out</a>'
        });
    };
    // Load the Purina Reg 2.0 sign in widget
    (function (d, s, id) {
        var js, purinajs = d.getElementsByTagName(s)[0];
        if (d.getElementById(id)) { return; }
        js = d.createElement(s); js.id = id;
        js.src = "//profiles.purina.com/js/signin-widget.js";
        purinajs.parentNode.insertBefore(js, purinajs);
    }(document, 'script', 'purinasignin-jscontroller'));
</script>
