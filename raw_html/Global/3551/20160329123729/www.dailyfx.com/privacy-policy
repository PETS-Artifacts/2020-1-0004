    <script>
        //Next code used to prevent unexpected menu close when using some components (like accordion, tabs, forms, etc), please add the next JavaScript to your page
        $(window).load(function () {
            $(document).on('click', '.navbar .dropdown-menu', function (e) {
                e.stopPropagation();
            });
        });
        //Start Fix MegaNavbar on scroll page
        var navHeight = $('#main_navbar').offset().top;
        FixMegaNavbar(navHeight);
        $(window).bind('scroll', function () {
            FixMegaNavbar(navHeight);
        });
        function FixMegaNavbar(navHeight) {
            if (!$('#main_navbar').hasClass('navbar-fixed-bottom')) {
                if ($(window).scrollTop() > navHeight) {
                    $('#main_navbar').addClass('navbar-fixed-top');
                    $('body').css({'margin-top': $('#main_navbar').height() + 'px'});
                    if ($('#main_navbar').parent('div').hasClass('container'))
                        $('#main_navbar').children('div').addClass('container').removeClass('container-fluid');
                    else if ($('#main_navbar').parent('div').hasClass('container-fluid'))
                        $('#main_navbar').children('div').addClass('container-fluid').removeClass('container');
                } else {
                    $('#main_navbar').removeClass('navbar-fixed-top');
                    $('#main_navbar').children('div').addClass('container-fluid').removeClass('container');
                    $('body').css({'margin-top': ''});
                }
            }
        }
    </script>
    <!-- End Nav Bar --><script src="https://cdn.socket.io/socket.io-1.0.5.js"></script>
<script src="//cdnjs.cloudflare.com/ajax/libs/async/0.9.0/async.js"></script>

<script>

    function padZeroPrice(value, pres) {
        if (pres === 5) {
            var valueround = Math.round(value * 100000) / 100000;
        } else {
            var valueround = Math.round(value * 1000) / 1000;
        }
        var s = Math.abs(valueround) + "00000";
        var i = s.indexOf('.');
        var m = valueround < 0 ? '-' : '';
        return m + s.substr(0, i) + s.substr(i, (pres + 1));
    }

    var price_callback = function (price) {

        var priceObj = JSON.parse(price);
        //console.log(priceObj);
        priceObj.Updated;
        priceObj.Symbol;
        //priceObj.Rates[i] <--- Rates is an array with a length of 4
        var symbol = priceObj.Symbol;
        //var priceLow = priceObj.Low;
        var temp = symbol.replace('/', '');
        temp = temp.toLowerCase();
        var rates = priceObj.Rates[1];
        var precision = 3;
        if (temp.indexOf('jpy') === -1) {
            precision = 5;
        }
        var ratesdecimal = rates.toFixed(precision);
        var currentPrice = $('#' + temp + '-price').html();
        var currentPrice0 = $('#' + temp + '-price0').html();
        //var currentBid = $('#' + temp + '-priceBid').html();
        //var currentAsk = $('#' + temp + '-priceAsk').html();

        //Bid
        var bidDif = $('#' + temp + '-priceBid').html() - priceObj.Rates[0];
        var lastStr = bidDif.toFixed(precision);
        var lastSlice = lastStr.slice(-1);

        var lastSliceUlt = lastSlice;
        if (lastStr < 0) {
            lastSliceUlt = '-' + lastSlice;
        }
        var parseSlice = parseInt(lastSliceUlt);
        if (0 < parseSlice && parseSlice <= 3) {
            $('#' + temp + '-priceBid').css("color", "#b04859");
        }
        if (3 < parseSlice && parseSlice <= 6) {
            $('#' + temp + '-priceBid').css("color", "#e35d73");
        }
        if (6 < parseSlice && parseSlice < 9) {
            $('#' + temp + '-priceBid').css("color", "#ff768e");
        }
        if (-3 < parseSlice && parseSlice < 0) {
            $('#' + temp + '-priceBid').css("color", "#3b7ca1");
        }
        if (-6 < parseSlice && parseSlice < -3) {
            $('#' + temp + '-priceBid').css("color", "#4ea3d4");
        }
        if (-9 < parseSlice && parseSlice < -6) {
            $('#' + temp + '-priceBid').css("color", "#65c4fb");
        }

        //Ask
        var AskDif = $('#' + temp + '-priceAsk').html() - priceObj.Rates[1];
        var lastStrAsk = AskDif.toFixed(precision);
        var lastSliceAsk = lastStrAsk.slice(-1);

        var lastSliceAskUlt = lastSliceAsk;
        if (lastStrAsk < 0) {
            lastSliceAskUlt = '-' + lastSliceAsk;
        }
        var parseSliceAsk = parseInt(lastSliceAskUlt);
        if (0 < parseSliceAsk && parseSliceAsk <= 3) {
            $('#' + temp + '-priceAsk').css("color", "#b04859");
        }
        if (3 < parseSliceAsk && parseSliceAsk <= 6) {
            $('#' + temp + '-priceAsk').css("color", "#e35d73");
        }
        if (6 < parseSliceAsk && parseSliceAsk <= 9) {
            $('#' + temp + '-priceAsk').css("color", "#ff768e");
        }
        if (-3 < parseSliceAsk && parseSliceAsk < 0) {
            $('#' + temp + '-priceAsk').css("color", "#3b7ca1");
        }
        if (-6 < parseSliceAsk && parseSliceAsk < -3) {
            $('#' + temp + '-priceAsk').css("color", "#4ea3d4");
        }
        if (-9 < parseSliceAsk && parseSliceAsk < -6) {
            $('#' + temp + '-priceAsk').css("color", "#65c4fb");
        }

        //current price
        if (currentPrice < priceObj.Rates[1]) {
            $('#' + temp + '-price').css("color", "#32a6e7");
        }
        if (currentPrice > priceObj.Rates[1]) {
            $('#' + temp + '-price').css("color", "#ff4866");
        }
        if (currentPrice0 < priceObj.Rates[1]) {
            $('#' + temp + '-price0').css("color", "#32a6e7");
        }
        if (currentPrice0 > priceObj.Rates[1]) {
            $('#' + temp + '-price0').css("color", "#ff4866");
        }
        if (temp.length !== 6 || temp === 'xauusd' || temp === 'xagusd' || temp === 'copper'
                || temp === 'xptusd' || temp === 'xpdusd' || temp === 'spx500'
                || temp === 'aus200' || temp === 'jpn225') {
            $('#' + temp + '-price').html(ratesdecimal);
            $('#' + temp + '-priceBid').html(priceObj.Rates[0]);
            $('#' + temp + '-priceAsk').html(priceObj.Rates[1]);
            $('#' + temp + '-priceHi').html(priceObj.Rates[2]);
            $('#' + temp + '-priceLow').html(priceObj.Rates[3]);

            $('#' + temp + '-price0').html(ratesdecimal);
            $('#' + temp + '-priceHi0').html(priceObj.Rates[2]);
            $('#' + temp + '-priceLow0').html(priceObj.Rates[3]);

            $('#' + temp + '-price1').html(ratesdecimal);
            $('#' + temp + '-priceHi1').html(priceObj.Rates[2]);
            $('#' + temp + '-priceLow1').html(priceObj.Rates[3]);
        } else {
            var temp1 = padZeroPrice(ratesdecimal, precision);
            var temp2 = padZeroPrice(priceObj.Rates[2], precision);
            var temp3 = padZeroPrice(priceObj.Rates[3], precision);
            $('#' + temp + '-price').html(temp1);
            $('#' + temp + '-priceBid').html(padZeroPrice(priceObj.Rates[0], precision));
            $('#' + temp + '-priceAsk').html(padZeroPrice(priceObj.Rates[1], precision));
            $('#' + temp + '-priceHi').html(temp2);
            $('#' + temp + '-priceLow').html(temp3);

            $('#' + temp + '-price0').html(temp1);
            $('#' + temp + '-priceHi0').html(temp2);
            $('#' + temp + '-priceLow0').html(temp3);

            $('#' + temp + '-price1').html(temp1);
            $('#' + temp + '-priceHi1').html(temp2);
            $('#' + temp + '-priceLow1').html(temp3);
        }
    };
    
    prices.subscribe(['EUR/USD','GBP/USD','USD/JPY','AUD/USD','USD/CAD','NZD/USD'], price_callback);

</script><nav class="navbar price-panel no-border-radius navbar-fixed no-fix hidden-xs"  role="navigation" style="margin-top: -20px;" >
