        getHrefRedirect: function(key) {
            var pair = key.replace('/', '');
            if (typeof (this.hrefRedirect[pair]) === 'undefined') {
                pair = pair.toLowerCase();
                return pair.slice(0, 3) + '-' + pair.slice(3, 6);
            } else {
                return this.hrefRedirect[pair];
            }
        },
        getTranslation: function (property, key) {
            if(this[property] && typeof key === "undefined") {
                return this[property];
            } else if (this[property] && typeof key !== "undefined" && typeof this[property] === "object") {
                return this[property][key] ? this[property][key] : key;
            } else {
                console.error('Translation "' + property + '" isn\'t presented in Translation List');
                return property;
            }
        },
        getInstrumentName: function (key) {
            var key = key.replace('/', '');
            if (!this.instrumentsNames[key]) {
                return key;
            }
            return this.instrumentsNames[key];
        }
    };
</script>

