{
    var initInfo = {editable: false,isEditMode: false,allowWebPartAdder: false,listId: "{5004689c-620c-4c38-ab6d-eef40cd78305}",itemId: 30,recycleBinEnabled: true,enableMinorVersioning: true,enableModeration: false,forceCheckout: true,rootFolderUrl: "\u002fabout-liberty-mutual-site\u002fPages",itemPermissions:{High:16,Low:196673}};
