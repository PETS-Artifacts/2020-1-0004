  <script>
   //open video popup from here
    function dispalyvideo(url) {
        var video = document.getElementById('videosource');
        video.src = url;
        video.play();
        jQuery('#myModalvideo').modal('show');
    }
    //when dismiss popup window then pause or stop video
    jQuery('#myModalvideo').on('hidden.bs.modal', function () {
        var video = document.getElementById('videosource');
        video.pause();
    })
  </script>

