<script type='text/javascript'>
	var userLang = navigator.language || navigator.userLanguage;
	var language = null;
	
	
	if (userLang = 'en-US' ) {
		var settings = 'Manual';
	}
	else if (userLang = 'fr') {
		var settings = 'auto';
	}
	
	if (userLang = 'fr' ) {
		var language = '&to=fr';
	}
	else if (userLang = 'es' ) {
		var language = '&to=es';
	}
	console.log(userLang);
