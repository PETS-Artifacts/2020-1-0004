		<script id="pagination1Tmpl" type="text/x-jquery-tmpl" >
			<input type="hidden" name="totalPages" id="totalPages" value="{{= totalPages}}">
			{{= ($item.endProductCount = ((resultsPerPage * currentPage) > totalResults) ? totalResults : (resultsPerPage * currentPage)), ''}}
			{{= ($item.startProductCount = ((resultsPerPage * (previousPage-1)) > totalResults) ? totalResults : (resultsPerPage * (previousPage-1))), ''}}
			<div class="displayCount">
				<p>
					results {{= (resultsPerPage * (currentPage-1)) + 1}} - {{= $item.endProductCount}} of {{= totalResults}}
				</p>
			</div>

			{{if totalPages > 1}}

			{{= ($item.paginationUrl = $item.baseUrl + '?' + $item.parameterizedRequestParameters + '&view_type=' + $item.viewType), ''}}
				<ul class="lpPagination">
					{{if currentPage > 1}}
						<li class="pagination--previous">
							<a class="pagination soft" href="{{= $item.paginationUrl}}&page={{= previousPage}}&offset={{= $item.startProductCount}}"><span class="screen-reader-only">Previous</span></a>
						</li>
					{{else}}
						<li class="pagination-previousDisabled"></li>
					{{/if}}
					{{tmpl(paginationView.pages,{
						currentPage : currentPage,
						totalPages : totalPages,
						startProductCount: $item.startProductCount,
						endProductCount : $item.endProductCount,
						resultsPerPage : resultsPerPage,
						baseUrl : $item.baseUrl,
						parameterizedRequestParameters : $item.parameterizedRequestParameters,
						viewType : $item.viewType
					}) "#pagination1ViewTmpl"}}
					{{if currentPage < totalPages}}
						<li class="pagination--next">
							<a class="pagination soft" href="{{= $item.paginationUrl}}&page={{= nextPage}}&offset={{= $item.endProductCount}}"><span class="screen-reader-only">Next</span></a>
						</li>
					{{else}}
						<li class="pagination-nextDisabled"></li>
					{{/if}}
				</ul>
			{{/if}}
		</script>

		<script id="pagination1ViewTmpl" type="text/x-jquery-tmpl" >
		{{= ($item.paginationUrl = $item.baseUrl + '?' + $item.parameterizedRequestParameters + '&view_type=' + $item.viewType), ''}}
			<li {{if $data == $item.currentPage}} class="current"{{else $data == $item.totalPages}} class="last"{{else $data == "..."}} class="gap"{{/if}}>
				{{if $data == $item.currentPage}}
					<span class="screen-reader-only">Page</span>{{= $data}} <span class="screen-reader-only">of {{= $item.totalPages}}</span>
				{{else $data == "..."}}
					<span class="screen-reader-only">Page</span>{{= $data}}
				{{else}}
					<a class="pagination soft" href="{{= $item.paginationUrl}}&page={{= $data}}&offset={{= ($data-1) * $item.resultsPerPage}}" title="view page {{= $data}}" rel="nofollow" >
						<span class="screen-reader-only">Page </span>
						{{= $data}}
						<span class="screen-reader-only">of {{= $item.totalPages}}</span>
					</a>
				{{/if}}
			</li>
		</script>

		<script id="pagination2Tmpl" type="text/x-jquery-tmpl" >
			{{= ($item.endProductCount = ((resultsPerPage * currentPage) > totalResults) ? totalResults : (resultsPerPage * currentPage)), ''}}
			{{= ($item.startProductCount = ((resultsPerPage * (previousPage-1)) > totalResults) ? totalResults : (resultsPerPage * (previousPage-1))), ''}}
			<ul class="results">
				<li class="last"><strong>{{= (resultsPerPage * (currentPage-1)) + 1}} - {{= $item.endProductCount}}</strong> of <strong>{{= totalResults}}</strong></li>
			</ul>
			{{= ($item.paginationUrl = $item.baseUrl + '?' + $item.parameterizedRequestParameters + '&view_type=' + $item.viewType ), ''}}
			<div class="pagination2">
				{{if currentPage > 1}}
					<div class="pagination-item previous">
						<a class="pagination soft" href="{{= $item.paginationUrl}}&page={{= previousPage}}&offset={{= $item.startProductCount}}"><span><span class="screen-reader-only">Previous</span></span></a>
					</div>
				{{else}}
					<div class="pagination-item previousDisabled">
						<span>Previous (not clickable)</span>
					</div>
				{{/if}}
				<div class="pagination-item current">
					page <strong>{{= currentPage}}</strong> of <strong>{{= totalPages}}</strong>
				</div>
				{{if currentPage < totalPages}}
					<div class="pagination-item next">
						<a class="pagination soft" href="{{= $item.paginationUrl}}&page={{= nextPage}}&offset={{= $item.endProductCount}}"><span><span class="screen-reader-only">Next</span></span></a>
					</div>
				{{else}}
