      data-title="{{= $item.parent.data.itemAttributes.title}}"
	  {{if typeof $item.outOfStock != 'undefined' && $item.outOfStock == 'true'}}
	  data-extra=" - out of stock"
	  {{/if}}
      data-default-title="{{= $item.parent.data.itemAttributes.title}}{{if $item.itemColor != ''}}{{html ' - '}}{{/if}}{{= $item.itemColor}}{{if typeof $item.outOfStock != 'undefined' && $item.outOfStock == 'true'}}{{html ' - out of stock'}}{{/if}}"
      data-default-href="/{{= $item.parent.data.productDetailPageURL}}#prodSlot=_{{= $item.parent.parent.currentPageNumber}}_{{= $item.parent.productCounter}}">
      <h3>
        <img class="tileImage{{if typeof $item.outOfStock != 'undefined' && $item.outOfStock == 'true'}}{{html ' grey-out'}}{{/if}}"
          original="{{= url}}"
          alt="{{= $item.parent.data.itemAttributes.title}}{{if $item.itemColor != ''}}{{html ' - '}}{{/if}}{{= $item.itemColor}}{{if typeof $item.outOfStock != 'undefined' && $item.outOfStock == 'true'}}{{html ' - out of stock'}}{{/if}}"
          data-default-alt="{{= $item.parent.data.itemAttributes.title}}{{if $item.itemColor != ''}}{{html ' - '}}{{/if}}{{= $item.itemColor}}{{if typeof $item.outOfStock != 'undefined' && $item.outOfStock == 'true'}}{{html ' - out of stock'}}{{/if}}" />
