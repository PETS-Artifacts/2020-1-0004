	var initiateFetchSearchResponse = function() {
		var templateStoreUrl = '';
		var response_group = 'Items,VariationSummary';

		// Over writing for temporary
		Target.globals.searchResponsePath = 'http://tws.target.com/searchservice/item/search_results/v2/by_keyword';
