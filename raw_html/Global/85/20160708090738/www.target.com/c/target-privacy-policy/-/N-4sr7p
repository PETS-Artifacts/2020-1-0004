				if (!$.isEmptyObject(customPriceField)) {
					$.extend(ajaxData, customPriceField);
				}
			}
			
		

		Target.templateRenderer.fetchSearchResponse(ajaxData, templateStoreUrl, placementsArr);
	};

	initiateFetchSearchResponse();
</script>

		
		
