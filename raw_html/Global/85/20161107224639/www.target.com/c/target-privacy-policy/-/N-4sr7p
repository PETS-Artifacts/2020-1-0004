<script type="text/javascript">
  
  var isSecure = (window.location.protocol !== "http:");
  var noSeoCall = (window.location.pathname.indexOf('target-crush') !== -1 || window.location.pathname.indexOf('mcategories') !== -1);
  var seoHttpRequestPromise = null;
  var seoHttpRequest = null;
  var seoReadyStateHandler = null;
  var dateSearchParam = '';
  var isSapphireEnabled = 'true' === 'true';
