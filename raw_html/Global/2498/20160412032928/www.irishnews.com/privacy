              <script>
              ADTECH.config.placements["irishnews_728x90"] = { responsive : { useresponsive: true, bounds: [{id: "irishnews_300x50", min: 0, max: 727 },{id: "irishnews_728x90", min: 728, max: 9999 }]}, params: { target: '_blank' }};
              ADTECH.enqueueAd("irishnews_728x90");
