	<div class="nav navbar-nav ng-cloak cartHide header-cart" sp-cart-class="cartHide">
<div class="cart hidden-xs" ng-controller="CartDropdownCtrl" ng-mouseover="showCart()" ng-mouseout="hideCart()" ng-class="{open: isCartVisible()}" ng-cloak>
	[[initCartDropDownDelay()]]
	<a id="cart-summary" href="/cart">
		<div class="count-container"><span ng-bind="getCart().totalItems || 0" class="count">0</span>
			<span class="count-descriptor">ITEMS</span>
		</div>
		<div class="cart-link"></div>
	</a>
	<div class="cart-dropdown hidden-xs" ng-show="isCartVisible() && getCart().totalItems > 0">
		<div class="dropdown-checkout">
			<div class="apple-pay-minicart" ng-if="showApplePay()">
				<button type="button" class="apple-pay-button" ng-click="startApplePaySession()"></button>
			</div>
			<a href="/cart" title="checkout" class="checkout">Proceed to checkout</a>
		</div>
		<ul>
			<li ng-repeat="item in getCart().displayItems" class="cart-product">
				<a class="link-unstyled" ng-href="[[item.product.url]]">
