<script language="javascript">
	var matchText = function (node, word, regex) {
            var excludeElements = ['script', 'style', 'iframe', 'canvas'];
            var child = node.firstChild;
            if (child != null) {
                do {
                    if (child.nodeType) {
                        switch (child.nodeType) {
                            case 1:
                                if (child.tagName) {
                                    if (excludeElements.indexOf(child.tagName.toLowerCase()) > -1) {
                                        continue;
                                    }
                                }
                                matchText(child, word, regex);
                                break;
                            case 3:
                                if (child.data) {
                                    child.data.replace(regex, function (all) {
                                        var args = [].slice.call(arguments),
                                            offset = args[args.length - 2];
										var newTextNode;
										if (child.length<offset)
										{
											newTextNode = child.splitText(offset);
											newTextNode.data = newTextNode.data.substr(all.length);

											var span = document.createElement("span");
											span.className = "notranslate";
											span.textContent = word;
											node.insertBefore(span, child.nextSibling);


											child = newTextNode;
										}
                                    });
                                }
                                break;
                        }
                    }
                } while (child = child.nextSibling);
            }
            return node;
        }

        $(document).ready(function () {
            var jqxhr = $.get("/Portals/0/notranslate.txt?x="+(new Date()), function (data) {
                var listWords = data.split('|');
                var s = $('body').html();
                for (var i = 0; i < listWords.length; i++) {
                    var word = listWords[i].trim().replace('\r','').replace('\n','');
                    if (word!='')
                        matchText($('body')[0], word, new RegExp(word, "gi"));
                }
            })
        });
</script>

