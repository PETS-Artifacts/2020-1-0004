<article data-history-node-id="83259" role="article" about="/terms-service-and-privacy-policy" typeof="schema:WebPage" class="node node--type-page node--view-mode-full">
    <div class="row">
      <div class="col-12">
        
                  <span property="schema:name" content="Abduzeedo Terms of Service and Privacy Policy" class="rdf-meta hidden"></span>

