<script type="text/javascript">
  var breakpoint = {}
  breakpoint.refreshValue = function() {
    this.value = window.getComputedStyle(document.querySelector('body'), ':before').getPropertyValue('content').replace(/\"/g, '')
  }
  document.addEventListener('DOMContentLoaded', function(e) {
    breakpoint.refreshValue()
    $('#gcc-enews-email').on('keyup', function(ev) {
      var $button = $(this).parents('.gcc-enews').find('button')
      if ($('.gcc-enews').valid()) {
        $button.prop('disabled', false)
      }
      if (!ev) ev = window.event;
      var keyCode = e.keyCode || e.which;
      if (keyCode == '13') {
        $button.prop('disabled', true)
      }
    })
    $('#gcc-header-toggle-menu').click(function(e) {
      e.preventDefault()
      $('body').toggleClass('with-sidebar')
      $('#gcc-mobile-menu').animate({
        scrollLeft: 0
      }, 0);
    })
    $('#gcc-site-menu-overlay').click(function(e) {
      $('body').removeClass('with-sidebar')
    })
    $('#gcc-inline-search').on('focus', function() {
      breakpoint.refreshValue()
      var containerWidth = $(this).parents('#gcc-header-search').prev().width()
      var navWidth = 0
      $(this).parents('#gcc-header-search').prev().find('> ul > li').each(function() {
        navWidth += $(this).width()
      })
      var searchWidth = (containerWidth - navWidth) + 'px'
      if (breakpoint.value === 'large') {
        $(this).parents('#gcc-header-search').css('width', searchWidth)
      }
    })
    
    
    $('#gcc-mobile-menu').html($('#gcc-header-nav')[0].outerHTML);
    $('#gcc-mobile-menu').append('<div id="gcc-header-subnavigation" class="dib w5 dn-l"><div class="flex justify-between ws-normal lh-solid pa1 f7 bg-white-10 white-80 items-center w-100"><button id="gcc-nav-back" class="bn bg-transparent outline-0 ma0 pa0"><span class="material-icons v-mid black-40 f3 w1">chevron_left</span> <span class="f7 black-40 v-mid lh-solid">Back</span></button><label class="white-30 mr2 fw4 f7"></label></div><div id="gcc-header-subnavigation-content"></div></div>');
    $('#gcc-nav-back').on('click', function(ev) {
      $('#gcc-mobile-menu').animate({
        scrollLeft: 0
      }, 200);
    });
    
    $('#gcc-mobile-menu > nav > ul > li > ul > li > a').on('click', function(ev) {
      if (breakpoint.value !== 'large') {
        ev.preventDefault()
        var width = $('#gcc-mobile-menu').width()
        var $li = $(this).next().find('li')
        var $submenu = '<ul class="list m0 p0 br3 b--navy br--bottom justify-between">';
        $li.each(function() {
          $submenu += '<li class="bb b--white-10 pl2 w-100"><a class="db fw3 white mv2 f5" href="' + $(this).find('a').attr('href') + '">'
          $submenu += $(this).find('a').text()
          $submenu += '<span class="material-icons v-mid f5 fr fn-l lh-copy mr2">chevron_right</span></a></li>'
        })
        $submenu += '</ul>'
        $('#gcc-header-subnavigation label').text($(this).attr('title'))
        $('#gcc-header-subnavigation-content').html($submenu)
        $('#gcc-mobile-menu').animate({
          scrollLeft: width
        }, 200)
      }
    })
  })
</script>

<script type="text/javascript">
  var updateInputClass = function($el) {
    var isFilled = false;
    if ($el.is('select')) {
      isFilled = $el[0].selectedIndex > 0 || $el.val() != '' ? true : false;
    } else {
      isFilled = $el.val().trim() === '' ? false : true;
    }
    if (isFilled) {
      $el.addClass('filled');
    } else {
      $el.removeClass('filled');        
    }
  }
  document.addEventListener("DOMContentLoaded", function(event) {
    window.setTimeout(function() {
      $('.gcc-input > input, .gcc-pw > input, .gcc-dropdown > select').on('blur focus change input paste', function() {
        updateInputClass($(this));
      });
      $('.gcc-input > input, .gcc-pw > input, .gcc-dropdown > select').each(function() {
        updateInputClass($(this));
      });
    }, 200)
  })
</script>

<script>
  var logout = function() {
    const localStorageReduxKey = 'redux';
    if (window.localStorage.getItem(localStorageReduxKey) !== null) {
      window.localStorage.removeItem(localStorageReduxKey);
    }

    window.location.href = '/manage/logout';
  }
</script>

