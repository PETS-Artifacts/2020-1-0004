<script type="text/javascript">
  document.addEventListener('DOMContentLoaded', function(event) {
    
    if(window.location.href.indexOf("mandi") > -1) {
      document.body.className += 'mandi-pip ';
    }
      

    
    if(
      window.digitalData &&
      window.digitalData.page &&
      window.digitalData.page.pageInfo && 
      window.digitalData.page.pageInfo.pageType === 'product detail'
    ){
      var standardGuide = ['cellular shades', 'mini blinds', 'roman shades'];
      var shadesGuide = ['roller shades', 'solar shades'];
      var sheerGuide = ['dual sheer shades', 'sheer shades | horizontal sheer shades'];
      var verticalGuide = ['vertical blinds'];
      var prodCat = window.digitalData.product[0].productInfo.productCategory
    
      if ($.inArray( prodCat, standardGuide ) !== -1){
        document.body.className += 'standard-guide';
      }
      if ($.inArray( prodCat, shadesGuide ) !== -1){
        document.body.className += 'shades-guide';
      }
      if ($.inArray( prodCat, sheerGuide ) !== -1){
        document.body.className += 'sheer-guide';
      }
      if ($.inArray( prodCat, verticalGuide ) !== -1){
        document.body.className += 'vertical-guide';
      }
    }
  })

</script>


