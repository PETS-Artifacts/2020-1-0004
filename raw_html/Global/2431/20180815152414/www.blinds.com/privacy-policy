<style type="text/css">
  #hw-ak-callout .gcc-context {
    top: 3rem;
    right: -1.5rem;
    left: auto;
    width: 85vw;
  }
  @media screen and (min-width: 30em) {
    #hw-ak-callout .gcc-context {
      width: 370px;
      left: -1rem;
    }
    #hw-ak-callout .gcc-context:before {
      left: 1rem;
    }
    #hw-ak-callout .gcc-context:after {
      right: auto;
    }
  }
</style>

<script type="text/javascript">
  var hasShutters = false;
  var allShutters = false;
  var isHawaiiOrAlaska = function() {
    return $('#shipping-state').val() === 'AK' || $('#shipping-state').val() === 'HI';
  };
  var validateShutters = function() {
    if (hasShutters && isHawaiiOrAlaska()) {
      var shippingHTML = ' \
        <div id="hw-ak-callout" class="absolute top-0 right-2 z-max bottom-0 flex items-center gcc-hide-child"> \
          <i class="material-icons pointer yellow">info</i> \
          <div \
            class="absolute gcc-context ba b--near-white br2 z-2 gcc-child z-max pa3 left--1" \
            style=" \
              visibility: visible; \
              opacity: 1; \
            " \
          > \
            <p class="b f6">Shutters cannot be shipped to Hawaii or Alaska.</p> \
            <p class="fw5 f6"> \
              Any order that contains shutters with a HI or AK shipping address will be canceled. \
              <span class="b"> \
                To complete your order, please <a href="/Ordering/Cart" class="near-black underline">click here</a> \
                to return to your cart, \
                then select the Remove Item option for all shutters. \
              </span> \
            </p> \
            <p class="fw5 f6"> \
              We apologize for the inconvenience. If you have questions or would like \
              advice on shutter alternatives, please call <a href="tel:8005051905" class="near-black underline">800-505-1905</a> \
              to speak with one of our design consultants. \
            </p> \
          </div> \
        </div> \
      ';
      var $parentContainer = $('[data-bind*="provinces.states"]').parents('.gcc-dropdown').parent();
      $parentContainer.addClass('relative');
      $parentContainer.prepend(shippingHTML);
      if (allShutters) {
        $('#checkout-main button[type="submit"]').prop('disabled', true);
        $('.payment-option.paypal .bdc-checkout-payment-method-paypal-caller').hide();
      } else {
        $('#checkout-main button[type="submit"]').prop('disabled', false);
      }
    } else {
      $('#checkout-main button[type="submit"]').prop('disabled', false);
      $('.payment-option.paypal .bdc-checkout-payment-method-paypal-caller').show();
      $('#hw-ak-callout').remove();
    }
  };
  var validateOversized = function() {
    var maxWidth = 0;
    var maxHeight = 0;
    var cartLineItems = _.get(window, 'analyticsObj.Cart.LineItems');
    if (cartLineItems) {
      $.each(cartLineItems, function(key, lineItem) {
        $.each(lineItem.LineItemChoices, function(key, choice) {
          if (choice.OptionShortLabel == 'Width') {
            var width = parseInt(choice.ChoiceDisplayValue.split(' ')[0]);
            maxWidth = (width > maxWidth) ? width : maxWidth;
          }
          if (choice.OptionShortLabel == 'Height') {
            var height = parseInt(choice.ChoiceDisplayValue.split(' ')[0]);
            maxHeight = (height > maxHeight) ? height : maxHeight;
          }
        });
      });
      if (isHawaiiOrAlaska() && (maxWidth >= 96 || maxHeight >= 96)) {
        if ($('#hw-ak-oversized').length === 0) {
          $('.bdc-checkout-payment-method > h3').append(' \
          <div id="hw-ak-oversized" class="gcc-alert gcc-alert-warning mt2"> \
            <ul class="list f7 fw5 ma0 pa0"> \
              <li> \
                <p class="b"> \
                  For oversized products (96&quot; or over in width or height) with a HI or AK shipping address, \
                  we&rsquo;ll contact you by phone and email to collect a shipping fee. \
                </p> \
                <ul class="mb0"> \
                  <li>$30 fee per oversized product.</li> \
                  <li>If you do not answer or respond within 24 hours to confirm fee payment, your order will be canceled.</li> \
                </ul> \
              </li> \
            </ul> \
          </div> \
          ');
        } else {
          $('#hw-ak-oversized').show();
        }
      } else {
        $('#hw-ak-oversized').remove();
      }
    }
  };
  document.addEventListener('DOMContentLoaded', function(e) {
    var currentPage;
    try {
      currentPage = _.get(window, 'digitalData.page.pageInfo.pageName');
    } catch (e) {}
    if (currentPage === 'checkout page') {
      $(document).on('click', function(event) {
        var $hideChild = $(event.target).closest('.gcc-hide-child');
        if ($hideChild.length === 1) {
          if ($(event.target).is('i')) {
            var $child = $hideChild.find('.gcc-child');
            if ($child.css('visibility') === 'visible') {
              $child.css('visibility', 'hidden');
              $child.css('opacity', '0');
            } else {
              $child.css('visibility', 'visible');
              $child.css('opacity', '1');
            }
          }
        } else {
          $('.gcc-child').css('visibility', 'hidden');
          $('.gcc-child').css('opacity', '0');
        }
      });
      var shuttersQty = 0;
      var lineItems = _.get(window, 'digitalData.cart.item');
      if (lineItems) {
        $.each(lineItems, function(key, obj) {
          if (obj.productInfo.productName.toLowerCase().indexOf('shutter') >= 0) {
            hasShutters = true;
            shuttersQty++;
          }
        });
        if (shuttersQty === lineItems.length) {
          allShutters = true;
        }
      }
      if ($('#shipping-state').length === 1) {
        $('#shipping-state').on('change', function() {
          validateShutters();
          if (!allShutters) {
            validateOversized();
          }
        });
      }
      window.setTimeout(function() {
        validateShutters();
        if (!allShutters) {
          validateOversized();
        }
      }, 2000);
    }
  });
</script>


