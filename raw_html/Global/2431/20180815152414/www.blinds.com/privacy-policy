<script type="text/javascript">
  document.addEventListener('DOMContentLoaded', function(event) {
    const categories = []

    if ( window.pageContextJavascript.ajaxUrl === "/Catalog/SiteProduct/EstimateSearchFilterMandi/" ){
      document.body.className += 'mandi-plp';
      
      $('li[data-id="Category"] :checkbox:not(:checked)').each(function(){
        categories.push($(this).attr('id'));
      })
      
      $(document).ajaxComplete(function() {
        hideCategories()
      })
    }

    function hideCategories(){
      var i;
      var current; 
      for (i = 0; i < categories.length; i++) { 
        current = 'input[id="' + categories[i] + '"]';          
        $(current).parent('label').parent('li').hide()
      }
    }
  })

</script>


