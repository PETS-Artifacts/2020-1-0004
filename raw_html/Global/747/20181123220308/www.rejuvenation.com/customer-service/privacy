<script>
  // NOTE: define searchTerm globally to be used for the searchRouter
  window.searchTerm     = _.escape("")
  var elasticHosts      = ["https://search.rejuvenation.com"]
  var categoryIndexName = "production-categories"
  var productIndexName  = "production-products-categories"
  var productAutoCompleteModel = new Rejuvenation.Models.ProductAutocompleteQuery({
    text: '',
    elasticHosts: elasticHosts,
    indexName: productIndexName,
    previewModeTime: 1543010588
  })
  var categoryAutoCompleteModel = new Rejuvenation.Models.CategoryAutocompleteQuery({
    text: '',
    elasticHosts: elasticHosts,
    indexName: categoryIndexName
  })
  new Rejuvenation.Views.SearchBox({el: $("#header-search.js-replace-search"), productAutoComplete: productAutoCompleteModel, categoryAutoComplete: categoryAutoCompleteModel, searchTerm: window.searchTerm})
  new Rejuvenation.Views.CategoryAutoCompleteDropdown({ el: $("#header-search .js-replace-autocomplete .categories"), model: categoryAutoCompleteModel})
  new Rejuvenation.Views.ProductAutoCompleteDropdown({ el: $("#header-search .js-replace-autocomplete .products"), model: productAutoCompleteModel})
  
  // Report Search redirects to GTM.
  var q = "";
  if (!!q) {
    // console.log('automatic redirect from search with query: ' + q);
    Rejuvenation.GTM.datalayer_search('Search Redirect', q, 1);
  }
</script>
