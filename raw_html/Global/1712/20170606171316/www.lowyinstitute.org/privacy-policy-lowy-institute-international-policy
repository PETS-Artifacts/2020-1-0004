    <script>
      function setCookie(cname, cvalue, exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
        var expires = "expires="+d.toUTCString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
      }

      function getCookie(cname) {
        var name = cname + "=";
        var ca = document.cookie.split(';');
        for(var i = 0; i < ca.length; i++) {
          var c = ca[i];
          while (c.charAt(0) == ' ') {
            c = c.substring(1);
          }
          if (c.indexOf(name) == 0) {
            return c.substring(name.length, c.length);
          }
        }
        return "";
      }


      jQuery(window).load(function() {
        var SurveyPopup = getCookie('SurveyPopup');
        if(SurveyPopup==''){
          jQuery('#l-survey-modal').addClass('show-modal');
          setCookie('SurveyPopup','displayed',2);
        }
      });
      jQuery(document).ready(function($) {
        $('.btn__to-close').on('click',function(e){
            e.preventDefault();
            $('#l-survey-modal').removeClass('show-modal');
        });
        /*$('.close-survey-modal, .l-survey-modal-overlay').on('click', function(e) {
          e.preventDefault();
          $('#l-survey-modal').removeClass('show-modal');
        });
        $(document).keyup(function (e) {
          if (e.keyCode === 27) {
            $('#l-survey-modal').removeClass('show-modal');
          }
        });*/
      });
    </script>
