                        }else if ( $('body').hasClass('device-tablet') ){
                            dataStore.site.device = 'tablet';
                        }else if ( $('body').hasClass('device-mobile') ){
                            dataStore.site.device = 'mobile';
                        }else{
                            dataStore.site.device = 'desktop';
                        }

                        // site device orientation
                        if ( $('body').hasClass('device-orientation-landscape') ){
                            dataStore.site.orientation = 'landscape';
                        }else if ( $('body').hasClass('device-orientation-portrait') ){
                            dataStore.site.orientation = 'portrait';
                        }else{
                            dataStore.site.orientation = 'landscape';
                        }

                    });
                </script>

                <script src="https://api.ridestyler.net/js?DataType=jsonp&key=053a709eab01439f84735e5aaa6a21ea"></script>
                    <script type="text/javascript" src="https://static.ridestyler.net/clients/atd/visualizer/scripts.js"></script>

                    <script type="text/javascript">
                        // Create a window object, if it doesn't already exist,
                        // to be accessed in productlist.jsp
                        window.visualizerData = window.visualizerData || {
                            vehicleDescription: "",
                            vehicleOESize: "",
                            products: []
                        };
                    </script>
