<script type="text/javascript">
	function changeStore(newStoreId,newCatalogId,newLangId,newContryCode){
		var formObj = document.getElementById("switchStore");
		formObj.switchToStoreId.value = newStoreId;
		formObj.switchToCatalogId.value = newCatalogId;
		formObj.switchToLangId.value = newLangId;
		formObj.countryCode.value = newContryCode;
		formObj.submit();
	}
</script>
