<script type="text/javascript">
 //Set the default NLS to use in the store.
 if(storeNLS == null) {
	 dojo.requireLocalization("storetext", "StoreText");
	 storeNLS = dojo.i18n.getLocalization("storetext","StoreText");
 }
 
	// set user's default store
	function rememberUserStore(){
		var selectedOptionVal = dijit.byId('selectStore').attr('value');
		var isRememberSelected = document.getElementById("remember").checked; 
		
		var cookieKey = "UserSelectedStore";
		if(isRememberSelected){
			var cookieValue = selectedOptionVal;					
			dojo.cookie(cookieKey, cookieValue, {path:'/', expires:30});
		}
		window.location.href = selectedOptionVal;
	}
	//open user's default store if previously set
	dojo.addOnLoad(function() {
		var cookieKeyTemp = "UserSelectedStoreTemp";
		var cookieValueTemp = dojo.cookie(cookieKeyTemp);
		if(typeof cookieValueTemp == 'undefined'){
			dojo.cookie(cookieKeyTemp, "1", {path:'/'});
			
			var cookieKey = "UserSelectedStore";
			var cookieValue = dojo.cookie(cookieKey);
			if((typeof cookieValue != 'undefined') && (window.location != cookieValue)){
				window.location.href = cookieValue;
			}
		}
	});
</script>


