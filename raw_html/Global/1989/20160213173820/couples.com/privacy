    $(document).ready(function() {
        
        $('#countries').change( function() {
        
            getAirports();
        });
        
        
        function getAirports() {
            $.ajax({
                url: 'public_docs/couplesresorts/html_countries/' + $('#countries').val() + '.html', //This line will call different html files based on selected country.
                data: {},
                success: function (data) {
                    //$("#airport").html('<option value="">Departure City</option>'+data);
                    $('#airport').html(data).selectric();
                },
                dataType: 'html'
            });
        }
    })
</script>


