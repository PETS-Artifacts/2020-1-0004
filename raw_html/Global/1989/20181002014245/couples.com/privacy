    $(document).ready(function(){

        $("#submit-button").click(function(){
			
			var redirect = "";
            var valid = true;
            var modal_container = $("div[data-em=\"modal-container\"]");
            redirect = $("#submit-button").attr("data-redirect");
            $("span.error").remove();

            $( ':input[type=text]', modal_container).each(function(){
                if (this.value === '') {
                    valid = false;
                }
            });
            $( ':input[type=email]', modal_container).each(function(){
                if (this.value === '') {
                    valid = false;
                }
            });
            $( ':input[type=checkbox]', modal_container).each(function(){
                if ($(this).is(':not(:checked)')) {
                    valid = false;
                }
            });

            if (valid) {

                var data = $("input, select", modal_container).serializeArray();
                data.push({name:'popupid', value:324});
                data.push({name:'operation', value:'processExitIntent'});
                //data.push({name:'subscribe', value:'y'});

                $.post('https://couples.com/ajax/functions.php', $.param(data), function (resp) {
                    if (resp.error) {
                        //alert('Error');
                        console.log(resp.msg);
                    } else {
	                    if(redirect != '' && redirect != undefined) {
		                    window.location.href = redirect;
	                    } else {
	                        bioEp.showThankYou();		                    
	                    }
                        bioEp.hidePopup();
                    }
                }, 'json');

                                    $('.exitintent-sucess').on('click', function() {
                        ga('send', 'event', '', '', '');
                    });
                
            } else {
                $("<span class='error'>All fields are required</span>").insertBefore("#submit-button")
            }

        });

        $("#close-link, .close-popup").click(function(){
            bioEp.hidePopup();
        });

                    $(document).on('click', "#bio_ep_bg", function(){
                bioEp.hidePopup();
            });
        
    });

</script>

