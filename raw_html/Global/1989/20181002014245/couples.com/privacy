<script>

    $(document).ready(function () {
        
        $("#email-signup").validate({

            // Specify the validation rules
            rules: {"first_name_input":"required","email_address":"required","city_input":"required","country_input":"required","subscribe":"required"},
            // Specify the validation error messages
            messages: {"first_name_input":"This is a required field","email_address":"This is a required field","city_input":"This is a required field","country_input":"This is a required field","subscribe":"This is a required field"},

            submitHandler: function (form) {

                $.post('https://couples.com/ajax/functions.php', $(form).serialize(), function (data) {
                    if (data.error) {
                        $("#email-signup #errMsg").html(data.msg);
                        $("#email-signup #errMsg").fadeIn();

                        
                    } else {
                                                $("#email-signup").slideUp('normal', function () {
                            // $("#thankyou").fadeIn();
                            // Replaced by following line to avoid #id selector issues when multiple instances of the form
                            $(this).closest(':has(.thankyouMsg)').children('.thankyouMsg').fadeIn();
                            if (IS_LIVE) {
