<script src='https://www.google.com/recaptcha/api.js'></script>
<script>
    (function($){
        var players = $('.jwplayer_init');
        $.each(players, function(i,player){

            var fileType = $(player).attr('data-filetype');
            var playerId = $(player).attr('id');
            var placeholderImage = $('.m_article-video').parents('.content-wrapper').data('placeholder'); // ??

            var opts = {
                file: $(player).attr('data-fileurl'),
                width: '100%',
                skin: {name: 'seven'},
            }
            if (fileType === 'audio' && placeholderImage == undefined) {
                opts.height = '80px';
            } else {
                opts.image = placeholderImage;
                opts.aspectratio = '16:9';
            }

            var playerInstance = jwplayer(playerId);
            playerInstance.setup(opts)
            playerInstance.onPlay(function(){
                var htmlTitle = $(document).attr('title');
                ga('send', 'event', 'Multimedia Play', fileType, htmlTitle);
            })
        })
    })(jQuery);
</script>
</body>
