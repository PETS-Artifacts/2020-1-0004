        <script type="text/javascript">
        
         $(document).ready( function() {
 
 	// sec level nav dropdown click
	$('.secnav-arrow').click( function() {
		var parentID = $(this).parent().parent().attr('id').replace('sec-','');
		// close other open dropdowns
		for (var d in dropdowns) {
			if (parentID != d && dropdowns[d]) {
				toggleDropdown(d);
			}
		}
		toggleDropdown(parentID);
		return false;
	});	
});
        </script>
		
