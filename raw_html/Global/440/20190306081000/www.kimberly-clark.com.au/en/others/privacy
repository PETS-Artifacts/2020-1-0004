		<script type="text/javascript">
		
			 function SearchSite(inField, e) {
                var charCode;

                if (e && e.which) {
                    charCode = e.which;
                } else if (window.event) {
                    e = window.event;
                    charCode = e.keyCode;
                }

                if (charCode == 13) {
                    Search(inField);
                    return false;
                }
            }

            function Search(strval) {
				window.location = "/en/others/googlesearch?q=" + strval.value;
            }
            function Searchbox() {
				window.location = "/en/others/googlesearch?q=" + document.getElementById('txt_search').value;
            }
		</script>
		
		
		

