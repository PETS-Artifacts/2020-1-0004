    <script type="text/javascript">
        $(document).ready(function () {
            $('.increaseFont').click(function () {
                curSize = parseInt($('.news-wrapper p,.news-wrapper a').css('font-size')) + 2;
                if (curSize <= 24)
                    $('.news-wrapper p,.news-wrapper a').css('font-size', curSize);
            });
            $('.decreaseFont').click(function () {
                curSize = parseInt($('.news-wrapper p, .news-wrapper a').css('font-size')) - 2;
                if (curSize >= 8)
                    $('.news-wrapper p,.news-wrapper a').css('font-size', curSize);
            });
        });
    </script>    
