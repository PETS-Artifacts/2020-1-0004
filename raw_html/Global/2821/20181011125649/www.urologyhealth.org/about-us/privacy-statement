    <script type="text/javascript">
        window.addEventListener("load", function () {
            var tmpName = '';
           var tmpData = $.getJSON('https://api.wipmania.com/jsonp?callback=?', function (data) {
//                alert('\nCountry: ' + data.address.country);

                tmpName = data.address.country;

            });
           tmpData.success(function (response) {
               // The request is done, and we can do something else
            var countryList = new Array("Austria", "Belgium", "Bulgaria", "Croatia", "Republic of Cyprus", "Czech", "Republic", "Denmark", "Estonia", "Finland", "France", "Germany", "Greece", "Hungary", "Ireland", "Italy", "Latvia", "Lithuania", "Luxembourg", "Malta", "Netherlands", "Poland", "Portugal", "Romania", "Slovakia", "Slovenia", "Spain", "Sweden", "United Kingdom");
//            var countryList = new Array('Austria', 'Belgium', 'Bulgaria', 'Croatia', 'Republic of Cyprus', 'Czech', 'Republic', 'Denmark', 'Estonia', 'Finland', 'France', 'Germany', 'Greece', 'Hungary', 'Ireland', 'Italy', 'Latvia', 'Lithuania', 'Luxembourg', 'Malta', 'Netherlands', 'Poland', 'Portugal', 'Romania', 'Slovakia', 'Slovenia', 'Spain', 'Sweden', 'United Kingdom');
            var nameCnt = 'Croatia';
            var tmpNm1 = tmpName;
            var a = countryList.indexOf(tmpNm1);
            if (a < 0) {
          //      alert('a: ' + a);

            } else {
			 document.getElementById('lp-eu-privacy').style.display = "";
            var tmpCook = getCookie('ufcAcctCookie');
                if (tmpCook != 'Accept') {
                    document.getElementById('cookie-consent2').style.display = "";
                    $('#cookie-consent2').slideDown('slow');
                } else {
                    document.getElementById('cookie-consent2').style.display = "none";
                    $('#cookie-consent2').slideUp('slow');
                }
            }
                $('#consent').on('click', function () {
                    setCookie('ufcAcctCookie', 'Accept', 365);
                    //                document.cookie = "cookies=yes; max-age=" + (1 * 365 * 24 * 60 * 60);
                    document.getElementById('cookie-consent2').style.display = "none";
                    $('#cookie-consent2').slideUp('slow');
                });
                $('#learn-more').on('click', function () {
                    location.href = 'https://www.auanet.org/about-us/aua-governance/privacy-policy';
                })
            
           });
        });

    </script>
	
