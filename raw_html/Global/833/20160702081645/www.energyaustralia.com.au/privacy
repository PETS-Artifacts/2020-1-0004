    <script>
        var url = window.location.href;
        var urlArray = url.split("/");
        var breadCrumbsString;

        function removeA(arr) {
            var what, a = arguments, L = a.length, ax;
            while (L > 1 && arr.length) {
                what = a[--L];
                while ((ax= arr.indexOf(what)) !== -1) {
                    arr.splice(ax, 1);
                }
            }
            return arr;
        }

        String.prototype.capitalize = function() {
            return this.charAt(0).toUpperCase() + this.slice(1);
        }

        removeA(urlArray,'');
        removeA(urlArray,'http:');
        var httpString = "http://www.energyaustralia.com.au";

        if(urlArray.length > 1){
            for(var i=1; i< urlArray.length; i++){
                if (typeof urlArray[i] === 'string' || urlArray[i] instanceof String){
                    breadCrumbsString = urlArray[i].replace(/-/g, ' ').capitalize();
	                if(breadCrumbsString.indexOf("?")!=-1){
		                breadCrumbsString=breadCrumbsString.substring(0,breadCrumbsString.indexOf("?"));
	                }
                }
                httpString = httpString +"/" + urlArray[i];
                $('#breadcrumbList').append('<li property="itemListElement" typeof="ListItem"><a property="item" typeof="WebPage" ><span property="name"></span></a><meta property="position" ></li>');
                $('#breadcrumbList a:last').prop("href", httpString);
                $('#breadcrumbList span:last').html(breadCrumbsString);
                $('#breadcrumbList meta:last').prop('content',i+1);
            }
        }
    </script>

