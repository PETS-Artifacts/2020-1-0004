var filetypes = /\.(pdf|doc.*|xls.*|ppt.*|dot.*|xlt.*|pot.*|txt|vsd.*)$/i;

ga('require', 'outboundLinkTracker', {
  events: ['click', 'auxclick'],
  fieldsObj: {
      eventCategory: "Document Tracking",
      eventAction: "download"
  },
  shouldTrackOutboundLink: function(link, parseUrl) {
    const href = link.getAttribute('href') || link.getAttribute('xlink:href');
    const url = parseUrl(href);
    return (!link.getAttribute('onclick')) && url.hostname == location.hostname && (url.pathname.slice(0, 7) == '/__data' || url.pathname.match(filetypes));
  }
});
</script>
