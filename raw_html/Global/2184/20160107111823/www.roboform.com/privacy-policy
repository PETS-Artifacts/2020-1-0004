<script type="text/javascript">
function qClicked(evt) {
  document.getElementById("q").style.backgroundPosition="0 0";
}

var searchqBTN = document.getElementById("q");
if(searchqBTN){
  if(searchqBTN.addEventListener) {
    searchqBTN.addEventListener("click", qClicked, false);
  } else {
    searchqBTN.attachEvent("onclick", qClicked);
  }
}

var searchqBTNtxt = document.getElementById('q').value;
if(searchqBTNtxt){
  document.getElementById("q").style.backgroundPosition="0 0";
}











</script>
