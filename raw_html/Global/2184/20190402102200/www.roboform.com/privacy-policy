<script>
var cookie = $.cookie("rf");
if(cookie==null) {$(".cookies-notification-container").show();}
$(".got-it").click(function () {
  $(".cookies-notification-container").hide();
  var date = new Date;
  date.setDate(date.getDate() + 365);
  document.cookie = "rf=rf-cookie; path=/; expires=" + date.toUTCString();
});
