<script>
  $('form.market_appraisal input.postcode').on('focusout', function(){
    var form = $(this).parents('form');
    var postcode = $(form).find('input.postcode').val();
    $.get('/search/nearest',{
        item: 'Presence',
        owner_type: 'Domain',
        postcode: postcode
      }, function(data, status){
        var select = $(form).find('.office_input select');
        $.each(data, function(){
          $(select).append('<option value="' + this.presence.owner_id + '" data-foreign-id="' + this.presence.office_foreign_id + '">' + this.presence.name + '</option>');
        });
        $(form).find('.office_input').slideDown();
      }
    );
  });
</script>


