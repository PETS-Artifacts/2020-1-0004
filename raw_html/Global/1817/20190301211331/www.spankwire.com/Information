        /**
         * Load images within viewport immediately.
         *
         */
        var images = document.getElementsByTagName('IMG');
        var imgUrl,
            imgSrc,
            len = images.length,
            cnt = 0;

        for (var i = 0; i < len; i++) {
            if (cnt >= page_params.imagesToLoadImmediately) break;
            imgUrl = images[i].getAttribute('data-original') || null;
            imgSrc = images[i].getAttribute('src') || null;
            if (imgUrl && (imgSrc != imgUrl)) {
                images[i].src = imgUrl;
                cnt++;
            }
            ;
        }
    });

    function lazyLoadScript(async, url, where) {
      where = where || 'body';
      return new Promise(function(resolve, reject) {
        var script = document.createElement('script');

        script.async = async;
        script.src = url;
        script.onload = resolve;
        script.onerror = reject;

        switch (where) {
          case 'head':
            document.head.appendChild(script);
            break;
          default:
            document.body.appendChild(script);
            break;
        }
      });
    }

    window.addEventListener('load', function () {
        if (window.abb) {
            //EXBL start
            window.lazyLoadScript(false, 'https://cdn1-static-spankwire.spankcdn.net/dist/js/exblss1.js?cache=190931551433616', 'body').then(function () {
                window.exbl = new Exbl();
                window.exblItems.forEach(function (item) {
                    window.exbl.addItem(item);
                });
                return window.exbl.render();
            });
            //EXBL end
        } else {
            window.lazyLoadScript(false, 'https://cdn1-static-spankwire.spankcdn.net/dist/js/utils.js?cache=190931551433616', 'head').then(function () {
                return window.utils.renderSpots(spots);
            });
        }
    });
</script>
<!-- /HEAD.JS + LOAD SCRIPTS -->

<!-- HubTraffic -->
<script type="text/javascript">
    var rta = document.createElement('script');
    rta.type = 'text/javascript';
    rta.id = 'htScript';
    rta.async = true;
    rta.src = document.location.protocol + '//ht.spankwire.com/js/ht.js?site_id=7';
    rta.onload = function()
    { htUrl = 'ht.spankwire.com'; }
    var s = document.getElementsByTagName('script')[0];
    s.parentNode.insertBefore(rta, s);
</script>
<!-- HubTraffic ends -->

<script type="text/javascript">
    var phtProps = {
        isAction: false,
        autoPlayOnNewPage: false,
        remoteRouting: true,
        secure: true,
        include: ['a', '.js-video-pht'],
        pdfPath: 'https://cdn1-static-spankwire.spankcdn.net/dist/static/pht/pht.pdf?cache=190931551433616',
        exclude: ['#PornhubNetworkBar', '.js-close-cookie', '#main-nav', '.noPopunder'],
        noPopClass: 'disPopunder',
        fullscreen: false,
        debug: false,
        remoteRoutingData: window.page_params.hasOwnProperty('routingDataForSoska') ? window.page_params.routingDataForSoska : {"device_type":"desktop","hostname":"www.spankwire.com","traffic_type":"organic","country_code":"US","referral":"www.spankwire.com","utm_source":"","gender":"straight"}    };
    var regularPopInit = function (name) {
        name = name || 'pht-full.js';
        (function (d, t) {
            var g = d.createElement('script');
            s = d.getElementsByTagName(t)[0];
            g.src = 'https://cdn1-static-spankwire.spankcdn.net/dist/static/pht/' + name + '?cache=190931551433616';
            g.async = false;
            s.appendChild(g);
        }(document, 'body'));

    };
    if (window.abb || phtProps.remoteRoutingData.device_type == 'tablet' || phtProps.remoteRoutingData.device_type == 'mobile') {
        regularPopInit('pht-mobile.js');
    } else {
        regularPopInit();
    }
</script>
