    function shouldRedirectToBrowserNotSupportedPage()
    {
        return (window.location.href.indexOf("browser-not-supported") === -1) &&
                (window.location.href.indexOf("obb-logout") === -1) &&
                (window.location.href.indexOf("bob-logout") === -1);
    }

    /*@cc_on
        if (/^10/.test(@_jscript_version)) {
            if (shouldRedirectToBrowserNotSupportedPage()) { window.location.replace("/errors/browser-not-supported"); }
        }
    @*/
