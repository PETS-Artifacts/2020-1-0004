	(function($){
		$(document).ready(function(){
			window.__dealtxt = setInterval(function(){
				if($('._60532-fl').hasClass('y')){
					$('._60532-fl').attr('style','color:#F00F00; font-family:avalondemi; font-style:normal;')
				} else {
					$('._60532-fl').attr('style','color:#00C1D5; font-family:avalondemi; transform: scale(1.4,1.5); text-shadow: 0.5px 0px 0px;')
