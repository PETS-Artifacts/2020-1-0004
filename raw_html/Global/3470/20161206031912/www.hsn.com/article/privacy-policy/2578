<input class="hDate" type="hidden" value="">

<script type="text/javascript">
jQuery(document).ready(function($) {
	var noAjax = jQuery('input#header-noajax').length > 0 ? jQuery('input#header-noajax').val() : false;
	
	if (!noAjax)
	{
		if (HSN.OnAccountStatusLoad)
		{
			// 082416 - After Release this will be default
			HSN.OnAccountStatusLoad(function(data) {
				HSN.TodaysSpecialCountdown.Register("#todayspecial.zHeaderPromo .HP_TS_Timer", new Date(data.serverTime));
				
				// Put data in .hDate for others to use
				$('.hDate').val(data.serverTime);
				$("#todayspecial.zHeaderPromo .zText").show();
			});
		}
		else
		{
			HSN.Instance().Events.On(HSN.AccountNavigation.EVENT.ACCOUNT_STATUS_COMPLETE, function(data) {
				HSN.TodaysSpecialCountdown.Register("#todayspecial.zHeaderPromo .HP_TS_Timer", new Date(data.serverTime));
				
				// Put data in .hDate for others to use
				$('.hDate').val(data.serverTime);
				$("#todayspecial.zHeaderPromo .zText").show();
			});
		}
	}
	else
	{
		$("#todayspecial.zHeaderPromo .zText").html("DON'T MISS OUT!")
		$("#todayspecial.zHeaderPromo .zText").show()
	}
	
	// 01/07/16 - TS Add presale to flyout
	if (window.HSN && HSN.Instance && HSN.Instance() && HSN.Instance().FlyoutManager && (window.location.hostname.indexOf("community") < 0))
	{
		$.ajax(
			"/headerfooter/todaysspecial",
			{
				cache: false,
				data: {},
				dataType: 'html',
				success: function (data, status, xhr) {
					var myFlyout = jQuery('#todayspecial .flyout')
					
					myFlyout.html(data)
					
					// Make TS Horizontal
					myFlyout.addClass("zTsFlyout")
					var Tmp = myFlyout.find(".body .ts > div")
					Tmp.eq(0).css("clear", "none").css({cssClear: "none", width: "36%", padding: "3%"})
					Tmp.eq(1).css("clear", "none").css({cssClear: "none", width: "55%", padding: "3% 3% 3% 0%"})
					
					myFlyout.find(".body .ts").append($("<div style='clear: both; float: none;'>"))
					
					// Add TS Presales
					var zPresales = $("#zFlyoutPresaleContainer")
					myFlyout.append(zPresales)
					zPresales.show()
					
					// Add TS Header
					$("<span>TODAY'S SPECIAL: </span>").insertBefore($("#TSBug_PD_Amount"))
					
					// Fix front end bug in mutli TS
					myFlyout.find(".body").append($("<div style='clear: both;'></div>"))
					
					// Link alterations for the TS
					// - Change all links to the "buy now" links, wrap cells with same link so it's all clickable
					// Note: Soldout removes the button, and the href from the link
					// Note: MultiTS changes class="buttonarea" to id="buttonarea0" and other stuff
					$(".zTsFlyout .ts").each(function(){
						var zCur = $(this)
						var zCurButton = zCur.find(".product .button")
						var zLink = zCurButton.attr("href")
						var zCM = zCurButton.attr("manual_cm_re")
						
						if (zLink)
						{
							// Change existing links so they all match
							zCur.find("a")
								.attr("href", zLink)
								//.attr("manual_cm_re", zCM)
								//.attr("manual_cm_sp", zCM)
							
							// Wrap cell with link
							var zNewLink = $("<a class='zLinkContainer' />")
								.attr("href", zLink)
								//.attr("manual_cm_re", zCM)
								//.attr("manual_cm_sp", zCM)
							
							zCur.wrap(zNewLink)
						}
					})
				},
			}
		);
		
		// Find existing TS flyout, and annihilate it
		$(HSN.Instance().FlyoutManager.Flyouts).each(function(){
			if (this.Url == "/headerfooter/todaysspecial")
				this.RemoveHandlers()
		})
		
		// Replace the flyout with a new, non ajax version
		var NewFlyout = new HSN.Flyout({
			closeClass: 'ir',
			trigger: '#todayspecial',
			//triggerEvent: HSN.Flyout.TRIGGER_EVENT.HOVER,
			disableHover: Modernizr.touch,
			triggerEvent: Modernizr.touch ? HSN.Flyout.TRIGGER_EVENT.TOUCH_WITH_CLOSE: HSN.Flyout.TRIGGER_EVENT.HOVER,
			flyout: '#todayspecial .flyout'
		});
		
		HSN.Instance().FlyoutManager.Add(NewFlyout)
	}
	// END: 01/07/16 - TS Add presale to flyout
});
</script>

</div>
</div>
            </div>
        </div>
    </div>

</nav>


