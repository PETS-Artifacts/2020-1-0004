	/*video modal spacing*/
	body:not(.appTablet):not(.mobile) .modal [id^="hsn-video-player"] {
		width: 770px;
		height: 454px;
	}
	#torso { padding-bottom: 37px; }
	.TextContainer { position: relative; }
	.watch-nav-container .hsn2-logo { background-image: url("https://img.hsni.com/images/formatting/pg_hsn2_top_v2.png?v=7"); }
	/*replicant lb*/
	.cms-cell img[class$="-lb"],
