<script>
	(function($){
		//don't run on the ts email or signin pages
		if(!!(/(\/content\/.*\/333|\/signin)/i).test(window.location.pathname)
		  || !!$('body').is('.community,.blogs')){
			return false;
		}
		//show the carousel container
		$('._dealsToast').addClass('_go');
		//create the carousel
		$(document).ready(function(){
			var _dealsToastCC = $('._dealsToast ._zc ._items').slick({
				prevArrow: '<div class="_prev" />',
				nextArrow: '<div class="_next" />',
				appendArrows: $('._dealsToast ._controls'),
				lazyLoad:'ondemand',
				swipeToSlide: Modernizr.touchevents,
				slidesToShow: 4,
				slidesToScroll: 1,
				infinite: false
			});
		});
		//click event on icon, opens drawer with stuff in it
		$('._dealsToast ._icon a').on('click', function(ev) {
			ev.preventDefault();
			if ($('._dealsToast').hasClass('show')) {
				$('._dealsToast').removeClass('show');
				$('._dealsOverlay').removeClass('show');
				$('header, #torso, footer').removeClass('blur');
			} else {
				$('._dealsToast').addClass('show');
				$('._dealsOverlay').addClass('show');
				$('header, #torso, footer').addClass('blur');
			}
			return false;
		}); 
		//close the toast
		$('._dealsOverlay').on('click', function(ev) {
			ev.preventDefault();
			$(this).removeClass('show');
			$('._dealsToast').removeClass('show');
			$('header, #torso, footer').removeClass('blur');
		});
		
	})(jQuery);
</script>
