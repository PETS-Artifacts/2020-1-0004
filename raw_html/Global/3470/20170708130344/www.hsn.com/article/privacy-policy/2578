    <dl class="scroll" style="height:356px;">
                    <dt>Departments</dt>
            <dd>
                <ul>
                                            <li><a href="/shop/beauty/bs" manual_cm_sp="BreadBox-_-Departments-_-Beauty" manual_cm_re="BreadBox-_-Departments-_-Beauty"><span class="name float-left" title="Beauty">Beauty</span><span class="count float-left"></span></a></li>
                        <li><a href="/shop/coins-and-currency/co" manual_cm_sp="BreadBox-_-Departments-_-CoinsCurrency" manual_cm_re="BreadBox-_-Departments-_-CoinsCurrency"><span class="name float-left" title="Coins &amp; Currency">Coins &amp; Currency</span><span class="count float-left"></span></a></li>
