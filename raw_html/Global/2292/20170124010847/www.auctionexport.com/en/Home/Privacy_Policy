            <script>
                function fn_SubmitKeywords() {
                    var searchBox = $('#searchBox');
                    if (searchBox.val().toLowerCase() == "example: toyota corolla 2009 gray") {
                        searchBox.val('');
                    }

                    var filteredValue = searchBox.val().replace("\r\n", "");
                    if (filteredValue.length > 0) {
                        filteredValue = filteredValue.substring(0, 100);
                    }
                    searchBox.val(filteredValue);
                }
            </script>
                </td>
            </tr>
                            <tr class="c_MainMiddleTR">
                    <td>&nbsp;</td>
                    <td style="width: 1080px; vertical-align: top; padding-bottom: 7px; " id="id_Container_Content_Main">
                        
                        


