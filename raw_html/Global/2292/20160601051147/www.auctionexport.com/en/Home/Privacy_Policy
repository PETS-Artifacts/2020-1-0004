<script>
    function fn_SubmitKeywords() {
        var searchBox = $('#searchBox');
        if (searchBox.val().toLowerCase() == "example: toyota corolla 2015 black") {
            searchBox.val('');
        }

        var filteredValue = searchBox.val().replace("\r\n", "");
        if (filteredValue.length > 0) {
            filteredValue = filteredValue.substring(0, 100);
        }
        searchBox.val(filteredValue);
    }
</script>
