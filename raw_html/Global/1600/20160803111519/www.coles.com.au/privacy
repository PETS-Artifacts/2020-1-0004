			    <h1 class="heading large-7">Privacy</h1>
		    </div>
	    </div>
    </div>
</section>

<section class="row back-to-parent">
    <div class="small-12 medium-12 large-12 columns">
        <a id="content_1_lnkParent" class="button-secondary" href="/">Back to Home</a>
    </div>
