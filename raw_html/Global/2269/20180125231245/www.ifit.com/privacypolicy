  function queueAnalytics(name, type, body) {
    if (getGaTrackerName()) return false;
    if (!gaTimerId) setupAnalyticsQueue();
    gaQueuedEvents.push({ name: name, type: type, body: body });
    return true;
  }

  function setupAnalyticsQueue() {
    if (!window.ga) return;
    var count = 0;

    gaTimerId = setInterval(function() {
      if (count++ > 10) return clearTimeout(gaTimerId);
      if (!getGaTrackerName()) return;
      var event;
      while (event = gaQueuedEvents.shift()) {
        window.ga(getGaTrackerName() + '.' + event.name, event.type, event.body);
      }
      clearTimeout(gaTimerId);
    }, 100);
  }
