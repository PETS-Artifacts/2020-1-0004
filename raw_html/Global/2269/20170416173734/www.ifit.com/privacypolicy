(function () {
  var gaTrackerName = null
    , gaQueuedEvents = []
    , gaTimerId
    ;
    
  function getGaTrackerName() {
    if (gaTrackerName) return gaTrackerName;
    if (!window.ga || !window.ga.getAll) return '';
    var trackers = ga.getAll();
    for (var i = 0; i < trackers.length; i++) {
      var tracker = trackers[i];
      for (var p in tracker) {
        if (!tracker.hasOwnProperty(p)) continue;
        if (
          tracker[p]
          && tracker[p].data
          && tracker[p].data.values
          && tracker[p].data.values[':trackingId'] === 'UA-26150035-17'
        ) {
          return gaTrackerName = tracker[p].data.values[':name'];
        }
      }
    }
    return '';
  }
  
  function queueAnalytics(name, type, body) {
    if (getGaTrackerName()) return false;
    if (!gaTimerId) setupAnalyticsQueue();
    gaQueuedEvents.push({name: name, type: type, body: body});
    return true;
  }
  
  function setupAnalyticsQueue() {
    if (!window.ga) return;
    var count = 0;
    
    gaTimerId = setInterval(function () {
      if (count++ > 10) return clearTimeout(gaTimerId);
      if (!getGaTrackerName()) return;
      var event;
      while (event = gaQueuedEvents.shift()) {
        window.ga(getGaTrackerName() + '.' + event.name, event.type, event.body);
      }
      clearTimeout(gaTimerId);
    }, 100);
  }
  
  window.sendAnalytics = function (name, type, body) {
    if (!window.ga) return;
    if (queueAnalytics(name, type, body)) return;
    return window.ga(getGaTrackerName() + '.' + name, type, body);
  }
