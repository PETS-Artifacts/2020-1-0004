	<div class="right_head">
		<h3>POLL</h3>
	</div><!--facebook_head-->
	<div class="poll_content">
		<form accept-charset="UTF-8" action="/wps/pages/vote/129/social-partnership/nation_news" method="post" name="vote"><div style="margin:0;padding:0;display:inline"><input name="utf8" type="hidden" value="&#x2713;" /><input name="authenticity_token" type="hidden" value="CbML2SRo7VCKgkaAf3kX2yRvc7a0i5CxLhXZAlWSNRc=" /></div>
