<script>
    $(document).ready(function () {

        var navigationLinks = '[{"ID":{"Guid":"1db1a51b-249a-4e82-944d-d30dfdd120f5","IsGlobalNullId":false,"IsNull":false},"NavigationTitle":"Legal Notices","NavigationUrl":"/about/legal-notices","IsSelected":false,"ChildNavigationLinkItem":[]},{"ID":{"Guid":"f1413f4b-c251-4e83-a1bb-a06b02541aa6","IsGlobalNullId":false,"IsNull":false},"NavigationTitle":"Copyright Statement","NavigationUrl":"/about/legal-notices/copyright","IsSelected":false,"ChildNavigationLinkItem":[]},{"ID":{"Guid":"f54a4469-d2d9-48c6-911f-5bbeeb95c1c4","IsGlobalNullId":false,"IsNull":false},"NavigationTitle":"Health Management Apps","NavigationUrl":"/about/legal-notices/health-management-apps","IsSelected":false,"ChildNavigationLinkItem":[]},{"ID":{"Guid":"944553d3-b642-45ea-ba56-4c9974819850","IsGlobalNullId":false,"IsNull":false},"NavigationTitle":"Nondiscrimination","NavigationUrl":"/about/legal-notices/nondiscrimination","IsSelected":false,"ChildNavigationLinkItem":[]},{"ID":{"Guid":"cfcf6eef-cf7a-4baa-9cac-742e3e76552b","IsGlobalNullId":false,"IsNull":false},"NavigationTitle":"Privacy","NavigationUrl":"/about/legal-notices/privacy","IsSelected":true,"ChildNavigationLinkItem":[]},{"ID":{"Guid":"3727ade1-f1e5-48d8-98e7-bfb4bf92fced","IsGlobalNullId":false,"IsNull":false},"NavigationTitle":"Terms of Use","NavigationUrl":"/about/legal-notices/terms","IsSelected":false,"ChildNavigationLinkItem":[{"ID":{"Guid":"2b993234-aa8a-481b-8357-8cdda733ab15","IsGlobalNullId":false,"IsNull":false},"NavigationTitle":"Banner Health Social Media Terms of Use Agreement","NavigationUrl":"/about/legal-notices/terms/social-terms","IsSelected":false,"ChildNavigationLinkItem":[]},{"ID":{"Guid":"44a965c9-fff3-417b-a214-0d072c0e3c72","IsGlobalNullId":false,"IsNull":false},"NavigationTitle":"Social Terms en Espanol","NavigationUrl":"/about/legal-notices/terms/social-terms-en-espanol","IsSelected":false,"ChildNavigationLinkItem":[]}]}]';
        var json = JSON.parse(navigationLinks);
        $("#ul_inline_navigation").empty();

        if (json != null && json != undefined) {

            if (json.length == 0) {
                $("#inline_navigation").hide();
            }
        }
        else {
            $("#inline_navigation").hide();
        }

        $.each(json, function (index, parentlink) {
            
            var html = GenerateHtmlForLink(parentlink);
            $("#ul_inline_navigation").append(html);

        });

        function GenerateHtmlForLink(link) {

            var html = '';

            if (link.ChildNavigationLinkItem.length > 0) {

                if (link.IsSelected) {

                    html += '<li class="active-menu"><a href="' + link.NavigationUrl + '">' + link.NavigationTitle + '</a><ul>';
                   // $('#litInlineNavParentMenuTitle').text(link.NavigationTitle);
                }
                else {

                    html += '<li><a href="' + link.NavigationUrl + '">' + link.NavigationTitle + '</a><ul>';
                }

                //if (link.IsChildMenuSelected){
                //    $('#litInlineNavParentMenuTitle').text(link.NavigationTitle);
                //}

                $.each(link.ChildNavigationLinkItem, function (index, childLink) {

                    html += GenerateHtmlForLink(childLink);
                });

                return html + '</ul></li>';
            }
            else {

                if (link.IsSelected) {

                    html = '<li class="active-menu"><a href="' + link.NavigationUrl + '">' + link.NavigationTitle + '</a></li>';
                    $('#litInlineNavParentMenuTitle').text(link.NavigationTitle);

                } else {

                    html = '<li><a href="' + link.NavigationUrl + '">' + link.NavigationTitle + '</a></li>';
                }
                return html;
            }
        }

        $('.second-nav-menu').secondNav();

        $(window).resize(function () {
            $(".second-nav-menu ul li").on('mouseenter mouseleave', function (e) {
                if ($('ul', this).length) {
                    var elm = $('ul:first', this);
                    var off = elm.offset();
                    var l = off.left;
                    var w = elm.width();
                    //var docH = $(".container").height();
                    var docW = $(".second-nav-menu").width();

                    var isEntirelyVisible = (l + w <= docW);

                    if (!isEntirelyVisible) {
                        $(this).addClass('drop-left');
                    } else {
                        $(this).removeClass('drop-left');
                    }
                }
            });
        });

       
        // To initially run the function
        $(window).resize();
        
        if ($("#site-header").length) {

            $("#site-header").addClass('header-space-for-second-nav');
            $(".second-nav-menu").addClass('second-nav-menu-mda');
            $("#inline_navigation .inline-nav-menu-title").addClass('inline-nav-menu-title-MD');
        }

        if ($(window).width() > 768) {
            $('li.has-sub').each(function () {
                if ($(this).parent().css('display') != 'none') {
                    var parentWordLength = $(this).find('a:first').text().length, childLength = 0, isCustom = 'F';
                    $(this).find('ul:first > li').each(function () {
                         childLength = $(this).find('a:first').text().length;
                        if (childLength > parentWordLength) {
                            isCustom = 'T1';
                         }
                        });
                        if (isCustom == 'T') {
                          $(this).find('a:first').css("width", $(this).find('ul:first').outerWidth());
                        } else {
                            $(this).find('ul:first').css("width",'100%');
                        }
                    }
            });
        }        

    });
</script>



                        

