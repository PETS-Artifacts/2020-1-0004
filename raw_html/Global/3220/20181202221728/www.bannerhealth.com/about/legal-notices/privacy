<script>
    var hdnfldUrl;
    $(function () {
        hdnfldUrl = $('div.hidHidden :hidden').val();

        $('a.signin-modalpopup-btn').on('click', function () {
            $(".modal-body").html('<iframe width="100%" height="100%" frameborder="0" scrolling="yes" allowtransparency="false" src="' + hdnfldUrl + '"></iframe>');
            $('.modal-content').css({ height: ($(window).height() - 60) });
        });
    });
</script>

