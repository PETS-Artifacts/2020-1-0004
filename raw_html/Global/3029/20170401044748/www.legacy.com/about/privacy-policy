<!--GTM push -->
<script>
/* this binds a click end to pass analytics data to google */
var readyFunct = function() {
  document.onclick = function(e) {
    e = e || event
    var target = e.target || e.srcElement

    //fires dataLayer push if anchor tag does not have a data-ga-no-auto-tag attribute value of true
    if (!target.hasAttribute("data-ga-no-auto-tag") && target.getAttribute("data-ga-no-auto-tag") !== 'true') {
      // Bubble up four levels to make sure that what you clicked is inside of an A and does not have a data-ga-no-auto-tag attribute
      if (
        (target.nodeName != 'A' && target.parentElement.nodeName != 'A' && target.parentElement.parentElement.nodeName != 'A' && target.parentElement.parentElement.parentElement.nodeName != 'A')
         || (target.hasAttribute("data-ga-no-auto-tag") && target.getAttribute("data-ga-no-auto-tag"))
         || (target.parentElement.hasAttribute("data-ga-no-auto-tag") && target.parentElement.getAttribute("data-ga-no-auto-tag"))
         || (target.parentElement.parentElement.hasAttribute("data-ga-no-auto-tag") && target.parentElement.parentElement.getAttribute("data-ga-no-auto-tag"))
         || (target.parentElement.parentElement.parentElement.hasAttribute("data-ga-no-auto-tag") && target.parentElement.parentElement.parentElement.getAttribute("data-ga-no-auto-tag"))
      ) return

      //adds valyes to the datalayer if found as an attribute on the anchor tag
      var dataLayerEvent = {'event': 'GAEvent'};
      if (target.hasAttribute('data-ga-evt-cat')) {
        dataLayerEvent.eventCategory = target.getAttribute('data-ga-evt-cat');
      } else {
        dataLayerEvent.eventCategory = '';
      }
      if (target.hasAttribute('data-ga-evt-action')) {
        dataLayerEvent.eventAction = target.getAttribute('data-ga-evt-action');
      } else {
        dataLayerEvent.eventAction = 'click';
      }
      if (target.hasAttribute('data-ga-evt-label')) {
        dataLayerEvent.eventLabel = target.getAttribute('data-ga-evt-label');
      } else {
        dataLayerEvent.eventLabel = undefined
      }
      if (target.hasAttribute('data-ga-evt-value')) {
        dataLayerEvent.eventValue = target.getAttribute('data-ga-evt-value');
      } else {
        dataLayerEvent.eventValue = undefined
      }
      dataLayer.push(dataLayerEvent);
    }
  }
}

function ready(fn) {
  if (document.readyState != 'loadinggit status'){
    fn();
  } else {
    document.addEventListener('DOMContentLoaded', fn);
  }
}

ready(readyFunct);
</script>


