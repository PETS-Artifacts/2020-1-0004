	</script><script>$(document).on('ready', function () {
            
            ko.applyBindings(new tft.viewModels.SearchViewModel(), document.getElementById('topNav'));
            
            var classes = document.getElementsByTagName('html')[0].className.split(/\s+/);
            for(var i = 0; i < classes.length; i++) {
                if (classes[i].length >= 10 && classes[i].substr(0,10) == 'top-level-') {
                    var data = classes[i].replace('top-level-', '');
                    var items = $('ul.nav li[data-nav*=' + data + ']');
                    if (items.length > 0) {
                        $(items[0]).addClass('active');
                    }
                    break;
                }
            }
        });</script><!-- URL HTTS to HTTP --><script>$(function () {

            $("a").each(function () {
                if (location.protocol.indexOf('https') > -1) {
                    var href = $(this).attr('href');
                    
                    if (typeof href !== 'undefined' && href !== false) {

                        if (href.indexOf('https') > -1 && href.indexOf('nccpa') > -1) {

                            var nonssl = href.replace('https', 'http');
                            $(this).attr('href', nonssl);

                        }
                        else if (href.indexOf('http') !== 0) {
                            var nonssl = '';
                            var char1 = href.substring(0, 1);
                            if (char1 == '#' || char1 == '~' || href.indexOf('javascript:') > -1 || href.indexOf('mailto') > -1 || href.indexOf('tel:') > -1 ) {
                                nonssl = href;
                            }
                            else if (char1 != '/')
                                nonssl = "http://" + location.host + '/' + href;
                            else
                                nonssl = "http://" + location.host  + href;
                            $(this).attr('href', nonssl);
                        }
                    }
                }
            })

            $('map').imageMapResize();
            
        })
        function openModal(id){
            $('#' + id).modal({
                keyboard: false
            })
        }</script></body></html>