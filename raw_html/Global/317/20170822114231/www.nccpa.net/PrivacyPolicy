                                var nonssl = '';
                                var char1 = href.substring(0, 1);
                                if (char1 == '#' || char1 == '~' || href.indexOf('javascript:') > -1 || href.indexOf('mailto') > -1 || href.indexOf('tel:') > -1 ) {
                                    nonssl = href;
                                }
                                else if (char1 != '/')
                                    nonssl = "http://" + location.host + '/' + href;
                                else
                                    nonssl = "http://" + location.host  + href;
                                $(this).attr('href', nonssl);
                            }
                        }
                    }
                })

                $('map').imageMapResize();
            
            })
            function openModal(id){
                $('#' + id).modal({
                    keyboard: false
                })
            }</script></body></html>