    <script type="text/javascript">
        var didScroll;
        var lastScrollTop = 0;
        var delta = 5;
        var navbarHeight = $('#header').outerHeight();
        var pageId = 2503;

        $(window).scroll(function (event) {
            didScroll = true;
        });

        setInterval(function () {
            if (didScroll) {
                hasScrolled();
                didScroll = false;
            }
        }, 250);

        function hasScrolled() {
            var st = $(this).scrollTop();

            // Make sure they scroll more than delta
            if (Math.abs(lastScrollTop - st) <= delta)
                return;

            // If they scrolled down and are past the navbar, add class .nav-up.
            // This is necessary so you never see what is "behind" the navbar.
            if (st > lastScrollTop && st > navbarHeight) {
                // Scroll Down
                $('#nav-closeout').removeClass('nav-down').addClass('nav-up');
            } else {
                // Scroll Up
                if (st + $(window).height() < $(document).height()) {
                    $('#nav-closeout').removeClass('nav-up').addClass('nav-down');
                }
            }

            lastScrollTop = st;
        }

        function hideCloseOut() {
            $(".nav-closeout-minimized").show();
            $(".nav-closeout-yellow").hide();
        }

        function expandCloseOut() {
            $(".nav-closeout-minimized").hide();
            $(".nav-closeout-yellow").show();
        }
    </script>

</div> <!-- header container-fluid -->

<script>
                                        function is_touch_device()
{
                                        return !!('ontouchstart' in window);
}

function openSearch()
{
                                            $(".header-search-container").show();
                                            $(".search-icons-desktop > .search-icon-clear").css('display', 'block');
                                            $(".search-icons-desktop > .search-icon-search").hide();
                                            $(".dnb-main-container").css('opacity', 0.3);
}

function closeSearch()
{
                                            $(".header-search-container").hide();
                                            $(".search-icons-desktop > .search-icon-clear").hide();
                                            $(".search-icons-desktop > .search-icon-search").show();
                                            $(".dnb-main-container").css('opacity', 1);
}

document.querySelector('#nav-site-search-bar-desktop').addEventListener('keypress', function (e) {
                                        var key = e.which || e.keyCode;
                                        if (key === 13)
                                        { // 13 is enter
                                            searchSite();
                                        }
});

                                        function searchSite()
{
                                        var q = $('#nav-site-search-bar-desktop').val();
                                        if (q)
                                        {
                                            window.location.href = "/search?q=" + q;
                                        }
                                        else
                                        {
                                                $(".header-search-container > .error-message").show();
                                        }
}

document.querySelector('#nav-site-search-bar-mobile').addEventListener('keypress', function (e) {
                                        var key = e.which || e.keyCode;
                                        if (key === 13)
                                        { // 13 is enter
                                            searchSiteMobile();
                                        }
});


                                        function searchSiteMobile()
{
                                        var q = $('#nav-site-search-bar-mobile').val();
                                        if (q)
                                        {
                                            window.location.href = "/search?q=" + q;
                                        }
                                        else
                                        {
                                                $(".header-search-container > .error-message").show();
                                        }
}

                                        $(".close-login-janrain").click(function () {

                                            //Calling bootstrap mehtod to close modal
                                            $("#login-modal").modal('hide');

});

                                        $(".navbar-toggle").click(function () {
                                        var menuIcon = $("#icon-menu").css('display') == 'none';
                                        if (menuIcon)
                                        {
                                                $("#icon-menu").show();
                                                $("#icon-close").hide();
                                        }
                                        else
                                        {
                                                $("#icon-menu").hide();
                                                $("#icon-close").show();
                                        }
});

                                        $(document).ready(function () {
                                        

                                        dnb.Locations.Search.RegisterSearchCurrentBox($(".dnb-location-selector .dnb-search-box"), $("#input-geolocation"), "gme-daveandbustersinc");
                                        dnb.Locations.Search.RegisterSearchCurrentBox($(".dnb-location-selector-mobile .dnb-search-box"), $("#input-geolocation-mobile"), "gme-daveandbustersinc");

                                        //Checking if we are on tablet interface to see if we need to modify how the dropdowns are functioning
                                        if (is_touch_device())
                                        {
                                                //$("#navbar > ul.navbar-nav > li > a").each(function(index, elem){
                                                //    $(elem).click(function (event) {
                                                //        var parent = $(event.target.parentNode);
                                                //        //if (!parent.hasClass("expanded")) event.preventDefault();
                                                //    });
                                                //});
                                                $('.dropdown-toggle').click(function(e) {
                                                    //Set width of all toggles to 100%
                                                    $('.dropdown-toggle').each(function(){
                                                        $(this).css('width', '100%');
                                                });
                                                    //Set this toggle to 0px
                                                    $(e.target).css('width', '0');
                                            });
                                        }
                                        else
                                        {
                                                $('.dropdown-toggle').each(function() {
                                                    $(this).css('width', '0');
                                            });
                                        }


                                        var text = $('.event-prompt-head').text();
                                        if (text)
                                        {
                                            text = text.replace("\&amp;", "&");
                                                $('.event-prompt-head').text(text);
                                        }

                                        var html = $('.event-prompt-head').html();
                                        if (html)
                                        {
                                            html = html.replace("&lt;br /&gt;", "<br>");
                                                $('.event-prompt-head').html(html);
                                        }
});
</script>

