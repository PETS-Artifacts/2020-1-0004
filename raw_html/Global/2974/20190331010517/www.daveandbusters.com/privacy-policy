    <script>
        function janrainWidgetOnload() {
            janrain.events.onProviderLoginToken.addHandler(function(response) {
                angular.element($('#janrain-signup-container')).scope().janrainAuthenticateSocial(response);
            });
        }
    </script>
</div>
