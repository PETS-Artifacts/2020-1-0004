<script>

    $(document).ready(function () {
        var oldhHeight = $(window).height();


        if (navigator.userAgent.match(/Android/i)
            || navigator.userAgent.match(/webOS/i)
            || navigator.userAgent.match(/iPhone/i)
            || navigator.userAgent.match(/iPad/i)
            || navigator.userAgent.match(/iPod/i)
            || navigator.userAgent.match(/BlackBerry/i)
            || navigator.userAgent.match(/Windows Phone/i)) {


            $(window).resize(function () {
                var nh = $(window).height();
                var select = $(".closest-location-information");

                //compare new and old width
                //oldwidth = nw;

                if (nh / oldhHeight < .9) {

                    select.css("display", "none");

                }
                else {

                    select.css("display", "block");

                }

            });

        }
    });

    function directions(param) {
        // preparing address string for google maps query before exposing to view
        var str = param;
        var tokens = str.split(" ");
        if (navigator.userAgent.match(/iPhone/i) || navigator.userAgent.match(/iPad/i) || navigator.userAgent.match(/iPod/i)) {
            var flag = "ios";
        }

        var googleMapsQuery = "";

        for (var i = 0, len = tokens.length; i < len; i++) {
            googleMapsQuery += (tokens[i]) + "+";
        }



        if (flag) {
            window.open("maps://maps.google.com/maps/&daddr= " + googleMapsQuery);
        }
        else {
            window.open("https://www.google.com/maps/dir/?api=1&destination=" + googleMapsQuery, '_blank');

        }

        console.log(googleMapsQuery);
    }

</script>


