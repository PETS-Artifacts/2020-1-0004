    <script>
    // check the mockApiRest parameter in which case we load the dnb.api.mocks javascript to
    // mock some API calls.
    var mock = document.location.search.indexOf("mockRestApi=true") > -1;
    if (mock) {
        console.log("Loading the Mock Api");

        // In order to mock the DnB Web Api, we need to load the angular-mocks.js and then the dnbApi-mocks.js
        $.ajax({
            url: '/js/angular/angular-mocks.js',
            dataType: "script",
            success: function () {
                $.ajax({
                    url: '/Scripts/services/dnb.api-mocks.js',
                    dataType: "script",
                    success: function () {
                        angular.element(document).ready(function () {
                            angular.bootstrap(document, [
                                'dnbApiModuleMock',
                                'cardWidgetModule',
                                'ngCookies',
