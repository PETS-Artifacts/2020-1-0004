    var s_account = "dabprod"
var s = s_gi(s_account)
/************************** CONFIG SECTION **************************/
/* You may add or alter any code config here. */
/* Conversion Config */
s.currencyCode = "USD"
/* Link Tracking Config */
s.trackDownloadLinks = true
s.trackExternalLinks = true
s.trackInlineStats = true
s.linkDownloadFileTypes = "exe,zip,wav,mp3,mov,mpg,avi,wmv,pdf,doc,docx,xls,xlsx,ppt,pptx,sis,sisx,deb,install" // added 20110707
s.linkInternalFilters = "javascript:,daveandbusters.com,razordriven.com,bestsportsbarever.com" //updated 20121113
s.linkLeaveQueryString = false
s.linkTrackEvents = "None"
s.linkTrackVars = "prop12,prop13,prop14,prop15,eVar12,eVar13,eVar14,eVar15"; //updated KMH 20130116

/* Form Analysis Config (should be above doPlugins section) */
s.formList = "EventPlanner"
s.trackFormList = true
s.trackPageName = true
s.useCommerce = false

s.varUsed = "prop10,prop16,prop17" // CHANGE TO THE CORRECT s.prop
s.eventList = "" //Abandon,Success,Error

/* Plugin Config */
s.usePlugins = true
function s_doPlugins(s) {
    /* Add calls to plugins here */

    /********************************************************************
    *
    * Commented example of how to use this is doPlugins function
    *
    *******************************************************************/

    /*Set pageName to URL filepath - ADDED KMH 20130116*/
    if (!s.pageName)
        s.pageName = window.location.pathname;

    /*Turn PageName to LowerCase - ADDED KMH 20130116*/
    if (s.pageName)
        s.pageName = s.pageName.toLowerCase();

    s.hbx_lt = "auto" // manual, auto
    s.setupLinkTrack("prop12,prop13,prop14,", "SC_LINKS");


    /* Link Click - Page Link URL - ADDED KMH 20130116 */
    if (s.prop14) {
        //Modified by Vishu: TFS Workitem#156
        //s.prop15 = s.prop15 + "|" + s.pageName;
        s.prop15 = s.prop13 + "|" + s.pageName;
        s.eVar15 = s.prop15 = s.prop15.toLowerCase();
    }

    if (s.prop12)
        s.eVar12 = s.prop12;

    if (s.prop13)
        s.eVar13 = s.prop13;

    if (s.prop14)
        s.eVar14 = s.prop14;


    /* Populate taxonomy */
    if (s.pageName && s.pageName != '') {
        var splitPageName = s.pageName.split("/");
        s.eVar36 = s.prop4 = splitPageName[1] + "/" + splitPageName[2] + "/" + splitPageName[3] + "/" + splitPageName[4] + "/" + splitPageName[5];
        s.eVar35 = s.prop3 = splitPageName[1] + "/" + splitPageName[2] + "/" + splitPageName[3] + "/" + splitPageName[4];
        s.eVar34 = s.prop2 = splitPageName[1] + "/" + splitPageName[2] + "/" + splitPageName[3];
        s.eVar33 = s.prop1 = splitPageName[1] + "/" + splitPageName[2];
        s.channel = splitPageName[1];
    }



    // Plugin Example: getNewRepeat 1.0
    //s.prop5=s.getNewRepeat();

    s.prop18 = s.getNewRepeat();
    s.eVar8 = s.prop18;

    /* Individual ID - added 20110816 */
    if (!s.prop29)
        s.prop29 = s.getQueryParam('indvdl');
    s.prop29 = s.getAndPersistValue(s.prop29, 's_prop29', 1825);

    if (s.prop29)
        s.evar29 = s.prop29;

    /* External Campaigns */
    if (!s.campaign)
        s.campaign = s.getQueryParam('cmpid');

    /* Campaign Tracking Code populated to Traffic Variable 33 - added 20120216 */
    if (s.campaign)
        s.prop33 = s.campaign;

    /* Internal Campaigns */
    if (!s.eVar4)
        s.eVar4 = s.getQueryParam('intcmp');

    /* Promotional Offer - sepromo added 20110707 - id added 20110725 */
    if (!s.prop31)
        s.prop31 = s.getQueryParam('sepromo');

    if (!s.prop31)
        s.prop31 = s.getQueryParam('id');

    if (s.prop31)
        s.eVar31 = s.prop31;

    /* Displayed Phone Number - added 20110707 */
    if (!s.prop32)
        s.prop32 = s.getQueryParam('ad');
    if (s.prop32)
        s.eVar32 = s.prop32;

    /* Page URL - added 20110707 */
    if (!s.prop19)
        s.prop19 = s.pageURL;
    if (s.prop19)
        s.eVar19 = s.prop19;


    /* Site Search */
    if (!s.prop5)
        s.prop5 = s.getQueryParam('search'); //added 20110707
    if (s.prop5) {
        s.prop5 = s.prop5.toLowerCase();
        s.eVar5 = s.prop5;
        s.events = s.apl(s.events, "event13", ",", 2); //added 20110707			
        var t_search = s.getValOnce(s.eVar5, 'ev5', 0);
        if (t_search) {
            s.events = s.apl(s.events, "event5", ",", 2);
        }
    }
    //optional Search Orgination Page
    s.prop11 = s.getPreviousValue(s.pageName, 'gpv_p11', 'event5');

    /* Direct Influence Pages */
    s.eVar3 = s.pageName;

    /* Conversion Events - added 20110707  */
    if (!s.prop24)
        s.prop24 = s.events;
    if (s.prop24)
        s.eVar24 = s.prop24;


    if (s.prop23)
        s.eVar23 = s.prop23;


    /* Visit Number - persist session value and assign eVar - ADDED KMH 20130116 */
    s.eVar9 = s.prop34 = s.getVisitNum();

    /* Days Since Last Visit - persist session value and assign eVar - ADDED KMH 20130116 */
    s.eVar10 = s.prop35 = s.getDaysSinceLastVisit('s_prop35');

    //Sitecatalyst and Sales force Integration
    s.evar17 = s.prop37 = s.getQueryParam('cid');
    //CreateCookie
    if (s.evar17 != '' && s.evar17 != '')
        createCookie("s_prop37", s.evar17, 30);

    /* Store Location - updated KMH 20130405 */
    if (!s.prop20)
        s.prop20 = s.getQueryParam('StoreId');

    if (s.prop20) {
        var pgNm = s.pageName;
        if (pgNm == "/contactplanner/find-a-planner") {
            s.eVar20 = s.prop20;
            s.events = "event18";
        }
        else if (pgNm == "/contactplanner/click-to-contact") {
            s.eVar20 = s.prop20;
            s.events = "event22";
        }
        else {
            s.eVar20 = s.prop20;
            s.events = "event21";
        }
    }

    /* Enhanced download tracking */
    s.url = s.downloadLinkHandler();
    if (s.url) {
        s.setupLinkTrack("prop12,prop13,prop14", "SC_LINKS");
        s.linkTrackVars = "prop12,prop13,prop14,eVar12,eVar13,eVar14,events";
        s.linkTrackEvents = "event4";
        s.eVar12 = s.prop12;
        s.eVar13 = s.prop13 = "download-" + s.prop13;
        s.eVar14 = s.url;
        s.events = "event4";
    }

    s.setupFormAnalysis();

}
s.doPlugins = s_doPlugins

/************************** PLUGINS SECTION *************************/
/* You may insert any plugins you wish to use here.                 */
/*
* Plugin: getNewRepeat 1.0 - Return whether user is new or repeat
*/
s.getNewRepeat = new Function(""
+ "var s=this,e=new Date(),cval,ct=e.getTime(),y=e.getYear();e.setTime"
+ "(ct+30*24*60*60*1000);cval=s.c_r('s_nr');if(cval.length==0){s.c_w("
+ "'s_nr',ct,e);return 'New';}if(cval.length!=0&&ct-cval<30*60*1000){s"
+ ".c_w('s_nr',ct,e);return 'New';}if(cval<1123916400001){e.setTime(cv"
+ "al+30*24*60*60*1000);s.c_w('s_nr',ct,e);return 'Repeat';}else retur"
+ "n 'Repeat';");
/*
* Plugin: setupLinkTrack 2.0 - return links for HBX-based link 
*         tracking in SiteCatalyst (requires s.split and s.apl)
*/
s.setupLinkTrack = new Function("vl", "c", ""
+ "var s=this;var l=s.d.links,cv,cva,vla,h,i,l,t,b,o,y,n,oc,d='';cv=s."
+ "c_r(c);if(vl&&cv!=''){cva=s.split(cv,'^^');vla=s.split(vl,',');for("
+ "x in vla)s._hbxm(vla[x])?s[vla[x]]=cva[x]:'';}s.c_w(c,'',0);if(!s.e"
+ "o&&!s.lnk)return '';o=s.eo?s.eo:s.lnk;y=s.ot(o);n=s.oid(o);if(s.eo&"
+ "&o==s.eo){while(o&&!n&&y!='BODY'){o=o.parentElement?o.parentElement"
+ ":o.parentNode;if(!o)return '';y=s.ot(o);n=s.oid(o);}for(i=0;i<4;i++"
+ ")if(o.tagName)if(o.tagName.toLowerCase()!='a')if(o.tagName.toLowerC"
+ "ase()!='area')o=o.parentElement;}b=s._LN(o);o.lid=b[0];o.lpos=b[1];"
+ "if(s.hbx_lt&&s.hbx_lt!='manual'){if((o.tagName&&s._TL(o.tagName)=='"
+ "area')){if(!s._IL(o.lid)){if(o.parentNode){if(o.parentNode.name)o.l"
+ "id=o.parentNode.name;else o.lid=o.parentNode.id}}if(!s._IL(o.lpos))"
+ "o.lpos=o.coords}else{if(s._IL(o.lid)<1)o.lid=s._LS(o.lid=o.text?o.t"
+ "ext:o.innerText?o.innerText:'');if(!s._IL(o.lid)||s._II(s._TL(o.lid"
+ "),'<img')>-1){h=''+o.innerHTML;bu=s._TL(h);i=s._II(bu,'<img');if(bu"
+ "&&i>-1){eval(\"__f=/ src\s*=\s*[\'\\\"]?([^\'\\\" ]+)[\'\\\"]?/i\")"
+ ";__f.exec(h);if(RegExp.$1)h=RegExp.$1}o.lid=h}}}h=o.href?o.href:'';"
+ "i=h.indexOf('?');h=s.linkLeaveQueryString||i<0?h:h.substring(0,i);l"
+ "=s.linkName?s.linkName:s._hbxln(h);t=s.linkType?s.linkType.toLowerC"
+ "ase():s.lt(h);oc=o.onclick?''+o.onclick:'';cv=s.pageName+'^^'+o.lid"
+ "+'^^'+s.pageName+' | '+(o.lid=o.lid?o.lid:'no &lid')+'^^'+o.lpos;if"
+ "(t&&(h||l)){cva=s.split(cv,'^^');vla=s.split(vl,',');for(x in vla)s"
+ "._hbxm(vla[x])?s[vla[x]]=cva[x]:'';}else if(!t&&oc.indexOf('.tl(')<"
+ "0){s.c_w(c,cv,0);}else return ''");
s._IL = new Function("a", "var s=this;return a!='undefined'?a.length:0");
s._II = new Function("a", "b", "c", "var s=this;return a.indexOf(b,c?c:0)"
);
s._IS = new Function("a", "b", "c", ""
+ "var s=this;return b>s._IL(a)?'':a.substring(b,c!=null?c:s._IL(a))");
s._LN = new Function("a", "b", "c", "d", ""
+ "var s=this;b=a.href;b+=a.name?a.name:'';c=s._LVP(b,'lid');d=s._LVP("
+ "b,'lpos');r"
+ "eturn[c,d]");
s._LVP = new Function("a", "b", "c", "d", "e", ""
+ "var s=this;c=s._II(a,'&'+b+'=');c=c<0?s._II(a,'?'+b+'='):c;if(c>-1)"
+ "{d=s._II(a,'&',c+s._IL(b)+2);e=s._IS(a,c+s._IL(b)+2,d>-1?d:s._IL(a)"
+ ");return e}return ''");
s._LS = new Function("a", ""
+ "var s=this,b,c=100,d,e,f,g;b=(s._IL(a)>c)?escape(s._IS(a,0,c)):esca"
+ "pe(a);b=s._LSP(b,'%0A','%20');b=s._LSP(b,'%0D','%20');b=s._LSP(b,'%"
+ "09','%20');c=s._IP(b,'%20');d=s._NA();e=0;for(f=0;f<s._IL(c);f++){g"
+ "=s._RP(c[f],'%20','');if(s._IL(g)>0){d[e++]=g}}b=d.join('%20');retu"
+ "rn unescape(b)");
s._LSP = new Function("a", "b", "c", "d", "var s=this;d=s._IP(a,b);return d"
+ ".join(c)");
s._IP = new Function("a", "b", "var s=this;return a.split(b)");
s._RP = new Function("a", "b", "c", "d", ""
+ "var s=this;d=s._II(a,b);if(d>-1){a=s._RP(s._IS(a,0,d)+','+s._IS(a,d"
+ "+s._IL(b),s._IL(a)),b,c)}return a");
s._TL = new Function("a", "var s=this;return a.toLowerCase()");
s._NA = new Function("a", "var s=this;return new Array(a?a:0)");
s._hbxm = new Function("m", "var s=this;return (''+m).indexOf('{')<0");
s._hbxln = new Function("h", "var s=this,n=s.linkNames;if(n)return s.pt("
+ "n,',','lnf',h);return ''");
/*
