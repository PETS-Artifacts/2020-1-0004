		<script>
			var go_youtubeplayer = {};
			/*
			 * Youtube Player API for home banner
			 */
		 	if (typeof ga_videolist !== 'undefined') {


				var tag = document.createElement('script');
				tag.src = "https://www.youtube.com/iframe_api";
				var firstScriptTag = document.getElementsByTagName('script')[0];
				firstScriptTag.parentNode.insertBefore(tag, firstScriptTag);

				function onYouTubeIframeAPIReady() {
					for(var i = 0; i < ga_videolist.length; i++) {

						var lo_item = ga_videolist[i];
						var li_playerid = lo_item.id;
						var li_autoplay = document.getElementById('video-player-' + li_playerid ).getAttribute('data-autoplay');
						var li_mute = document.getElementById('video-player-' + li_playerid ).getAttribute('data-mute');

						var lo_player = new YT.Player('video-player-' + li_playerid , {
							playerVars: {
								'autoplay': li_autoplay,
								'controls': 0,
								'mute': li_mute,
								'rel': 0,
								'modestbranding': 0,
								'allowfullscreen': 1,
								'frameborder': 0,
								'showinfo': 0,
								// 'loop': 1,
								'playlist': lo_item.video
								// 'playsinline': 0
								},
								frameborder: 0,
								videoId: lo_item.video,
								events: {
									'onStateChange': onPlayerStateChange
								},
						});

						go_youtubeplayer['video-player-' + li_playerid] = lo_player;

						$(document).on('click', '.video-player-overlay', function() {
							console.log('HELLO!!!');
						});

					} //endfor

				} //onYouTubeIframeAPIReady

				function onPlayerStateChange(event) {

					var la_nodes = event.target.a.nextSibling.parentNode.childNodes;

					if (event.data == YT.PlayerState.PLAYING || event.data == YT.PlayerState.BUFFERING) {

						for (var i = 0; i<la_nodes.length; i++) {
							var lo_item = la_nodes[i];
							if (lo_item.classList) {
								if (lo_item.classList.value.includes('playbutton')) {
									lo_item.classList.remove('j-visible');
								}
							}
						} //endfor

					} else {

						for (var i = 0; i<la_nodes.length; i++) {
							var lo_item = la_nodes[i];
							if (lo_item.classList) {
								if (lo_item.classList.value.includes('playbutton')) {
									lo_item.classList.add('j-visible');
								}
							}
						} //endfor
					}

				} //onPlayerStateChange

		 	}
		</script>

		
