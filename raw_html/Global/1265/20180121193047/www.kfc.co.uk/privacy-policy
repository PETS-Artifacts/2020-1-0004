    <!-- Bootstrapper -->
    <script type="text/javascript">
    if ( !Modernizr.objectfit || !Modernizr.srcset ) {
        var $imgs = document.querySelectorAll('.block__img, .img--cover, .img--contain, .img--responsive');
        var emptyImgSrc = 'data:image/gif;base64,R0lGODlhAQABAIAAAAAAAP///yH5BAEAAAAALAAAAAABAAEAAAIBRAA7';

        for (var i = 0; i < $imgs.length; i++) {
            var $img = $imgs[i];
            var imgSrc = $img.getAttribute('fallback') || $img.getAttribute('src');
            var hasSrcBinding = $img.hasAttribute('src.bind') || $img.hasAttribute('srcset.bind') || $img.hasAttribute('srcset.one-time');
            if (imgSrc !== null && imgSrc.indexOf(emptyImgSrc) === -1) {

                $img.style.backgroundImage = 'url(' + imgSrc + ')';
                $img.setAttribute('src', emptyImgSrc);
                $img.removeAttribute('fallback');
                $img.removeAttribute('srcset');
                $img.removeAttribute('srcset.bind');
                $img.removeAttribute('srcset.one-time');
                $img.removeAttribute('sizes');
            }
        }
    }
</script>
