	$ = jQuery;
	function getAlert(){
		$.getJSON('/staticfiles/json/notify.json',
		function(data){
			var data_id, data_message, data_url,todaysdate,ent_time,datediffer,FP_Alert_Story_cookie;
			todaysdate = Math.round(new Date().getTime()/1000.0);
			if(data){
				for(var i=0;i<data.length;i++){
					data_id			= data[i]['id'];
					data_message	= data[i]['message'];
					data_url		= data[i]['url'];
					ent_time		= data[i]['ent_time'];
					datediffer = todaysdate - ent_time;
					FP_Alert_Story_cookie = FPgetcookie('FP_Alert_Story_'+data_id);
					if(data_id!=''){
						if(data_id!='' && datediffer<=900){
							if(FP_Alert_Story_cookie==data_id){
							}else{
								show_story_alert(data_id,data_message,data_url);
								 setTimeout( "$('#bottom_popup').animate({right:'10px',bottom:'10px'});",1000 );
								break;
							}
						}
					}
				}
			}
			setTimeout(function(){ $('#bottom_popup').animate({right:'-370px',bottom:'-150px'}); }, 60000);
		});
	}


	function show_story_alert(data_id,data_message,data_url) {
		if ( navigator.appName.indexOf("Microsoft")==-1 ){
			FP_Alert_Story = "";
			FP_Alert_Story = FPgetcookie('FP_Alert_Story_'+data_id);
			if ( typeof FP_Alert_Story == "undefined" || FP_Alert_Story == "" || FP_Alert_Story != data_id )	{
				FPAdJ = jQuery;
				var alert_html = "";
				var anchor_tag = "";
				if(data_url==''){
					anchor_tag = data_message;
				}else{
					anchor_tag = '<a href="'+data_url+'?utm_source=FP_Flashnews">'+data_message+'</a>';
				}

				alert_html += '<div class="contbox"><a href="javascript:void(0);" onCLick="$(\'#bottom_popup\').animate({right:\'-370px\',bottom:\'-150px\'});" class="close_bottom_popup"></a><p class=bptitle><span class=f_flash_icon></span> F<span class=dot>.</span><span>FLASH</span></p>';
				alert_html += '<p class="bottom_infobox">'+anchor_tag+'</p>';
				alert_html += '</div>';
				document.getElementById("bottom_popup").innerHTML = alert_html;

				var c_name = 'FP_Alert_Story_'+data_id; exhours = 1; var now = new Date(); var time = now.getTime(); time += exhours * 600 * 1000; now.setTime(time);
				var c_value = escape(data_id) + ((exhours==null) ? "" : "; expires="+now.toUTCString()) + "; path=/; domain=.firstpost.com; ";
				document.cookie = c_name + "=" + c_value;
			}
		}
	}

	$(document).ready(function(){
		getAlert();
		setInterval(getAlert,30000);
	});
