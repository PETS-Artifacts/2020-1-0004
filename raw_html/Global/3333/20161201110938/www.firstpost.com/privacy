function pushHindiNAVGA () {
    
	var cat = document.getElementById("eventCatName").value ;

	var pg_nav_cat = '';
	if( cat == '' || cat == 'Front Page' || typeof cat == 'undefined' || cat == null ) {
		pg_nav_cat = 'Home';
	} else {
		pg_nav_cat = cat;
	}

	var cEvent = 'FP_' + pg_nav_cat + '_Catpg_Top_Nav_HindiFP';
	
	ga('send', 'event', 'Top_Nav_Hyperlink', 'HindiFP_Top_Nav_Click', cEvent );
}	

function pushHindiFooterGA () {
    
    var cat = document.getElementById("eventCatName").value ;

    var pg_nav_cat = '';
    if( cat == '' || cat == 'Front Page' || typeof cat == 'undefined' || cat == null ) {
        pg_nav_cat = 'Home';
    } else {
        pg_nav_cat = cat;
    }

    var cEvent = 'FP_' + pg_nav_cat + '_Catpg_Footer_HindiFP';
    
    ga('send', 'event', 'Footer_Hyperlink', 'HindiFP_Footer_Click', cEvent );
}    


