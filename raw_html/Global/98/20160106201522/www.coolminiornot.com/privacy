    jQuery(document).ready(function () {

        $('.userProfile').click(function () {

            $.post("http://www.coolminiornot.com/supporters/profile", { 'uid':$(this).attr('uid') }, function (data) {

                if (data.length !== 0) {

                    if (data.profilepic != null) {

                        $('#profilePic').attr('src', data.profilepic);
                        $('#imgProfileAvatar').attr('src', data.avatar);
                        $('#imgProfileAvatar').show();

                    } else {

                        $('#profilePic').attr('src', data.avatar);
                        $('#imgProfileAvatar').hide();
                    }
                    $('.profileUserName').html(data.username);
                    $('.vbProfile').attr('href', 'http://www.coolminiornot.com/forums/member.php?' + data.uid + "-" + data.username);
                    $('.vbProfile').attr('title', '' + data.username + "’s full profile");

                    if (isBlank(data.age)) {

                        $('#spanPainting').hide();
                    } else {

                        $('#profileAge').html(data.age);
                    }
                    if (isBlank(data.about)) {

                        $('#spanAbout').hide();
                    } else {

                        $('#profileAbout').html(data.about);
                    }
                    if (isBlank(data.homepage)) {

                        $('#spanHomepage').hide();
                    } else {

                        $('#profileHomepage').attr('href', data.homepage);
                    }


                    if (data.subscription == 'yes') {
                        $('#profileSubscriber').show();
                    } else {
                        $('#profileSubscriber').hide();
                    }

                    $('#profilePrivate').attr('href', 'http://www.coolminiornot.com/forums/private.php?do=newpm&u=' + data.uid);
                    $('#profilePictures').attr('href', '/artist/' + data.usernameEncode);
                    $('#profileFavourites').attr('href', '/favourites/' + data.usernameEncode);

                    $("#profileWindow").dialog("open");

                } else {
                    alert('This user not contain info');
                }
            }, "json");

            return false;
        });

    });

</script>    <!-- End Footer -->
