    <!-- JavaScript added by the renderings -->
    <script>
        (function ($) {
            var render = true;
            function GetScore() {
                if (render)
                    $.get("/cricketcomau/LiveScore/LiveScore", { renderAsync: render })
                        .then(function (score) {
                            if ($.trim(score) != "") {
                                $('#score-module').html(score).slideDown(1000);
                                $('#all-content').addClass("all-content-live-score");
                                $('.row.row-ad.row-leaderboard-only-tablet').addClass("ad-live-score-margin");
                            } else {
                                render = false;
                                $("#score-module").hide();
                                $('#all-content').removeClass("all-content-live-score");
                                $('.row.row-ad.row-leaderboard-only-tablet').removeClass("ad-live-score-margin");
                            }
                        });
            }
            $(document).ready(GetScore);
            setInterval(GetScore, 40000);
        })(jQuery);
    </script><script type="text/javascript">
