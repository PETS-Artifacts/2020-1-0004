<script>
        (function ($) {
            var render = true;
            function GetScore() {
                if (render)
                    $.get("/cricketcomau/LiveScore/LiveScore", { renderAsync: render })
                        .then(function (score) {
                            if ($.trim(score) != "") {
                                $('#score-module').html(score).slideDown(1000);
                                videostreamicon();
                                $('#all-content').addClass("all-content-live-score");
                                $('.row.row-ad.row-leaderboard-only-tablet').addClass("ad-live-score-margin");
                            } else {
                                render = false;
                                $("#score-module").hide();
                                $('#all-content').removeClass("all-content-live-score");
                                $('.row.row-ad.row-leaderboard-only-tablet').removeClass("ad-live-score-margin");
                            }
                        });
            }
            $(document).ready(GetScore);
            setInterval(GetScore, 40000);
        })(jQuery);
    </script><script>

        function videostreamicon()
        {
            $('a[data-skinny]').each(function (index, element) {
                var matchid = $(this).attr('data-skinny');
                countrylistskinnyfunction(matchid);
            });
        }

        function countrylistskinnyfunction(matchid) {
            //get country
            posts = $.ajax({
                type: 'GET',
                url: 'https://api.cricket.com.au/appconfig/getloc',
                async: false,
                dataType: 'json',
                done: function (results) {
                    JSON.parse(results);
                    return results;
                }

            }).responseJSON;
           
			var found = {};
            $("a[data-skinny$='" + matchid + "']").each(function (index,element) {
                var countrylists = $(this).attr('data-countrylist');
                var countrylist1 = countrylists.split(',');                     
                for (var i = 0 ; i < countrylist1.length; i++) {
                    if (countrylist1[i].trim() == posts.COUNTRY) {
                        $(this).show();
						$(this).attr("data-iscountry", "true");
					}
                }
				var $this = $(this);
                if(found[$this.data('iscountry')]){
                    $this.remove();   
                }
                else{					
					found[$this.data('iscountry')] = true;
                }
            });
           
        }
    </script><script type="text/javascript">
