            <div class="map-contents-wrap no-gutter row">
                <div class="results col-md-6">
                    <h3 class="map">Locations</h3>
                    <div class="locations-list">
                        <div class="group-container" data-bind="foreach: { data: $component.GroupedResults() }">
                            <h2 data-bind="text: LocationType.Name()"></h2>
                            <ul data-bind="foreach: { data: Results, afterRender: $component.InitializeScrolling }">
                                <li data-bind="css: { selected: Selected }, event: { mouseover: MouseOver, mouseout: MouseOut }">
                                    <address>
