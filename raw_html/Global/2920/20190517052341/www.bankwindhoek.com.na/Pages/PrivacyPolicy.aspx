function GetSurveyLayout(xData, Status) {
	
	$('#MainSurveyContent').append($('<div class="col-xs-12" id="SurveyContent"></div>'));
	
    $(xData.responseXML).SPFilterNode("z:row").each(function() {
	    var QuestionRow = Math.floor($(this).attr("ows_QuestionNo"));
		var Rating = $(this).attr("ows_ScoreRatingNo");
        var SurveyQuestion = $(this).attr("ows_SurveyQuestion");
		var SurveyOptions = $(this).attr("ows__x0066_yg4");

			$('#SurveyContent').append($('<div class="row" id="question' + QuestionRow + '"></div><div class="Surveyrow" id="answer' + QuestionRow + '"></div>'));
			
		document.getElementById("question" + QuestionRow).textContent = SurveyQuestion;
		
		if (Rating > 2){
			var i = Rating;
			var RadioButtons = "";
			var Score = 1;
			
			while (i > 0) {
			RadioButtons += "<div class='col'><input id='Improvement' name='QuestionRow"+ QuestionRow +"' value='ScoreValue"+ Score +"' type='radio'> <label class='SurveyLabel'>"+ Score +"</label></div>";
			i = i - 1;
			Score++;
			}
			document.getElementById("answer" + QuestionRow).innerHTML = RadioButtons;
			
		}
		else if (Rating == 2) {
				document.getElementById("answer" + QuestionRow).innerHTML = "<div class='col'><input id='Improvement' name='YesNo"+ QuestionRow +"' value='Yes' type='radio'> <label class='SurveyLabel'>Yes</label></div><div class='col'> <input id='Improvement' name='YesNo' value='No' type='radio'> <label class='SurveyLabel'> No</label></div>";	
		}
		else if (Rating == 1) {
				var OptionArray = [];
				var SurveyOption = "";
				OptionArray = SurveyOptions.split(',');
				y = 0;
				x = OptionArray.length;
				
				while (x > 0) {
					SurveyOption += "<div class='col'><input id='Improvement' name='SurveyQuestion"+ QuestionRow +"' value='"+ OptionArray[y].trim() +"' type='radio'> <label class='SurveyLabel'>"+ OptionArray[y].trim() +"</label></div>";
					x = x - 1;
					y++;
				}
				
				document.getElementById("answer" + QuestionRow).innerHTML = SurveyOption;
				
		}
		else {
		document.getElementById("answer" + QuestionRow).innerHTML = "<div class='col-xs-12'><input id='Improvement' name='QuestionRow"+ QuestionRow +"' type='text' class='SurveyInputText'></div>";
		}
		
      });
}
