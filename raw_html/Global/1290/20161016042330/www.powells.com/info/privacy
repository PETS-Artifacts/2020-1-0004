    $(document).ready(function () {
        if ($('#search-solr-wrapper').css('display') != 'none') {
            $("#dnn_ctr1336_ViewSearch_txtSolrSearch").focus();
        }

        ($(window).width() <= 780)
        {
            $("#dnn_ctr1336_ViewSearch_txtSolrSearch").blur();
        }

        $('#selectSolrFilter').on('change', function () {
            $("#dnn_ctr1336_ViewSearch_txtSolrSearch").focus();
