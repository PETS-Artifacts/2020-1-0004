    function validateEmailCSMDisplay(id) {
        $.validity.start();
        $("#" + id).require(arrayFindValue('1', 'Recipient Email')).match("email", arrayFindValue('66', 'Email'));
        var result = $.validity.end();
        return result.valid;
    }  
