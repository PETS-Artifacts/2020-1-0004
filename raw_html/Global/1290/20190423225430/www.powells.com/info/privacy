            var jsName = PageURL + "js/fullcalendar.min.js?" + Math.floor((Math.random() * 1000) + 1);
            $.getScript(jsName, function () { _jsLoaded = 1; });
        }

        function LoadCalenderJS() {
            var jsName = PageURL + "js/fullcalendar.js?" + Math.floor((Math.random() * 1000) + 1);
