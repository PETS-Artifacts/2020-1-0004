    function ShowShoppingCartPopupBlogRoll(productItemID) {
        try {
            var manager = $find("dnn_ctr3299_BlogRoll_windowmgrAddToCart");
            var url = $("#dnn_ctr3299_BlogRoll_hfUrl").val();
