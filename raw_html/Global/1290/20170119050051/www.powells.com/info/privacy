<script type="text/javascript">
    $(document).ready(function () {

        $("#dnn_ctr11221_ContestDisplay_btnSubmitContestDisplay").click(function (event) {
            if (_dataResponse == 1) {
                var txtID = $("#dnn_ctr11221_ContestDisplay_txtEmailContestDisplay").attr("id");
                if (validateEmailCSMDisplay(txtID) == false) {
                    event.preventDefault();
                }
            }
        });
    });

    function validateEmailCSMDisplay(id) {
        $.validity.start();
        $("#" + id).require(arrayFindValue('1', 'Recipient Email')).match("email", arrayFindValue('66', 'Email'));
        var result = $.validity.end();
        return result.valid;
    }  
</script>

