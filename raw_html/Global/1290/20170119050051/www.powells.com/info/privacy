<script type="text/javascript">
    var AJAX_HIT = '';
    var li = $('.suggest li');
    var liSelected;
    $(document).ready(function () {
        if ($('#search-solr-wrapper').css('display') != 'none') {
            $("#dnn_ctr1336_ViewSearch_txtSolrSearch").focus();
        }
        ($(window).width() <= 780)
        {
            $("#dnn_ctr1336_ViewSearch_txtSolrSearch").blur();
        }

        $('#selectSolrFilter').on('change', function () {
            $("#dnn_ctr1336_ViewSearch_txtSolrSearch").focus();
        });

        $('#search-solr-wrapper,#search-solr-wrapper>span,#search-solr-wrapper table,#search-solr-wrapper tr').css('height', '24px');

    });

    $("#dnn_ctr1336_ViewSearch_btnSolrSearch").click(function (event) {
        var siteSearch = new PowellsSearch();
        if ($("#dnn_ctr1336_ViewSearch_txtSolrSearch").val() != '') {
            siteSearch.GetResultantUrl($("#dnn_ctr1336_ViewSearch_txtSolrSearch").val(), $("#dnn_ctr1336_ViewSearch_hfQueryStringKw"), event);
        }
        else {
            return false;
        }
    })

    function GetPredictiveData(searchText) {
        var self = this;
        var query = '';
        var predictiveArray = [];
        query = self.buildSearchParamsForPredictiveText(searchText);
        if (query != '') {
            $.support.cors = true
            AJAX_HIT = $.ajax({
                url: SolrSearchURLAutoComplete,
                dataType: 'jsonp',
                data: query,
                cache: false,
                traditional: true,
                jsonp: 'json.wrf',
                success: function (result) {
                    for (var i = 0; i < result.suggest['AutoComplete'][searchText].suggestions.length; i++) {
                        var singleCopy = result.suggest['AutoComplete'][searchText].suggestions[i];

                        var title = singleCopy['term'];

                        if (predictiveArray.indexOf(title) == -1) {
                            predictiveArray.push(title);

                            document.getElementById("suggestionDiv").style.visibility = 'visible'

                            $('#suggestionDiv').append('<li style="list-style: none; margin-bottom:5px; overflow:hidden"><a style="display:block; white-space: nowrap" name="' + title.replace(/<b>|<\/b>/g, '') + '" href="' + HostUrl + '/SearchResults?kw=title:' + title.replace(/<b>|<\/b>|<|(|\?|:|.|\n|)|\*|>/g, '').replace(/&|:|"/g, ' ') + '"><span class="tab">' + title + '</span></a></li>');
                        }
                    }
                }
            })
        }
    }

    function buildSearchParamsForPredictiveText(searchText) {
        var queryString = '';
        var self = this;
        queryString = 'suggest=true&wt=json&suggest.q=' + searchText;
        return queryString;
    }

    $(document).on("blur", ".selected", function () {
        $(this).removeClass("selected");
    });

    $(document).mouseup(function (e) {
        var container = $('#suggestionDiv');

        if (!container.is(e.target) // if the target of the click isn't the container...
            && container.has(e.target).length === 0) // ... nor a descendant of the container
        {
            container.hide();
        }
    });

    $(window).keyup(function (e) {
        $('#suggestionDiv').show(0);
        li = $('.suggest li');
        if (e.keyCode == 40 || e.keyCode == 38) {
            if (e.which === 40) {
                if (liSelected) {
                    liSelected.removeClass('selected');
                    next = liSelected.next();
                    if (next.length > 0) {
                        liSelected = next.addClass('selected');
                        $("#dnn_ctr1336_ViewSearch_txtSolrSearch").val(liSelected[0].childNodes[0].name);
                    } else {
                        liSelected = li.eq(0).addClass('selected');
                        $("#dnn_ctr1336_ViewSearch_txtSolrSearch").val(liSelected[0].childNodes[0].name);
                    }
                } else {
                    liSelected = li.eq(0).addClass('selected');
                    $("#dnn_ctr1336_ViewSearch_txtSolrSearch").val(liSelected[0].childNodes[0].name);
                }
            } else if (e.which === 38) {
                if (liSelected) {
                    liSelected.removeClass('selected');
                    next = liSelected.prev();
                    if (next.length > 0) {
                        liSelected = next.addClass('selected');
                        $("#dnn_ctr1336_ViewSearch_txtSolrSearch").val(liSelected[0].childNodes[0].name);
                    } else {
                        liSelected = li.last().addClass('selected');
                        $("#dnn_ctr1336_ViewSearch_txtSolrSearch").val(liSelected[0].childNodes[0].name);
                    }
                } else {
                    liSelected = li.last().addClass('selected');
                    $("#dnn_ctr1336_ViewSearch_txtSolrSearch").val(liSelected[0].childNodes[0].name);
                }
            }
    }
    else {
        if (self.AJAX_HIT && self.AJAX_HIT.readyState != 4) {
            AJAX_HIT.abort();
        }
        var searchedKeyword = $("#dnn_ctr1336_ViewSearch_txtSolrSearch").val();
            document.getElementById("suggestionDiv").innerHTML = '';
            if (searchedKeyword.length > 3) {
                if (SolrSearchURLAutoComplete) {
                    GetPredictiveData(searchedKeyword);
                }
            }
        }
    });
</script>

</div><!-- End_Module_1336 --></div>
</div>



