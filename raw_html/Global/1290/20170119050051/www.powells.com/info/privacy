        { console.log(err); }
    }
    function FnCloseWindowBlogRoll() {
        try {
            if ($("#hfSCUrl").val() != "")
                top.location.href = $("#hfSCUrl").val();
            else
                top.location.href = top.location.href;
        }
        catch (err)
        { console.log(err); }
    }

    function imgErrorBlogRoll(image, width, height) {
        try {
            image.onerror = "";
            image.src = $("#dnn_ctr3299_BlogRoll_hfBlogRollNoImage").val();
            $(image).height(height).width(width);
            return true;
        }
        catch (err)
        { console.log(err); }
    }

