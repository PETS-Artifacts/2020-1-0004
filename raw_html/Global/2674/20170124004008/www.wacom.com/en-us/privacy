    function searchKeyPress(e) {
        e = e || window.event;
        if (e.keyCode == 13) {
            doSearch();
            return false;
        }
        return true;
    }
    function doSearch() {
        var xurl = location.host;
        var searchValue = document.getElementById("ctl31_tbSearchBox").value;
        var resultValue = encodeURIComponent(searchValue);
        window.location.href = "/en-us/main-search" + "?q=" + resultValue;
    }
</script>
