 <script type="text/javascript" language="javascript" >
           
            function URLOpen()
            {
             var ctrl = document.getElementById('ctl01_siteHeader_drpquickLink');
             var selectedindex = ctrl.selectedIndex;
             var URLlink = ctrl.options[selectedindex].value;
                if (URLlink != "")
                    {
                    window.open(URLlink);
                    }
            }

            function SearchURLOpen()
            {
               
                var ctrl = document.getElementById('ctl01_siteHeader_txtSearch');

                

                var URLlink = 'https://www.google.com/cse?cx=014502244521301309670:tl8mwjbhotw&ie=UTF-8&q=' + ctrl.value;

                if (URLlink != "")
                {
                    window.open(URLlink,'_self');
                }
            }

 	function clickButton(e){   
  
         var evt = e ? e : window.event;  
          if (evt.keyCode == 13){   
 	       SearchURLOpen();
               return false;   

        }   

      }  
  </script>
