<script>
$(document).ready(function(){
if(window.device == "small" || window.device == "x-small"){
$(".menuHighlight").addClass('menuHighlightActive');
$(".menuHighlight #mega-menu-mobile-list").addClass('hide');
}
}).click(function(){
if((window.device == "small" || window.device == "x-small") && !($("#mega-menu-mobile-list").is(':visible'))){
$(".menuHighlight").addClass('menuHighlightActive');
$(".menuHighlight #mega-menu-mobile-list").addClass('hide');
}
});
$(".menuHighlight> a").on('click', function(e){
e.stopPropagation();
if($(".menuHighlight #mega-menu-mobile-list").is(':visible')){
$(".menuHighlight").addClass('menuHighlightActive');
$(".menuHighlight #mega-menu-mobile-list").addClass('hide');
}else{
$(".menuHighlight").removeClass('menuHighlightActive');
$(".menuHighlight #mega-menu-mobile-list").removeClass('hide');
}
});
</script>
