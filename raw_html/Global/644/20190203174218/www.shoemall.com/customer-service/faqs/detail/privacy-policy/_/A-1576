<script language="javascript">
$(document).ready(function(event) {
$(".headerSearchBox").endecaSearchSuggest({
minAutoSuggestInputLength : 3,
autoSuggestServiceUrl : '/cartridges/SearchBox/TaisAutoSuggest.jsp',
collection : "/content/Shared/Auto-Suggest Panels",
searchUrl : '',
refinements : '',
navFilter : "",
containerClass : "suggestions text-left hide",
defaultImage : '/img/no_image_auto_suggest.gif'
});
$(".faqSearchBox").endecaSearchSuggest({
minAutoSuggestInputLength : 3,
autoSuggestServiceUrl : '/cartridges/FaqSearchBox/TaisAutoSuggest.jsp',
collection : "/content/Shared/Auto-Suggest Panels",
searchUrl : '',
refinements : '',
navFilter : "",
containerClass : "suggestions text-left hide",
defaultImage : '/img/no_image_auto_suggest.gif'
});
$('.searchBox').on('keydown', function(e) {
var code = e.keyCode || e.which;
var $c = $(this);
var searchItems = $c.parent().siblings('.dimSearchSuggContainer').find('.searchItems ul');
var selectedOption = searchItems.find('li.selected'), nextSelection;
if (code === 13) {
if ($c.val() != "") {
if (selectedOption.length != 0) {
var $a = selectedOption.find('a');
$c.val($a.text());
window.location.href = $a.attr('href');
} else
$('#searchForm').submit();
} else {
$("#serarchError").removeClass("hidden").html("Please enter a search term");
return false;
}
} else if (code == 40 || code == 38) {
if (code == 40) {
if (selectedOption.length == 0) {
nextSelection = searchItems.children(':first');
} else {
nextSelection = selectedOption.next();
if (nextSelection.length == 0)
nextSelection = selectedOption;
selectedOption.removeClass('selected');
}
} else {
nextSelection = selectedOption.prev();
if (nextSelection.length == 0)
nextSelection = selectedOption;
selectedOption.removeClass('selected');
}
nextSelection.addClass('selected');
}
});
});
</script>

