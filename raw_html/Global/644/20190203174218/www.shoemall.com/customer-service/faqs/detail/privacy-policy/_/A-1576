<script type="text/javascript">
if(!$(".page-content.checkout-page").length){
var url = $("#headerAjaxUrl").val();
var redirectUrl = $("#redirectUrl").val();
var sessionLogoutAttr = $("#sessionLogoutAttr").val();
var timestamp = new Date().getTime();
var fsparam ="";
if($("#fsparam").html() !== undefined){
fsparam = "&"+$("#fsparam").html();
}
var queryString = window.location.search;
if(queryString != ""){
queryString = queryString.substr(1);
}
var queryStringArray = queryString.split("&");
var catalogKeyCode="";
var utm_source="";
var utm_medium="";
var promotionCode="";
for (i = 0; i < queryStringArray.length; i++) {
if(queryStringArray[i].indexOf("catalogKeyCode") >= 0){
catalogKeyCode="&"+queryStringArray[i];
}
if(queryStringArray[i].indexOf("utm_source") >= 0){
utm_source="&"+queryStringArray[i];
}
if(queryStringArray[i].indexOf("utm_medium") >= 0){
utm_medium="&"+queryStringArray[i];
}
if(queryStringArray[i].indexOf("promotionCode") >= 0){
promotionCode="&"+queryStringArray[i];
}
}
jNgApp.loadDynamicContent("#headerContainer",url+"?"+"redirectUrl=" + redirectUrl+"&"+"pageName=" + "FaqDetail"+"&"+"reload=" + false+'&time='+timestamp+fsparam+catalogKeyCode+utm_source+utm_medium+promotionCode+'&logOutParam='+sessionLogoutAttr,function(){
var modalName = $("#headerContainer #modalName").val();
if(modalName == 'emailSignupModal'){
$(window).on("jNgApp:load",function(){showEmailSignup();});
}
if(modalName == 'promoModal'){
$(window).on("jNgApp:load",function(){showPromoModal();});
}
});
}
</script>
