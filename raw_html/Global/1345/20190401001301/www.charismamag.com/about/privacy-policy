	<script>

jQuery(function(jQuery)
{

	window.CM_ShareButtons = { };

	CM_ShareButtons.processShare = function()
	{
		var el = jQuery(this);

		if(el.hasClass('more_button')) return false;

		if (typeof ga === "function")
		{
			var network = el.text();
			
			if(network=='Share') network = 'Facebook';

			var url = el.parents('.shareBtns').data('shareurl');

			ga('send', 'event', 'Social Media share buttons', 'clicked ' + network, url);
		}

		if(el.hasClass('pop'))
		{
			CM_ShareButtons.doPopup(el);
			return false;
		}

		return true;
	}

	CM_ShareButtons.doPopup = function(el)
	{
		
		var width = el.data('width') || 800,
			height = el.data('height') || 500;

		var availWidth = screen.availWidth || 1024,
			availHeight = screen.availHeight || 700;

		// To account for the possible OS task bar
		var heightBuffer = 100;

		// Make sure the dimensions aren't greater than the available screen space
		width = (width <= availWidth) ? width : availWidth;
		height = (height <= availHeight-heightBuffer) ? height : availHeight-heightBuffer;

		// Position the popup to the center of the screen
		var px = Math.floor((availWidth - width) / 2),
			py = Math.floor((availHeight - height) / 2);
		
		// Move the window up slightly
		if(py>50)
		{
			py-=50;
		}
		else
		{
			py = Math.floor(py/2);
		}
		
		var popup = window.open(el.attr('href'), "social",
			"width="+width+",height="+height+
			",left="+px+",top="+py+
			",location=0,menubar=0,toolbar=0,status=0,scrollbars=1,resizable=1");

		if (popup)
		{
			popup.focus();
			if (el.preventDefault) el.preventDefault();
		}

		return !!popup;
	};
	
	jQuery('.shareBtns .more_button').click(function()
	{
		jQuery(this).parents('.shareBtns').children('.canHide').toggleClass('hidden');
		jQuery(this).toggleClass('active');
	});

	jQuery('.shareBtns a.btn-share').on('click', CM_ShareButtons.processShare);

});


function flipboard_flipit()
{
	// This is Flipboard's bookmarklet code
	javascript:void((function(d,w,p,s,r,t,l){t=(w.screenTop||w.screenY)+50;l=(w.screenX||w.screenLeft)+(w.innerWidth||d.documentElement.offsetWidth||0)/2-s/2;w.__flipboard=w.open('https://share.flipboard.com/bookmarklet/popout?v=2&title='+encodeURIComponent(d.title)+'&url='+encodeURIComponent(w.location.href)+'&t='+p+'&utm_medium=bookmarklet'+'&utm_campaign=tools'+'&utm_source='+encodeURIComponent(w.location.href),'__flipboard_flipit','width='+s+',height='+r+',top='+t+',left='+l+',location=no,resizable=yes,status=no,scrollbars=no,personalbar=no,toolbar=no,menubar=no');s=d.createElement('script');s.setAttribute('type','text/javascript');s.setAttribute('src','https://d2jsycj2ly2vqh.cloudfront.net/bookmarklet/js/popout-helper.min.js?t='+p);d.body.appendChild(s);setTimeout(function(){w.__flipboard.focus()},50);})(document,window,(new Date().getTime()),535,565));
}



</script>

	
