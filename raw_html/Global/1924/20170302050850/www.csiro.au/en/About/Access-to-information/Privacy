<script>
    jQuery.noConflict();
    jQuery(document).ready(function ($) {
        var termTemplate = "<span style='font-weight:bold;'>%s</span>";
        var urlauto_21bb59b090a64db68aa9864fcd168527_ = "https://search.csiro.au/suggest?q=_query_&max=10&site=Main&client=csiro_frontend&access=p&format=rich";
        var urlsearch_21bb59b090a64db68aa9864fcd168527_ = "https://search.csiro.au/search?q=_query_&btnG=Google%2BSearch&client=csiro_frontend&output=xml_no_dtd&proxystylesheet=csiro_frontend&proxyreload=0&sort=date%253AD%253AL%253Ad1&wc=200&wc_mc=1&oe=UTF-8&ie=UTF-8&ud=1&exclude_apps=1&site=Main&filter=0&getfields=*&sourcepage={12EF7F1C-3979-4E63-BCD7-72CFA235ADA4}";
        $("#21bb59b090a64db68aa9864fcd168527").click(function () {
            dosearch_21bb59b090a64db68aa9864fcd168527_($(this).closest(".fn-search").find(".search-term").val());
        });
        $("#21bb59b090a64db68aa9864fcd168527-searchbox").keypress(function (e) {
            if (e.which == 13) {
                dosearch_21bb59b090a64db68aa9864fcd168527_($(this).val());
            }
        });
        function dosearch_21bb59b090a64db68aa9864fcd168527_(query) {
            if (query == "") { return false;}
            window.location.href = urlsearch_21bb59b090a64db68aa9864fcd168527_.replace("_query_", query).replace("_meta_", query);
        }
        //**** autocomplete on the search box
        $("#21bb59b090a64db68aa9864fcd168527-searchbox").autocomplete({

            source: function (request, response) {
                $.ajax({
                    dataType: "json",
                    type: 'get',
                    url: "/api/sitecore/AutoComplete/Get",
                    data: { url: urlauto_21bb59b090a64db68aa9864fcd168527_.replace("_query_", request.term) },
                    success: function (data) {
                        response(data.terms);
                    },
                    error: function (data) {
                    }
                });
            },
            open: function (e, ui) {
                var
                    acData = $(this).data('ui-autocomplete'),
                    styledTerm = termTemplate.replace('%s', acData.term);

                acData
                    .menu
                    .element
                    .find('li')
                    .each(function () {
                        var me = $(this);
                        me.html(me.text().replace(acData.term, styledTerm));
                    });
            },
            select: function (event, ui) {
                $(this).closest(".fn-search").find(".search-term").val(ui.item.value);
                dosearch_21bb59b090a64db68aa9864fcd168527_(ui.item.value);
            },
            minLength: 2,
            autoFocus: false
        });

    });

</script>

