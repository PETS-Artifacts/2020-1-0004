//Used in CPE pages
if (jQuery('.top-nav-buttons').length) {
    //Gets the height of the top-nav-buttons with the biggest height, used to apply it to outerHeight of all of top-nav-buttons so height is equal
    var maxHeight = Math.max.apply(null, jQuery(".top-nav-buttons").map(function () {
        return jQuery(this).outerHeight();
    }).get());

    //Uses maxHeight to make outerHeight of each .top-nav-buttons equal
    jQuery(".top-nav-buttons").each(function () {
        jQuery(this).outerHeight(maxHeight);
    });
}
