<script>
function toggle_visibility(id) {
	  var e = document.getElementById(id);
	       if(e.style.display === "block"){
	         document.getElementById(id).style.display = "none";
	         }
	       else{
	        document.getElementById(id).style.display = "block";
	          } 
	    }

</script>
