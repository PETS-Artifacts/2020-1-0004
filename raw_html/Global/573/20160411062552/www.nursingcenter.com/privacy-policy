<script type="text/javascript">

    function GoToSearch() {

        //alert(document.forms.id);

        var v = document.getElementById('__VIEWSTATE');
        if (v)
            v.parentNode.removeChild(v);

        var url = "http://nursingjobs.careers.adicio.com/careers/jobsearch/results";

        document.forms["form"].method = "POST";
        document.forms["form"].target = "_blank";
        document.forms["form"].action = url;
        document.forms["form"].submit();

    }

</script>	   
        
