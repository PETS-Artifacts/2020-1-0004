    myFeaturedFrame.declareVariable('State', '%jobState%');
    if (document.documentElement.clientWidth < 1000) {
        myFeaturedFrame.disableScroll();
    }

    myFeaturedFrame.declareVariable('Url', '%jobLink%');
    myFeaturedFrame.setTemplate('<div class="jobsHolder" onmouseover="this.style.background=\'#cccccc\';myFeaturedFrame.scroll er.stopScroll();" \ onmouseout="this.style.background=\'\';myFeaturedFrame.scroller .startScroll();" \   onclick=window.open(\'%jobLink%\') style="cursor: pointer;">\   <ul><li class="title">%jobTitle%</li>\   <li class="employer">%jobEmployer%</li>\   <li class="location">%jobCity%, %jobState%</li>\   </ul></div>'); myFeaturedFrame.setScrollDelay(30); myFeaturedFrame.outputFromString(xmlString);  </script>

