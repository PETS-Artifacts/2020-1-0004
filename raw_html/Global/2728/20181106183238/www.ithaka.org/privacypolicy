  <script type="text/javascript">
      (function() {
          var isSmallDevice = window.outerWidth < 960,
              mainHeader = document.getElementById("header"),
              mobileWrapper = document.getElementById("block-block-12"),
              mobileIcon = document.querySelector("#block-block-12 .header"),
              mobileMenu = document.querySelector("#block-block-12 .header ul.menu"),
              bannerOriginalPosition = 88,
              mobileMenuOriginalPosition = 133;

          function getPosition(el) {
              var xPos = 0,
                  yPos = 0;

              while (el) {
                  if (el.tagName == "BODY") {
                      // deal with browser quirks with body/window/document and page scroll
                      var xScroll = el.scrollLeft || document.documentElement.scrollLeft;
                      var yScroll = el.scrollTop || document.documentElement.scrollTop;

                      xPos += (el.offsetLeft - xScroll + el.clientLeft);
                      yPos += (el.offsetTop - yScroll + el.clientTop);
                  } else {
                      // for all other non-BODY elements
                      xPos += (el.offsetLeft - el.scrollLeft + el.clientLeft);
                      yPos += (el.offsetTop - el.scrollTop + el.clientTop);
                  }

                  el = el.offsetParent;
              }
              return {
                  x: xPos,
                  y: yPos
              };
          }

          function updatePosition() {
              var bannerPosition = getPosition(document.querySelector("div.gdpr-banner")),
                  currentHeader = parseInt(mainHeader.style.top),
                  updatedPosition = bannerOriginalPosition + bannerPosition.y,
                  mobileMenuUpdatedPosition = mobileMenuOriginalPosition + bannerPosition.y;

              if(currentHeader > 0) {
                  if(updatedPosition < 0) {
                      updatedPosition = 0;
                  }
                  mainHeader.style.top = mobileWrapper.style.top = mobileIcon.style.top = updatedPosition + "px";
                  mobileMenu.style.top = mobileMenuUpdatedPosition + "px";
              } else if(Math.abs(bannerPosition.y) < bannerOriginalPosition) {
                  updatedPosition = currentHeader + Math.abs(bannerPosition.y);

                  if(updatedPosition < 0) {
                      updatedPosition = 0;
                  }

                  mainHeader.style.top = mobileWrapper.style.top = mobileIcon.style.top = updatedPosition + "px";
              }
          }

          function adjustHeader(eventData) {
              var handleClose = function() {
                  mainHeader.style.top = "0";
                  mobileWrapper.style.top = "0";
                  mobileIcon.style.top = "0";
                  mobileMenu.style.top = "45px";
              };

              if(!isSmallDevice) {
                  if(eventData.data.showBanner) {
                      mainHeader.style.top = "88px";
                      mobileWrapper.style.top = "88px";
                      mobileIcon.style.top = "88px";
                      mobileMenu.style.top = "133px";

                      window.addEventListener("scroll", updatePosition, false);
                  } else if(eventData.data.status === "bannerClosed") {
                      handleClose();
                      window.removeEventListener("scroll", updatePosition, false);
                  }
              }
          }


          window.addEventListener("message", adjustHeader);

      })();
  </script>
