                    ele.hide({
                        duration: 250,
                        direction: 'right',
                        complete: function() {
                            var nextBlurb = index == (totalBlurbs-1) ? topBlurbs[0] : topBlurbs[index+1];
                            jQuery(nextBlurb).show({duration:250, direction: 'left'});
                        }
                    });
