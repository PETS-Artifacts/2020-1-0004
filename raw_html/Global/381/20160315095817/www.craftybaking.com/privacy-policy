</form><script type="text/javascript">
$(document).ready(function(){
    $('#ajaxauth_login').on('submit', function(event){
        event.preventDefault();
        $('#ajaxauth_login_error_container').hide();
        $('#ajaxauth_login_loader').show();
        $.post(
            'https://www.craftybaking.com/?ACT=130',
            $('#ajaxauth_login').serialize(),
            function(msg) {
                $('#ajaxauth_login_loader').hide();
                if (msg.indexOf('You are now logged in.') >= 0)
                {
                    
                $('#ajaxauth_login').replaceWith("");
                    redirect_after_login();
                    
                    
                } else {
                    var out = /<ul>[\s\S]*<\/ul>/.exec(msg);
                    $('#ajaxauth_login_error_container').html(''+out);
                    $('#ajaxauth_login_error_container').show();
                }
                return false;
            }
        );
        return false;
    });
});
</script>
						
