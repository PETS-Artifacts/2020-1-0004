<script>
    function findLijitInputBox(lwp_ele) {
        for (var i = 0; i <= lwp_ele.elements.length; i++) {
            element = lwp_ele.elements[i];
            if (element.tagName == 'INPUT' && element.type == 'text') {
                return element;
            }
        }
    }
function lwp_hijack_search() {
    var lwp_hj_form = document.getElementById('searchform');
    var lwp_hj_input = findLijitInputBox(lwp_hj_form);
    lwp_hj_input.value = '';
    lwp_hj_input.onblur = function(){
        if (lwp_hj_input.value == '') {
            lwp_hj_input.value = '';     
        }
        //else {
        //    lwp_hj_input.value = '';
        //}
    };
    lwp_hj_input.onfocus = function(){if (lwp_hj_input.value == 'Lijit Search'){lwp_hj_input.value = '';}};
    lwp_hj_form.onsubmit = lwp_hijack_submit;

    }
function lwp_hijack_submit(e) {
    if (self.lwp_dosearch) {
        var lwp_hj_form = document.getElementById('searchform');
        var lwp_hj_input = findLijitInputBox(lwp_hj_form);
        if (lwp_hj_input.value != 'Lijit Search'){
            lwp_dosearch(lwp_hj_input.value);
        }
        return false
    }
}
var lijitpoll = {
    lwp_interval: 50,
    findEle: function (a, b) {
        if (document.getElementById && setTimeout) {
            var c, lwp_el = document.getElementById(a);
            if (lwp_el) {
                clearTimeout(c);
                b()
            } else {
                c = setTimeout(function () {
                    lijitpoll.findEle(a, b)
                },
                lijitpoll.lwp_interval)
            }
        }
    }
};
lijitpoll.findEle('searchform', lwp_hijack_search);
     </script>
