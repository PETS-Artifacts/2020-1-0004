<script>

function get_browser(){
    var N=navigator.appName, ua=navigator.userAgent, tem;
    var M=ua.match(/(opera|chrome|safari|firefox|msie)\/?\s*(\.?\d+(\.\d+)*)/i);
    if(M && (tem= ua.match(/version\/([\.\d]+)/i))!= null) M[2]= tem[1];
    M=M? [M[1], M[2]]: [N, navigator.appVersion, '-?'];
    return M[0];
    }
function get_browser_version(){
    var N=navigator.appName, ua=navigator.userAgent, tem;
    var M=ua.match(/(opera|chrome|safari|firefox|msie)\/?\s*(\.?\d+(\.\d+)*)/i);
    if(M && (tem= ua.match(/version\/([\.\d]+)/i))!= null) M[2]= tem[1];
    M=M? [M[1], M[2]]: [N, navigator.appVersion, '-?'];
    return M[1];
    }
    
    var browser=get_browser();
	var browser_version=get_browser_version();
	var browser_number=browser_version.substr(0,1);
	
/* 	alert(browser + " " + browser_version + " which is version " + browser_number); */
	
		if (location.pathname.indexOf("SupportedBrowsers") == -1){
			if(browser == "MSIE") {
				if((browser_number > 3) && (browser_number < 8)) {
					window.location.replace("http://www.yogaalliance.org/SupportedBrowsers");
				}
				else if ((browser_number == 8) || (browser_number == 9)) {
					document.getElementById("ya-browser-test").style.display="block";
				}
			}
			else if(browser == "Safari") {
				if ((browser_number == 3) || (browser_number == 4)) {
					window.location.replace("http://www.yogaalliance.org/SupportedBrowsers");
				}
				else if(browser_number == 5) {
					document.getElementById("ya-browser-test").style.display="block";
				}
			}
		}
		else {
			document.getElementById("ya-browser-test").style.display="block";
			document.getElementById("ya-browser-test-links").style.display="none";
			document.getElementById("ya-browser-test-nolinks").style.display="block";	
		}
	
</script>


	

