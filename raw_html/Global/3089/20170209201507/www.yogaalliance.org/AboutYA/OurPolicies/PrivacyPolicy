<script>
    $(document).ready(function () {
        var loginBox = $('.ya-loginbox');
        var clickedInside = true;
        var loginButton = $('#loginLink');

        if (loginButton.length > 0) {
            var loginContainer = $('#LoginContainer');

            /**login-logout status**/
            var idTopHeaderContainer = $('#id-top-header-container');
            idTopHeaderContainer.addClass("ya-logged-in").removeClass("ya-logged-out");
            /**end login-logout status**/

            var emailField = loginContainer.find('.LoginEmail');

            loginButton.mouseup(function (login) {
                loginBox.toggle("slide", { direction: "up" }, 500); // TODO: Implement CSS transitions? Will be a lot faster than this...
                emailField.focus();
            });

            $(this).mousedown(function (clicked) {
                if (loginContainer.has(clicked.target).length > 0) {
                    if (loginBox.is(':visible')) {
                        clickedInside = true;
                        return;
                    }
                }
                clickedInside = false;
            });

            $(this).mouseup(function (clicked) {
                if (loginContainer.has(clicked.target).length == 0) {
                    if (loginBox.is(':visible') && !clickedInside) {
                        loginBox.hide("slide", { direction: "up" }, 500); // TODO: Implement CSS transitions? Will be a lot faster than this...
                    }
                }
            });

            loginBox.find('.ya-tagline').show();
            loginBox.find('#HeaderTagLine').hide();

            function setLoginBoxStyle() {
                loginBox.addClass('ya-popuploginbox');
                loginBox.css({
                    //'position': 'absolute',
                    //'top': (loginContainer.height() + loginContainer.position().top + 10) + 'px',
                    //'left': (loginContainer.position().left - 60) + 'px'
                });
            }

            setLoginBoxStyle();

            $(window).on('resize', function () {
                setLoginBoxStyle();
            });
        } else {
            var loginContainer = $('#LoginContainer');

            /**login-logout status**/
            var idTopHeaderContainer = $('#id-top-header-container');
            idTopHeaderContainer.removeClass("ya-logged-in").addClass("ya-logged-out");
            /**end login-logout status**/

            $('.ya-create-account').show();

            loginBox.show();
            loginBox.css('position', 'relative');
            loginBox.find('.ya-logo').hide();

            // Make this just a header, instead of the DIV (hide/show)
            loginBox.find('.ya-tagline').hide();
            loginBox.find('#HeaderTagLine').show();
        }

        var errorBox = $('#LoginBox').find($('.alert-error'));
        var errorLabel = errorBox.find($('.LoginError'));

        if (errorLabel.text().length == 0) {
            errorBox.hide();
        } else {
            errorBox.show();
            loginBox.show();
        }
    });
</script>

