                  var cartNumItems = "CartNumItems=";
                  var cookieArray = document.cookie.split(';');
                  var foundCart = false;
                  var filledCart = false;
                  for(var i = 0; i < cookieArray.length; i++){
                      var cookieItem = cookieArray[i];
                      while (cookieItem.charAt(0)==' ') cookieItem = cookieItem.substring(1);
                      if (cookieItem.indexOf(cartNumItems) == 0) {
                        foundCart = true;
                        var numItems = cookieItem.substring(cartNumItems.length, cookieItem.length);
                        if(numItems > 1){
                          document.write("View Cart (" + numItems + " Items)");
                          filledCart = true;
                        } else if(numItems == 1){
                          document.write("View Cart (1 Item)");
                          filledCart = true;
                        } else document.write("View Cart (0 Items)");
                        break;
                      }
                  }
                  if(!foundCart){
                    document.write("View Cart (0 Items)");
                  }
                    
