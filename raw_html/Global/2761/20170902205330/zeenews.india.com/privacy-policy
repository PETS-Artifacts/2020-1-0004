<script type="text/javascript">
console.log(window.innerWidth + "width");
if(window.innerWidth < 768){
	// checking google bot begins
	var botPattern = "(googlebot\/|Googlebot-Mobile|Googlebot-Image|Google favicon)";
	var re = new RegExp(botPattern, 'i');
	//var userAgent = 'Googlebot/2.1 (+http://www.googlebot.com/bot.html)';
	var userAgent = navigator.userAgent;
	var bot=false;
	if (re.test(userAgent)) {
		bot=true;
	}
	// checking google bot ends

	if(typeof(Storage) !== "undefined" && bot==false) {
		//console.log('if');
		if (sessionStorage.pageInterCount) {
			sessionStorage.pageInterCount = Number(sessionStorage.pageInterCount)+1;
		} else {
			sessionStorage.pageInterCount = 1;
		}
	}
	if(sessionStorage.pageInterCount == 2){		
		document.getElementById("iwp-intersetial").style.display = "block";
		document.getElementById("iwp-intersetial").style.width = window.innerWidth+"px";
		document.getElementById("frameIntersetial").src = "http://zeenews.india.com/ads/intersetial.html";
		//document.getElementById("frameIntersetial").src = "adframeIntersetial.html";
	}
}
    jQuery(document).on('click','.interClos',function(){
		$('#iwp-intersetial').remove();
	});
	if(bot==false){
		var IntTm_limit = 10;
		jQuery('.iwp-intTimer span').text(IntTm_limit);
		var iwpIntTm = null;
		iwpIntTm = setInterval(function(){
			if(IntTm_limit < 0){
				clearInterval(iwpIntTm);
				//console.log('if');
				jQuery('.iwp-intTimer span').text(IntTm_limit);
				$('#iwp-intersetial').hide();
			} else {
				jQuery('.iwp-intTimer span').text(IntTm_limit);
				//console.log('else');
			}
			IntTm_limit--;
		},1000);
	}

</script>
