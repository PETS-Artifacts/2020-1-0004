    UA.then(function(sdk) {
		var uaState;

		if (!sdk.isSupported) {
			uaState = "Unsupported";
		}
		else if (!sdk.canRegister) {
			uaState = "Blocked";
		}
		else if (sdk.channel.optedIn) {
			uaState = "OptedIn";
		}
		else if (!sdk.channel.optedIn) {
			uaState = "NoSelection";
		}
		else {
			uaState = "Unknown";
		}

		$.ajax({
		    type: "POST",
		    url: "/audiobook/logUAState",
		    data: "uaState="+uaState
		});
	}).catch(function(err) {
		$.ajax({
		    type: "POST",
		    url: "/audiobook/logUAState",
		    data: "uaState=Unsupported"
		});
	});

</script>


