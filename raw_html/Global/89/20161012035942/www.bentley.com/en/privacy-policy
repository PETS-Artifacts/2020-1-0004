                        for (i = 1; i < temp.length; i++) {
                            pagen += temp[i];
                            if (i != temp.length - 1) {
                                pagen += ":";
                            }
                        }
                        if (actualPageName != "") {
                            pagen += ":" + actualPageName;
                        }
                        s.pageName = pagen;
