    /* You may give each page an identifying name, server, and channel on
        the next lines. */
    var url = document.location.pathname.toLowerCase().replace(/%20/g, "+"); // this is the file structure + the file name

    var host = window.location.hostname.toLowerCase();
    var lastChar = "";
    var actualPageName = "";

    if (url) {
        var temp = s.split(url, '/');//temp[0] will be ""

        if (temp.length > 0) {

            s.prop2 = "bentley";//top channel

            if (temp.length == 1) {//has to be web root www.bentley.com/ 
                //s.prop1 = temp[1]; //region
                s.prop3 = "home";
                s.prop4 = "home";
                s.prop5 = "home";
                s.prop6 = "home";
                s.prop7 = "home";
                s.channel = "home";

                s.pageName = s.prop1;

            }
            else if (temp.length == 2) {//has to be region root www.bentley.com/en/, etc.
                s.prop1 = temp[1]; //region
                s.prop3 = temp[1];
                s.prop4 = temp[1];
                s.prop5 = temp[1];
                s.prop6 = temp[1];
                s.prop7 = temp[1];
                s.channel = temp[1];

                s.pageName = s.prop1;
                
            }
            else {

                    if (temp[1] && temp[2]) {

                        s.channel = temp[1];
                        
