                                    s.prop5 = s.prop4;
                                    s.prop6 = s.prop4;
                                    s.prop7 = s.prop4;
                                }
                            }
                            else {
                                s.prop4 = s.prop3;
                                s.prop5 = s.prop3;
                                s.prop6 = s.prop3;
                                s.prop7 = s.prop3;
                            }
                        }
                        else {
                            s.prop3 = s.channel;
                            s.prop4 = s.channel;
                            s.prop5 = s.channel;
                            s.prop6 = s.channel;
                            s.prop7 = s.channel;
                        }
                        

                        var i = 1;
