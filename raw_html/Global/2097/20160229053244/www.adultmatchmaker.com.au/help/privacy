<script type="text/javascript">
	
					function mainmenu() {
						$('.nav-main ul').css({ display: 'none' }); // Opera Fix
						$('.nav-main li').hover(
							function () { $(this).find('ul:first').css({ visibility: 'visible', display: 'none'}).show(150).css({overflow: 'visible'}); }, 
							function () { $(this).find('ul:first').css({ visibility: 'hidden'}); }
                        );
					}

					$(mainmenu);
	$(function() {
		$('[data-default-focus=true]:visible').focus();
		$('[data-aside-url]').hide().delay(2000).each(function() {
			var $div = $(this);
			$div.load($div.data("aside-url"), function() { $div.fadeIn("slow"); });
		});
	});
	if ($.validator && $.validator.unobtrusive) {
		$.validator.unobtrusive.adapters.addBool("mandatory", "required");
	}

	
</script>






    
	
	<script type="text/javascript">

		$(function () {
			$('.mobile-nav').click(function () {
				toggleNav();
			});
		});

		function toggleNav() {
		    $('#main-nav').toggleClass('show-nav');		    
		}

		$(document).keyup(function (e) {
			if (e.keyCode == 27) {
			    if ($('#main-nav').hasClass('show-nav')) {
					toggleNav();
				}
			}
		});
	</script>

