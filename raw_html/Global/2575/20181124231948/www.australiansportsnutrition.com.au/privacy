		'jquery'],
		function($){
			$('.mobmenu-list .dropdownmenu').on('click', function(e){
				e.preventDefault();
				if($(this).parent('li').hasClass('active'))
					$(this).parent('li').removeClass("active");
				else
					$(this).parent('li').addClass("active");
			});
			$('.mobmenu-list .submenu').on('click', function(e){
				e.preventDefault();
				var targetMenu = $(this).attr("data-target-menu");
				if(targetMenu){
					$("#"+targetMenu).addClass("open");
				}
			});
			$('.mobile-sub-menu .btn-menu-back').on('click', function(e){
				e.preventDefault();
				$(this).parent(".mobile-sub-menu").removeClass("open");

			});
		}
	);</script>    </div>
