    var screenSize = jQuery(window).width();
    if(screenSize <= '880') {
        jQuery("ul.megamenu a").removeClass("active");
        jQuery("ul.megamenu a").click(function(e){
            e.preventDefault();
            var url = jQuery(this).attr("href");
            jQuery(this).addClass("active");
            setTimeout(() => {
                window.location = url;
            }, 200);
        });
    }
