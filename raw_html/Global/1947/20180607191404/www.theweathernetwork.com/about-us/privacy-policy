function voteHandlertcm12102177(){	var submitResult = true;	var voteStatus = readCookie('Pelmtcm12102177');	dataLayer.push({	'event': "eventTracker",	'eventCategory' : window._config.ga_product,	'eventAction': 'clicks',	'eventLabel': "voteButton" });	if(voteStatus=='voted') {	alert("You have already voted on this poll. Multiple votes are not allowed.");	submitResult = false;	}else{	$("form#quick_poll_tcm12102177").attr("action", "https://www.theweathernetwork.com/poll/result/if-you-could-teach-your-dog-one-trick-what-would-it-be/102177/");	$("form#quick_poll_tcm12102177").append('<input type="hidden" value="1" name="pollAction">');	createCookie('Pelmtcm12102177','voted', 1);	$("form#quick_poll_tcm12102177").submit();	}	return submitResult;
