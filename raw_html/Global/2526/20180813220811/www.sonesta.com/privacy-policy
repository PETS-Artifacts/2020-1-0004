  <script type="text/javascript">
    if(sessionStorage.getItem('foutFontsLoaded')) document.write('<link rel="stylesheet" href="https://cloud.typography.com/6554094/7880952/css/fonts.css" type="text/css">');
    else loadCSS('https://cloud.typography.com/6554094/7880952/css/fonts.css');
    new FontFaceObserver('Gotham A').load(null, 10000)
    .then(function () {
      // Notify the document that we have our custom fonts
      document.documentElement.className += " fonts-loaded";
      // Save a cookie which tells the browser it has
      // seen the fonts before and should load them
      // from the browser cache
      sessionStorage.setItem('foutFontsLoaded', 'true')
    });
  </script>
