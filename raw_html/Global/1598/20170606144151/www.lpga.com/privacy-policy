        });
    });
</script>
<!-- END : /global/rail.html-->
	</div>
    <!-- END : /global/rail.html-->
    
    <!-- For sponsor modules -->
<script>
	require(["modules/sponsorsSlider"], function(sponsorsSlider) {
		if ($('.sponsors-slider').length) {
			$('.sponsors-slider').each(function() {
				return new sponsorsSlider({
					element: $(this)
				});
			});
		}
	});
</script>

	

