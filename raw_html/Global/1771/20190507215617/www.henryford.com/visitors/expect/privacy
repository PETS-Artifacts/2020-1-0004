<!-- TODO: don't know how to handle this other way (in tabs plugin) -->
<script>
    $(".main-nav .holder .container .accordion li a.redirect-holder").each(function() {
        var $this = $(this);
        var idx = $this.data("index");
        var href = $this[0].href;
        var a = $(".nav-container li a.redirect-tabitem[data-index='" + idx + "']");
        if (a.length && href) {
            a.on("click", function(e) {
                e.preventDefault();
                e.stopPropagation();
                window.location.href = href;
            });
        }
    });
</script>

