        <div class="add_maincontent_padding" >
            <h1>Privacy Policy</h1>
            <script type="text/javascript">
                $('#breadcrumb_dynamic a:last-child').css("font-weight", "bold");
                $('#breadcrumb_dynamic a:last-child').css("color", "#000");
