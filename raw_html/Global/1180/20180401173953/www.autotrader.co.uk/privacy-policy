        function matchesPolyfill(ElementPrototype) {
            ElementPrototype.matches = ElementPrototype.matches || ElementPrototype.matchesSelector ||
            ElementPrototype.mozMatchesSelector ||
            ElementPrototype.msMatchesSelector ||
            ElementPrototype.oMatchesSelector ||
            ElementPrototype.webkitMatchesSelector ||
            function (selector) {
                var node = this, nodes = (node.parentNode || node.document).querySelectorAll(selector), i = -1;
                while (nodes[++i] && nodes[i] != node);return !!nodes[i];
            }
        }
        matchesPolyfill(Element.prototype);


        window.jQuery = window.jQuery  || function(elem) {
            var selectedEl, elLength;

            if(typeof(elem) === 'string') {
                selectedEl = document.getElementById(elem.replace('#', ''));
                elLength = selectedEl ? 1 : 0;
            } else {
                selectedEl = elem;
                elLength = 1;
            }

            function setAttribute(attrName, attrValue) {
                selectedEl.setAttribute(attrName, attrValue);
            }

            function getAttribute(attrName) {
                return selectedEl.getAttribute(attrName);
            }

            return {
                on : function(eventType, cssMatcher, handler){
                        elem.addEventListener(eventType, function(event){
                            if(event.target.matches(cssMatcher)) {
                                handler(event);
                            }
                      }, true);
                },

                attr: function (attrName, attrValue) {
                    switch(arguments.length) {
                        case 1: return getAttribute(attrName);
                        case 2: setAttribute(attrName, attrValue);
                                break;
                    }
                },

                length: elLength
            };
        };

        window.$ = window.$ || function(elem) {
                return {attr : function(attribute){
                    return elem.getAttribute(attribute);
                }};
            };


        function cookieValue(name) {
            try {
                return (document.cookie.match('(^|[^a-zA-Z])%s=(.+?)(;|&|$)'.replace('%s', name)) || [])[2] || null;
            } catch (err) {return '';}
        }
    function addBaseDataLayerValues(dataLayer) {
        if(!dataLayer) {
            dataLayer = {"platform":"mobile-web","environment":"www"};
        }
        dataLayer.urs_id = cookieValue('URS_DATABASE_ID');
        dataLayer.logged_in_status = dataLayer.urs_id ? 'signed-in' : 'not-signed-in';
        dataLayer.territory = (cookieValue('postcode') || '').toUpperCase().indexOf('BT') === 0 ? 'ni' : 'uk';
        dataLayer.social_connection = cookieValue('PROVIDER');
        var abTestCookieValue = cookieValue('abTestGroups');
        if (abTestCookieValue) {
            abTestCookieValue = abTestCookieValue.replace('\"','').replace('\"','');
            dataLayer.experiment_variant = abTestCookieValue;
        }
        return dataLayer;
    }
    utag_data = {"platform":"mobile-web","environment":"www"};

    var connection = navigator.connection || navigator.mozConnection || navigator.webkitConnection;
    if (connection) {
        if (connection.type) {
            window.utag_data.network_connection_type = connection.type
        }

        if (connection.downlinkMax) {
            window.utag_data.network_connection_downlinkMax = connection.downlinkMax
        }
    }

    addBaseDataLayerValues(utag_data);
