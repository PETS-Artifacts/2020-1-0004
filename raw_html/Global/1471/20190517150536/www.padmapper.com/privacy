    if (!window.PerformanceObserver) {
      window.PerformanceObserver = function() {
        this.observe = this.disconnect = this.takeRecords = function() {};
      }
    }
    var observer = new window.PerformanceObserver(function (list) {
      var entries = list.getEntries();
      for (var count = 0, l = entries.length; count < l; count++) {
        // "name" will be either 'first-paint' or 'first-contentful-paint' for entryType paint.
        var entry = entries[count];
        var entryType = entry.entryType || '';
        var metricName = entry.name || '';
        var time = Math.round(entry.startTime + entry.duration);

        window && window.ga && window.ga('send', 'event', {
          eventCategory: 'Performance Metrics',
          eventAction: entryType + ": " + metricName,
          eventValue: time,
          eventLabel: JSON.stringify(entry.attribution),
          nonInteraction: true,
        });
      }
    });
    try {
      observer.observe({ entryTypes: ['longtask', 'paint'] });
    } catch (e) {
      if (mode === 'dev') {
        console.log('Could not observe with Performance Observer:', e);
      }
    }
