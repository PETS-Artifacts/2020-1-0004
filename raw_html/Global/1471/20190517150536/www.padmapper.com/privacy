          <script nonce="8ec1c97c-3b9b-434b-aa59-84f0b467cbe2">
          !function() {
            "use strict";
            var deferredStylesNode = document.getElementById("deferred-styles");
            var tmp = document.createElement("div");
            tmp.innerHTML = deferredStylesNode.textContent;
            deferredStylesNode.parentElement.removeChild(deferredStylesNode);
            var loadDeferredStyles = function() {
              var criticalStylesNode = document.getElementById("critical-styles");
              var fragment = document.createDocumentFragment();
              while (tmp.firstChild) fragment.appendChild(tmp.firstChild);
              criticalStylesNode.parentNode.insertBefore(fragment, criticalStylesNode);
            };
            var bundles = Array.prototype.slice.call(
              document.querySelectorAll("link[rel=preload][data-chunk][as=script]")
            );
            var loadBundles = function() {
              var fragment = document.createDocumentFragment();
              bundles.forEach(function(link) {
                var bundle = document.createElement("script");
                bundle.src = link.getAttribute("href");
                bundle.setAttribute("data-chunk", link.getAttribute("data-chunk"));
                bundle.crossOrigin = "anonymous";
                bundle.setAttribute("nonce", "8ec1c97c-3b9b-434b-aa59-84f0b467cbe2");
                fragment.appendChild(bundle);
              });
              document.body.appendChild(fragment);
            };
            var loadPolyfill = function() {
              var polyfill = document.createElement("script");
              polyfill.src = "https://d214hhm15p4t1d.cloudfront.net/9_13_22/p/n/js/polyfill." + whichPolyfill() + ".min.js";
              polyfill.crossOrigin = "anonymous";
              polyfill.setAttribute("nonce", "8ec1c97c-3b9b-434b-aa59-84f0b467cbe2");
              polyfill.onload = function() {
                window.__polyfilled = true;
                loadBundles();
              };
              polyfill.onerror = function(error) {
                console.error("Polyfill load error!", error);
              };
              document.body.appendChild(polyfill);
            };
            document.addEventListener("DOMContentLoaded", function() {
              var raf =
                window.requestAnimationFrame ||
                window.mozRequestAnimationFrame ||
                window.webkitRequestAnimationFrame ||
                window.setTimeout;
              raf(function() { loadDeferredStyles(); });
              raf(function() { loadPolyfill(); });
            });
          }();
          window.onload = function() {
            window.__loaded = true;
          };
          </script>
          
          <script nonce="8ec1c97c-3b9b-434b-aa59-84f0b467cbe2">
            if (!window.performance) {
              window.performance = {
                clearMarks: function() {},
                mark: function() {},
                measure: function() {},
                now: function() { return Date.now(); }
              };
            }
            var targets = document ? document.querySelectorAll('input, button, [role=button]') : [];
            var onClick = function(event) {
              if (event && event.target) {
                const lag = window.performance.now() - event.timeStamp;
                // The RAIL performance model mentions that for a user interface to feel smooth,
                // it should respond within 100 ms of user input
                if (lag > 100) {
                  window && window.ga && window.ga('send', 'event', {
                    eventCategory: 'Performance Metrics',
                    eventAction: 'input-latency',
                    eventLabel: event.target.className + ':click',
                    eventValue: Math.round(lag),
                    nonInteraction: true,
                  });
                }
                for (var count = 0, l = targets.length; count < l; count++) {
                  targets[count].removeEventListener('click', onClick);
                }
              }
            }
            for (var count = 0, l = targets.length; count < l; count++) {
              targets[count].addEventListener('click', onClick);
            }
          </script>
        
        
